      <script>
        $(function () {
          $.ajax({
            url: 'http://kaoshi.edu.sina.com.cn/college/?s=api2015&a=get_college_by_keyword',
            dataType: 'jsonp'
          }).always(function (data) {
            if (data.msg == 'ok') {
              var list = data.data
              var show_list = []

              $('#zyt_v2_form1').on('submit', function () {
                if (show_list.length == 0) {
                  alert('请输入正确的学校名称')
                  return false
                }
                $('#zyt_v2_ipt_id').val(show_list[0].id)
                $('#zyt_v2_ipt').val('').trigger('input')
                return true
              })
              $('#zyt_v2_sug_container').on('click', 'li', function () {
                $('#zyt_v2_ipt_id').val($(this).data('id'))
                $('#zyt_v2_ipt').val($(this).text()).trigger('input').focus()
                $('#zyt_v2_sug').hide()
              })
              $('#zyt_v2_ipt').val('').on('input', function () {
                var val = this.value
                show_list = []
                $('#zyt_v2_sug').hide()
                $('#zyt_v2_sug_container').empty()
                if (val == '') { return }
                for (var i = 0; i < list.length; i++) {
                  var item = list[i]
                  var keyword = item.keyword || item.name
                  if (keyword.indexOf(val) > -1) {
                    show_list.push(item)
                    $('#zyt_v2_sug_container').append('<li title="' + item.name + '" data-id="' + item.id + '">' + item.name + '</li>')
                  }
                }
                show_list.length > 0 && $('#zyt_v2_sug').show()
              }).trigger('input')
            }
          })
        })
      </script>
