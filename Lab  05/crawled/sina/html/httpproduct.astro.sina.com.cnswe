			var userInfo = '';
			function Archives(el,objIndex){
				this.objIndex = objIndex;
				this.handler = el;
				this.list = el ? u.nextElementSibling(el) : null;
				this.uForm = u.parent(el,'form');
				this.uName = u.byClass('u-name',this.uForm,'input')[objIndex];
				this.uLastName = u.byClass('u-lastname',this.uForm,'input')[0];
				this.uFirstName = u.byClass('u-firstname',this.uForm,'input')[0];
				this.uWarning = u.byClass('warning-text',this.uForm,'span')[0];
				this.showStatus = false;
				this.init();
			}
			Archives.prototype.init = function(){
				var that = this;

				function render(data){
					var fragment = document.createDocumentFragment(),len = len=data.length;
					if(!len){that.handler.style.display = 'none';}
					for(var i=0;i<len;i++){
						var newNode = document.createElement('span');
						newNode.setAttribute('data-index',i);
						newNode.setAttribute('title',data[i].show_name);
						newNode.innerHTML = data[i].show_name;
						fragment.appendChild(newNode);
					}
					u.emptyHtml(that.list).appendChild(fragment);
				}
				if(!hasGetUserInfo){
					hasGetUserInfo = true;
					net.getCopyInfo('',function(d){
						if(d.status === 0){
							usersInfo = d.data;
							render(usersInfo);
						}
					});
				}else{
					var infoInterval = setInterval(function(){
						if(usersInfo){
							render(usersInfo);
							clearInterval(infoInterval);
						}
					},100);
				}
				u.addEvent(this.handler,'click',function(){
					if (loginLayer && !usersInfo) {
						if(u.isMobile()){
							window.open('https://passport.sina.cn/signin/signin?entry=wapsso&vt=4&r=&revalid=1','_self');
						}else{
							loginLayer.show();
						}
					} else {
						if (!that.showStatus){
							that.show();
						}else{
							that.hide();
						}
					}

				});
				u.addEvent(this.list,'click',function(e){
					var e = e || window.event;
					var src = e.target || e.srcElement;
					if(src && src.nodeName.toUpperCase() === 'SPAN'){
						that.selectInfo(+src.getAttribute('data-index'));
						that.hide();
					}
				});
			}
			Archives.prototype.updateNode = function(data,index){
				var that = this,
					existNode = u.byAtrName('data-index',index,that.list,'span')[0];
				that.handler.style.display = '';

				if (existNode){
					existNode.title = data.show_name;
					existNode.innerHTML = data.show_name;
				}else{
					var newNode = document.createElement('span');
					newNode.setAttribute('data-index',index);
					newNode.setAttribute('title',data.show_name);
					newNode.innerHTML = data.show_name;
					that.list.appendChild(newNode);
				}
			}
			Archives.prototype.show = function(){
				this.list.style.display = 'block';
				this.showStatus = true;
			}
			Archives.prototype.hide = function(){
				this.list.style.display = 'none';
				this.showStatus = false;
			}
			Archives.prototype.selectInfo = function(index){

				var data = usersInfo[index],
					formIndex = this.objIndex,
					hour = data.birthtime && data.birthtime.slice(0,2),
					minute = data.birthtime && data.birthtime.slice(3,5),
					hourIndex = +hour,
					formRadio = formRadioArr[this.objIndex],
					minuteIndex = +minute,
					chineseTimeIndex = hourIndex === 23 ? 0 : Math.ceil(hourIndex/2),
					thisDate = new Date(),
					formatDate = thisDate.getFullYear() + '-' + (thisDate.getMonth() + 1) + '-' + thisDate.getDate();
				this.uForm.setAttribute('data-id',data.id);
				if(this.uName){
					this.uName.value = data.show_name;
				}
				if(this.uLastName){
					this.uLastName.value = data.last_name;
				}
				if(this.uFirstName){
					this.uFirstName.value = data.first_name;
				}
				if(chinesetimeIndexArr[formIndex].length){
					formSelectArr[formIndex].trigger(chinesetimeIndexArr[formIndex][0],chineseTimeIndex);
				}else if(hourIndexArr[formIndex].length){
					formSelectArr[formIndex].trigger(hourIndexArr[formIndex][0],hourIndex);
					formSelectArr[formIndex].trigger(minuteIndexArr[formIndex][0],minuteIndex);
				}
				if (formRadio.el.length && data.gender){
					if(+data.gender){
						formRadio.trigger(0,1);
					}else{
						formRadio.trigger(0,0);
					}
				}
				while(localSelectArr.length>0){
					var ls=localSelectArr.pop();
					delete ls;
				}
				while(dateSelectArr.length>0){
					var ds = dateSelectArr.pop();
					delete ds;
				}

				for(var _i=0,_len=dateIndexArr[formIndex].length;_i<_len;_i++){
					dateSelectArr.push(new f.dateSelect({
						self : formSelectArr[formIndex],
						index : dateIndexArr[formIndex][_i],
						begin : 1910,
						end : 2040,
						date : data.birtharray[0] != 0 ? data.birthday : formatDate,
						before : false
					}));
				}
				for(var _i=0,_len=localIndexArr[formIndex].length;_i<_len;_i++){
					localSelectArr.push(new f.localSelect({
						self : formSelectArr[formIndex],
						index : localIndexArr[formIndex][_i],
						code : data.loc_code || 'CH010100'
					}));
