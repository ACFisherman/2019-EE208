//上一步
var last_b = false;
$('.popbox .ppre').click(function () {
	$(this).hide();
	var father = $('#'+$(this).attr('data-id'));
	father.parent().find('.tiptext').text('请选择您感兴趣的栏目');
	father.parent().find('.nnks').text('下一步');
	father.parent().find('.item').show();
	$('.popbox .item').click(firstselect);
	last_b = false;
});

//点击【下一步】
$('.popbox .nnks').click(function () {
	last_b = true;
	var self = $(this);
	$('.popbox .ppre').show();//显示上一步
	var result_arr = [];//存放结果
	var father = $('#'+$(this).attr('data-id'));
	father.parent().find('.tiptext').text('您可以拖拽调整显示顺序');
	var title = $('#'+$(this).attr('data-titleid'));
	//console.log($(this).attr('data-titleid'),title);
	//确认设置
	//console.log($(this).text());
	if($(this).text()=='确定'){

		//提取数据
		father.find('.item.cur').each(function(index,el){
			//console.log(el);
			var $this = $(el);
			result_arr.push([$this.attr('data-index'),$this.text()]);
		});
		//console.table(result_arr);
		var suoyin = self.attr('data-titleid');
		storemarks(suoyin,result_arr);
		fillmark(title,result_arr);
		hide_popbox();
	}
	father.find('.item').unbind('click');
	$(this).text('确定');
	father.find('.item:not(.cur)').hide();
	//确定后可以移动啦
	var drag_start = {x:0,y:0};//存放相对位置
	var draging = false;
	var drag_obj;
	var target_obj;
	var itemspos = father.offset();
	var count = father.find('.item.cur').size();
	var index = 0;
	//console.log(itemspos);
	var size = {w:father.find('.item.cur').outerWidth()+19,h:father.find('.item.cur').outerHeight()+19};
	//console.log(size);
	$('.popbox .item.cur').on('mousedown', function (e) {
		//console.log(last_b);
		if(!last_b) return;
		draging = true;
		drag_start.x = e.pageX;
		drag_start.y = e.pageY;
		/*if(self.text()=='确定'){
			draging = true;
		}*/
		drag_obj = $(this);
		target_obj = false;
		index = $(this).index();
	});

	$(document).on('mousemove', function (e) {
		if(draging){
			var left = e.pageX - drag_start.x;
			var top = e.pageY - drag_start.y;
			drag_obj.css({top:top,left:left,'z-index':99});
			//console.log(e.pageX,itemspos.left,size.w);
			var n = (e.pageX - itemspos.left)/size.w;
			//console.log(n);
			var nx =(e.pageY - itemspos.top)/size.h;
			//console.log(parseInt(n),parseInt(nx));
			var num = parseInt(n)+5*Math.floor(nx);
			//console.log(num);
			if(num-1<0) num=0;
			if(num-1>count) num=count-1;
			//console.log(father.find('.item.cur'));
			target_obj = father.find('.item.cur').eq(num);
			//console.log(num,target_obj.text());
		}
	});

	$(document).on('mouseup', function (e) {
		//console.log(draging);
		if(!draging) return;
		draging = false;
		drag_obj.css({top:0,left:0,'z-index':0});
		if(!target_obj) return;
		if(target_obj.text()!=drag_obj.text()){
			//drag_obj.insertBefore(target_obj);
			replaceDom(father.find('.item.cur'),drag_obj,target_obj);
		}
		//target_obj.remove();
		function replaceDom(list,a,b){
			var copya = a.clone(true);
			var copyb = b.clone(true);
			//console.log(a.text(), b.text());
			copya.insertBefore(b); b.remove();
			copyb.insertBefore(a); a.remove();
		}
	});
});

//获取和设置用户设置
function get_set_with_uid(data,set){
	if(!data['id']){return false}
	$.ajax({
		type : "GET",
		async : true,
		url : '//api.g.sina.com.cn/pgame/ad_data_statistics/pc_store',
		data :data,
		dataType: 'jsonp',
		success : function(d) {
			if(set) return;
			if(d&&d.result){
				fillwithmark(d['code']);
			}else if(d&&d['code']=='0003'){
				fillwithmark(feed_item);
			}else{
				//填充默认标签
				fillwithmark(feed_item);
			}
		},
		error : function() {
			console.log("用户设置异常！");
			if(set) return;
			//填充默认标签
			fillwithmark(feed_item);
		}
	});
}


//点击右侧导航，跳转
$('.right_nav a').click(function (e) {
	e.preventDefault();
	var id = $(this).attr('href');

	if(id=='#top') {
		top = 0;
	}else{
		var top = $(id).offset().top;
	}
	$('html,body').animate({scrollTop:top-10 },400);
});
$(window).scroll(function(e){
	var top = $(this).scrollTop();
	var posarr = [];
	var cur = 0;
	$('.right_nav a').each(function (i, el) {
		var id = $(el).attr('href');
		if(id=='#top') return;
		var thistop = $(id).offset().top;
		offset = top-thistop+160;
		posarr.push(offset);
		if(offset<0){
			cur = i;
			return false;
		}
	});
	//console.log(posarr);
	if(posarr[5]&&(posarr[5]>0)) cur=6;
	$('.right_nav a').eq(cur-1).addClass('hover').siblings('a').removeClass('hover');
	//app 二维码 浮动
	if(top>100){
		$(".apppop,.totop").fadeIn();
	}else{
		$(".apppop,.totop").fadeOut();
	}
	//左侧漂浮广告
	if(top>700){
		$(".gamead1").fadeIn();
	}else{
		$(".gamead1").fadeOut();
	}
});

//右侧小幻灯部分
$('.div_con').each(function (i,el) {
	var left = $(this).find('.FP_arr_left');
	var right = $(this).find('.FP_arr_right');
	var lis = $(this).find('.FP_li');
	lis.eq(0).fadeIn();
	var len = lis.size();
	var self = $(this);
	var cur = 0;
	right.click(function () {
		cur++;
		if(cur>=len) cur=0;
		lis.eq(cur).fadeIn(500).siblings('.FP_li').fadeOut(500);
	});
	left.click(function () {
		cur--;
		if(cur<0) cur=len-1;
		lis.eq(cur).fadeIn(500).siblings('.FP_li').fadeOut(500);
	});
});
	
