<script type="text/javascript">
    function travel_dsp() {
        this.dsp_source = [];
        this.dsp_load_url_batch = '//travel.weibo.com/dsp/ad/get-ad-batch/';
        this.dsp_feed_hot_ad = 0;
		this.dsp_feed_exit_ad = 0;
    }

    travel_dsp.prototype.load_dsp = function (dsp_source) {
        if (dsp_source) {
            $.ajax({
                url: this.dsp_load_url_batch,
                type: 'GET',
                data: {
                    'position': dsp_source.join()
                },
                dataType: 'jsonp',
                success: function (data) {
                    if (data.code == '1000') {
                        for (var item in data.data) {
                            if (data.data[item].length > 1) {
                                var dsp_one = data.data[item][Math.floor(Math.random() * data.data[item].length + 1) - 1];
                            } else {
                                var dsp_one = data.data[item][0];
                            }
                            dsp.dsp_render(dsp_one);
                        }
                    } else {
                    }
                }
            });
        }
    };
    travel_dsp.prototype.dsp_render = function (dsp) {
        if (!dsp) {
            return false;
        }
        switch (dsp.position) {
            case 'DSP00000010111': //焦点图第二帧
			case 'DSP00000010117': //焦点图第四帧
				var div = '<a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '"><img src="' + dsp.img + '" width="630" height="355"></a><div class="g-home"></div>';
                var li = '<a href="javascript:void(0)"><img src="' + dsp.img + '"><span class="mask"></span></a>';
                $("div[data-dsp=" + dsp.position + "]").html(div);
                $("li[data-dsp=" + dsp.position + "]").html(li);
				break;
			case 'DSP00000010118': //要闻1
            case 'DSP00000010112': //要闻2_1
            case 'DSP00000010113': //要闻2_2
                $("a[data-dsp=" + dsp.position + "]").attr('href', dsp.link).attr('suda-uatrack', 'key=travel_2017_wap&value=' + dsp.position).text("广告：" + dsp.title);
                $("a[data-dsp=" + dsp.position + "]").parent("span").replaceWith($("a[data-dsp=" + dsp.position + "]"));
                break;
			case 'DSP00000010119': //视频
				var li='<div class="img180 pos-r"><a href="' + dsp.link + '" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '"><img src="' + dsp.img + '" alt="" width="320" height="180"/></a><div class="mask-img pos-a"><a href="' + dsp.link + '"></a></div></div><div class="face-travel"><a  href="' + dsp.link + '" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '"><img src="//tva4.sinaimg.cn/crop.0.0.180.180.50/5993966fjw8emxqwmwpmkj2050050dfz.jpg" alt="头像"/><span>新浪旅游</span></a><div class="volume right font-gary">5,038</div></div><div class="clearfix"></div><div class="txt"><p><a href="' + dsp.link + '" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '">【广告】' + dsp.title + '</a></p></div>';
				$("li[data-dsp=" + dsp.position + "]").html(li);
				break;
			case 'DSP00000010120': //图集
				var li='<a href="' + dsp.link + '" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '"><p><img src="' + dsp.img + '"  width="320" height="180"/></p><p>【广告】' + dsp.title + '</p></a>';
		$("li[data-dsp=" + dsp.position + "]").html(li);
				break;
            case 'DSP00000010114': //热门feed流
                var li = '<li data-dsp="' + dsp.position + '"><div class="img-235 left pos-r"><a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '"><img src="' + dsp.img + '" width="235" height="160" alt=""/><div class="g-home"></div></a></div><div class="txt-400 right"><h2><a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '">' + dsp.title + '</a></h2><div class="font-gary"><p><a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '">' + dsp.summary + '</a></p></div></div><div class="clearfix"></div></li>';
                if (!this.dsp_feed_hot_ad) {
                    this.dsp_feed_hot_ad = 1;
                    $("#hotcont>ul").find("li:eq(1)").after(li);
                }
                break;
			case 'DSP00000010121': //出境游feed流
				var li = '<li data-dsp="' + dsp.position + '"><div class="img-235 left pos-r"><a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '"><img src="' + dsp.img + '" width="235" height="160" alt=""/><div class="g-home"></div></a></div><div class="txt-400 right"><h2><a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '">' + dsp.title + '</a></h2><div class="font-gary"><p><a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '">' + dsp.summary + '</a></p></div></div><div class="clearfix"></div></li>';
                if (!this.dsp_feed_exit_ad) {
                    this.dsp_feed_exit_ad = 1;
                    $("#exitcont>ul").find("li:eq(1)").after(li);
                }
				break;
            case 'DSP00000010115':
                var p = '<p><a href="' + dsp.link + '" target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '"><img src="' + dsp.img + '" width="300" height="200"/><div class="g-right"></div></a></p>';
                $("div[data-dsp=" + dsp.position + "]").html(p);
            case 'DSP00000010116':
                var dt = '<dt><a href="' + dsp.link + '"  target="_blank" suda-uatrack="key=travel_2017_pc&value=' + dsp.position + '">广告：' + dsp.title + '</a></dt>';
                var dd = '<dd>' + dsp.summary + '</dd>';
                var ad_dt = $("dl[data-dsp=" + dsp.position + "]").find("dt:last");
                var ad_dd = $("dl[data-dsp=" + dsp.position + "]").find("dd:last");
                if (ad_dt && ad_dt.length > 0) {
                    $("dl[data-dsp=" + dsp.position + "]").find("dt:last").replaceWith(dt);
                    $("dl[data-dsp=" + dsp.position + "]").find("dd:last").replaceWith(dd);
                } else {
                    $("dl[data-dsp=" + dsp.position + "]").html(dt + dd);
                }
            case 'DSP00000010001':
                //PC_导航_默认搜索词
                $("input[data-dsp=" + dsp.position + "]").val(dsp.title);
        }
    };

    window.dsp = new travel_dsp();
</script>
