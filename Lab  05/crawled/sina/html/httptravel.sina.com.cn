<script type="text/javascript">
    $(function(){
        //顶部回车搜索
        $(".travelSearch").on('keydown', function (e) {
            if(e.keyCode == 13){common_2017.sinaTravelSearch();}
        });
        //顶部搜索点击
        $('#travel-search-button').on('click',function(){
            common_2017.sinaTravelSearch();
        });
        if($(".topic").data("topic"))
        {
			common_2017.rightTopic();
		}
        window.cardpage=1;
        window.page=1;
        //首次加载新闻列表
        common_2017.newslist_load('hotcont',cardpage,page);
        //点击切换标签
        $(".newslisttap").on("click",function(){
            window.cardpage=1,window.page=1;
            $(".newslist").find("ul").empty();
            $(".newslist").find(".feed-card-page").empty();
			if('exitcont'==$(this).data("type")&&1==window.dsp.dsp_feed_exit_ad)
			{
				window.dsp.dsp_feed_exit_ad=0;
			}
            common_2017.newslist_load($(this).data("type"),cardpage,page);
        });
        //新闻列表滚动加载
        $(window).scroll(function() {
            var scrollTop = $(this).scrollTop();
            var scrollHeight = $(".traSlideCon:visible").height();
            var windowHeight = $(this).height();
            var bottomTop=$(".traSlideCon:visible").offset().top;
            var pagination=$("#pagination");
            
            if (scrollTop + windowHeight >= scrollHeight+bottomTop) {
                //console.log(window.cardpage);
                if($(pagination).children().length>0)
                {
                    $(pagination).siblings(".loading").remove();
                }
                if(window.cardpage>=3) return false;
				if(common_2017.dataLoading === false)
				{
					window.cardpage++;
                	common_2017.newslist_load($("#line_title").find(".item-cur").data("type"),window.cardpage,window.page);
				}
            }
        });
    });
</script>
