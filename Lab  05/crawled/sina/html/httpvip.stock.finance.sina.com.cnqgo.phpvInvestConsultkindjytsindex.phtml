                _up.click(_goUp);
                _down = $('#weekDown');
                _down.click(_goDown);
                _draw();
            };
        } ();
        $(function ()
        {
            setInterval(function () { StandardBJTime += 1; },1000);
            weekday.init();
        });
    } (jQuery);
