<!-- 获取评论数 -->
<script type="text/javascript">
    $(function(){
        var dataids = [];
        $('[node-type="comment"]').each(function(){
            dataids.push($(this).attr("newslist"));
        });
            
        var a = dataids.join(",");
        function cmtNum(data){
            var numberlist= data.result.count;
            for(var o in numberlist){
                var str = numberlist[o].total == 0 ? "评论" : numberlist[o].total;
                $("[newslist='"+o+"']").find('[comment-num="comment"]').text(str);      
            }
        }
        $.ajax({
            url: "http://comment5.news.sina.com.cn/cmnt/count?format=json&newslist="+a,
            dataType: "jsonp",
            jsonp: 'callback', 
            jsonpCallback: "cmtNum",
            success: function(data){
                cmtNum(data);
            }
        });
		//明星特别任务播放量
        $("#entList  li").each(function(){
			function getSeparatedNum(str, separator) {
              str = (typeof(str) == 'undefined' ? '' : str) + '';
              separator = separator || ',';

              return str.replace(/(\d+)(\.\d+)?/g, function(num, a, b) {
                return (a.length > 3 ? (a.indexOf(separator) < 0 || a.indexOf(separator) > 3 ? arguments.callee(null, a.replace(/(\d+)(\d{3})/, '$1' + separator + '$2')) : a) : a) + (b || '');
              });
            }
            var _this = $(this);
            var $vids = $(this).attr("video-id");
            $.ajax({
                url: "http://count.video.sina.com.cn/getVideoView",
                data: {
                    video_ids: $vids
                },
                dataType: "jsonp",
                jsonp: "jsonp",
                success: function(res){
                    if(res && res.code == "1"){
                        var count = res.data.video_id;
                        for(var videoId in count){
                            var d = getSeparatedNum(count[videoId]);
                            _this.find('[data-action="count"]').html(d);
                        }
                    }
                }
            });
        })
    })
</script>
