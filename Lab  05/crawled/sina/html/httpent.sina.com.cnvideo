 var videoList = $SCOPE.videoList;
 var player = playVideo(playerConfig, videoConfig);
 var Show = {
    init : function(){
        this.bind();//视频绑定相关
        this.playerInit();//初始化播放器
    },
    bind : function(){
        var _this = this;
        var $videoList = $("#pl_related");
        /**
         * 播放相关视频
         */
        $videoList.on('click', ".DC_imgitem_a", function () {
            _this.playCurrentVideo($(this));
        });
    },
    playerInit : function(){
        var _this = this;
        if(location.hash){
            /(#(\d*))?/.test(location.hash);
            var hash = RegExp.$2;
            this.index = this.getIndex();
            if(this.index == 0){
                player.playVideo({
                    video_id: videoConfig.video_id     //视频id，必须
                });
				window.location.hash = videoConfig.video_id;
            }else{
                player = playVideo(playerConfig,$.extend({video_id: hash}, this.videoConfig));
            }
            var node =  $("#pl_related .DC_imgitem_a").eq(this.index);
            this.setCurrentStyle(node);
            this.setPlayCount(this.index,hash );
            this.setPlayNumd(hash);
        }else{
            location.hash = videoConfig.video_id;
            this.index = this.getIndex();
            var node =  $("#pl_related .DC_imgitem_a").eq(this.index);
            player.playVideo({
                video_id: videoConfig.video_id     //视频id，必须
            });
            this.setCurrentStyle(node);
            this.setPlayCount(this.index,videoConfig.video_id);
            this.setPlayNumd(videoConfig.video_id);
        }
        player.on("playCompleted", function(){
            _this.playNext();
        }); 
    },
    setPlayNumd : function($vid){
        function getSeparatedNum(str, separator) {
          str = (typeof(str) == 'undefined' ? '' : str) + '';
          separator = separator || ',';

          return str.replace(/(\d+)(\.\d+)?/g, function(num, a, b) {
            return (a.length > 3 ? (a.indexOf(separator) < 0 || a.indexOf(separator) > 3 ? arguments.callee(null, a.replace(/(\d+)(\d{3})/, '$1' + separator + '$2')) : a) : a) + (b || '');
          });
        }
        $.ajax({
            url: "http://count.video.sina.com.cn/getVideoView",
            data: {
                video_ids: $vid
            },
            dataType: "jsonp",
            jsonp: "jsonp",
            success: function(res){
                if(res && res.code == "1"){
                    var count = res.data.video_id;
                    for(var videoId in count){
                        var d = getSeparatedNum(count[videoId]);
                        $("#pl_playCount").html(d);
                    }
                }
            }
        });
    },
    setPlayCount: function($this,$hash){
        var _this = this;
        var $title = $("#pl_related .DC_imgitem_a").eq($this).find('.dtl .tl').text();
        $(".msg_cnt .tl a").html($title);
        var $href = $SCOPE['videoList'][$this].url;
        $(".msg_cnt .tl a").attr("href",$href);
        $('[data-action="detail_expand"]').attr("href",$href);
        var $detailPart = $("#pl_related .DC_imgitem_a").eq($this).find('[data-type="data-detailpart"]').html();
        var $detailAll = $("#pl_related .DC_imgitem_a").eq($this).find('[data-type="data-detailAll"]').html();
        var $dataTime = $("#pl_related .DC_imgitem_a").eq($this).find('[data-type="data-time"]').html();
        var $dataLabel = $("#pl_related .DC_imgitem_a").eq($this).find('[data-type="data-label"]').html();
        var $dataFrom = $("#pl_related .DC_imgitem_a").eq($this).find('[data-type="data-from"]').html();
        var $dataChannel = $("#pl_related .DC_imgitem_a").eq($this).find('[data-type="data-channel"]').html();
        var $datavName = $("#pl_related .DC_imgitem_a").eq($this).find('[data-type="data-vname"]').html();
        $("#detail_part").html($detailPart);
        //$("#detail_all").html($detailAll);
        $("#pl_playLabel").html($dataLabel);
        $("#pl_playDate").html($dataTime);
        $("#videoFrom").html($dataFrom);
        $("#videoChannel").html($dataChannel);
        $("#videoName").html($datavName);
		var $count = $("#pl_related .DC_imgitem_a").eq($this).find('[data-action="count"]').html();
        $("#pl_playCount").html($count);
    },
    renderPlayCount: function(res){
        var count = res.data.video_id;
        for(var videoId in count){
            $("#pl_playCount").html(count[videoId]);
        }
    },
    playCurrentVideo: function($this){
        this.index = $this.index(); 
        this.setCurrentStyle($this);
        this.playVideo($this);
		$("#myMovie").attr("poster",$SCOPE['videoList'][$this.index()].poster);
        $("#myMovie").attr("style","object-fit:cover");
    },
    playVideo : function($this){
        player.playVideo({
            video_id: $this.attr("video-id")     //视频id，必须
        });
        location.hash = $this.attr("video-id");
        this.setPlayCount($this.index(),$this.attr("video-id"));
    },
    playNext: function(){
        var index = ++this.index;
        if(index > videoList.length){
            return;
        }else if(index == videoList.length){
            var $node = $("#pl_related .DC_imgitem_a").eq(0);
            this.playVideo($node);
        }
        var node = $("#pl_related .DC_imgitem_a").eq(index);
        this.setCurrentStyle(node);
        this.playVideo(node);

        this.setPlayCount(index,node);
    },
    setCurrentStyle: function($this){
        this.index = $this.index();
        $("#pl_related .DC_imgitem_a").eq(this.index).addClass("cur").siblings().removeClass("cur");
    },
    getIndex: function(){
        var videoId = location.hash.split("#").slice(1);
        for(var i = 0, len = videoList.length; i < len; i++){
            if(videoId == videoList[i].video_id){
				return i;
				//alert("===")
            }
        }
		return 0;
    }
 }
 Show.init();
</script> 
