        width: 224px;
        height: 224px;
        position: absolute;
        top: 0;
        left: 0;
        z-index: 1;
        cursor: pointer;
    }
    .wrapper{
        width: 112px;
        height: 224px;
        position: absolute;
        top:0px;
        overflow: hidden;
    }
    .right1{
        right:0;
    }
    .left1{
        left:0;
    }
    .circleProgress{
        width: 218px;
        height: 218px;
        border: 3px solid #fff;
        border-radius: 50%;
        position: absolute;
        top:0;
        -webkit-transform: rotate(45deg);
        transform: rotate(45deg);
        -o-transform: rotate(45deg);
        -mox-transform: rotate(45deg);
        -ms-transform: rotate(45deg);
    }
    .rightcircle{
        border-top:3px solid #18bc7c;
        border-right:3px solid #18bc7c;
        right:0;
        -webkit-animation: circleProgressLoad_right 3s linear infinite;
        animation: circleProgressLoad_right 3s linear infinite;
        -o-animation: circleProgressLoad_right 3s linear infinite;
        -mox-animation: circleProgressLoad_right 3s linear infinite;
        -ms-animation: circleProgressLoad_right 3s linear infinite;
    }
    .leftcircle{
        border-bottom:3px solid #18bc7c;
        border-left:3px solid #18bc7c;
        left:0;
        -webkit-animation: circleProgressLoad_left 3s linear infinite;
        animation: circleProgressLoad_left 3s linear infinite;
        -o-animation: circleProgressLoad_left 3s linear infinite;
        -mox-animation: circleProgressLoad_left 3s linear infinite;
        -ms-animation: circleProgressLoad_left 3s linear infinite;

    }
    @keyframes circleProgressLoad_right{
          0%{
              transform: rotate(-135deg);
          }
          25%,50%{
              transform: rotate(45deg);
          }
          75%,100%{
              transform: rotate(225deg);
          }
      }
