<script type="text/javascript">
    $(function(){
        // 积分榜初始化
        function initJF(){
            $.ajax({
                
                url: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=teamOrder&type=10&use_type=group&season=2019",
                type: "GET",
                dataType: "jsonp",

                data: {
                    dpc:1
                },
                cache:true,
                jsonpCallback:'CB_54AF837E_D9DE_4380_80F3_64B80F938574',
                success: function(data){
                    $(".jf_rank .rk_list ul").each(function(){
                        var $this = $(this), dataGroup = $this.attr("data-group"),
                            singleData = data.result.data[dataGroup];
                        setJFInfo(singleData, 0, $this);
                    })
                },
                error: function(){
                    window.console && window.console.log("error");
                }
            })
        }
        function setJFInfo(d, i, $w){
            var temp = d[i];
            if(!d[i]) return;
            if(i%4){
                $w.append('<li><span class="rk_order">'+temp.team_order+'</span><span class="jf_team"><a href="http://match.sports.sina.com.cn/football/team.php?id='+temp.sl_id+'&l_type='+ temp.l_type +'" target="_blank">'+temp.team_cn+'</a></span><span class="jf_count">'+temp.count+'</span><span class="rk_result">'+temp.score+'</span></li>');
                i++;
                setJFInfo(d, i, $w);
            } else{
                $.ajax({
                    
                    url: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=getteam&id=" + temp.sl_id,

                    type: "GET",
                    dataType: "jsonp",
                    data: {
                        dpc: 1
                    },
                    jsonpCallback:'CB_8062CB892_B53D_4964_8E02_CA2CA01AD597_'+temp.sl_id,
					
                    cache: true,
                    success: function(data){
                        $w.append('<li class="hvr"><span class="rk_order">'+temp.team_order+'</span><span class="jf_team"><a href="http://match.sports.sina.com.cn/football/team.php?id='+temp.sl_id+'&l_type='+ temp.l_type +'" target="_blank"><img src="'+data.result.data.logo_small+'" alt="'+temp.team_cn+'" />'+temp.team_cn+'</a></span><span class="jf_count">'+temp.count+'</span><span class="rk_result">'+temp.score+'</span></li>');
                        i++;
                        setJFInfo(d, i, $w)
                    },
                    error:function(){
                        window.console && window.console.log("error");
                    }
                })
            }
        }
        // 射手榜初始化
        function initShs(session, limit){
            $.ajax({
                
                url: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=playerorder&item=13&type=10&season=" + session + "&limit=" + limit,
                 type: "GET",
                 dataType: "jsonp",
                 data: {
                     dpc:1
                 },
                 cache: true,
                 jsonpCallback:'CB_D51CA315_E3C8_4A7A_B5B1_2A1BF056C054_'+session+'_'+limit,
				
                success: function(data){
                    for(var i=0;i<limit;i++){
                        var singleData = data.result.data[i];
                        if(typeof(singleData) != "undefined"){
                            $(".scorer_rank .rk_list ul").append('<li><span class="rk_order">'+singleData.num+'</span><span class="sc_team"><a href="http://match.sports.sina.com.cn/football/player.php?id='+singleData.player_id+'" target="_blank">'+singleData.player_name+'</a> | <a href="http://match.sports.sina.com.cn/football/team.php?id='+singleData.sl_team_id+'&l_type='+ singleData.l_type +'" target="_blank">'+singleData.team_name+'</a></span><span class="rk_result">'+singleData.item1+'</span></li>');
                        }
                    }
                },
                error: function(){
                    window.console && window.console.log("error");
                }
            })
        }
        initJF();
        var session = 2019, limit = 5;
        initShs(session, limit);
        // 榜单切换
        $(".rk_tabs span").click(function(){
            var $this = $(this), index = $this.index();
            $this.addClass("rk_tabs_on").siblings().removeClass("rk_tabs_on");
            $(".rk_conts .rk_cont").eq(index).show().siblings().hide();
        })
        // 积分榜切换
        $(".jf_group span").mouseover(function(){
            var $this = $(this), index = $this.index();
            $this.addClass("hvr").siblings().removeClass("hvr");
            $(".jf_rank .rk_list ul").eq(index).show().siblings("ul").hide();
        })
        // 滑过效果
        $(".rk_list").delegate("li:not('.rk_list_title')","mouseenter",function(){
            var $this = $(this), $img = $this.find(".jf_team img");
            $this.addClass("hvr").siblings().removeClass("hvr");                            
        })
    })
</script>

                </div>
                <!--积分榜-->
