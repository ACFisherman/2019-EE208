     getInterface: function(){
        var that= this, url = [];

        //小组赛
        if(that.getRange("2019-08-30 16:00", "2019-09-18 16:00")){
            url.push(that.urlType1("2019-09-18"));
        }
        if(that.getRange("2019-09-18 16:00", "2019-09-19 16:00")){
            url.push(that.urlType1("2019-09-19"));
        }
        if(that.getRange("2019-09-19 16:00", "2019-10-02 16:00")){
            url.push(that.urlType1("2019-10-02"));
        }
        if(that.getRange("2019-10-02 16:00", "2019-10-03 16:00")){
            url.push(that.urlType1("2019-10-03"));
        }
        if(that.getRange("2019-10-03 16:00", "2019-10-23 16:00")){
            url.push(that.urlType1("2019-10-23"));
        }
        if(that.getRange("2019-10-23 16:00", "2019-10-24 16:00")){
            url.push(that.urlType1("2019-10-24"));
        }
        if(that.getRange("2019-10-24 16:00", "2019-11-06 16:00")){
            url.push(that.urlType1("2019-11-06"));
        }
        if(that.getRange("2019-11-06 16:00", "2019-11-07 16:00")){
            url.push(that.urlType1("2019-11-07"));
        }
        if(that.getRange("2019-11-07 16:00", "2019-11-27 16:00")){
            url.push(that.urlType1("2019-11-27"));
        }
        if(that.getRange("2019-11-27 16:00", "2019-11-28 16:00")){
            url.push(that.urlType1("2019-11-28"));
        }
        if(that.getRange("2019-11-28 16:00", "2019-12-11 16:00")){
            url.push(that.urlType1("2019-12-11"));
        }
        if(that.getRange("2019-12-11 16:00", "2019-12-12 16:00")){
            url.push(that.urlType1("2019-12-12"));
        }

        //淘汰赛-待淘汰赛抽签后调整
        if(that.getRange("2019-12-12 16:00", "2020-02-21 16:00")){
            url.push(that.urlType2(7, 2019));
        }
       if(that.getRange("2020-02-21 16:00", "2020-03-14 16:00")){
            url.push(that.urlType2(8, 2019));
        }
       if(that.getRange("2020-03-14 16:00", "2020-04-11 16:00")){
            url.push(that.urlType2(9, 2019));
        }
       if(that.getRange("2020-04-11 16:00", "2020-04-18 16:00")){
            url.push(that.urlType2(10, 2019));
        }
        if(that.getRange("2020-04-18 16:00", "2020-09-01 16:00")){
            url.push(that.urlType2(11, 2019));
            url.push(that.urlType2(12, 2019));
            url.push(that.urlType2(13, 2019));
        }
        //更新至此处
	/*样例，留作对照
        if(that.getRange("2017-12-07 16:00", "2018-03-01 16:00")){
            url.push(that.urlType2(7, 2016));
        }
        if(that.getRange("2018-03-01 16:00", "2018-03-25 16:00")){
            url.push(that.urlType2(8, 2016));
        }
        if(that.getRange("2018-03-25 16:00", "2018-04-15 16:00")){
            url.push(that.urlType2(9, 2016));
            url.push(that.urlType2(10, 2016));
        }
        if(that.getRange("2018-04-15 16:00", "2018-09-01 16:00")){
            url.push(that.urlType2(11, 2016));
            url.push(that.urlType2(12, 2016));
            url.push(that.urlType2(13, 2016));
        }
        if(that.getRange("2018-09-01 16:00")){
            url.push(that.urlType2(1, 2016));
        } */
        return url;
    },



    urlType1: function(d){
        return "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=Intlfootball&_sport_a_=getTypeMatchesByDate&l_type=10&begin=" + d + "&end=" + d;
    },
    urlType2: function(n, y){
        return "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=livecast&_sport_a_=matchesByType&type=10&rnd=" + n + "&season=" + y;
    },
    getRange: function(){
        var now = new Date();
        now = Date.parse(now);
        var that = this, len = arguments.length;
        if(len == 1){
            if(Date.parse(that.frmtConvert(arguments[0]))<now) return true;
            else return false;
        }
        if(len == 2){
            if(Date.parse(that.frmtConvert(arguments[0]))<now && now<Date.parse(that.frmtConvert(arguments[1]))) return true;
            else return false;
        }
    },
    frmtConvert: function(t){
        return t.replace(/-/g,"/");
    }
}
ContestInit.init();
</script>
