        <script type="text/javascript">
            $(function(){
                (function(){
                    $(".og_tab_title li").on("click",function(){
                        $(".og_tab_title li").removeClass("og_tab_title_focus");
                        $(this).addClass("og_tab_title_focus");
                        $(".og_tab_cont").hide();
                        $(".og_tab_icon").hide();
                        $(".og_tab_cont").eq($(this).index()).show();
                        $(".og_tab_cont").eq($(this).index()).find(".og_tab_icon").show()
                    })
                })()
            })
        </script>
