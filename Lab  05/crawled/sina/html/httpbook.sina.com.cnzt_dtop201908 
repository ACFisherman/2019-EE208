       jsLoader(ARTICLE_JSS.lib).jsLoader(ARTICLE_JSS.jq).jsLoader(ARTICLE_JSS.jqSelectDate, function() {
                    $(function() {
						$("#book_date li:even").addClass("bg1");
                        var url = window.location.href,
							lastDate = '2019/8', //最近一期时期
                            currentDate = url.match(/\/top(\d*)/);
                        currentDate = currentDate && currentDate[1];
                        if (currentDate) {
                            currentDate = currentDate.replace(/(\d{4})(\d{0,2})/, function(match, $1, $2) {
                                $2 = $2 || 12;
                                return $1 + '\/' + $2
                            });
                        }
                        currentDate = currentDate || lastDate;
						var currentMonth = currentDate.split('/')[1];
						$('.data-name-title').addClass('date_num_'+currentMonth);
                        $.SelectDate({
                            box: '#dateBox',
                            prev: '.left-btn',
                            next: '.right-btn',
                            textYear: '.year',
                            textMonth: '.month',
                            layout: '.layout',
                            currentDate: new Date(currentDate + '/01'),
                            lastDate: new Date(lastDate + '/01'), //最后一期时间
                            str: '<li><a href="[=url]" target="_blank">[=month]</a></li>',
                            curStr: '<li><a class="current" href="[=url]" target="_blank">[=month]</a></li>',
                            noStr: '<li><a class="no">[=month]</a></li>',
                            url: 'http://book.sina.com.cn/'
                        });
						
