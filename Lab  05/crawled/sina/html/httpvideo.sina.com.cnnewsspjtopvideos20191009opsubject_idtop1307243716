						var loginLayer = window.SINA_OUTLOGIN_LAYER;//获取登录浮层
						if(loginLayer){
							loginLayer.set('extra',{
								css: 'http://i.sso.sina.com.cn/css/outlogin/v1/outlogin_skin_reversion.css'
							});
							//配置登录选项，并初始化登录浮层
