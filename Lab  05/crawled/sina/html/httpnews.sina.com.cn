              }
            }
            target.list = target.list.replace(/,$/, "")
            object.loadHQ(target);
          }
          else {
            var now = new Date();
            if (now.getSeconds() < 30 && now.getMinutes() % 3 == 0) {
              var rn = now.getTime();
              for (var j = 0; j < target.lines.length; j++) {
