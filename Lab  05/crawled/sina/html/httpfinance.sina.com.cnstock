<script>
	//pad fix
	(function(){
		var isPAD = false;
		if('ontouchstart' in window){
			isPAD = true;
			FN.delegate(document.body,'tablinkbtns','click',function(e){
				e = e || window.event;
				FN.preventDefault(e);
			});
		}
		window.isPAD = isPAD;
	})();

	//select-top
	new DivSelect('slt_01');

	//login
	(function(){
		var w1 = document.getElementById('p01_r01_c02_list'),
			w2 = document.getElementById('p01_r01_c02_login'),
			btn = document.getElementById('p01_r01_c02_login_btn'),
			userinfo = document.getElementById('userLogin'),
			userPanel = window.SINA_USER_PANEL,
			STK = userPanel ? userPanel.STK : null,
			loginSuccess = function(){
				w1.style.display = '';
				w2.style.display = 'none';
				if(typeof loginSuccessCallbacks !== 'undefined'){
					for(var i = 0, l = loginSuccessCallbacks.length; i < l; i++){
						if(typeof loginSuccessCallbacks[i] === 'function'){
							loginSuccessCallbacks[i]();
						}
					}
				}
			},
			logoutSuccess = function(){
				w1.style.display = '';
				w2.style.display = '';
				if(typeof logoutSuccessCallbacks !== 'undefined'){
					for(var i = 0, l = logoutSuccessCallbacks.length; i < l; i++){
						if(typeof logoutSuccessCallbacks[i] === 'function'){
							logoutSuccessCallbacks[i]();
						}
					}
				}
			};
		if(!userPanel){return;}
		STK.Ready(function(){
			userPanel.set('outloginLayer',{
				ready : function(){
					var loginLayer = userPanel.getOutLogin(),
						logined = false;
					if(!loginLayer){return;}
					STK.addEvent(btn,'click',function(e){
						var pos = FN.gEleDocPos(btn);
						loginLayer.set('plugin',{
							parentNode : document.body,
							relatedNode : document.body
						}).set('styles',{
							left : (pos.x - 250) + 'px',
							top : (pos.y + 30) + 'px'
						}).show();
					});

					if(loginLayer.isLogin()){logined = true;}

					setInterval(function(){
						if(loginLayer.isLogin() !== logined){
							loginLayer.init();
						}
					},8e3);

					loginLayer.set('extra',{
						css: '//i.sso.sina.com.cn/css/outlogin/v1/outlogin_skin_finance.css'
					}).set('plugin',{
						parentNode : document.body,
						relatedNode : document.body
					}).set('sso',{
						entry : 'account'
					}).set('styles',{
					}).register('login_success',function(e){
						logined = true;
						loginSuccess();
					}).register('logout_success',function(e){
						logined = false;
						logoutSuccess();
					}).init();
				},
				triggerEvent : 'click',
				loginBtns : ['p01_r01_c02_login_btn']
			}).set('container',{
				node : userinfo
			}).register('plugin_ready',function(){
			}).init();
		});
	})();

	//select-searcher
	new DivSelect('searcher_sel');

	//suggest-searcher
	(function(){
		var ss = new SuggestServer();
		ss.bind({
			'input' : 'suggest01_input',
			'default' : '简称/代码/拼音',
			'type' : '',
			'link' : 'http://biz.finance.sina.com.cn/suggest/lookup_n.php?country=@type@&q=@code@',
			'target' : '_blank',
			'width' : 250,
			'callback' : null
		});
		document.getElementById('searcher_sel').onchange = function () {
			switch (this.selectedIndex) {
				case 0 :
					ss.changeType('stock');
					ss.changeLink("//biz.finance.sina.com.cn/suggest/lookup_n.php?country=@type@&q=@code@");
					ss._objectConfig['value'] = '@2@';
					ss._stringOriginalValue = '简称/代码/拼音';
					if(ss._elementInput.value == '请输入关键字') {
						ss._elementInput.value = '简称/代码/拼音';
					}
					break;
				case 1 :
					ss.changeLink("http://guba.sina.com.cn/?s=bar&name=@code@");
					ss._objectConfig['value'] = '@2@';
					ss._stringOriginalValue = '简称/代码/拼音';
					if(ss._elementInput.value == '请输入关键字') {
						ss._elementInput.value = '简称/代码/拼音';
					}
					break;
