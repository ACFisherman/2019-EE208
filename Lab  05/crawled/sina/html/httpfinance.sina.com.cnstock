<script>
// 平台动态
(function() {
    var investorSafeguard = window.investorSafeguard;
    var API = {};
    API.getCompanyKeywordsList = function(data, doneCb, failCb, alwaysCb, tryTime) {

        fetch(function(tryTime, failCb) {
            $.ajax({
                url: '//wq.finance.sina.com.cn/api/order/get_keywords',
                method: 'GET',
                dataType: 'jsonp',
                data: data,
                xhrFields: {
                    withCredentials: true
                }
            }).done(doneCb).fail(failCb).always(alwaysCb);
        }, tryTime, failCb);
    };;

    function fetch(handler, tryTime, failCb) {
        handler(tryTime, function() {
            if (tryTime > 1) {
                fetch(handler, --tryTime, failCb);
            } else {
                failCb && failCb.apply(this, arguments)
            }
        });
    }
    var pageCount = 1;
    var container = $("#m_safeguard_list_blk");

    function init() {
        bindEvents();
        correlation();
    }

    function correlation() {
        API.getCompanyKeywordsList({
            page: pageCount,
            num: 8
        }, function(res) {
            if (res.result.status.code == 0) {
                var num = res.result.data.data.length;
                var dataList = res.result.data.data;
                var html = '';
                if (num < 1) {
                    handlerErro('EMPTY_DATA');
                    return;
                };
                if (pageCount >= res.result.data.total_page) {
                    pageCount = 1;
                } else {
                    pageCount++;
                };
                for (var i = 0; i < dataList.length; i++) {
                    var illegal_type = {
                        '0': '内幕交易',
                        '1': '虚假陈述',
                        '2': '操纵市场',
                        '3': '其他'
                    }[dataList[i].illegal_type];
                    var companyCode = dataList[i].company_market ? '（' + dataList[i].company_market + dataList[i].company_symbol + '）' : '';

                    html += ' <tr><td>' + dataList[i].name + '</td><td><a href="' + dataList[i].company_url + '" target="_blank">' + dataList[i].company_name + '</a></td><td> ' + dataList[i].create_time.substring(5, 16) + ' </td></tr> ';
                };

                container.find('tbody').html(html);

            } else {
                handlerErro('UNEXCEPT_DATA');
            }
        }, function() {
            handlerErro('UNEXCEPT_DATA');
        });
    }

    function handlerErro(error) {
        var str;
        switch (error) {
            case 'NETWORK_ERRO':
                str = '网络异常，请刷新重试';
                break;
            case 'UNEXCEPT_DATA':
                str = '服务器异常，请刷新重试';
                break;
            case 'EMPTY_DATA':
                str = '暂无更多';
                break;
            default:
                str = '';
        }
        container.find('tbody').html('<tr><td colspan="4"><div class="article-error"><p>' + str + '</p></div></td></tr>');
    }

    function bindEvents() {
        $("#u_change1").click(function(event) {
            correlation();
        });
    }

    init();
})();
</script>
