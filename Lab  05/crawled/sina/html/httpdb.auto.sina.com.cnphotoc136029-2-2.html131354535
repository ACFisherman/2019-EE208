        var popRight = parseInt((WinWidth-1000)/2 - 10 - 174-20);
        popRight = popRight >0 ? popRight : 0;
        haddleButton.css('right',popRight);
        bottomWrap.hide();
        haddleButton.show().css('display','block');
    }
    (function(window,$) {
        set_collect_close();
    })(window,jQuery)
</script>
