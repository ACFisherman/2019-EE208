!function(){                                                BAIDU_VIDEO_ADM_FILL_ASYC(
                                                    'PCIndexPageCarousel01',
                                                    'adv-carousel-item01',
                                                    function(wrap) {
                                                        var utils = BAIDU_VIDEO_ADM_TOOLS;
                                                        var elem = this.elem;
                                                        var id = this.id;
                                                        var $elems = $('.' + elem);
                                                        var $wrap = $(wrap);
                                                        var fork = $elems.not(wrap)[0];
                                                        var html = this.ad[0].data[0].html;
                                                        var _elem = elem + '_fork';
                                                        fork.id = _elem;

                                                        html = html.replace(elem, _elem);

                                                        var _id = id.substr(0, id.length-2);
                                                        var regID = new RegExp(id, 'ig');
                                                        // html = html.replace(regID, _id);

                                                        fork.innerHTML = '.' + html;
                                                        try {
                                                            fork.removeChild(fork.firstChild);
                                                        }
                                                        catch (error) {
                                                        }
                                                        var scripts = fork.getElementsByTagName('script');

                                                        utils.shim(scripts, 'array').forEach(function (item) {
                                                            // ie8以下带有defer属性的script标签会自动执行，这里需要排除
                                                            if (!(utils.broswser.name === 'ie'
                                                                && utils.broswser.version < 9 && item.defer
                                                                || item.getAttribute('type') && item.getAttribute('type') !== 'text/javascript'
                                                            )) {
                                                                utils.globalEval(item.innerHTML);
                                                            };
                                                        });

                                                        $elems.show();
                                                    }
                                                )
                                            }();
