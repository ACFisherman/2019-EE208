		  var width = img.offsetWidth;
		  var height = img.offsetHeight;
		  if( width>78 ){
		      width>height?( img.style.width='78px' ):( img.style.height='78px' );
		  }else if( height>78 ){
		      img.style.height='78px';
		  }
	  }
