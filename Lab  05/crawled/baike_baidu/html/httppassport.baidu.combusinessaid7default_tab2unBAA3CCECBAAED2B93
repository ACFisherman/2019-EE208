function showMsg() {
    if (typeof(redmsg) != 'undefined') {
        if (!redmsg || redmsg.length < 1) return;
        document.getElementById('mnum').innerHTML = redmsg;
    }
}
if (loginflag && loginflag == 1) {
    document.write("<script src=http://msg.baidu.com/ms?ct=18&cm=3&tn=bmSelfUsrStat&mpn=13227114&un=><\/script>");
    setTimeout("showMsg()", 500);
}


function getData() {
    var head = document.getElementsByTagName("head")[0];
    var s = document.createElement("script");
    s.type = "text/javascript";
    s.src = "http://pdc.baidu.com/imstate/?callback=imstate&type=json&uids=pp.1.3e7c9047.iSzkyAtR4E_eU_bGepMtvg&" + new Date().getTime();
    head.appendChild(s);
};

function startbdh() {
    startBaiduHi("message", "", "海天寒夜", {
        clk: "icon",
        src: "psp"
    });
    return false;
};

function imstate(json) {
    var status = json["pp.1.3e7c9047.iSzkyAtR4E_eU_bGepMtvg"];
    if (!status) return false;
    if (status.imuser == '1') {
        var el = document.getElementById('userStatus');
        el.style.display = '';
        if (status.online == '1') {
            el.className += ' imonline';
            document.getElementById("userStatusWord").innerHTML = '在线交谈';
        } else {
            el.className += ' imoffline';
            document.getElementById("userStatusWord").innerHTML = '离线留言';
        }
        document.getElementById('userStatusWord').onclick = startbdh;
    }
};
