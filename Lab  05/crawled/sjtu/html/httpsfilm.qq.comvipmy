    function getQrAd () {
        var deferred = $.Deferred();
        $.ajax('//vip.video.qq.com/fcgi-bin/comm_cgi?name=spp_VipActConf_V2&cmd=2&posId=47&platform=1', {
            cache: false,
            dataType: 'jsonp',
            success: function (data) {
                if(data && data.posList && data.posList.length) {
                    var list = data.posList
                    if (list && list.length) {
                        if (list[0].code === 0) {
                            var confData = list[0].confData
                            if (confData && confData.length) {
                            var conf = confData[0]
                            if (conf.begTime && conf.endTime) {
                                var begTime = new Date(conf.begTime.replace('-', '/'))
                                var endTime = new Date(conf.endTime.replace('-', '/'))
                                var now = new Date().getTime()
                                if (begTime.toString() !== 'Invalid Date' &&  endTime.toString() !== 'Invalid Date') {
                                    if (begTime.getTime() < now && endTime.getTime() > now) {
                                        deferred.resolve(conf);
                                        return
                                    }
                                } else {
                                deferred.resolve(conf);
                                    return
                                }
                            } else {
                                deferred.resolve(conf);
                                return
                            }
                            }
                        }
                    }
                }

                deferred.resolve('');
            },
            error: function () {
                deferred.resolve([]);
            }
        });

        return deferred;
    }

    function init() {
         // 站内场景增加推广位
         getQrAd().then(function (qr) {
            if (!qr) {
                $('.js_wxQrImg').style({
                    width: '84px', height: '84px', padding: '3px'
                }).html('<img src="https://puui.qpic.cn/vupload/0/20190508_1557305047539_n82hyjdaqw.png/0" class="img_weixin" alt="微信官方账号">')
                return
            }
            var html = [
                '<div style="box-shadow: 0px 5px 16px rgba(0, 0, 0, 0.18); width: 100%; height: 100%;">',
                    '<div style="float: left; padding: 39px 0 39px 20px;">',
                        '<div style="margin-bottom: 7px;">',
                        '<img src="' + qr.little_icon + '" style="width: 25px; float: left"><span style="color: ' + qr.title_color + '; font-size: 14px; height: 25px; line-height: 25px">' + qr.title + '</span>',
                        '</div>',
                        '<div><span style="color: ' + qr.subtitle_color + '; font-size: 12px">' + qr.subtitle + '</span></div>',
                    '</div>',

                    '<img src="' + qr.QR_code + '" style="float: left;width: 90px;height: 90px;padding: 19px 10px">',
                '</div>'
            ]
            $('.js_wxQrImg').html(html.join('\n'))
            $('.js_wxQrImg').css({
                width: '324px', height: '128px', padding: 0
            })
        })
    }

    txv.login.addReadyCallback(function () {
        init(txv.login.isLogin());
    });

    txv.login.addLoginCallback(function(){
        init(true);
    });

    txv.login.addLogoutCallback(function(){
        init(false);
    });
</script>
