							}
							this._stringCompare = __arrayCountryEx.length > 0 ? "a," + __arrayCountryEx.join("a,") : "";
							drawflash(this._objectData["country"] + this._stringCompare, this._objectData["event"], this._stringDateFrom, this._stringDateTo);
						};
						this._initialize = function () {
							this._stringServicePath = "//money.finance.sina.com.cn/forex/api/jsonp.php";
							this._elementCurrent = this._get("h2Current");
							this._stringTemplateCurrent = this._elementCurrent.innerHTML;
							this._elementLatest = this._get("divLatest");
							this._stringTemplateLatest = this._elementLatest.innerHTML;
							this._elementList = this._get("divList");
							this._stringTemplateList = this._elementList.innerHTML;
							this._elementPages = this._get("divPages");
							this._stringTemplatePages = this._elementPages.innerHTML;
							this._elementRelate = this._get("divRelate");
							this._stringTemplateRelate = this._elementRelate.innerHTML;
							this._stringCompare = "";
							this._numberPage = 1;
							this._numberPerPage = 20;
							this._stringDateFrom = "";
							this._stringDateTo = "";
							this._stringDateFromLast = "";
							this._stringDateToLast = "";
							this._objectData = {};
							new Calendar("inputCalendarFrom");
							new Calendar("inputCalendarTo");
							window["changeme"] = this._bind(this._change);
							window["changeus"] = this._bind(this._compare);
							window["gotoPage"] = this._bind(this._gotoPage);
							this._formSearch = this._get("formSearch");
							this._aevent(this._formSearch, "submit", this._bind(this._submit));
						};
						this._initialize.apply(this, arguments);
					}();
          changeme(countryname, eventname, flag);
	</script>
