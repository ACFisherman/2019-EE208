<script type="text/javascript">
    var WEIBO_ID = 0;
    var NEED_BIND_TEL = 0;
    var NEED_BIND_WINXIN =
        0;
    window.isLogin = false;

    window.IMG_PATH_PREFIX = 'http://s3.licaishi.sina.com.cn/';

    seajs.use('$', function () {
        $(function () {
            seajs.use('tree.widget.header');
        })
    });

    seajs.use('$', function () {
      this.resetTimer = setInterval(function(){
        if($('[data-name="plan"]').html()){
          $('[data-name="plan"]').remove()
          clearInterval(this.resetTimer)
        }
      }, 500)
        $('#myPage').on('click', function () {
            seajs.use('tree.login.loginManager', function () {
                var LoginManager = T.login.loginManager;
                LoginManager.checkImmediate();
                if (!T.login.loginManager.user()) {
                    seajs.use('tree.login.loginLayer', function () {
                        T.login.loginLayer.open();
                    });
                } else {
                    $('#myPage').find('a').attr('href', '/web/myPlan');
                }
            })

        });

    })
</script>
