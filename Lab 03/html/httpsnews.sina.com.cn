            leftLink: "//news.sina.com.cn/zt_nys/xzgcl70n/",
            // 是否显示左侧按钮关闭链接, 1：显示，0：隐藏
            isShowLeftLink: 0,
        };
        // =======飘红配置结束==========

        // 检查参数
        if (!CONFIG.activityName) {
            return;
        };
        if (!CONFIG.closeBtnHoverBG) {
            CONFIG.closeBtnHoverBG = CONFIG.closeBtnBG;
        };
        if (!CONFIG.leftLink) {
            CONFIG.leftLink = '#url';
        };

        var pthis = {};

        //获取对象
        pthis.$ = function(id) {
            if (document.getElementById) {
                return document.getElementById(id);
            } else {
                return document.all[id];
            }
        };

        //获取cookie
        pthis.getAdCookie = function(N) {
            var c = document.cookie.split("; ");
            for (var i = 0; i < c.length; i++) {
                var d = c[i].split("=");
                if (d[0] == N) return unescape(d[1]);
            }
            return "";
        };

        //设置cookie
        pthis.setAdCookie = function(N, V, Q) {
            var L = new Date();
            var z = new Date(L.getTime() + Q * 60000);

            //document.cookie=N+"="+escape(V)+";path=/;expires="+z.toGMTString()+";";
            document.cookie = N + "=" + escape(V) + ";path=/;domain=news.sina.com.cn;expires=" + z.toGMTString() + ";";

        };

        //构造函数
        pthis.init = function() {
            try {
                document.write([
                    '<style type="text/css">',
                    'body{background:url(' + CONFIG.activityBG + ') no-repeat 50% 0;padding-top:0;margin-top:0;}',
                    '.topAD{background:#fff;}',
                    '#wrap{padding-top:0;margin-top:0;background:#fff}',
                    '#' + CONFIG.activityName + 'Btncls{background:url(' + CONFIG.closeBtnBG + ') no-repeat;width:50px;height:22px;padding:0;margin:0;position:absolute;right:0;bottom:' + CONFIG.closeBtnBottom + 'px;cursor:pointer;display:block;;z-index:2;}',
                    '#' + CONFIG.activityName + 'Btncls:hover{background:url(' + CONFIG.closeBtnHoverBG + ') no-repeat}',
                    '#page{background:#fff;}',
                    '</style>',
                    '<div id="' + CONFIG.activityName + 'TopBar" style="clear:both;width:1020px;height:' + CONFIG.headHeight + 'px;margin:0 auto;padding:0;overflow:hidden;position:relative;">',
                    '<div id="' + CONFIG.activityName + 'Btncls" style="' + (CONFIG.isShowLeftLink ? '' : 'display:none;') + '" title="关闭背景"></div>',
                    '<a id="' + CONFIG.activityName + 'TopBlank" style="clear:both;height:80px;width:930px;position:absolute;left:50%;margin-left:-465px;top:0;line-height:0;font-size:0;overflow:hidden; " href="' + CONFIG.leftLink + '" target="_blank" title="' + CONFIG.activityChineseName + '"></a>',
                    '</div>'
                ].join(''));

                pthis.$(CONFIG.activityName + 'Btncls').onclick = function() {
                    document.documentElement.style.background = '#fff';
                    document.body.style.background = "#fff";
                    pthis.$(CONFIG.activityName + 'TopBar').style.display = "none";
                    pthis.$(CONFIG.activityName + 'TopBlank').style.display = "block";
                    pthis.setAdCookie(CONFIG.activityName, 0, 1440);
                };
            } catch (e) {}
        }

        var cookie = pthis.getAdCookie(CONFIG.activityName);
        cookie = cookie == "" ? 1 : cookie;
        if (cookie == 1) {
            pthis.init();
        }
})();
</script><!-- 飘红结束 -->

	
