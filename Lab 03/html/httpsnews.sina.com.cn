					$(scrollItems[--currentIndex]).show();
					if (currentIndex === 0) {
						prevBtn.removeClass('pre').addClass('pre_dis');
					}
				}
			});
			nextBtn.on('click', function (evt) {
				if (currentIndex < total - 1) {
					prevBtn.removeClass('pre_dis').addClass('pre');
					$(scrollItems[currentIndex]).hide();
					$(scrollItems[++currentIndex]).show();
					if (currentIndex === (total-1)){
