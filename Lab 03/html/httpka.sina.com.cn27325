		var tabs = function(e1, e2){
		    var e1 = $('a', e1);
		    var ee = e2;
		    var e2 = $(e2);
		    e1.click(function(){
		        if(!$(this).hasClass('current')){
			        e1.removeClass('current');
			        $(this).addClass('current');
			        var idx = e1.index(this);
			        e2.slideUp(0);
			        $(e2[idx]).slideDown(0);
			        var comidx = 3;
			        comidx-=1;			        
			        if(idx == comidx){
			        	var url = "http://widget.weibo.com/distribution/comments.php?width=0&url=auto&dpc=1";
