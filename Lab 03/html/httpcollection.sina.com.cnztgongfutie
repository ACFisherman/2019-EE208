seajs.use(['jquery','sina.SubShow'],function($,SubShow){
	var lab = $('.main_l li'),
		cont = $('.main_r .main_cont'),
		l = Math.min(lab.length,cont.length),
		ssc = new SubShow('none','onclick','','selected');
	for(var i = 0; i < l; i++){
		lab[i].setAttribute('id','lab0' + i);
		cont[i].setAttribute('id','cont0' + i);
		ssc.addLabel('lab0' + i,'cont0' + i);
	}
});
