<script>
String.prototype.substr2=function(a,b){
 var s = this.replace(/([^\x00-\xff])/g,"\x00$1");
 return(s.length<b)?this:s.substring(a,b).replace(/\x00/g,'');
}
function showHotnews(data_arr){
	var html= '';
	var backcolor='';

	data = data_arr['data'];
	for(var i in data){
		data[i].title=data[i].title.replace('"','&quot;');

		var ranktitle = data[i].title;
		var rankstitle = ranktitle.substr2(0,37);
		html+='<li class="clearfix"><em>'+(parseInt(i)+1)+'</em><a href="'+data[i].url+'" target="_blank" title="'+ranktitle+'">'+rankstitle+'</a></li>';
	}
	document.write(html);
}
</script>

