    /*资讯图片延迟加载*/
    $('.tuijian img[data-src]').live('inview', function(event, isVisible) {
        if (!isVisible) {
            return;
        }
        var _this = $(this);
        _this.load(function() {
            _this.css({opacity: 1});
        });
        _this.attr('src', _this.attr('data-src'));
        _this.removeAttr('data-src');
    });

    /*资讯图片延迟加载*/
