    function loadRecom(page){
        page = page ? page : 1;
        $.ajax({
            type: 'GET',
            cache : false,
            dataType:'json',
            url:"/ajax_list/get_search_article_list?kw=S9&page="+page,
            success:function(msg){
                if(msg.status == "succ"){
                    var html = "";

                    for(var i in msg.game_list){
                        var data = msg.game_list[i];
                        html += '<div class="main_list_onepic"><div class="article_pic"><a href="/article/'+data._id+'.shtml"><img src="http://image2.sina.com.cn/gm/t_img/kangame/images/nonebg1.jpg" data-src="'+data.recommendpic+'" />';
                        if(data.videoid ){
                            html += '<i class="video_btn"></i>';
                        }
                        html += '</a></div><h3 class="article_title"><a  href="/article/'+data._id+'.shtml" target="_blank">'+data.title+'</a></h3>';
                        html += '<p class="article_introduction">'+data.summary+'</p><div class="new_list_other"><span class="time">'+data.cTime+'</span><span class="from">作者：';
                        html += '<a href="javascript:void(0);">'+data.author+'</a></span><span class="article_share bdsharebuttonbox" data-tag="share_1"><a title="分享到新浪微博" href="javascript:void(0);" class="share_btn weibo" data-cmd="tsina"></a><a title="分享到微信" href="javascript:void(0);" class="share_btn weixin" data-cmd="weixin"></a><a href="javascript:void(0);" data-cmd="more" class="share_btn mshare" data-cmd="more"></a><a href="http://comment5.news.sina.com.cn/comment/skin/default.html?channel=yx&newsid=comos-'+data._id+'" target="_blank" class="share_btn commont"></a></span></div></div>';
                    }

                    var totalNum = msg.num;
                    var pagenum = Math.ceil(totalNum/10);
                    var pagehtml = "";
                        pagehtml = msg.page;
                    
                    html += pagehtml;
                    $(".search_result1").html(html);
                    $(window).scrollTop(0);
                    _bd_share_main.init();
                }
            }
        });
    };
  
//右侧 浮动 
     (function(){
        var RightTop = $('.top_right').offset().top;
        $(window).scroll(function() {
            var LeftTop = $('.top_left').offset().top;
            var Leftheight = $('.top_left').height();
            var RightHeight = $('.top_right').height();
            var WindowHeight = $(window).height();
            var ScrollTop = $(window).scrollTop();
            if(Leftheight>RightHeight){
                if(ScrollTop>=RightTop+RightHeight-WindowHeight){
                if(ScrollTop<=LeftTop+Leftheight-WindowHeight){
                    $('.top_right').addClass('rfixed');
                    $('.top_right').removeClass('rfixed2');
                }else if(ScrollTop>LeftTop+Leftheight-WindowHeight) {
                    $('.top_right').addClass('rfixed2');
                }
                }else{
                    $('.top_right').removeClass('rfixed rfixed2');
                }
            }
        })
    })();

</script>
