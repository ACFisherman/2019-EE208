    <script type="text" id="qx_tmpl">
        <div class="query_result qx_result">
            <h2 class="result_title"><%=it.title%></h2>
            <p class="hint">点击学校/专业名称查询该校历年录取情况</p>
            <%for(var i=0;i<it.data.length;i++){%>            <table class="qx_data data_table">
                <tbody>
                    <tr>
                        <th colspan="4"><%=it.data[i].pcStr%></th>
                    </tr>
                    <tr>
                        <th class="qx_data_col1">院校名称</th>
                        <th class="qx_data_col2">专业名称</th>
                        <th class="qx_data_col3">同分录取人数</th>
                        <th class="qx_data_col4">专业录取人数</th>
                    </tr>
                    <%for(var j=0;j<it.data[i].length;j++){%>                    <tr>
                        <!--TODO-->
                        <td><a target='_blank' href="<%=it.data[i][j].href_college%>"><%=it.data[i][j].college_name%></a></td>
                        <td><a target='_blank' href="<%=it.data[i][j].href_major%>"><%=it.data[i][j].major_name%></a></td>
                        <td><%=it.data[i][j].score_num%></td>
                        <td><%=it.data[i][j].major_num%></td>
                    </tr>
                    <%}%>                </tbody>
            </table>
            <%}%>        </div>
    </script>
