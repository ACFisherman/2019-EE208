    <script type="text/javascript">
        var rankUrl = "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index";
        var rankData = "_sport_t_=football&_sport_s_=opta&_sport_a_=teamOrder&type=3";
        
		// TODO 20190506
		_jsonp2019(
			rankUrl+'&'+ rankData,
			{
				name: 'CB_75AB6CC0_51F9_4785_A121_4F78919ED66C',
				dpc:true
			},
			function (_,data) {
				getRankList(data);
			}
		)
		// SINA.IO.getJSONP(rankUrl, rankData, getRankList);
		
        function getRankList(data){
            var datas = data.result && data.result.data;
            var str = "";
            if(datas){
				var data_num = 18;
				if(data_num != "" && data_num < datas.length){
					data_num = 18;
				}else{
					data_num = datas.length;
				}
                for(var i = 0; i < data_num; i ++){
					if(parseInt(datas[i].team_order) < 3+1)str += '<tr class="item"><td class="blk">'+datas[i].team_order+'</td><td><a href="http://match.sports.sina.com.cn/football/team.php?id='+datas[i].sl_id+'&l_type='+ datas[i].l_type +'" target="_blank" class="more">'+datas[i].team_cn+'</a></td><td class="num">'+datas[i].win+'</td><td class="num">'+datas[i].draw+'</td><td class="num">'+datas[i].lose+'</td><td class="red">'+datas[i].score+'</td></tr>';
                    else str += '<tr class="item"><td class="num">'+datas[i].team_order+'</td><td><a href="http://match.sports.sina.com.cn/football/team.php?id='+datas[i].sl_id+'&l_type='+ datas[i].l_type +'" target="_blank" class="more">'+datas[i].team_cn+'</a></td><td class="num">'+datas[i].win+'</td><td class="num">'+datas[i].draw+'</td><td class="num">'+datas[i].lose+'</td><td class="red">'+datas[i].score+'</td></tr>';
                }
            }
            var t = $("#ranklist").html();
            $("#ranklist").html(t+str);
            $("#ranklist .item").hover(function(){
                $(this).toggleClass("selected");
            });
        }
    </script>
