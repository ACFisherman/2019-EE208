		var _jsonp2019 = (function(){
			var debug=function(){};var count=0;function noop(){}function jsonp(url,opts,fn){if("function"==typeof opts){fn=opts;opts={}}if(!opts){opts={}}var prefix=opts.prefix||"__jp";var id=opts.name||(prefix+(count++));var param=opts.param||"callback";var timeout=null!=opts.timeout?opts.timeout:60000;var enc=encodeURIComponent;var target=document.getElementsByTagName("script")[0]||document.head;var script;var timer;if(timeout){timer=setTimeout(function(){cleanup();if(fn){fn(new Error("Timeout"))}},timeout)}function cleanup(){if(script.parentNode){script.parentNode.removeChild(script)}window[id]=noop;if(timer){clearTimeout(timer)}}function cancel(){if(window[id]){cleanup()}}window[id]=function(data){debug("jsonp got",data);cleanup();if(fn){fn(null,data)}};url+=(~url.indexOf("?")?"&":"?")+param+"="+enc(id);url+=(opts.dpc?"&dpc=1":"");url=url.replace("?&","?");debug('jsonp req "%s"',url);script=document.createElement("script");script.src=url;target.parentNode.insertBefore(script,target);return cancel};
			return jsonp;
		})();
		
        var listId = $("#com_weibo").attr("newsid");

        var resultUrl = "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index";
        var rndData = '_sport_t_=Intlfootball&_sport_a_=getLeaguesInfo&app_key=3571367214';
		var resultData = "app_key=3571367214&_sport_t_=livecast&season=2019&_sport_a_=matchesByType&type=3&rnd=";
        var total_rnd;//整个赛季总轮次数
        var current_rnd;
        function getResult(rnd){
            if(rnd > 0 && rnd < total_rnd + 1){
                current_rnd = rnd;
				
				// dpc
				_jsonp2019(
					resultUrl+'&'+ resultData+rnd,
					{
						name: 'CB_75AB6CC0_51F9_4785_A121_4F78919ED661',
						dpc:true
					},
					function (_,ret) {
						getResultList(ret)
					}
				)
				// SINA.IO.getJSONP(resultUrl, resultData+rnd, getResultList);//指定轮次
            }
            else return;
        }
        function getResultList(data){
            var datas = data.result && data.result.data;
            var str = "";
            if(datas){
                for(var i = 0; i < datas.length; i ++){
                    var str1, str2;
                    if(i % 3 == 2)str1 = '<div class="result_item norm">';
                    else str1 = '<div class="result_item">';
                    var str2 = '<a class="score" target="_blank" href="'+datas[i].LiveUrl+'"><span class="matchtime">'+datas[i].date.split("-").join(".")+'&nbsp;'+datas[i].time+'</span><span>'+datas[i].Team1+'</span><span class="red">'+chk(datas[i].status,datas[i].Score1)+'</span><br><span>'+datas[i].Team2+'</span><span class="red">'+chk(datas[i].status,datas[i].Score2)+'</span></a>';
                    var link1, link2, link3, link4;
                    if(datas[i].VideoLiveUrl == "")link1 = '<div class="match_links">';
                    else link1 = '<div class="match_links"><a href="'+datas[i].VideoLiveUrl+'" target="_blank" class="wht">直播</a>';
                    if(datas[i].NewsUrl == "")link2 = '<span class="white">战报</span>';
                    else link2 = '<a href="'+datas[i].NewsUrl+'" target="_blank" class="wht">战报</a>';
                    if(datas[i].VideoUrl == "")link3 = '<span class="white">视频</span>';
                    else link3 = '<a href="'+datas[i].VideoUrl+'" target="_blank" class="wht">视频</a>';
                    if(datas[i].ImgUrl == "")link4 = '<span class="white">图集</span></div></div>';
                    else link4 = '<a href="'+datas[i].ImgUrl+'" target="_blank" class="wht">图集</a></div></div>';
                    str += str1+str2+link1+link2+link3+link4;
                }
                $("#current_rnd").html(current_rnd);
            }
            $("#resultlist").html(str);
        }
        function chk(s,t){
            if(s==1 || s==2)return "-";
            else if(s==3) return t;
        }
        function pre_rnd(){
            getResult(current_rnd - 1);
        }
        function next_rnd(){
            getResult(current_rnd + 1);
        }
        // dpc
		_jsonp2019(
			resultUrl+'&'+ rndData,
			{
				name: 'CB_75AB6CC0_51F9_4785_A121_4F78919ED66D',
				dpc:true
			},
			function (_,data) {
				var datas = data.result && data.result.data;
				if(datas){
					current_rnd = parseInt(datas["3"].cur_rnd);
					total_rnd = parseInt(datas["3"].max_rnd);
					getResult(current_rnd);//获取当前轮赛况
				}
			}
		)
        // SINA.IO.getJSONP(resultUrl, rndData, function(data){
        //     var datas = data.result && data.result.data;
        //     if(datas){
        //         current_rnd = parseInt(datas["3"].cur_rnd);
        //         total_rnd = parseInt(datas["3"].max_rnd);
        //         getResult(current_rnd);//获取当前轮赛况
        //     }
				// });
    </script>
</div>
			</div>
