                        node.className = 'right_fixed_area';
                    }else{
                        top = getOffsetTop(node);
                        node.className = '';
                    }
                }
                if(win.addEventListener){
                    win.addEventListener('scroll', check, false);
                }else if(win.attachEvent){
                    win.attachEvent('onscroll', check);
                }
                check();
            };
            scrollFix('right_fixed_area');
        })();
    </script>
