                                $('#j_wrap_2 .table-a__more').attr('href','https://slamdunk.sports.sina.com.cn/player/rank#season_type=reg&item_type=average&item='+item);
                            }
                        });
                        event.preventDefault();
                    }]
                ]
            });
            loader1.read();
        }
        renderTable({s:"leaders_daily", a:"player",date:getTimestamp+''});
        renderTable({s:"leaders", a:"player_top", season_type:"reg"});
        Tab2 = new Tab.single('#j_wrap_2 .tabsType',{
            afterChange:function(nav,cont){
