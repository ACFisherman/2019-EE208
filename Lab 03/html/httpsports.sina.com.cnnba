                },
                currentClz: 'current',
                // 检测数据返回的正确性
                hasRightResult: function(data) {
                    try {
                        if (data.result.status.code === 0) {
                            return true;
                        }
                    } catch (e) {
                        return false;
                    }
                },
                template:doT.template([
                    '{{for (var prop in it){}}',
                    '<div tab-type=tab-cont class="clearfix tab-cont {{=prop}}">',
                    '<div tab-type="tab-wrap" tab-data="" class="table2">',
                    '<div class="tabs tab-a clearfix"><a tab-type="tab-nav" class="nav-selected" href="" data-item="points">得分</a><a tab-type="tab-nav" href="javascript:;" data-item="rebounds">篮板</a><a tab-type="tab-nav" href="javascript:;" data-item="assists">助攻</a><a tab-type="tab-nav" href="javascript:;" data-item="steals">抢断</a><a tab-type="tab-nav" href="javascript:;" data-item="blocks">盖帽</a></div>',
                    '<div class="conts sub-conts" >',
