                    // 加载成功
                    ['success', function(event, data) {
                        var self = event._self;
                        //获取所需格式的数据
                        if(reqData.s==='leaders_daily'){
                            window.__sina_sports_day_stats=data.result.data.items;
                        }else if(reqData.s==='leaders'){
                            window.__sina_sports_per_game=data.result.data.items;
                        }
                        var __sina_sports_day_stats=window.__sina_sports_day_stats;//每日统计
                        var __sina_sports_per_game=window.__sina_sports_per_game;//场均
                        var array=['points','rebounds','assists','steals','blocks']//points 得分， reb 篮板 ，ass 助攻 ，ste 抢断， blo 盖帽
                        var dataFormat=function(d){
                            var res={};
                            if(!d){return;}
                            for(var i=0,len=d.length;i<len;i++){
                                var name = d[i].item.name||d[i].item;
                                if(array.toString().indexOf(name)!=-1){
                                    res[name]=d[i]['players'];
                                    for(var j=0,len=d[i]['players'].length;j<len;j++){
                                        res[name][j].player_url='https://slamdunk.sports.sina.com.cn/player?pid='+d[i]['players'][j].pid;
                                        res[name][j].team_url='https://slamdunk.sports.sina.com.cn/team?tid='+d[i]['players'][j].tid;
                                    };
                                }
                            }
                            return res;
                        };
                        var resData={};
                        resData.dayStats=dataFormat(__sina_sports_day_stats);
                        resData.perGame=dataFormat(__sina_sports_per_game);
                        self.dom.list.hide().html(self.opt.template(resData)).show();
                        $("#j_wrap_2 .main-conts .tab-cont:eq(0)").addClass("cont-selected");
                        $("#j_wrap_2 .sub-conts .points").addClass("cont-selected");
                        $("#j_wrap_2 .perGame .mean").html('场均');
                        Tab3 = new Tab.single('.dayStats .table2',{
