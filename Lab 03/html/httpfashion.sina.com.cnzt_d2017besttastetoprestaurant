<script type="text/javascript">
   //右侧浮层
	var width = $(window).width();
    var left = (width-1000)/2+1010+'px';
    $('.right-fix').css({'left':left,'display':'block'});
    $('.btn_fix0').hover(
        function(){
            $(this).find('#fix0_c').css('display','block');
        },
        function(){
            $(this).find('#fix0_c').css('display','none');
    });
    $('.btn_fix2').hover(
        function(){
            $(this).find('#fix2_c').css('display','block');
        },
        function(){
            $(this).find('#fix2_c').css('display','none');
    });
     //回到顶部隐藏
    $(window).scroll(function(){

        var $windowHeight = $(window).height();
        var $windowWidth = $(window).width();
        var navLeft = ($windowWidth-1000)/2+'px';
        if($(this).scrollTop() > 600){
            $('#nav').css({'position':'fixed','top':0,'left':0,'z-index':99999});
        }else{
            $('#nav').css({'position':'static'});
        }
        if($(this).scrollTop() < ($windowHeight*5/3)){
            $("#btn_fix0").css('display','none');
        }else{
            $("#btn_fix0").css('display','block');
        }
    });
    //返回顶部
    $('#btn_fix0').on('click', function(){
        $("html, body").animate({ scrollTop: 0 }, 500);
    });
    //导航栏操作
    $("#nav ul li").click(function(){
        $(this).find('a').addClass("nav-cur");
        $(this).siblings().find('a').removeClass("nav-cur");
        var id = $(this).find('a').attr('data-id');
        $(document).scrollTop( $('#'+id).position().top - 40 );
    });
</script>
