function cutString(s, n, e) {
	var k = 0;
	for (var i=0; i<s.length; i++) {
		if (s.charCodeAt(i)<=127) {
			k += 1;
		} else {
			k += 2;
		}
		if (k>n) {
			var ns = s.substr(0,i-1)+e;
			return (ns);
		}
	}
	return (s);
}
</script>

