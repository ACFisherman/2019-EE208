<script language="javascript">
	function goto_page(page_num, str_pass)
	{
		if(isDigit(page_num) == true)
		{
			if(page_num >= 1 && page_num <= 1000)
			{
				window.location = "/corp/view/vCB_BulletinGather.php?"+str_pass+"page=" + page_num + "";
				return false;
			}
			else
			{//输入小于首页页号则跳到首页
				window.location = "/corp/view/vCB_BulletinGather.php?"+str_pass+"page=1";
				return false;
			}
		}
		else
		{
			window.location = "/corp/view/vCB_BulletinGather.php?"+str_pass+"page=1";
			return false;
		}
	}
	function isDigit(s)
    {
    	var patrn=/^[0-9]{1,4}$/;
    	if (!patrn.exec(s)) return false;
    	return true ;
    }
</script>

