            <script type="text/javascript">
            var typeStr = {
                '0':'明星',
                '1':'美妆',
                '2':'T台',
                '3':'机场',
                '4':'视频',
                '5':'点评'    
            };
            function formateNewsDate(data,type){
                var ret = [];
                if(type=='all'){
                    ret = data;
                }else{
                    for(var i = 0;i<data.length;i++){
                        var tempData = data[i];
                        if(tempData.showtype==type){
                            ret.push(tempData);
                        }
                    }   
                }
                
                return ret;
            }
            $.ajax({
                url:'http://fashion.sina.com.cn/zt/s/api_ss2019women_news_bl.json',
                data:{},
                jsonpCallback:'nwFunction',
                dataType:'jsonp',
                success:function(res){
                    if(res&&res.result&&res.result.data){
                        //console.log(res.result.data.day2.length);
                        var _retData = res.result.data;
                        _randerData({
                            tmplId:'tmplNewsList',
                            targetId:'newsList01',
                            data: _retData  
                        });
                        $('.item-news').on('mouseover',function(){
                            $(this).addClass('isHover');
                        });
                        $('.item-news').on('mouseout',function(){
                            $(this).removeClass('isHover');
                        });
                        $('.tab-btns-inner a').on('click',function(){
                            var _type= $(this).attr('data-type');
                            $('.tab-btns-inner a').removeClass('selected');
                            $(this).addClass('selected');
                            _randerData({
                                tmplId:'tmplNewsList',
                                targetId:'newsList01',
                                data: formateNewsDate(_retData,_type)  
                            });
                            $('.item-news').on('mouseover',function(){
	                            $(this).addClass('isHover');
	                        });
	                        $('.item-news').on('mouseout',function(){
	                            $(this).removeClass('isHover');
	                        });
                        });
                    }
                }
            });
            </script>
