                <script type="text/javascript">
                var firstShow = 2;//首次加载条数
                var pageSize = 2;//每次展开更多
                var _randerData = function(option){
                    //数据填充
                    var _tmplId = option.tmplId||'',
                        _targetId = option.targetId||'',
                        _data = option.data;
                    var interText = doT.template(document.getElementById(_tmplId).text);
                    document.getElementById(_targetId).innerHTML = interText(_data);
                }
                $.ajax({
                    url:'http://fashion.sina.com.cn/zt/s/api_ss2019women_daily_bl.json',
                    data:{},
                    jsonpCallback:'dayFunction',
                    dataType:'jsonp',
                    success:function(res){
                        if(res&&res.result&&res.result.data){
                            //console.log(res.result.data.day2.length);
                            var _retData = res.result.data;
                            _randerData({
                                tmplId:'tmplDaysList',
                                targetId:'daysList',
                                data: _retData  
                            });
                            if($('.days-item').length>firstShow){
                                for(var i = 0;i<firstShow;i++){
                                    $('.days-item').eq(i).removeClass('hide');  
                                }
                                $('.days-list .btn-showmore').on('click',function(){
                                    var hideItems = $('.days-item.hide');
                                    //如果隐藏的还超过 pageSize,继续显示
                                    if(hideItems.length>pageSize){
                                        for(var j = 0;j<firstShow;j++){
                                            $('.days-item.hide').eq(j).removeClass('hide'); 
                                        }
                                    }else{
                                        $('.days-item').removeClass('hide');
                                        $('.days-list .btn-showmore').addClass('hide'); 
                                    }
                                });
                            }else{
                                $('.days-item').removeClass('hide');
                            }
                        }
                    }
                });
                </script>
