<script id="indexMatchTpl" type="text/x-dot-template">
    {{~it:item}}
    <a onclick="window.open('/match/{{=item.match_id}}')" class="index_top_match_content swiper-slide cl">
        <div class="index_match_team fll">
            <p><img src="{{=item.team_a_image}}">{{=item.team_a_short_name}}</p>
            <p><img src="{{=item.team_b_image}}">{{=item.team_b_short_name}}</p>
        </div>
        <div class="index_match_right flr">
            {{? item.status == 0 }}
            <!--未开始-->
            <p class="index_match_time">{{=item.start_date.substring(6, 10)}}</p>
            <p class="index_match_time">{{=item.start_time}}</p>
            {{?? item.status == 1}}
            <!--进行中-->
            <p class="index_match_live">LIVE</p>
            {{??}}
            <!--已结束-->
            <p class="{{=item.team_a_win > item.team_b_win ? 'win' : 'lost'}}">{{=item.team_a_win}}</p>
            <p class="{{=item.team_a_win < item.team_b_win ? 'win' : 'lost'}}">{{=item.team_b_win}}</p>
            {{?}}
        </div>
    </a>
    {{~}}
</script>        
