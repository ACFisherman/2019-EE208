	<script type="text/javascript">

		//显示各种信息


		$('.sider_tabs_con a:.LH').html('领').addClass('btn ling tab_btn');
		$('.ka_menu_con li').hover(function(){
			$(this).addClass('hover').find($('.km_con')).show();
		},function(){
			$(this).removeClass('hover').find($('.km_con')).hide();
		})

		$('.km_pic1').hover(function(){
			$(this).find($('.km_pic1_con')).show();
		},function(){
			$(this).find($('.km_pic1_con')).hide();
		})

		$('.km_pic2').hover(function(){
			$(this).find($('.km_pic2_con')).stop().animate({'width':'120px'},200);
		},function(){
			$(this).find($('.km_pic2_con')).stop().animate({'width':'0px'},200);
		})

		$('.ka_gm').hover(function(){
			$(this).find('i').stop().fadeIn();
			$(this).find('.ka_gm_intro').stop(true,false).fadeIn();
		},function(){
			$(this).find('i').stop().fadeOut();
			$(this).find('.ka_gm_intro').stop(true,false).fadeOut();
		})

		$('.calendar').hover(function(){
			$(this).find($('.cal_con')).show();
			
		},function(){
			$(this).find($('.cal_con')).hide();
		})
		//标签切换
		var tabs = function(e1, e2){
 
		    var e1 = $('a', e1);
		    var e2 = $(e2);
		    e1.mouseover(function(){
		        if(!$(this).hasClass('current')){
			        e1.removeClass('current');
			        $(this).addClass('current');
			        var idx = e1.index(this);
			        e2.slideUp(0);
			        $(e2[idx]).slideDown(0);
		        }
		        $('.scroll-pane').jScrollPane({showArrows: true});
		    });   
		    e1.click(function(){
		        return false;
		    })  
		};
		tabs('.newgames .sider_tabs li', '.newgames .sider_tabs_con ul');
		tabs('.rank .sider_tabs li', '.rank .sider_tabs_con ul');
		//tabs('.pri_tabs', '.pri_tabs_con ul');
		tabs('.gifts .main_tabs li', '.gift_tabs_con');
		tabs('.log .main_tabs li', '.log_con .log_con_ul');


                //自动标签切换
               var modifytimer = function(num){
                    e1 = ".pri_tabs";
                    e2 = ".pri_tabs_con";
                    $('a',e1).removeClass('current');
                    $("a:eq("+num+")",e1).addClass('current');
                }
                var modifytimer2 = function(num){
                    e1 = ".pri_tabs";
                    e2 = ".pri_tabs_con";
                    $('a',e1).removeClass('current');
                    $("a:eq("+num+")",e1).addClass('current');
                    //
                     clearInterval(timer);
                  
                    switch(num){
                        case 0:
                            $("#moveTab").css("-o-transform","translate3d(0px,0px,0px)");
                            $("#moveTab").css("-moz-transform","translate3d(0px,0px,0px)");
                            $("#moveTab").css("-webkit-transform","translate3d(0px,0px,0px)");
                            step=3;
                            
                            break;
                        case 1:
                             $("#moveTab").css("-o-transform","translate3d(-760px,0px,0px)");
                             $("#moveTab").css("-moz-transform","translate3d(-760px,0px,0px)");
                             $("#moveTab").css("-webkit-transform","translate3d(-760px,0px,0px)");
                            step=1;
                             
                            break;
                        case 2:
                             $("#moveTab").css("-o-transform","translate3d(-1520px,0px,0px)");
                             $("#moveTab").css("-moz-transform","translate3d(-1520px,0px,0px)");
                             $("#moveTab").css("-webkit-transform","translate3d(-1520px,0px,0px)");
                            step=2;
                            break;
                    }
                    step++;
                    if(step>3){
                      step=1; 
                    }
                     timer = setInterval("timertabs()",5000);
                }
                $(".pri_tabs_con").hover(function(){
                        clearInterval(timer);
                    },function(){
                        timer = setInterval("timertabs()",5000);
                 })
        
                var step=1;
                var timertabs = function(){
                    //console.log("step="+step);
                    switch(step){
                        case 1:
                            $("#moveTab").css("-o-transform","translate3d(-760px,0px,0px)");
                             $("#moveTab").css("-moz-transform","translate3d(-760px,0px,0px)");
                             $("#moveTab").css("-webkit-transform","translate3d(-760px,0px,0px)");
                            modifytimer(1);
                            break;
                        case 2:
                             $("#moveTab").css("-o-transform","translate3d(-1520px,0px,0px)");
                             $("#moveTab").css("-moz-transform","translate3d(-1520px,0px,0px)");
                             $("#moveTab").css("-webkit-transform","translate3d(-1520px,0px,0px)");
                             modifytimer(2);
                            break;
                        case 3:
                            $("#moveTab").css("-o-transform","translate3d(0px,0px,0px)");
                            $("#moveTab").css("-moz-transform","translate3d(0px,0px,0px)");
                            $("#moveTab").css("-webkit-transform","translate3d(0px,0px,0px)");
                             modifytimer(0);
                            break;
                    }
                    step++;

                    if(step>3){
                      step=1; 
                    }


                }

                var timer = setInterval("timertabs()",5000);
               
		//日历
		 $(function() {
			var nowdays = new Date();

			var dateConfig = {
				showOn: 'both',               //二个都显示
				buttonImage: 'calendar.gif',  //加载图片
				buttonImageOnly: true,        //显示图片的地方有一个突出部分，这个就是隐藏那玩意的
				changeFirstDay: false,        //这个参数干什么的呢，星期一是日历的第一个，不可以改动的
				numberOfMonths: 1,            //显示二个月，默认一个月
				minDate: nowdays,             //显示最小时间是今天
				dateFormat: 'yy-mm-dd',       //日期格式
				yearRange: '-5:+5'          //前后20年，不过这根minDate是今天有冲突，自己去试吧。
			};

			$('#showdate').datepick(dateConfig);
		});

		//滚动条
		$('.scroll-pane').jScrollPane({showArrows: true});

		//弹出框
		// popup($('.SG_Login_box'),$('.SG_form_closed'));
		
		//左边二维码显示
		//function autoPosition(){
		//	var client_width = document.documentElement.clientWidth;
		//	if(client_width < 1370){
		//      $('#wx_img').css({ "right":"","left":"-100px" });
		//    } else {
		//      $('#wx_img').css({ "right":"50%","left":"" });
		//    }
		//}
		//autoPosition();
		//$(window).resize(function(){
		//	autoPosition();
		//});
		
		$("#wx_img").hover(function(){
			var client_width = document.documentElement.clientWidth;
			if(client_width < 1300){
				$("#wx_img").css({"left":"0px"});
			}
		},function(){
			$("#wx_img").css({"left":"","right":"50%"});
		});
		
		
	</script>
	
