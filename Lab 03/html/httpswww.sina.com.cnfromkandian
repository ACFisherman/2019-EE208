                var focusScroll = new ScrollPic();
                focusScroll.scrollContId = "SI_Scroll_Wrap_t0" + i; //内容容器ID
                focusScroll.dotListId = "SI_Scroll_Dot_Lists_t0" + i;//点列表ID
