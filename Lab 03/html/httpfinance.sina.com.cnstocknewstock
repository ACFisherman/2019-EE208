<script>
    if(typeof loginSuccessCallbacks === 'undefined'){window.loginSuccessCallbacks = [];}
    if(typeof logoutSuccessCallbacks === 'undefined'){window.logoutSuccessCallbacks = [];}
    if(typeof logined === 'undefined'){window.logined = false;}
    var userPanel = window.SINA_USER_PANEL;
    userPanel.STK.Ready(function(){
        userPanel.set('outloginLayer', {						//配置登录浮层相关内容
            ready : function(){								//登录浮层渲染成功后执行的方法，必选
                var loginLayer = window.SINA_OUTLOGIN_LAYER;	//获取登录浮层
                loginLayer.set('sso', {
                    entry : 'finance'
                }).set('extra',{
                    css : 'http://i.sso.sina.com.cn/css/outlogin/v1/outlogin_skin_finance.css'
                }).register("layer_hide", function(){
                    loginLayer.set("plugin",{
                        position:"custom"
                    })
                }).register('login_success', function(){
                    window.logined = true;
                    for(var i=0; i<loginSuccessCallbacks.length; i++) {
                        loginSuccessCallbacks[i]();
                    }
                }).register('logout_success', function(){
                    window.logined = false;
                    for(var i=0; i<loginSuccessCallbacks.length; i++) {
                        logoutSuccessCallbacks[i]();
                    }
                }).init();

            },
            triggerEvent: 'hover'								//触发显示登录浮层的事件，可选择hover或click
        }).set('container',{									//设置用户面板的父节点，不设置则为body
            node: document.getElementById("userLogin")
        }).init();												// 初始化用户面板，必选
    })
</script>
