<script type="text/javascript">
$(function(){
    $("#gotopbtn").click(function(){
        $("html, body").animate({ scrollTop: 0}, 120);
	    sb();
    })
    window.onscroll=sb;
})
function sb(){
	if($("html, body").scrollTop()==0){
		$("#gotopbtn").hide();
	}else{
	    var sc_w = $(window).width();
	    var wtemp = (sc_w-950)/2;
	    wtemp = wtemp+950+20;
	    $("#gotopbtn").css({"left":wtemp});
		$("#gotopbtn").show();
	}
}
</script>
