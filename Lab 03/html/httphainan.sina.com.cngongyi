function imgcallback(msg, url,width){
    if(msg == ""){
	    $("#userpic").attr("value", url);
	    $("#show_wz").hide();
	    $("#show_img").show();
	    $("#show_img img").attr("src", "http://"+url);
        if(width < 280){
	        $("#show_img img").attr("width", width);
	    }else{
	        $("#show_img img").attr("width", "280");
	    }
	}else{
	    alert(msg);
		$("#show_wz").hide();
		res_file();
		$("#show_inp").show();
	}
}

