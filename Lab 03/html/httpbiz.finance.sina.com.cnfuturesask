<script language="javascript">
	function addEvent(oTarget, sEventType, fnHandler){
		if (oTarget.addEventListener) {
			oTarget.addEventListener(sEventType, fnHandler, false);
		}
		else if(oTarget.attachEvent) {
			oTarget.attachEvent("on" + sEventType, fnHandler);
		}
		else {
			oTarget["on" + sEventType] = fnHandler;
		}
	}
	var FutureOversea = {
		sinajsUrl: "http://hq.sinajs.cn/list=",
		hfLink: '<a href="http://finance.sina.com.cn/money/future/quote_hf.html?@CODE@" target="_blank">@NAME@</a>',
		
		rowArr: null,
		tempQuote: [],
		Init: function () {
			if (!this.rowArr) {
				this.rowArr = $("futureOverseaTbl").tBodies[0].rows;
			}
			if (!this.codeArr) {
				this.codeArr = [];
				for (var i in this.codeObj) {
					this.codeArr.push(this.codeObj[i]);
				};
			}
			this.scriptLoader();
			var quoteInterval = window.setInterval(this.scriptLoader.Bind(this),60000);
		},
		scriptLoader: function () {
			var sLoader = new IO.Script();
			var url = this.sinajsUrl + this.codeArr.join(",");
			sLoader.load(url, this.update.Bind(this));
		},
		update: function () {
			var j = 0;
			for (var i in this.codeObj) {
				this.tempQuote = window["hq_str_" + this.codeObj[i]].split(",");
				this.rowArr[j].cells[0].innerHTML = i;
				var _shortCode = this.codeObj[i].replace("hf_", "");
				this.rowArr[j].cells[0].innerHTML = this.hfLink.replace("@NAME@", i).replace("@CODE@", _shortCode);
				
				if (this.tempQuote == []) {
					this.rowArr[j].cells[1].innerHTML = "--";
					this.rowArr[j].cells[2].innerHTML = "--";
					j++;
					continue;
				}

				var sSymbol = (this.tempQuote[0] - this.tempQuote[7]) != 0 ? ((this.tempQuote[0] - this.tempQuote[7]) > 0 ? "+" : "-") : "";

				this.rowArr[j].cells[1].innerHTML = this.colorRender((this.tempQuote[0] * 1).toFixed(2));
				this.rowArr[j].cells[2].innerHTML = this.colorRender(sSymbol + (this.tempQuote[1] * 1).toFixed(2));
				j++;
			};
		},
		colorRender: function (targetText) {
			var tempColor = "#000",tempChange = this.tempQuote[0] - this.tempQuote[7];
			if (tempChange > 0) {
				tempColor = "#f00";
			}
			else if (tempChange < 0) {
				tempColor = "#008000";
			}
			return '<span style="color:' + tempColor + ';">' + targetText + '</span>';
		},
		codeObj: {
			"美黄豆":"hf_S",
			"美玉米":"hf_C",
			"日本橡胶":"hf_TRB",
			"NYBOT-11糖":"hf_SB",
			"LME3月铜":"hf_CAD",
			"LME3月锌":"hf_ZSD",
			"LME3月铝":"hf_AHD",
			"原油":"hf_CL",
			"黄金":"hf_GC",
			"白银":"hf_SI"
		}
	}

	addEvent(window, "load", FutureOversea.Init.Bind(FutureOversea));
	//-->
</script>
