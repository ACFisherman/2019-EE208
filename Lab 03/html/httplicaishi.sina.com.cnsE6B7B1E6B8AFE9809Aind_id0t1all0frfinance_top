        <script type="text/javascript">
            // 字符串去掉空格
            function stringTrim(str){
                return str.replace(/(^\s*)|(\s*$)/g, "");
            }
            // 将字符串转json
            function attrToJson(str){
                var params = {};
                var paramsStr = str ? str : '';
                var paramArray = paramsStr.split(';');
                for(var i=0; i<paramArray.length; i++){
                    if(paramArray[i].length > 0){
                        var paramArray = paramArray[i].split(":");
                        var key = stringTrim(paramArray[0]);
                        var val = stringTrim(paramArray[1]);
                        params[key] = val
                    }
                }
                return params;
            }
        </script>
        <!--神策埋点-end-->
