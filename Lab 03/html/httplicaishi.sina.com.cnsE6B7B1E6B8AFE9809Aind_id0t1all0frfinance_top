		seajs.use(['$','tree.widget.qaTimer'], function(){
    		T.Widget.QaTimer(function (el, times) {
    			if (!!times) {
    				for (var key in times) {
    					if (times.hasOwnProperty(key)) {
    						el.find('.' + key).text(times[key])
    					}
    				};
    			} else {
                    var qid = el.attr('data-id'),
                        url = (window.API_DOMAIN || 'http://licaishi.sina.com.cn') + '/ask/' + qid;
    				el.attr('timer-stop', 'stop').find('.w_vp_content')
    				.html('私密部分已经公开。<a href="'+ url +'">［立即查看］</a>');
    			}
    		});
    	});

