<script type="text/javascript">
    /*****************模块加载控制器定义*******************/
    window.$widget = [];
    window.$widgetconfig = {
        jspath: 'http://sjs2.sinajs.cn/video/theme/cms_widget/js/',
        csspath: 'http://sjs2.sinajs.cn/video/theme/cms_widget/css/',
        jsversion: '1.0', //js版本号
        cssversion: '1.0' //css版本号
    }
    window.$widgetids = [];
    window.$widgetadd = function(obj){
        var flag = false;
        var idx = -1;
        for(var i in window.$widgetids) {
            if(window.$widgetids[i] == obj.id) {
                flag = true; 
                idx = i;
                break;
            }
        }
        if(flag) {
            window.$widgetids.splice(idx,1,obj.id);
            window.$widget.splice(idx,1,obj);               
        }
        if(!flag) {
            window.$widgetids.push(obj.id);
            window.$widget.push(obj);
        }
    }
    window.$widget_single = function(id) {
        var r = [];
        for(var i in window.$widget) {
            if(id == window.$widget[i].id) {
                r.push(window.$widget[i]);
            }
        }
        return r;
    }
    window.$color_main = {
        base: '#cc0000',
        opposite: '#FF0000'
    };
    /**************php打印到页面上的数据*****************/
    //有些数据php可直接打印到页面上方便读取。所有打印的数据都在命名空间_SINANEWSDATA下
    window._SINANEWSDATA = {};
</script>
