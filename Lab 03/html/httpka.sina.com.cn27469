		//评分
		$('.cm3_r_star').mousemove(function(e) {
			var width = $(this).find('div').width();
			var left = e.pageX-$(this).offset().left;
			var star1 =  Math.floor(left/width);
			var star2 = left/width-star1;
			if(star2>0&&star2<0.5) {
				star2 = 1;
			}

			if(star2>=0.5&&star2<1){
				star1++;
				star2 =0;
			}
			$(this).find('div:lt('+star1+')').removeClass().addClass('star1');
			$(this).find('div:gt('+(star1-1)+')').removeClass().addClass('star3');
			if(star2==1){
				$(this).find('div:eq('+star1+')').removeClass().addClass('star2');
			}
			//实际分数
			var first =star1;
			var second =  star2==1?0.5:0;
			var score = first+second;
			// console.log(score);
			starscore = score;
		});
		$('.cm3_r_star').click(function(e){
			pingfen(starscore*2);
		})
		
