		function newcard_timer(Second){
		    
		    var diff = Second;
		    
		    if(diff <= 1 || diff >= 3598){
		        window.location.reload();
		        $('#newcard_count').hide()
		        $('#newcard_valid').show();
		        $('.BigLH').show();
		        //clearTimeout(timerID);
		        return false;
		    }
		    var diffMin = Math.floor( diff/60 );
		    var diffSec = Math.floor( diff%60 );
		    
		    var timeStr = diffMin +' 分'+ diffSec +' 秒';
		    
		    $('#newcard_timer').html(timeStr);
		    diff--;
	    	setTimeout("newcard_timer("+diff+")",1000);  
		    
		}
				
