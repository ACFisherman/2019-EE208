$(function(){
    $('#menuHandle').click(function(){
        if($(this).hasClass('opened')){
            $('.menu ul').slideUp();
            $(this).removeClass('opened')
        }else{
            $('.menu ul').slideDown();
            $(this).addClass('opened')
        }

    })
})

