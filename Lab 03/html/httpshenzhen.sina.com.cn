<script>


var Com = (function(mod){
    mod.tab = function(tabId,length){
        var _prev = $(tabId).find('.prev'),
            _next = $(tabId).find('.next'),
            _tabIndex = 1,
            _tabContent = $(tabId+'_Content' + _tabIndex);
        _prev.on("click", function(e){
            if(_tabIndex > 1){
                _tabIndex --;
                console.log(_tabIndex)
                _tabContent = $(tabId+'_Content' + _tabIndex);
                _tabContent.css('display','block').siblings().css('display','none');
            }
        });
        _next.on("click", function(e){
            if(1){
                if(_tabIndex == length){
                    _tabIndex = 0;
                }
                _tabIndex ++;
                console.log(_tabIndex)
                _tabContent = $(tabId+'_Content' + _tabIndex);
                _tabContent.css('display','block').siblings().css('display','none');
            }
        });
    },
    mod.tab1 = function(tabId){
        var _this = $(tabId).find('li'),
            _tabIndex = 0,
            _tabContent = $(tabId+'_Content' + _tabIndex);
        _this.on("mouseenter", function(e){
            _tabIndex = $(this).index() + 1;
            _tabContent = $(tabId+'_Content' + _tabIndex);
            _tabContent.css('display','block').siblings().css('display','none');
            $(this).addClass('on').siblings().removeClass('on');
        });
    },

    mod.slide = function(slideId){
    var prev = $(slideId + ' .prev'),
        next = $(slideId + ' .next'),
        index = 1,
        slideContent = $(slideId+'_Content' + 1);
        prev.on("click", function(e){
            if(index > 1){

                index --;

                console.log(index)
                slideContent = $(slideId+'_Content' + index);
                slideContent.css('display','block').siblings().css('display','none');
            }
        });
        next.on("click", function(e){
            if(index < 3){
                index ++;
                console.log(index)
                slideContent = $(slideId+'_Content' + index);
                slideContent.css('display','block').siblings().css('display','none');
            }
        });
    },
    mod.scrollTo = function(type){
        if(type==0){
            var $scrollTo = $('.scrollTo');
            $scrollTo.on('click',function(){
                $($(this).attr('href')).ScrollTo(800);
            })

        }
    },
    mod.scrollspy = function(){
        var $scrollspy = $('.scrollspy');
        $('.menu').animate({opacity:1},500)
        $(window).scroll(function(){
            //console.log(0)
            $scrollspy.each(function(){
                //console.log('元素'+$(this).offset().top)
                //console.log('滚动条'+$(window).scrollTop());
                if(($(this).offset().top - $(window).height() + 400) < $(window).scrollTop()){
                    $('.menu .'+ $(this).attr('id')).addClass('on').siblings().removeClass('on')
                }
            })

        })
    },
    mod.showCode = function(){
        $('.auto-code').css('display','block');
        setTimeout(function(){
            $('.auto-code').css('display','none');
        },3000)
    },
    mod.showCode1 = function(){
        $('.auto-code1').css('display','block');
        setTimeout(function(){
            $('.auto-code1').css('display','none');
        },3000)

    }

    return {
        'tab'      : mod.tab,
        'tab1'     : mod.tab1,
        'slide'    : mod.slide,
        'code'     : mod.showCode,
        'code1'    : mod.showCode1,
        'scrollTo' : mod.scrollTo,
        'scrollspy': mod.scrollspy
    };
})(window.Com || {})


