<script>
function tongji(event_name, hid, loc_source, news_source) {
    var event_params = new Object();
    event_params.city_en = 'bj';//传city_en
    event_params.hid = hid;
    event_params.bid = '8';//必填固定
    event_params.level1_page = 'index';//传level1_page
    event_params.level2_page = '';//传level2_page
    event_params.level3_page = '';//传level3_page

    event_params.loc_source = loc_source;//传左侧对应值
    event_params.news_source = news_source;//传左侧对应值

    //调用推送方法
    lejuTj.event(event_name, event_params);//替换事件名称
}

</script>
