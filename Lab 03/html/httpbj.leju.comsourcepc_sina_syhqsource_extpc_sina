<script type="text/javascript">
(function ($) {
var hids = '';
$('.ly').each(function(index,item){
	if($(item).attr("data-hid")!=undefined)
		hids += $(item).attr("data-hid") +",";
});
$.ajax({
	url: 'https://api.house.leju.com/api/pc/getHousesQrcode',
	type: 'get',
	dataType: 'jsonp',
	data: {site:window.city,hids:hids},
	success:function(data){
		if(data.errmsg=="success"){
			var dataJson = data.data;
			$('.ly').each(function(index,item){
				var hid = $(item).attr("data-hid");
				if(dataJson[hid]!=undefined){
				$('<img src=" '+dataJson[hid]+' "/> <div class="info"><i></i>微信扫码 一键拨打</div>').prependTo($(item));
			}	
		});
		}
	}
  });
$('#search_word').keyup(function(){
    var _text = $.trim($(this).val());
    var reg = /89|八九|八十九|8.9|8。9|64|6.4|6。4|六四|六十四|八九|八十九|6月4日|六月四日|祭奠|FOUR JUNE|陆肆|捌玖|4,june|four,june|four.june|坦克|公元2017年6月4日/g;
    var v = _text.match(reg);
    if(v){
        console.log(v);
        for(var i=0;i<v.length;i++){
            var targetWord = v[i];
            _text = _text.replace(targetWord,'');            
        }
        $('#search_word').val(_text);
    }
})
})(window.jQuery)
</script>

