<script>
(function (S){
	var aNavLv1 = S.query(".navLv1 li");
	var aNavLv2 = S.query(".navLv2 ul");
    var oNav = S.query(".nav")[0];

    var now = 0;

	S.each(aNavLv1,function(item,index) {
		S.addListener(item,"mouseover",function () {
            var gid = this.getAttribute("data-gid");
			for(var i=0; i<aNavLv2.length; i++) {
                if(aNavLv2[i].id == "nav2_" + gid) {
                    aNavLv2[i].style.display = "block";
                }
                else {
                    aNavLv2[i].style.display = "none";
                }
			}
		})
    })
    S.each(aNavLv2,function(item,index) {
        if(S.DOM.hasClass(item,"show")) {
            now = index;
        }
    })
    S.addListener(oNav,"mouseleave",function() {
        for(var i=0; i<aNavLv2.length; i++) {
            aNavLv2[i].style.display = "none";
        }
        aNavLv2[now].style.display = "block";
    })
})(SINA)

    Weibo.Login.autoLogin();
    var weibo_rec = new Weibo.Widgets.List({
        source: '#weibo-rec-list',
        showUserCard: true
    });
</script>
