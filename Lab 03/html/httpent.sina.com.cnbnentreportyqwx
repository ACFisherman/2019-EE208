<script language="javascript" type="text/javascript">
<!--//--><![CDATA[//><!--
var PageNum = 1;
var PageLength = 0;

(function(){
	//统计页数

	var i=1;
	while(document.getElementById("Page_" + i)){
		PageLength = i;
		i++;
	};
	if(PageLength != 0){toPage(1)}; //默认第1页
})();

function toPage(num){
	var i;
	if(num == "next"){num = PageNum + 1};
	if(num == "pre"){num = PageNum - 1};
	if(num<1){num=1};
	if(num>PageLength){num=PageLength};
	for(i=1;i<=PageLength;i++){
		if(i==num){
			document.getElementById("Page_" + i).style.display = "block";
		}else{
			document.getElementById("Page_" + i).style.display = "none";
		};
	};

	PageNum = num;
	getPageList();
};
function getPageList(){
	var HTML = "",i;
	if(PageNum == 1){
		HTML += '<span class="pagebox_pre_nolink ">上一页</span>';	
	}else{
		HTML += '<span class="pagebox_pre"><a  onclick="toPage(\'pre\');"  class="c">上一页</a></span>';
	};
	HTML += '<span class="pagebox_home"><a  onclick="toPage(1);"  class="c">首页</a></span>';
	for(i=1;i<=PageLength;i++){	    	
		if(i==PageNum){
			HTML += '<span class="pagebox_num_nonce">' + i + '</span>';
		}else{
			HTML += '<span class="pagebox_num"><a  onclick="toPage(' + i + ');">' + i + '</a></span>';
		};			
	};
	HTML += '<span class="pagebox_home"><a  onclick="toPage('+ PageLength +');"  class="c">末页</a></span>';
	if(PageNum == PageLength){
		HTML += '<span class="pagebox_pre_nolink c">下一页</span>';
	}else{
		HTML += '<span class="pagebox_pre"><a  onclick="toPage(\'next\');"  class="c">下一页</a></span>';
	};
	if(PageLength == 1){HTML = '';}
	document.getElementById("PageList_1").innerHTML = HTML;
};
function show_num_section(c_page,total,p_num){
	var result="";
	var from_num=(c_page-1)*p_num+1;
	var to_num=c_page*p_num;
	if(to_num>total){
		to_num=total;
	}
	if(from_num!="" && to_num!=""){
		result="当前显示第 "+from_num+" - "+to_num+" 条新闻，";
	}
	var obj_num_section=document.getElementById('num_section');
	if(result){
		obj_num_section.innerHTML=result;
	}
}
//--><!]]>
