<script>
    //超过一屏后初选导航；
    $(window).scroll(function(){
        var winHeight = parseInt($(window).height());
        var scrollpos = parseInt($(document).scrollTop());

        if(scrollpos > winHeight){
            $("#column-nav").show();
        }else{
            $("#column-nav").hide();
        }
    });

    //移动上去的时候；
    $(".brand-car-list-nav").find("dl").hover(function(){
        var index = $(this).index();

        $(this).addClass("active").siblings().removeClass("active");
        $(".brand-car-list-content").children("div").eq(index).addClass("active").siblings().removeClass("active");
    });

    //根据输入获取车型列表
    $("input[name='search']").keyup(function(){
        var value = $(this).val();
        var regex = /^[\u4e00-\u9fa5/s/-a-zA-Z0-9]{1,20}$/i;//定义匹配的正则
        if(!regex.test(value))
        {
            return false;
        }
        var token = $("input[name = 'token']").val();
        var data1 = {"token" : token , "keys" : value};
        var data2 = usedCar.create_sign(data1);
        // 搜索间隙显示正在查询中
        var str = '<li><span class="fl">检索中...</li>';
        if(value == '')
        {
            $("#search-list").hide();
            return true;
        }
        $("#search-list").find("ul").html(str);
        usedCar.search("/index.php?s=/Api/Usedcar/keywords",
                '{"token":"'+token+'","sign":"'+data2+'","keys":"'+value+'"}');
    });

    //选择车型
    $("#evaluate-brand").on("click","li",function(){
        var value = $(this).attr("brand");

        usedCar.getCarSeries("/index.php?s=/Index/getCarSeries",{"brand_id" : value});
        return false;
    });

    //选择车型
    $("#evaluate-series").on("click","li",function(e){
        var value = $(this).attr("series");

        usedCar.getCarId("/index.php?s=/Index/getCar",{"series_id" : value});
        e.stopPropagation();
    });

    //选择车款
    $("#evaluate-car").on("click","li",function(e){
        var value = $(this).attr("carid");

        $(this).parents("form").find("input[name = 'carid']").val(value);
    });

    //选择年
    $("#evaluate-year").on("click","li",function(){
        var value = parseInt($(this).html());

        $(this).parents("form").find("input[name = 'year']").val(value);
    });

    //选择月
    $("#evaluate-month").on("click","li",function(e){
        var value = Number($(this).attr('month'));

        $(this).parents("form").find("input[name = 'month']").val(value);
    });

    //数据处理提交
    $("#evaluate-btn").click(function(){
        var formDom = $(this).parents("form");
        var token = $("input[name = 'token']").val();
        var carid = formDom.find("input[name = 'carid']").val();
        var drivingmileage = formDom.find("input[name = 'drivingmileage']").val();
        var cityid = $("input[name = 'cityid']").val();
        var year = formDom.find("input[name = 'year']").val();
        var month = zerofill(formDom.find("input[name = 'month']").val());
        var buycardate = year+'-'+month+'-01';
        var data1 = {carid:carid,buycardate:buycardate,drivingmileage:drivingmileage,cityid:cityid,token:token};
        var data2 = usedCar.create_sign(data1);
        var data =  '{"carid":"'+carid+'","buycardate":"'+buycardate+'","drivingmileage":"'+drivingmileage+'","cityid":"'+cityid+'","token":"'+token+'","sign":"'+data2+'"}';

        if(carid != "" && drivingmileage != "" && cityid != "" && year != "" && month != ""){
            $(this).html("正在评估中...").addClass("active");
            usedCar.evaluate("/index.php?s=/Api/Usedcar/assessment",data);
        }else{
            $(this).html("马上评估");
            alert("请填写完整信息！");
        }


        return false;
    });

    //当热门车型被点击了后main的paddingtop改变；
    $(".car-brand").find(".first").click(function(){
       $("#main").css("paddingTop","20px");
   });

   $(".car-brand").find("li").not(".first").click(function(){
       $("#main").css("paddingTop","166px");
   });

   $('.car-tab-nav').find("li").eq(0).click(function(){
       var ind = $(".car-tab-content-buy .car-brand").find(".active").index();

       if(ind == 0){
           $("#main").css("paddingTop","20px");
       }else{
           $("#main").css("paddingTop","166px");
       }
   });

   $('.car-tab-nav').find("li").eq(1).click(function(){
       $("#main").css("paddingTop","166px");
   });

   $('.car-tab-nav').find("li").eq(2).click(function(){
       $("#main").css("paddingTop","166px");
   });


</script>
<script>
    //卖车的信息；
    //选择车型
    $(".check-brand-btn").on("click","li",function(){
        var value = $(this).attr("brand");
        var name = $(this).text();

        $(".check-series-btn").children("span").text("请选择车型")
                .parent().find("ul").html("");

        usedCar.getCarSeries("/index.php?s=/Index/getCarSeries",{"brand_id" : value},".check-series-btn");

        $("input[name='sellbrandname']").val(name);

        isComplete();

        return false;
    });

    //选择系列
    $(".check-series-btn").on("click","li",function(){
        var name = $(this).text();

        $("input[name='sellseriesname']").val(name);
        isComplete();
    });

    //选择年
    $(".check-licensedate-btn").on("click","li",function(){
        var year = parseInt($(this).text());
        var format_year = year+"-01-01";

        $("input[name = 'selllicensedate']").val(format_year);
        isComplete();
    });

    $(".check-cityid-btn").on("click","li",function(){
        var cityname = $(this).text() + '市';

        $("input[name = 'sellcityid']").val(cityname);
        isComplete();
    });

    $("input[name = 'sellname'],input[name = 'sellphone'],input[name = 'sellcode']").blur(function(){
        isComplete();
    });

    $("#pos-sell-btn").click(function(){
        var formDom = $(this).parents("form");
        var brandname = getInputVal("sellbrandname");
        var seriesname = getInputVal("sellseriesname");
        var cityid = getInputVal("sellcityid");
        var licensedate = getInputVal("selllicensedate");
        var name = formDom.find("input[name = 'sellname']").val();
        var phone = formDom.find("input[name = 'sellphone']").val();
        var code = formDom.find("input[name = 'sellcode']").val();
        var token = getInputVal("token");

        var data1 = {"token" : token , "brandname" : brandname , "seriesname" : seriesname , "city" : cityid ,
            "licensedate" : licensedate , "name" : name , "phone" : phone , "code" : code };
        var data2 = usedCar.create_sign(data1);

        if(isComplete()){
            usedCar.sellCar("/index.php?s=/Api/Usedcar/sell",
                    '{"token":"'+token+'","sign":"'+data2+'","brandname":"'+brandname+'","seriesname":"'+seriesname+'","city":"'+cityid+'"' +
                    ',"licensedate":"'+licensedate+'","name":"'+name+'","phone":"'+phone+'","code":"'+code+'"}');
        }else{
            alert("请正确填写信息！");
        }


        return false;
    });



</script>
