                    <script language="javascript" type="text/javascript">
                        var slide_02 = new Slider({
                            wrap: 'hdSlider_pics03',
                            itemClass: 'slider-item',
                            autoPlay: false,
                            onReady: function () {
                                var that = this,
                                    total = this.conf.isLoop ? this.states.total / 2 : this.states.total,

                                    btnPrev = this._id('hd_slider_prev03'),
                                    btnNext = this._id('hd_slider_next03');

                                // 前进/后退按钮
                                this._addEvent(btnPrev, 'click', function () {
                                    that.prev();
                                });
                                this._addEvent(btnNext, 'click', function () {
                                    that.next();
                                });
                            }
                        });
                    </script>
                    <!-- 滚动图 end -->

