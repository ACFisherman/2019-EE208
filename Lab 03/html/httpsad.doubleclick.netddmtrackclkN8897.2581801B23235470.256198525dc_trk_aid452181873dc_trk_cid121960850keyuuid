                    </div>
                    <script id="tmplNewsList" type="text/x-dot-template">
                            {{~it:value:index}}
                            <div class="item-news">
                                <a href="{{= value.url}}" target="_blank">
                                    <img alt="{{= value.title}}" src="{{= value.pic}}">
                                </a>
                                <div class="news-detail">
                                    <div class="inner">
                                    <a href="{{= value.url}}" target="_blank">
                                        <span>{{= typeStr[value.showtype]}}</span>
                                        <h4>{{= value.title}}</h4>
                                        <p>{{= value.intro}}</p> 
                                        </a>       
                                    </div>
                                </div>
                            </div>
                            {{~}}
                            </script>
                    <!--<a href="javascript:;" class="btn-showmore"><span>查看更多</span><em></em></a>-->
                </div>
            </div>
