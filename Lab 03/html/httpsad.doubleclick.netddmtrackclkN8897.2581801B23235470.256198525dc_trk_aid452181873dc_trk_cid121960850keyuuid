            SinaPage.loadWidget({
                require: [
                    {
                        url: "http://i2.sinaimg.cn/lx/deco/2014/0106/js/common.js"
                    }],
                onAfterLoad: function () {
                    console.log(1)
                    var scrollPic = new scrollPic3D();
                    scrollPic.boxId = 'scrollBox02'; //容器id
                    scrollPic.width = 1000; //容器宽度
                    scrollPic.height = 760; //容器高度
                    scrollPic.imgWidth = 345; //图片宽度
                    scrollPic.imgHeight = 760; //图片高度
                    scrollPic.descHeight = 0; //描述高度
                    scrollPic.showsNumber = 5; //显示数量
                    scrollPic.autoPlay = false; //是否自动播放
                    scrollPic.autoPlayTime = 5;
                    scrollPic.zoom = 0.5; //小图缩放比
                    scrollPic.timeLimit = 500;
                    scrollPic.init();
                    $('.sp3_left').on('click', function () {
                        scrollPic.pre();
                    });
                    $('.sp3_right').on('click', function () {
                        scrollPic.next();
                    });
                }
            });
        </script>
