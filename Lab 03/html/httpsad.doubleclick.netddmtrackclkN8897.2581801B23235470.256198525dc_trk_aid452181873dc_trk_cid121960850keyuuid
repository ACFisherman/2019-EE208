    <script type="text/javascript">
        $(window).bind('scroll load', function () {
            var e = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop;
            if (e < $('.part02').offset().top) {

                $(".right-menu").removeClass('right-menu-fixed');
                $(".right-menu").removeClass('menu02');
            } else if (e > $('.part02').offset().top && e < $('.part02').offset().top + $('.part02').height()) {
                $(".right-menu").addClass('right-menu-fixed');
                $(".right-menu").removeClass('menu02');
                $(".right-menu").addClass('menu01');
            } else if (e > $('.part02').offset().top + $('.part02').height()) {
                $(".right-menu").removeClass('menu01');
                $(".right-menu").addClass('menu02');
            }

        });
        //分享 和回到顶部
        $("body").delegate(".topBtn", "click", function (d) {
            try {
                document.documentElement.scrollTop = 0
            } catch (e) { };
            $("html, body").animate({
                scrollTop: 0
            },
                500)
        });
        $(".weixin_icon").on("mouseover", function (d) {
            $('.weixinImg').show();
        });
        $(".weixin_icon").on("mouseout", function (d) {
            $('.weixinImg').hide();
        });
    </script>
