$(function(){
	$.article_view({article_id:'75263',article_token:'14f3a71127d299972566bd2743a102ec'});
	$.right_data('//api.51xiancheng.com/me/pc/get_article_right',{source:'pc', site_id:'926',art_id:'75263'});
	/*QQ分享*/
	var p = {
			url:location.href, /*获取URL，可加上来自分享到QQ标识，方便统计*/
			desc:'', /*分享理由(风格应模拟用户对话),支持多分享语随机展现（使用|分隔）*/
			title:'喜来登住宿自助餐', /*分享标题(可选)*/
			summary:"喜来登已经开了一段时间了，自助餐吃的人不少，今天来体验一下老牌酒店住宿和用餐环境。"+"（来自城市生活精选指南@鲜城 ）", /*分享摘要(可选)*/
			pics:'https://wx1.sinaimg.cn/large/0020e2fegy3fj5c2h37pzj21bv0sg7wi.jpg', /*分享图片(可选)*/
			flash: '', /*视频地址(可选)*/
			site:'鲜城', /*分享来源(可选) 如：QQ分享*/
			style:'201',
			width:32,
			height:32
			};
			var s = [];
			for(var i in p){
			s.push(i + '=' + encodeURIComponent(p[i]||''));
			}
	var qq_url = 'http://connect.qq.com/widget/shareqq/index.html?'+(s.join('&'));
	$('#share_btn_qq').attr('href',qq_url).attr('target','_blank');
	/*QQ空间分享*/
	var qq_zone_url = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?'+(s.join('&'));
	$('#share_btn_qzone').attr('href',qq_zone_url).attr('target','_blank');
	/*获取微信分享二维码*/
	$('#item_article_qrcode').attr('src',"/interface/common/qrCode?redi=1&len=106&data="+encodeURIComponent(location.href));
	/*获取卡券二维码*/
	$('.payment-img img').attr('src',"/interface/common/qrCode?len=160&data="+encodeURIComponent(''));

})
