<!--pc 广告js end--><script type="text/javascript">
function rightBar() {
    $(window).scroll(function () {
        var scroll_top = $(document).scrollTop();
        if (scroll_top != 0) {
            $(".back-top").show();}
          else
               {$(".back-top").hide();}           
    });
    $(".back-top").click(function () {
        location.hash = "top";
        $(".back-top").hide();
    });
}
rightBar();
</script>
