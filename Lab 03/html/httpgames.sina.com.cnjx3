    <script type="text/javascript">
        //图片延时加载
        $('img[data-src]').live('inview', function(event, isVisible) {
            if (!isVisible) { return; }

            var img = $(this);

            // Show a smooth animation
            img.css('opacity', 0);
            img.load(function() { img.animate({ opacity: 1 }, 500); });
            // Change src
            img.attr('src', img.attr('data-src'));

            // Remove it from live event selector
            img.removeAttr('data-src');
        });

        /*part2切换*/
        $(".tab_lis li a").mousemove(function(){
            $(this).parent().addClass('tab_li_selected').siblings('li').removeClass('tab_li_selected');
            var nIndex=$(this).parent().index();
            $(this).parent().parent().siblings('div.tab_con').removeClass('tab_con_selected').eq(nIndex).addClass('tab_con_selected');
        });
        /*part3切换*/
        $("#tabLi1 li a").mousemove(function(){
            $(this).parent().addClass('tab_li_selected').siblings('li').removeClass('tab_li_selected');
