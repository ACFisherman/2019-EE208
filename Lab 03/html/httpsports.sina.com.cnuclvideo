<script>
	require([ 'js/component/common/newLoader' ], function( Loader ) {
		var getInfo =  function(){
			var href = location.search.indexOf('?') != -1 ?location.search.substring(1): '';
			var items = href.split('&');
			var arg = {},key,value;
			for(var i = 0; i < items.length; i++){
				var item = items[i].split('=');
				key = item[0];
				value = item[1];
				arg[key] = value;
			}
			return arg;
		};
		var seasonTitleDom = $('#ouguan__banner .match__season_title');
		var pageSeason;//查询参数
		var seasonTitle;//页面渲染title
		if(!getInfo().season){
			seasonTitle = seasonTitleDom.html();
			pageSeason = seasonTitle.split('-')[0]+'/'+seasonTitle.split('-')[1];
		}else{
			pageSeason = getInfo().season.split('-')[0]+'/'+getInfo().season.split('-')[1]+'赛季';
			seasonTitle = getInfo().season + '赛季';
			seasonTitleDom.html(seasonTitle);
		}

		var videoWrap = '#ouguan__video_wrap';
		var formatData = function(d, order) {
			var toStr = {}.toString;
			var _isArray = Array.isArray || function(arr) {
				return toStr.call(arr) == '[object Array]';
			};
			if (_isArray(d) || (!d)) {//数据为空
				return false;
			}else{
				var resData = [];
				for(var i = 0,len = order.length; i<len; i++){
					for(var item in d ) {
						if( item.toString() === order[ i ].toString()){
							resData.push(d[ item ]);
						};
					};
				};
				return resData;
			};
		};
		var render = function( dom, data ) {
			for(var i = 0, len = data.length; i < len; i++) {
				var d = data[ i ];

				var html = '<div class="ouguan__video_block clearfix"><p class="match__stage_title">' + d.match + '</p>';
				for ( var item in d ) {
					if ( item !== 'match' ) {
						var ditem = d[ item ];
						var arr = [];
						if ( parseInt(item) % 3 === 0) {
							arr.push('<div class="layout-fl thumbnail-a thumbnail-a-video" style="width:210px;height:120px;margin-bottom:20px;">');
						} else {
							arr.push('<div class="layout-fl thumbnail-a thumbnail-a-video" style="width:210px;height:120px;margin-left:20px;margin-bottom:20px;">');
						}
						arr.push('<a href="' + ditem.url + '" target="_blank">');
						arr.push('<div class="img" style="width:210px;height:120px">');
						arr.push('<img width="210" src="'+ditem.pic+'" alt="'+ditem.title+'">');
						arr.push('<div class="caption"><h3 class="title">'+ditem.stitle+'</h3></div>');
						arr.push('<i class="ico"></i>');
						arr.push('</div></a></div>');
						html += arr.join('');
					}
				};
				html += '</div>';
				dom.append(html);
			}
		};
		var createLoader = function(wrap, param) {

			return new Loader(wrap, {
				// 数据接口，和 jquery ajax 的参数一致
				read: {
					url: '//interface.sina.cn/sports/uclvideo.d.json',
					dataType: 'jsonp',
					data: {
						season: param.season,
						date: param.date
					}
				},
				timeout: 20e3, // 链接错误或网络错误时的超时时间

				//出错（超时）回调
				error: function( data ) {
					var self = this;
					self.dom.loading.hide();
					self.dom.tip.html('加载出错，请<span class="ouguan__video_reload" data-value="'+param.date + '">重试<span>').show();
				},

				//加载后回调
				success: function( data ) {
					var self = this;
					self.dom.loading.hide();
					var data = formatData(data.data, data.order);
					if (!data) {
						self.dom.tip.html('该比赛日暂无视频').show();
					}else{
						render( self.dom.list, data );
					}
				},

				//加载前回调
				beforeRead: function( data ) {
					var self = this;
					self.dom.tip.hide();
					self.dom.list.empty();
					self.dom.loading.show();
					// var self = this;
				}
			});
		};

		var timeid = null;
		var leftWrap = $('#ouguan__left');
		var pageInit = function(){
			var curStageVal = PAGEDATA.games;
			var curStageDom = leftWrap.find('[data-value='+curStageVal+']');
			var curStageDomP = curStageDom.parents('.ouguan__stage');
			if( (parseInt(curStageVal) !=0 ) && ( parseInt(curStageVal ) !=13 ) ) {
				curStageDom.addClass('stage__show');
				curStageDomP.addClass('ouguan__stage-cur selected');
			} else {
				curStageDomP.addClass('ouguan__stage-cur selected');
			}
			createLoader(videoWrap, { 'season': pageSeason, 'date': parseInt(PAGEDATA.games) } );
		};
		pageInit();


		var bind = function(){

			// var hasTouch = (typeof (window.ontouchstart) !== 'undefined');
			var enterStage = function(dom, domp){
				clearTimeout( timeid );
				leftWrap.find('.stage_cont').hide();
				leftWrap.find('.selected').removeClass('selected');
				domp.addClass('selected').find('.stage_cont').show();
			};
			var leaveStage = function(dom, domp){
				clearTimeout( timeid );
				timeid = setTimeout( function() {
					domp.removeClass('selected').find('.stage_cont').hide();
					leftWrap.find('.ouguan__stage-cur .stage_cont').show();
				}, 300 );
			};

			leftWrap.on('mouseenter', '.stage_cont, .stage_nav', function(){
				// clearTimeout( timeid );
				var $this = $(this);
				var $parent = $this.parents('.ouguan__stage');
				enterStage( $this, $parent);
				// leftWrap.find('.stage_cont').hide();
				// leftWrap.find('.selected').removeClass('selected');
				// $parent.addClass('selected').find('.stage_cont').show();
			});

			leftWrap.on('mouseleave','.stage_cont, .stage_nav', function(){
				// clearTimeout( timeid );
				var $this = $(this);
				var $parents = $this.parents('.ouguan__stage');
				leaveStage($this, $parents);
				// timeid = setTimeout( function() {
				//     parents.removeClass('selected').find('.stage_cont').hide();
				//     leftWrap.find('.ouguan__stage-cur .stage_cont').show();
				// }, 300 );
			});


			leftWrap.on('click', '[data-value]', function() {
				var $this = $(this);
				var stageDate = $this.attr('data-value');
				$('.ouguan__stage-cur').removeClass('ouguan__stage-cur');
				$('.stage__show').removeClass('stage__show');
				// $('.stage__cur').html('').removeClass('stage__cur').hide();
				if((parseInt(stageDate)!=0)&&(parseInt(stageDate)!=13)){
					$this.addClass('stage__show');
				}
				$this.parents('.ouguan__stage_list').show();
				$this.parents('.ouguan__stage').addClass('ouguan__stage-cur');
				createLoader(videoWrap, {'season': pageSeason, 'date': parseInt(stageDate)});
			});
		};
		bind();

	} );
</script>
