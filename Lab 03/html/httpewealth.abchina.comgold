    <iframe id="snsProxy" style="display:none"></iframe>
    <script type=text/javascript>
        var snsDomain = window.location.protocol + "//app.abchina.com/services/financial/SnsService/";
        var frm = document.getElementById("snsProxy");
        frm.src = snsDomain + "proxy.html";
        frm.onload = snsProxyOnload;
        function GetRandomNum(Min, Max) {
            var Range = Max - Min;
            var Rand = Math.random();
            return (Min + Math.round(Rand * Range));
        }
        function snsProxyOnload() {
            var proxy = document.getElementById('snsProxy').contentWindow;
            if (proxy.send) {
                //调用代理页面中的方法发送请求
                proxy.send(snsDomain + 'Services/SnsService.svc/GetRecommendedFinancialPlanners?maxCount=100&typeID=300001', fpCallback, 'get', 'text/json;charset=utf-8');
            }
        }
        function renderData($1) {
            var field = $1.substring(1, $1.length - 1);
            if (regData != null) {
                switch (field) {
                    case "Qualification":
                        var _qualification = regData[field];
                        if (_qualification == "") {
                            return "暂无";
                        }
                        _qualification = _qualification.replace("1", "AFP").replace("2", "CFP").replace("3", "基金从业资格").replace("4", "保险从业资格").replace("5", "证券从业资格").replace("6", "期货从业资格").replace("7", "CFA一级").replace("8", "CFA二级").replace("9", "CFA三级").replace("10", "其他");
                        if (_qualification.length > 15) {
                            _qualification = _qualification.substr(0, 13) + "..";
                        }
                        return _qualification;
                    case "ExpertiseAreas":
                        var _expertiseAreas = regData[field];
                        if (_expertiseAreas == "") {
                            return "暂无";
                        }
                        _expertiseAreas = _expertiseAreas.replace("1", "银行理财").replace("2", "基金").replace("3", "保险").replace("4", "债券").replace("5", "黄金").replace("6", "外汇").replace("7", "股票").replace("8", "信托");
                        if (_expertiseAreas.length > 15) {
                            _expertiseAreas = _expertiseAreas.substr(0, 13) + "..";
                        }
                        return _expertiseAreas;
                    case "Affiliation":
                        return getAffiliation(regData[field]);
                }
                return regData[field];
            }
            return "";
        }
        var regData;
        function getAffiliation(id) {
            var affiliation = "";
            switch (id) {
                case "1":
                    affiliation = "网上营业厅";
                    break;
                case "2":
                    affiliation = "北京总行";
                    break;
                case "3":
                    affiliation = "上海分行";
                    break;
                case "4":
                    affiliation = "河北分行";
                    break;
                case "5":
                    affiliation = "山东分行";
                    break;
            }
            return affiliation;
        }
        function fpCallback(e) {
            if (e.status !== 'success')
                return;
            var msg = e.responseText;
            var jsonobj = eval('(' + msg + ')');
            var listline = "";
            if (jsonobj.length > 0) {
                i = GetRandomNum(0, jsonobj.length - 1);
                regData = jsonobj[i];
                var temp = $("#listFinancialPlannerUser").html();
                var reg = new RegExp("#[a-zA-Z0-9]*#", "g");
                var content = temp.replace(reg, renderData);
                $("#listFinancialPlannerUser").html(content);
            }
            else {
                $("#listFinancialPlannerUser").html("暂无数据");
            }
            $("#listFinancialPlannerUser").show();
        }
    </script>

