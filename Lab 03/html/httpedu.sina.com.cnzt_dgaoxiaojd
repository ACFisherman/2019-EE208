    $(function () {
        var mySwiper = new Swiper('.swiper-container', {
            loop: true,
            // 如果需要分页器
            pagination: '.swiper-pagination',
            autoplay: 5000,
            autoplayDisableOnInteraction: false,
            simulateTouch: false,
            paginationClickable: true
        })
        var mySwiper1 = new Swiper('.swiper-container1', {
            loop: true,
            // 如果需要分页器
            pagination: '.swiper-pagination1',
            autoplay: 5000,
            autoplayDisableOnInteraction: false,
            // 如果需要前进后退按钮
            //nextButton: '#prev4',
            //prevButton: '#prev3',

            simulateTouch: false,
            paginationClickable: true

        })
        $('#prev4').on('click', function (e) {
            e.preventDefault()
            mySwiper1.swipeNext()
        })
        $('#prev3').on('click', function (e) {
            e.preventDefault()
            mySwiper1.swipePrev()
        })
        $('.zero_con ul li:gt(5)').hide();
        $('.more').click(function(event) {
            $('.zero_con ul li:gt(5)').slideToggle();
            if($(this).hasClass('cur')){
            	$(this).removeClass('cur');
                $(this).html('查看显示更多');
            }else {
            	$(this).addClass('cur');
                $(this).html('收起');

            }
        })
        $('.consultation_right span').on('click',function(){
            $(this).addClass('cur').siblings('span').removeClass('cur')
            var a =$(this).index();           
			$('.consultation_left').eq(a).addClass('cur').siblings('.consultation_left').removeClass('cur')
        })
        var windowWidth=$(window).width();
        var windowHeight=$("body").height();
        var a_banner=$('.banner').height();
        var fixedArrNum=new Array();
        $(".m1").each(function(){
            fixedArrNum.push($(this).offset().top)
        })
        //        	 console.log(fixedArrNum[0])
        var windowWidth=$(window).width();
        var windowHeight=$("body").height();
        $(window).scroll(function(){
            if ($(document).scrollTop() > a_banner) {
                $(".fu").css('display','block');
                $('.banner_wrap p').addClass('topnavs')
            } else {
                $(".fu").css('display','none');
                $('.banner_wrap p').removeClass('topnavs')
            }
            if($(document).scrollTop() > windowHeight - 800) {
                $(".fu").css('display','none');
                $('.banner_wrap p').removeClass('topnavs')
            }
            //        		console.log(fixedArrNum.length)
            for(var i=0;i<fixedArrNum.length;i++){

                if(($(document).scrollTop()+120)>fixedArrNum[i]){
                    if(fixedArrNum[i+1]?(($(document).scrollTop()+120)<fixedArrNum[i+1]):1){
                        $(".fu p").removeClass("cur");
                        $(".banner_wrap p span").removeClass("cur");
                        $(".fu p").eq(i).addClass("cur");
                        $(".banner_wrap p span").eq(i).addClass("cur");
                        break;
                    }

                }
            }
        })
        $(".fu p").each(function(index,obj){
            $(this).bind("click",function(){
                $("html,body").stop().animate({scrollTop:fixedArrNum[index]-90},600);
                return false;
            })
        })
        $(".banner_wrap p span").each(function(index,obj){
            $(this).bind("click",function(){
                $("html,body").stop().animate({scrollTop:fixedArrNum[index]-90},600);
                return false;
            })
        })
        var h1=$(window).height();
        $(window).scroll(function(event) {
            var num=$(window).scrollTop();
            if (num>h1) {
                $('#top').fadeIn(500);
            } else{
                $('#top').fadeOut(500);
            };
        });

        $('#top').click(function(event) {
            $('html,body').animate({scrollTop:0}, 500);
        });
    })
</script>


