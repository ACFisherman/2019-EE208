				function showContent(data_arr){
					var html= '';
					data = data_arr['result']['data'];
					for(var i in data){
						html+='<p><a href="'+data[i].url+'" target="_blank">'+data[i].title.substr(0,30)+'</a></p>';
					}
					document.getElementById("list_gdgx").innerHTML =html;
				}
