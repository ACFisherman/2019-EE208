<script type="text/javascript">
	var num = 0;
	var time;
	$('.section1 .left span').click( function(){
		var bFlag = $(this).hasClass('r');		
		if( bFlag ){
			num++;
			if( num == 3 ){ num = 0 }
		}else{
			num--;
			if( num == -2 ){ num = 1 }
		}
		play();		
	} );	
	time = setInterval( autoPlay,3000 );	
	$('.section1 .left').hover( function(){
		clearInterval( time );
	},function(){
		time = setInterval( autoPlay,3000 );
	} );		
	function play(){
		$('.section1 .left .list').stop().animate( {'left':-720*num} );
	}
	function autoPlay(){	
		num++;
		if( num == 3 ){ num = 0 }
		play();	
	}
//第二个轮播图第二个轮播图第二个轮播图第二个轮播图第二个轮播图第二个轮播图第二个轮播图第二个轮播图第二个轮播图
</script>
