		    }else if('pengyou' == p){
		            var url = _pengyou;
		    }else if('baidu' == p){
		            var url = _baidu;
		    }else if('wangyi' == p){
		            var url = _wangyi;
		    }
		    window.open(url,'_blank');
		}

		//复制
		function copyToClipboard(e){
			var e = $(e);
			var copyipt = e.siblings('.copy_box').find('input');
			if(navigator.appVersion.match(/\bMSIE\b/)){
				window.clipboardData.setData('text',copyipt.val());
				alert("复制成功！");
			}else{
				alert("您使用的浏览器不支持此复制功能，请使用Ctrl+C或鼠标右键。");
				copyipt.select();
			};
			//显示 已复制
			e.hide().next('input').show();
		}

		//评分
		$('.cm3_r_star').mousemove(function(e) {
			var width = $(this).find('div').width();
			var left = e.pageX-$(this).offset().left;
			var star1 =  Math.floor(left/width);
			var star2 = left/width-star1;
			if(star2>0&&star2<0.5) {
				star2 = 1;
			}

			if(star2>=0.5&&star2<1){
				star1++;
				star2 =0;
			}
			$(this).find('div:lt('+star1+')').removeClass().addClass('star1');
			$(this).find('div:gt('+(star1-1)+')').removeClass().addClass('star3');
			if(star2==1){
				$(this).find('div:eq('+star1+')').removeClass().addClass('star2');
			}
			//实际分数
			var first =star1;
			var second =  star2==1?0.5:0;
			var score = first+second;
			// console.log(score);
			starscore = score;
		});
		$('.cm3_r_star').click(function(e){
