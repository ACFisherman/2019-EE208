<title>潘家园_新浪收藏_新浪网</title>
<meta name="keywords" content="潘家园,潘家园网,古玩,旧货市场,收藏">
<meta name="description" content="潘家园旧货市场主营古旧物品、工艺品、收藏品、装饰品，市场形成于1992年，现在已成为一个古色古香的传播民间文化的大型古玩艺术品市场。">
<style type="text/css">
/* ================== css reset ================== */
html, body, ul, li, ol, dl, dd, dt, p, h1, h2, h3, h4, h5, h6, form, fieldset, legend, img { margin:0; padding:0; }
fieldset, img { border:none; }
address, caption, cite, code, dfn, th, var { font-style:normal; font-weight:normal; }
ul, ol, li { list-style:none; }
select, input { vertical-align:middle; }
select, input, textarea { font-size:12px; margin:0; }
table { border-collapse:collapse; }
body {padding:5px 0; font:12px/20px "宋体"; }
.clearfix:after { content:"."; display:block; height:0; visibility:hidden; clear:both; }
.clearfix { zoom:1; }
.clearit { clear:both; height:0; font-size:0; overflow:hidden; }
a { color:#333; text-decoration:none; }
a:visited { color:#800080; }
a:hover, a:active, a:focus { color:#f00; text-decoration:underline; }
a.isLine { text-decoration:underline; }
h1, h2, h3, h4, h5, strong,em { font-weight:normal; }
.wrap { width:950px; margin:0 auto;}
/* 页面导航 开始 */
.secondaryHeader {height:33px;overflow:hidden;background:url(http://www.sinaimg.cn/dy/images/header/2008/standardl2nav_bg.gif) repeat-x #fff;color:#000;font-size:12px; font-weight:100; }
.secondaryHeader a, .secondaryHeader a:visited { color:#000; text-decoration:none; }
.secondaryHeader a:hover, .secondaryHeader a:active { color:#c00; text-decoration:underline; }
.sHBorder {border:1px #e3e3e3 solid; padding:0 10px 0 12px; overflow:hidden; zoom:1; }
.sHLogo {float:left; height:31px; line-height:31px; overflow:hidden; }
.sHLogo span, .sHLogo span a { display:block; *float:left;display:table-cell; vertical-align:middle; *display:block;*font-size:27px;*font-family:Arial;height:31px; }
.sHLogo span, .sHLogo span a img { vertical-align:middle; }
.sHLinks { float:right; line-height:31px; }

/* 左图-右文 */
.zcbox {line-height:20px;padding-top:10px;}
.zcbox .zch {font-size:14px;color:#000;}
.zcbox .zch a {color:#000;}
.zcbox .zcimg {float:left;margin-right:7px; }
.zcbox .zcimg img {display:block;}
.zcbox .zcwords {width:135px;float:left;color:#666; }

/* 上图-下文 */
.zcbox2 {float:left;width:50%;text-align:center; }
.zcbox2 .zcimg img {display:block;margin:0 auto;}
.zcbox2 .zcwords {line-height:25px;height:25px;color:#797778; }
.zcbox2 .zcwords a {color:#797778;}
.zcbox2 a:visited {color:#800080  !important;}
.zcbox2 a:hover {color:#f00  !important;}

/* 标题 */
.zctit1 {height:27px;line-height:27px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc10.png) no-repeat center bottom; overflow:hidden; }
.zctit1 h2 {float:left;color:#404040;font-size:14px;padding:0 0 0 20px;font-weight:bold; }
.zctit1 h2 a {color:#404040;}
.zctit1 h2 a:visited {color:#404040;}
.zctit1 h2 a:hover {color:#f00;}
.zctit1 .more {float:right;padding-right:15px;}

.zctit2 {height:31px;line-height:31px;border-bottom:1px solid #f2e5d8;overflow:hidden; }
.zctit2 h2 {text-align:center; font-size:18px;font-family:"黑体";}
.zctit2 h2 a {color:#404040;}
.zctit2 h2 a:visited {color:#404040;}
.zctit2 h2 a:hover {color:#f00;}

.zctit3 {height:31px;line-height:31px;border-bottom:1px solid #f2e5d8;overflow:hidden; }
.zctit3 h2 {float:left;color:#404040;font-size:14px;padding:0 0 0 10px;font-weight:bold;}
.zctit3 h2 a {color:#404040;}
.zctit3 h2 a:visited {color:#404040;}
.zctit3 h2 a:hover {color:#f00;}
.zctit3 .more {float:right;padding-right:15px;}

.zctit4 {height:46px;line-height:46px;overflow:hidden;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc04.png) no-repeat;   }
.zctit4 h2 {font-size:14px;text-align:center;font-weight:bold;color:#404040;  }
.zctit4 h2 a {color:#404040;}
.zctit4 h2 a:visited {color:#404040;}
.zctit4 h2 a:hover {color:#f00;}

.titChange1 {height:29px;line-height:29px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/dp_wzc12.png) repeat-x left bottom; overflow:hidden;}
.titChange1 span {cursor:pointer;text-indent:-9999px;zoom:1;float:left;width:159px;margin-right:5px; background:url(http://www.sinaimg.cn/cj/collectioncomp/images/dp_wzc08.png) no-repeat;}
.titChange1 .zc_on {background:#fff;}
.zcchange1 {display:none; }

/* 图片字 */
.titImg1{margin-top:10px;width:70px;height:16px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/hlw_wzc25.png) no-repeat -200px 0}/*排行榜*/
a:hover .titImg1 {background-position:-300px 0}

/* 列表 */
.zclist {font-size:12px;}
.zclist a {}
.zclist a:visited {}
.zclist a:hover {}
.zclist_grey a {color:#666;}
.zclist_grey a:visited {color:#800080;}
.zclist_grey a:hover {color:#f00;}
.zclist li {padding-left:10px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc09.png) no-repeat left 8px;height:22px;line-height:22px;overflow:hidden;}
.zclist .extra {float:right;}
.zclist .extra a {color:#999;}
.zclist_noBg li {background:none;padding:0; }
.lh24 li {height:24px;line-height:24px;background-position:0 10px;}
.lh26 li {height:26px;line-height:26px;background-position:0 10px;}
/* other */
.aPd1 img { border:1px solid #fff;padding:1px;}
.aPd1:hover img { border:1px solid #e98a51;}
.aPd5 img { border:1px solid #686867; padding:5px;}
.aPd5:hover img { border:1px solid #2865ff;}

.aBd2 img { border:2px solid #686867;}
.aBd2:hover img { border:2px solid #2865ff;}

.zcgrey {color:#828282;}
.zcgrey:hover {color:#f00;}

.zcspace {height:10px;font-size:0px;background:#fff;overflow:hidden;clear:both;}
.zchide {display:none; }
.zcplay {position:absolute;left:13px;top:32px;cursor:pointer;}
.fs14  {font-size:14px;}
/* 导航 */
.navLogo {height:171px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc01.png) no-repeat;overflow:hidden;  }
.navLogo h1 {font-size:48px;font-family:"微软雅黑","黑体";line-height:50px;padding:54px 0 0 242px;}
.navLogo h2 {font-size:16px;font-family:Arial;font-weight:bold;line-height:20px;padding:5px 0 0 472px;  }
/* ============== 横切定义 ============== */
.zcpart1 {margin-top:10px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc03.png) repeat-y;}
.zcpart1 .zcbottom {height:7px;clear:both;font-size:0px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc02.png) no-repeat;overflow:hidden;}
.zcpart1 .zcleft {width:318px;overflow:hidden;float:left;margin-right:20px;}
.zcpart1 .zccenter {width:345px;overflow:hidden;float:left;}
.zcpart1 .zcright {width:260px;overflow:hidden;float:right;}

.zcblk01 {height:167px;overflow:hidden;  }

.zcblk02 {margin-top:10px;}
.zcblk02 .zccont {padding:18px 10px 15px;}
.zcblk02 .zclist {clear:both;zoom:1;padding:15px 0 0 10px; }
.zcblk02 li a {padding-right:20px; }

/* .zcblk02 li a {padding-right:20px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc08.png) no-repeat right 0; } */

.zcblk03 {margin-top:5px;}
.zcblk03 .zccont {padding-top:8px; }
.zcblk03 p {margin:12px 0 0 12px;}

.zcblk04 {margin-top:20px;}
.zcblk04 .zclist {padding-top:7px;}

.zcblk05 .zccont {padding:0 13px 0 18px;}
.zclvzq {font-size:14px;text-align:center;margin:8px 0 5px 0;line-height:20px;color:#404040;  }
.zclvzq2 {font-size:14px;margin:20px 0 10px 0;font-weight:bold; line-height:25px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc11.png) no-repeat center bottom;color:#404040;   }
.zcblk05 p a {color:#641a00;}

.zcblk06 {background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc13.png) repeat-y;}
.zcblk06 .zcbot {height:9px;clear:both;font-size:0px;background:url(http://www.sinaimg.cn/cj/collectioncomp/images/pm_wzc12.png) no-repeat;overflow:hidden;}

.zcblk07 {margin-top:10px;border:1px solid #c08b38;padding:4px;}

/* footer */
#footer { width:950px; line-height:28px; text-align:center; color:#000; font-size:12px; padding-top:20px; margin:0 auto; }
#footer a:link, #footer a:visited { color:#0135ad; text-decoration:underline; }
#footer a:hover, #footer a:active { color:#f00; text-decoration:underline; }
