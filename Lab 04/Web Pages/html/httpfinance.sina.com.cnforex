seajs.use(['sina.SubShow','jquery'],function(SubShow,$){
    var ssc = new SubShow('none','onclick','','selected');
    ssc.addLabel('main_b01_lab01','main_b01_cont01');
    ssc.addLabel('main_b01_lab02','main_b01_cont02');
    ssc.addLabel('main_b01_lab03','main_b01_cont03');

    var lab = $('.main_b01_w h3 span'),
            form = $('.main_b01_w form'),
            s0 = $('select[row=0]',form),
            s1 = $('select[row=1]',form),
            s2 = $('.cal_bank',form),
            s3 = $('.cal_reset',form),
            s4 = $('input[row=0]'),
            s5 = $('input[row=1]'),
            cont = $('#main_b02'),
            table = $('#main_b02 table'),
            sideref = $('.aside_b01'),
            reseting = false,
            siding = false,
            tbth = '<tr><th>银行</th><th>货币</th><th>买外币</th><th>卖外币</th><th>纸币卖银行</th><th>银行中间价</th><th>更新时间</th></tr>',
            tbtd = '<tr class="%bank%"><td class="ta_l">%bname%</td><td class="ta_l">%ft%</td><td class="ta_r xh_buy">%xh_sell%</td><td class="ta_r xh_sell">%xh_buy%</td><td class="ta_r xc_buy">%xc_buy%</td><td class="ta_r mid">%mid%</td><td class="ta_r">%update%</td></tr>',
            apib = '//stock.finance.sina.com.cn/forex/api/openapi.php/ForexService.getBankForexList',
            ALLDATA = {},
            getAncessor = function(t,tname){
                do{
                    t = t.parentNode;
                    if(t.nodeName.toLowerCase() === tname){
                        return t;
                    }
                }
                while(t.parentNode)
                return null;
            },
            sortBy = function(arr,attr){
                return arr.sort(function(a,b){
                    return a[attr] > b[attr] ? 1 : -1;
                });
            },
            formatNum = function(x){
                var t;
                if(x > 10000){x = Math.ceil(x);}
                t = isFinite(x) ? ((x == '0.000') ? '---' : x) : '---';
                return t;
            },
            getData = function(f,t,callback){
                var k = 0;
                $.ajax({
                    url : apib,
                    data : {
                        from : f
                    },
                    dataType : 'jsonp',
                    success : function(datas){
                        k += 1;
                        ALLDATA[f] = sortBy(datas.result.data,'bank');
                        if(k === 2){callback();}
                    }
                });
                $.ajax({
                    url : apib,
                    data : {
                        from : t
                    },
                    dataType : 'jsonp',
                    success : function(datas){
                        k += 1;
                        ALLDATA[t] = sortBy(datas.result.data,'bank');
                        if(k === 2){callback();}
                    }
                });
            },
            rankTable = function(f,t,c,r){
                var Fd = ALLDATA[f],
                        Td = ALLDATA[t],
                        ret = [],
                        tbh = '';
                for(var n = 0, l = Fd.length; n < l; n++){
                    var fd = Fd[n],
                            td = Td[n],
                            mid = (100 * parseFloat(fd.mid_price)/parseFloat(td.mid_price)).toFixed(4),
                            xh_sell = (100 * parseFloat(fd.xh_sell_price)/parseFloat(td.xh_sell_price)).toFixed(4),
                            xc_buy = (100 * parseFloat(fd.xc_buy_price)/parseFloat(td.xc_buy_price)).toFixed(4),
                            xh_buy = (100 * parseFloat(fd.xh_buy_price)/parseFloat(td.xh_buy_price)).toFixed(4);
                    ret.push({
                        bank : fd.bank,
                        bname : fd.bname,
                        ft : fd.name + '-' + td.name,
                        mid : formatNum(mid),
                        xh_sell : formatNum(xh_sell),
                        xc_buy : formatNum(xc_buy),
                        xh_buy : formatNum(xh_buy),
                        update : fd['updatetime'] || (function(){
                            var t = new Date(),
                                    y = t.getFullYear(),
                                    m = t.getMonth() + 1,
                                    d = t.getDate(),
                                    h = t.getHours(),
                                    i = t.getMinutes(),
                                    s = t.getSeconds();
                            return y + '-' + m + '-' + d + ' ' + h + ':' + i + ':' + s;
                        })()
                    });
                }
                if(c === 'xh_buy' && r === 0){c = 'xc_buy';}
                ret.sort(function(a,b){
                    var at = parseFloat(a[c]),
                            bt = parseFloat(b[c]);
                    return at - bt < 0 ? -1 : 1;
                });
                if((c === 'xh_sell' && r === 1) || (c === 'xh_buy' && r === 1) || (c === 'xc_buy') || (c === 'mid') && r === 1){ret.reverse();}
                tbh += tbth;
                for(var n = 0, l = ret.length; n < l; n++){
                    var d = ret[n];
                    tbh += tbtd.replace(/%bank%/,d['bank']).replace(/%bname%/,d['bname']).replace(/%ft%/,d['ft']).replace(/%mid%/,d['mid']).replace(/%xh_sell%/,d['xh_sell']).replace(/%xc_buy%/,d['xc_buy']).replace(/%xh_buy%/,d['xh_buy']).replace(/%update%/,d['update']).replace(/NaN/g,'---');
                }
                table.html(tbh);
                cont.slideDown();
                return ret;
            },
            doit = function(row){
                var tform = $(form[lab.index($('.main_b01_w h3 span.selected'))]),
                        d = $($('div',tform)[row]),
                        fs = $($('.cal_snum',d)[0]),
                        ts = $($('.cal_snum',d)[1]),
                        fd = $($('.cal_sname',d)[0]),
                        td = $($('.cal_sname',d)[1]),
                        F = fd.val(),
                        T = td.val(),
                        Fs = fs.val(),
                        C = tform.attr('ct'),
                        B = $('.cal_bank',tform).val(),
                        r = $('.cal_ratio',tform);
                tform.attr('row',row);
                getData(F,T,function(){
                    var ret = rankTable(F,T,C,row),
                            data,res;
                    $('.cal_snum',tform).val('');
                    fs.val(Fs);
                    if((C === 'xh_buy') && (row === 1)){C = 'xh_buy';}
                    else if((C === 'xh_buy') && (row === 0)){C = 'xc_buy';}
                    for(var i = 0, l = ret.length; i < l; i++){
                        if(ret[i]['bank'] === B){
                            data = ret[i];
                            break;
                        }
                    }
                    r.val(isNaN(data[C]) ? '银行无此报价' : data[C]);
                    if(fs.val() === ''){
                        fs.val('100');
                    }
                    res = (fs.val() * data[C] / 100).toFixed(4);
                    res = formatNum(res);
                    ts.val(res);
                });
            };

    //初始化
    doit(0);

    //lab切换
    lab.click(function(){
        if(siding){return;}
        $('.cal_reset',$(form[lab.index($('.main_b01_w h3 span.selected'))])).click();
        doit(0);
    });

    //输入
    s4.focus(function(){
        $(form[lab.index($('.main_b01_w h3 span.selected'))]).attr('row',0);
    });
    s5.focus(function(){
        $(form[lab.index($('.main_b01_w h3 span.selected'))]).attr('row',1);
    });

    //select计算
    s0.change(function(){
        if(reseting){return;}
        doit(0);
    });
    s1.change(function(){
        if(reseting){return;}
        doit(1);
    });
    s2.change(function(){
        if(reseting){return;}
        var k = parseInt($(form[lab.index($('.main_b01_w h3 span.selected'))]).attr('row')) || 0;
        doit(k);
    });

    //点击计算
    form.submit(function(e){
        e.preventDefault();
        var k = parseInt($(this).attr('row')) || 0;
        doit(k);
    });

    //重置
    s3.click(function(){
        reseting = true;
        setTimeout(function(){
            doit(0);
            reseting = false;
        },30);
    });

    //aside点击切换
    sideref.delegate('td','click',function(e){
        var t = $(this),
                tr = $(getAncessor(t[0],'tr')),
                tb = $(getAncessor(t[0],'table')),
                ct= t.attr('ct'),
                cy = tr.attr('cy'),
                bc = tb.attr('bc'),
                oform;
        siding = true;
        reseting = true;
        $('.main_b01_w h3 span[ct=' + ct + ']').click();
        oform = $(form[lab.index($('.main_b01_w h3 span.selected'))]);
        oform.attr('row',0);
        $($('.cal_sname',oform)[0]).val(cy);
        $($('.cal_sname',oform)[1]).val('CNY');
        $('.cal_bank',oform).val(bc);
        setTimeout(function(){
            doit(0);
            siding = false;
            reseting = false;
        },30);
    });
});
