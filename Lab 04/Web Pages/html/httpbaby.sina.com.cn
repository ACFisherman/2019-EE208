						var flag=false,p=obj;
						while (p!=document.body) {
							if (p==sina.$('date_widget')) {
								flag=true;
								break;
							}else {
								if (p.parentNode) {
									p=p.parentNode;
								}else {
									break;
								}
								flag=false;
							}
						}
						if (obj!=sina.$('show_date_widget') && !flag && sina.$('date_widget')) {//2010/6/9
							sina.$('date_widget').style.display='none';
						}
					}
