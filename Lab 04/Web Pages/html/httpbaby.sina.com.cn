<script type="text/javascript">
   jsLoader(BABY_JSS.jq, function () {
                        //fix right block
var PAGE_CONFIG = {
  
    fixed:{
        wrap:'blk_fixed',
        endPos:'fix_end',
        distanceTop:60
    }
};
(function($) {
    $.fn.scrollFix = function(options) {
        return this.each(function() {
            var defaults = {
                startTop: null,
                startBottom: null,
                distanceTop: 0,
                endPos: 0
            };
            var opts = $.extend({},
                defaults, options),
                obj = $(this),
                offset,
                offsetTop,
                offsetLeft,
                placeholder = jQuery('<div>'),
                documentHeight,
                optsTop,
                outerHeight,
                marginBottom,
                outerHeight,
                outerWidth,
                objWidth,
                startTop,
                startBottom,
                toBottom,
                ScrollHeight,
                endfix;
            if(obj.length<=0){
                return;
            }
            var bodyToTop;
            var setData = function(){
                if(placeholder){
                    offset = placeholder.offset();
                }else{
                    offset = obj.offset();
                }
                offsetTop = offset.top;
                offsetLeft = offset.left;
                // placeholder = jQuery('<div>');
                documentHeight = $(document).height();
                optsTop = opts.distanceTop;
                outerHeight = obj.outerHeight();
                marginBottom = (obj.css('marginBottom')).slice(0,-2);
                var testnum=/^[0-9]*$/gi;
                marginBottom=testnum.test(marginBottom)?marginBottom:0; 
                outerHeight = parseInt(outerHeight) + parseInt(marginBottom);
                outerWidth = obj.outerWidth();
                objWidth = obj.width();
                startTop = $(opts.startTop);
                startBottom = $(opts.startBottom);
                bodyToTop = parseInt(jQuery('body').css('top'), 10);
                if(!isNaN(bodyToTop)) {
                        optsTop += bodyToTop;
                }
                if ($.isNumeric(opts.endPos)) {
                    toBottom = opts.endPos
                } else {
                    toBottom = parseInt(documentHeight - $(opts.endPos).offset().top);
                }
                ScrollHeight = parseInt(documentHeight - toBottom), endfix = parseInt(ScrollHeight - outerHeight);
            };
            placeholder.insertBefore(obj);
            setData();
            $(window).scroll(function() {
                setData();
                var ScrollTop = $(window).scrollTop();
                if (startTop[0]) {
                    var startTopOffset = startTop.offset(),
                        startTopPos = startTopOffset.top;
                    offsetTop = startTopPos;
                }
                if (startBottom[0]) {
                    var startBottomOffset = startBottom.offset(),
                        startBottomPos = startBottomOffset.top,
                        startBottomHeight = startBottom.outerHeight();
                    offsetTop = parseInt(startBottomPos + startBottomHeight);
                }
                toTop = parseInt(offsetTop - optsTop);
                toTop = (toTop > 0) ? toTop : 0;
                var selfBottom = documentHeight - offsetTop - outerHeight;
                if ((ScrollTop > toTop) && (ScrollTop < endfix)) {
                    obj.fadeIn().css({
                        'position': 'fixed',
                        '_position': 'absolute',
                        'top': opts.distanceTop,
                        '_top': opts.distanceTop,
                        'width': objWidth
                    });
                    placeholder.css({
                        'height': outerHeight,
                        '_height': '0'
                    });
                } else if ((ScrollTop >= endfix)&& (selfBottom<=endfix) ){
                    obj.css({
                        'position': 'absolute',
                        'top': endfix,
                        'width': objWidth
                    });
                    placeholder.css({
                        'height': outerHeight,
                        '_height': '0'
                    }).insertBefore(obj)
                } else {
                    obj.css({
                        'position': 'static',
                        'top': ''
                    });
                    placeholder.css({
                        'height':'0',
                        'overflow':'hidden'
                    });
                    // placeholder.remove();
                }
            }).scroll()
        })
    }
})(jQuery);

 if(PAGE_CONFIG.fixed&&PAGE_CONFIG.fixed&&PAGE_CONFIG.fixed.endPos){
        $('#'+PAGE_CONFIG.fixed.wrap).scrollFix({
            distanceTop:PAGE_CONFIG.fixed.distanceTop,endPos:'#'+PAGE_CONFIG.fixed.endPos
        });
}

  });
</script>

