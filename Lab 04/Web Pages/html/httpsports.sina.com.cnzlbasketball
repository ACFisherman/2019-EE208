<script type="text/javascript">

    function hasClass(ele, cname) {
        var reg = new RegExp('(^| )' + cname + '( |$)'),
            ename = ele.className;
        return reg.test(ename);
    }
    function getByClass(searchClass, node, tag) {
        if (node == null) {
            node = document;
        }
        if (tag == null) {
            tag = '*';
        }
        var classElements = [],
            els = node.getElementsByTagName(tag),
            elsLen = els.length;
        for (i = 0; i < elsLen; i++) {
            if (hasClass(els[i], searchClass)) {
                classElements.push(els[i]);
            }
        }
        return classElements;
    }
    ;

    function getByAttr(aname, avalue, node, tag) {
        if (!node) {
            node = document;
        }
        if (!tag) {
            tag = '*';
        }
        var ret = [],
            eles = node.getElementsByTagName(tag),
            l = eles.length;
        for (var i = 0; i < l; i++) {
            var d = eles[i],
                a = d.getAttribute(aname);
            if (avalue !== null && a !== avalue || a === null) {
                continue;
            }
            ret.push(d);
        }
        return ret;
    }

    (function () {
        //画中画悬停
        var reIE = /MSIE\s([^;]*)/;
        var browser = {
            ie: 0,
            loaded: false
        };

        (function (browser) {
            function toNumber(str) {
                var count = 0;
                return parseFloat(str.replace(/\./g, function () {
                    count++;
                    return (count === 1) ? '.' : '';
                }));
            }

            var ua = (navigator && navigator.userAgent) || '';
            var temp = ua.match(reIE);
            if (temp && temp[1]) {
                browser.ie = toNumber(temp[1]);
            }
        })(browser);

        (function () {
            var utils = {
                hasClass: function (elem, cls) {
                    var reg = new RegExp('(^|\\s)' + cls + '($|\\s)');
                    return reg.test(elem.className);
                },
                removeClass: function (elem, cls) {
                    var reg = new RegExp('(^|\\s)' + cls + '($|\\s)', 'g');
                    elem.className = elem.className.replace(reg, ' ');
                },
                addClass: function (elem, cls) {
                    if (!utils.hasClass(elem, cls)) {
                        elem.className += ' ' + cls;
                    }
                },
                viewData: function () {
                    var e = 0,
                        l = 0,
                        i = 0,
                        g = 0,
                        f = 0,
                        m = 0;
                    var j = window,
                        h = document,
                        k = h.documentElement;
                    e = k.clientWidth || h.body.clientWidth || 0;
                    l = j.innerHeight || k.clientHeight || h.body.clientHeight || 0;
                    g = h.body.scrollTop || k.scrollTop || j.pageYOffset || 0;
                    i = h.body.scrollLeft || k.scrollLeft || j.pageXOffset || 0;
                    f = Math.max(h.body.scrollWidth, k.scrollWidth || 0);
                    m = Math.max(h.body.scrollHeight, k.scrollHeight || 0, l);
                    return {
                        scrollTop: g,
                        scrollLeft: i,
                        documentWidth: f,
                        documentHeight: m,
                        viewWidth: e,
                        viewHeight: l
                    };
                }
            };

            function getPosition(a) {
                for (var b = {
                    x: 0,
                    y: 0
                }; a.offsetParent;)
                    b.x += a.offsetLeft, b.y += a.offsetTop, a = a.offsetParent;
                return b
            }

            function addEvent(obj, eventType, func) {
                if (obj.attachEvent) {
                    obj.attachEvent("on" + eventType, func);
                } else {
                    obj.addEventListener(eventType, func, false)
                }
            };
            // mod lqf 20140618 {fix view too low}

            if (browser.ie != 6) {
                var container = document.getElementById('rightFixedArea');
                var guessContainer = document.getElementById('guess');
                var sidebar = getByClass("col300", document)[0];
                var navwrap = getByClass("nav-wrap", document)[0];

                var theY = getPosition(sidebar).y + sidebar.offsetHeight;
                setTimeout(function () {
                    theY = getPosition(sidebar).y + sidebar.offsetHeight;
                }, 1000);
                var guessFirstH = 0;

                function guessHeight() {
                    if (!!utils.hasClass(guessContainer, "right_fixed")) {
                        var h = 0;
                        if (navwrap.style.position === "fixed") {
                            h = navwrap.offsetHeight;
                        }
                        var N = Math.floor((document.documentElement.clientHeight - h - container.offsetHeight - 179 ) / 41);
                        if (N <= 0) {
                            N = 0;
                        }
                        var height = N * 41 + 179;
                        guessContainer.style.height = height + "px";
						guessContainer.style.position= "fixed";
                        guessContainer.style.overflow = "hidden";
                    } else {
                        guessContainer.style.height = "auto";
						guessContainer.style.position= "static";
                        guessContainer.style.overflow = "visible";
                    }
                }

                function adjustADPosition(bottomHeight) {
                    var adY = getPosition(sidebar).y + sidebar.offsetHeight - 10;
                    var adH = 0,
                        guessH = 343 || guessContainer.offsetHeight,
                        allH = adH + guessH;
                    if (adY != 0) {
                        theY = adY;
                        if (utils.hasClass(container, "right_fixed")) {
                            theY += adH;
                        }

                        if (utils.hasClass(guessContainer, "right_fixed")) {
                            theY += guessH;
                        }
                    }

                    var vD = utils.viewData();
                    if (vD.documentHeight - bottomHeight - vD.scrollTop >= allH) {
                        if (vD.scrollTop >= theY) {
                            utils.addClass(container, 'right_fixed');
                            utils.addClass(guessContainer, 'right_fixed');
                            var h = 0;
                            if (navwrap.style.position === "fixed") {
                                h = navwrap.offsetHeight;
                            }

                            container.style.top = h + "px";
                            guessContainer.style.top = (h + adH) + "px";
                        } else {
                            utils.removeClass(guessContainer, 'right_fixed');
                            utils.removeClass(container, 'right_fixed');
                        }
                    } else if (vD.documentHeight - bottomHeight - vD.scrollTop >= adH) {
                        utils.removeClass(guessContainer, 'right_fixed');
                        if (vD.scrollTop >= theY) {
                            utils.addClass(container, 'right_fixed');
                            var h = 0;
                            if (navwrap.style.position === "fixed") {
                                h = navwrap.offsetHeight;
                            }

                            container.style.top = h + "px";
                        } else {
                            utils.removeClass(guessContainer, 'right_fixed');
                            utils.removeClass(container, 'right_fixed');
                        }
                    } else {
                        utils.removeClass(guessContainer, 'right_fixed');
                        utils.removeClass(container, 'right_fixed');
                    }
                    guessHeight();
                }

                var scrollTimeout = null;
                var scrollCallback = function () {
                    adjustADPosition(130);
                };
                var resizeTimeout = null;
                addEvent(window, 'scroll', function () {
                    if (scrollTimeout) window.clearTimeout(scrollTimeout);
                    scrollTimeout = window.setTimeout(scrollCallback, 10);
                });
                addEvent(window, 'resize', function () {
                    if (resizeTimeout) window.clearTimeout(resizeTimeout);
                    resizeTimeout = window.setTimeout(scrollCallback, 10);
                });

                addEvent(window, 'resize', function (ev) {
                    guessHeight();
                });

            }
        })();
    })()
</script>

