<script type="text/javascript" language="javascript">
/* tools */
if( typeof $ == 'undefined' )$ = function(id){return document.getElementById(id)};
if( typeof $C == 'undefined' )$C = function(t){return document.createElement(t)};
if( typeof $T == 'undefined' )$T = function(t){return document.createTextNode(t)};
if( typeof _A == 'undefined' )_A = function(t, c){t.appendChild(c)};
if( typeof _I == 'undefined' )_I = function(t, c){t.innerHTML = c};
if( typeof S_Finance == 'undefined' )S_Finance = {};
/* 常量或者通用方法 */
var Class = {
  create: function() {
    return function() {
      this.Init.apply(this, arguments);
    }
  }
};
/* 简单Util库 封装必要功能 */
if( typeof Util == 'undefined' )Util = {};
Util.AddEvent = function(el, name, cb, useCapture) {
    if (el.addEventListener) {      
      el.addEventListener(name, cb, useCapture);
    } else if (el.attachEvent) {
      el.attachEvent('on' + name, cb);
    }
};
Function.prototype._Bind = function() { 
  	var __m = this, object = arguments[0], args = new Array(); 
  	for(var i = 1; i < arguments.length; i++){
		args.push(arguments[i]);
	}
  	return function() {
  	var c_args = [];
		for(var k = 0; k < arguments.length; k++){
			c_args.push(arguments[k]);
		} 
		return __m.apply(object, c_args.concat(args));
  	}
};
/* configs */
var AppConf = {
	Interval_Uppage :  12*60*1000,	/* 整个页面停止更新的时间间隔 */
	Interval_Updata :  10*1000,		/* 数据更新的时间间隔 */
	Interval_Uppic :  60*1000,		/* 数据更新的时间间隔 */
	Interval_1s : 1*1000,           /* 延迟一秒,做闪烁效果用 */
	_isUppage:true,
	veracitynum:2,					/* 精确到小数点后面多少位数 */
	Debug:false
};
S_Finance.constant = {
	cn_name:null,
whqh_arr:{'DXF':"",'SF':"",'CD':"",'JY':"",'BP':"",'EC':""},
	add2portfolio:function(){
		var url ='http://biz.finance.sina.com.cn/hq/index.php?page=futures_my.php%3Fopt%3Dadd%26futures_list%3D'+fullcode;
		window.location.href = url;
	},
	_getCurrtime:function(time1){
		var curtime = new Date();
		var kk = curtime.getTime();
		if(time1>0){
			kk = parseFloat(kk/time1).toFixed(0);
		}
		return kk;
	},
	_formatnum:function(intnum){
		if(intnum<10) return "0"+intnum;
		else return intnum;
	},
	_goto:function(url){
		window.location.href=url;
	},
	load_data_script : function(url, idname){
		var obj = $(idname);
		if(obj){
			obj.parentNode.removeChild(obj);
		};
		var newscript = $C("script");
		newscript.type = "text/javascript";
		newscript.src = url;
		newscript.id = idname;
		_A(document.body, newscript);
	}
};
/*  up page data */
S_Finance.Uppage = Class.create();
S_Finance.Uppage.prototype = {
	oldtime : null,
	settimeid1 : null,
	diyici:null,
	Init : function(){
		this.oldtime = S_Finance.constant._getCurrtime();
		this.diyici = 1;
		// 改变 fullcode he img_type 的数据
		S_Finance.upconstants._whichcode();
		S_Finance.upconstants._changepic(img_type);
		// 修改精确位数 黄金的保持两位有效数字 
                 S_Finance.upconstants._changeVeracitynum();
		// 时间显示
		setInterval(S_Finance.upconstants.time_rota._Bind(this),1000);

		try{
			fillObjWithList($("jys"), JYS);
			chgJYS();
		}catch(er){}

		Util.AddEvent(document,"keydown",this._up_Page._Bind(this));
		Util.AddEvent(document,"mousemove",this._up_Page._Bind(this));
	},
	_stopUppage:function(){
		AppConf._isUppage=false;
	},	
	_up_Page : function(){
		AppConf._isUppage = true; /* 保持更新 */

		var curtim1 = S_Finance.constant._getCurrtime();
		var difren = curtim1-this.oldtime;
		this.oldtime = curtim1;

		if (this.settimeid1!=null && this.settimeid1!=""){
			clearTimeout(this.settimeid1);
		}
		this.settimeid1 = setTimeout(this._stopUppage._Bind(this),AppConf.Interval_Uppage);
		if (this.diyici==1 || difren>=AppConf.Interval_Uppage){
			S_Finance.upconstants._uppic_chart();	// 图片定期更新
			S_Finance.upconstants._updata();
			this.diyici = 0;
		}
	}
};
S_Finance.upconstants = {
	// 本地时间
	time_rota:function(){
		var now;
		var yy,mm,dd;
		var h,m,s;
		now = new Date();
		yy=now.getFullYear();
		mm=now.getMonth()+1;
		dd=now.getDate();
		h=now.getHours();
		m=now.getMinutes();
		s=now.getSeconds();

		mm=((mm < 10) ? "0" : "") + mm;
		dd=((dd < 10) ? "0" : "") + dd;
		h=((h < 10) ? "0" : "") + h;
		m=((m < 10) ? "0" : "") + m;
		s=((s < 10) ? "0" : "") + s;
		_I($("local_time"),"北京时间:" + yy+"-"+mm+"-"+dd+"  "+ h+":"+m+":"+s);
	},
	_changepic:function (param){
		var temprandom = S_Finance.constant._getCurrtime(); /* 加随机数 */
		/* 图片地址重新赋值 */
		$("chart").src='http://image.sinajs.cn/newchart/v5/futures/global/'+param+'/'+fullcode+'.gif?'+temprandom;
		$("chart").style.display = "block"; 
		return ;
	},
	/*  up picture begin */
	_uppic_chart:function(){
		var newurl = $('chart').src;
		var newurlpre = newurl.split("?");
		newurl = newurlpre[0]+"?"+S_Finance.constant._getCurrtime();
		$('chart').src = newurl;
		this._cycle_chart();
	},
	_cycle_chart:function(){
		if (AppConf._isUppage==1){
			setTimeout(S_Finance.upconstants._uppic_chart._Bind(this),AppConf.Interval_Uppic);
		};
	},
	_changeVeracitynum:function(){
		if(fullcode in S_Finance.constant.whqh_arr) { AppConf.veracitynum = 4;}
	},
	/*  up picture end */
	_whichcode:function(){
	  var str=window.location.href;
	  var num=str.indexOf("?");
	  var num1=str.indexOf("?code=");
	  var num2=str.indexOf("&img_type=");
	  var params=str.substr(num+1);
        if(params == 'NAS' || params == 'ES' || params == 'DJS')
        {
            document.getElementById('tipDelay').style.display = '';
        }
	  // 1. 先判断num2，如果有图片类型的参数，则需要将fullcode重新赋值，而且图片也需要重新定位
	  // 2. 如果没有图片类型的参数，则判断num1是否有code参数，如果有code参数，fullcode重新赋值，
	  // 3. 如果没有code参数，则判断是否有?,有则fullcode重新赋值，如果没有，则均用默认的参数。

	  if(num2 != -1)img_type=str.substr(num2+10);

	  if(num1 != -1){
		   if(num2 != -1){
				fullcode=str.substring(num1+6,num2);
		   }else{
				fullcode=str.substr(num1+6);
		   }
	  }else{
		   if(num != -1){
				if(num2 != -1){
				 	params=str.substring(num+1,num2);
				}else{
				 	params=str.substr(num+1);
				}
				try{
					fullcode = params;
				}catch(er){
					if(AppConf.Debug) alert(er.name);
				}
		   }
	  }
	 },
	/*  up data begin */
	_updata:function(){
		if (AppConf._isUppage){
			var theurl = "http://hq.sinajs.cn/format=js&func=S_Finance.upconstants.setData();&list=hf_"+fullcode;
			S_Finance.constant.load_data_script(theurl,"datascript");
		}
	},
	_getFormatTime:function(){
		var temtime = new Date();	// 本地时间
		/*try{
			if(StandardBJTime != null && StandardBJTime != undefined && StandardBJTime != "undefined" && StandardBJTime != ""){
				temtime.setTime(StandardBJTime*1000+1000);
				alert(temtime);
			}
		}catch(er){}*/

		var ye = temtime.getFullYear();
		var mo = temtime.getMonth()+1;
		var da = temtime.getDate();

		// 时区 london,chicago,newyork,japan

		return ye+"-"+S_Finance.constant._formatnum(mo)+"-"+S_Finance.constant._formatnum(da);
	},
	_display_hqdata:function(){
		// 右侧行情数据，以及期货名称，期货code，期货页面title
		// LAST,CHANGE,BID,ASK,HIGH,LOW,TIMEUPDATE,Prev,Open,TOTALVOL,BIDSIZE,ASKSIZE
		eval("this.hq_str_symbol = hq_str_hf_"+fullcode);
		if (this.hq_str_symbol != null && this.hq_str_symbol != undefined && this.hq_str_symbol != "undefined" && this.hq_str_symbol != ""){
			try {
				var l_sdata = this.hq_str_symbol.split(',');
				var riqi = S_Finance.upconstants._getFormatTime();// 需要做一个标准时间
				var ChangePercent = (l_sdata[0]-l_sdata[7])*100/(l_sdata[7]);
				if(parseFloat(l_sdata[0]-l_sdata[7])>0){
					var color = "color:#008000;";
				}else if(l_sdata[0]-l_sdata[7]<0){
					var color = "color:#f00;";
				}else {
					var color = "";
				}
				var l_ul_li = '<ul>';
					l_ul_li += '    <li><h4>代码</h4><span>'+fullcode+'</span></li>';
					l_ul_li += ' 	<li><h4>名称</h4><span>'+all_hf_code[fullcode]+'</span></li>';
					l_ul_li += ' 	<li><h4>本地日期</h4><span>'+l_sdata[12]+'</span></li>';
					l_ul_li += ' 	<li><h4>最新行情时间</h4><span>'+l_sdata[6]+'</span></li>';
					l_ul_li += ' 	<li><h4>最新成交价</h4><span style="'+color+' font-weight:bold;">'+parseFloat(l_sdata[0]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>涨跌幅</h4><span style="'+color+' font-weight:bold;">'+parseFloat(ChangePercent).toFixed(2)+'%</span></li>';
					l_ul_li += ' 	<li><h4>涨跌额</h4><span style="'+color+' font-weight:bold;">'+parseFloat(l_sdata[0]-l_sdata[7]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>开盘价</h4><span>'+parseFloat(l_sdata[8]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>最高价</h4><span>'+parseFloat(l_sdata[4]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>最低价</h4><span>'+parseFloat(l_sdata[5]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>买价</h4><span>'+parseFloat(l_sdata[2]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>卖价</h4><span>'+parseFloat(l_sdata[3]).toFixed(AppConf.veracitynum)+'</span></li>';
				//	l_ul_li += ' 	<li><h4>动态结算价</h4><span>'+parseFloat(l_sdata[9]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>昨日结算价</h4><span>'+parseFloat(l_sdata[7]).toFixed(AppConf.veracitynum)+'</span></li>';
					l_ul_li += ' 	<li><h4>买量</h4><span>'+("0">=l_sdata[10].toLowerCase()?"--":l_sdata[10])+'</span></li>';
					l_ul_li += ' 	<li><h4>卖量</h4><span>'+("0">=l_sdata[11].toLowerCase()?"--":l_sdata[11])+'</span></li>';
					l_ul_li += ' 	<li><h4>持仓量</h4><span>'+l_sdata[9]+'</span></li>';
					l_ul_li += ' 	<li>(期货行情由倚天财经提供)</li>';
					l_ul_li += '</ul>';
				_I($("futures_info"),l_ul_li);

				// name,code,title
				_I($("futures_name"),all_hf_code[fullcode]);

				_I($("futures_code"),"("+fullcode+")");				
			} catch(er){
				if(AppConf.Debug)alert(er.name);
			}
		}
	},

	setData:function(){
		S_Finance.upconstants._display_hqdata();// 右侧行情数据，有出错处理 try
		setTimeout(this._updata._Bind(this),AppConf.Interval_Updata);
	}
};
function clear(obj1)
{
	var len=obj1.length;
	for (i = 0; i < len; i ++)
	{
		obj1.options[0]=null;
	}
};
function fillObjWithList(obj1, lst1)
{
	clear(obj1);
	for(i=0; i<lst1.length; i++)
	{
		obj1.options[i]  = new Option(lst1[i]);
	}
};

function fillObjWithAry(obj1, ary1)
{
	clear(obj1);
	for(i=0; i<ary1.length; i++)
	{
		obj1.options[i] = new Option(ary1[i][0]);
	}
};

function chgJYS()
{
	var jyspos = $("jys").selectedIndex;
	fillObjWithList($("pinzhong"), PZ[jyspos]);
	chgPZ();
};

function chgPZ()
{
	var jyspos = $("jys").selectedIndex;
	var pzpos = $("pinzhong").selectedIndex;
	fillObjWithAry($("yf"), YF[jyspos][pzpos]);
	chgYF();
};

function chgYF()
{
	var jyspos = $("jys").selectedIndex;
	var pzpos = $("pinzhong").selectedIndex;
	var yfpos = $("yf").selectedIndex;
	var code = YF[jyspos][pzpos][yfpos][1];
};

function toUrl()
{
	var jyspos = $("jys").selectedIndex;
	var pzpos = $("pinzhong").selectedIndex;

	var yfpos = $("yf").selectedIndex;
	var code = YF[jyspos][pzpos][yfpos][1];
	var prefix = code.replace(/[0-9]/g, "").toUpperCase();
	if("IF"==prefix) {
		url = "http://finance.sina.com.cn/futures/quote/cff/bc.shtml?code="+code;
		window.open(url);
	}
	else {
		url = "quote.html?" + code;
		window.location.href = url; //上线以后，将国内期货页面也相应的增加外盘期货查询地址
	}
