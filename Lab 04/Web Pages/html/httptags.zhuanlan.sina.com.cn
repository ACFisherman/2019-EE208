function FeedsLoader(cfg){
	this.api = cfg.api;
	//this.wcont = cfg.wcont;
	//new start
	this.wlist=cfg.wlist;
	//new end
	this.wbtn = cfg.wbtn;
	this.wpage = cfg.wpage;
	this.wprev = cfg.wprev;
	this.selector = cfg.selector || 'li',
	this.pagen = cfg.pagen || 5;
	this.step = cfg.step || 3;
	this.size = cfg.size || 8;
	this.cname = cfg.cname;
	//this.ts = cfg.ts || '';
	//this.te = cfg.te || '';
	this.self = cfg.self;
	this.joiner = cfg.joiner || function(){};
	this.callback = cfg.callback || function(){};
    this.onblank = cfg.onblank || function(){},
	this.total = 0;
	this.nIndex = 0;
	//new start
	this.isInit=false;
	//new end
	this.init();
};
FeedsLoader.prototype = {
	init : function(){
		this.initEvent();
		this.go(1);
	},
	initEvent : function(){
		var that = this,
			wbtn = that.wbtn,
			timer;
		$(window).on('scroll',function(){
			timer && clearTimeout(timer);
			timer = setTimeout(function(){
				if(wbtn.is(':visible') && wbtn.offset().top < $(window).scrollTop() + $(window).height() - 150){
					that.showMore(false);
				}
			},20);
		});
		that.wpage.delegate('a','click',function(e){
			var t = $(this);
			if(t.hasClass('disabled')){return;}
			if(t.hasClass('llt')){
				e.preventDefault();
				that.llt();
				return;
			}
			if(t.hasClass('rrt')){
				e.preventDefault();
				that.rrt();
				return;
			}
			if(t.hasClass('lt')){
				e.preventDefault();
				that.lt();
				return;
			}
			if(t.hasClass('rt')){
				e.preventDefault();
				that.rt();
				return;
			}
			e.preventDefault();
			that.go(t.attr('page'));
		});
	},
	showMore : function(b){
		var that = this,
			nIndex = that.nIndex,
			selector = that.selector,
			//wcont = that.wcont,
			//new start
			wlist=that.wlist,
			//new end
			wprev = that.wprev,
			wpage = that.wpage,
			wbtn = that.wbtn,
			//lists = $(selector,wcont),
			//new start
			lists=$(selector,wlist),
			//new end
			//hlists = $(selector + ':hidden',wcont),
			//new start
			hlists=$(selector+':hidden',wlist),
			//new end
			ll = lists.length,
			hll = hlists.length,
			size = that.size,
			l = Math.min(hll,size),
			i;
		if(nIndex === 1 && wprev.length > 0 && $(selector,wprev).length){
			wprev.show();
			if(b){
				if(hll > 0){
					wbtn.show();
					wpage.hide();
				}else{
					wbtn.hide();
					wpage.show();
				}
				return;
			}else{
				if(hll > size){
					wbtn.show();
					wpage.hide();
				}else{
					wbtn.hide();
					wpage.show();
				}
			}
		}else{
			wprev.hide();
			if(hll > size){
				wbtn.show();
				wpage.hide();
			}else{
				wbtn.hide();
				wpage.show();
			}
		}
		for(i = 0; i < l; i++){
			$(hlists[i]).show();
		}
	},
	paintPage : function(){
		var that = this,
			wpage = that.wpage,
			cname = that.cname,
			total = that.total,
			size = that.size,
			step = that.step,
			pagen = that.pagen,
			mpage = Math.ceil(total / size / step),
			nIndex = that.nIndex,
			l,s,e,
			h = [];
		wpage.html('');
		if(mpage <= 1){return;}
		s = 1 + pagen * Math.floor((nIndex - 1) / pagen);
		e = Math.min(mpage,s + pagen - 1);
		if(s > pagen){h.push('<a href="javascript:;" class="llt">上' + pagen + '页</a>');}
		if(nIndex === 1){h.push('<a href="javascript:;" class="disabled">上一页</a>');}
		else{h.push('<a href="javascript:;" class="lt">上一页</a>');}
		for(; s <= e; s++){
			h.push('<a href="javascript:;" page="' + s + '" class="' + (s === nIndex ? cname : '') + '">' + s + '</a>');
		}
		if(nIndex >= mpage){h.push('<a href="javascript:;" class="disabled">下一页</a>');}
		else{h.push('<a href="javascript:;" class="rt">下一页</a>');}
		if(e < mpage){h.push('<a href="javascript:;" class="rrt">下' + pagen + '页</a>');}
		wpage.html(h.join(''));
	},
	llt : function(){
		var that = this,
			pagen = that.pagen;
		that.go(1 + pagen * (Math.floor((that.nIndex - 1) / pagen) - 1));
	},
	rrt : function(){
		var that = this,
			pagen = that.pagen;
		that.go(1 + pagen * (Math.floor((that.nIndex - 1) / pagen) + 1));
	},
	lt : function(){
		var that = this;
		that.go(that.nIndex - 1);
	},
	rt : function(){
		var that = this;
		that.go(that.nIndex + 1);
	},
	go : function(page){
		var that = this,
			mpage = Math.ceil(that.total / that.step / that.size),
			api = that.api,
			size = that.size,
			step = that.step,
			num;
		//new start
		if (!arguments.callee.count){   
            arguments.callee.count=1;
		} else{
			that.isInit=true;
		}
		//new end
		page = parseInt(page);
		if(isNaN(page)){return;}
		if(page === that.nIndex){return;}
		if(page <= 0){that.nIndex = 1;}
		else if(page > mpage){that.nIndex = Math.max(1,mpage);}
		else{that.nIndex = page;}
		num = size * step;
		api = api.replace(/%page%/,page).replace(/%num%/,num);
		$.ajax({
			url : api,
			cache : true,
			dataType : 'jsonp',
			jsonpCallback : that.self + '.paint'
		});
	},
	paint : function(s){
		if(!s || !s.result || !s.result.data || !s.result.data.list || !s.result.data.list.length){
			if(this.nIndex > 1){
				this.go(this.nIndex - 1);
				return;
			} else if(!this.wprev.length || !$(this.selector,this.wprev).length){
                this.onblank();
                return;
            }
		}
		var that = this,
			nIndex = that.nIndex,
			//wcont = that.wcont,
			//new start
			wlist=that.wlist,
			//new end
			odatas = s.result.data.list,
			//new start
			pageNo = s.result.data.page,
			//new end
			datas = [],
			l,
			h = [],
			i;
		if(nIndex === 1 && that.wprev.length > 0 && $(that.selector,that.wprev).length){
			for(i = 0, l = odatas.length; i < l; i++){
				if($('[bid="' + odatas[i]['articleurl'] + '"]').length > 0){continue;}
				datas.push(odatas[i]);
			}
			l = (that.step - 1) * that.size;
		}else{
			datas = odatas;
			l = that.step * that.size;
		}
		that.total = s.result.data.total_count - 0;
		//h.push(that.ts);
		//new start
		i=(!that.isInit&&pageNo==1)?8:0;
		//new end
		h.push(that.ts);
		for(i ; i < l; i++){
			h.push(that.joiner(datas[i]));
		}
		//h.push(that.te);
		//wcont.html(h.join(''));
		//new start
		(!that.isInit&&pageNo==1)?wlist.append(h.join('')):wlist.html(h.join(''));
		//new end
		//$(that.selector,wcont).hide();
		//new start
		$(that.selector,wlist).hide();
		//new end
		that.showMore(true);
		that.paintPage();
		that.callback();
	}
};

