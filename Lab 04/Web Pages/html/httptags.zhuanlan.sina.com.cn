				x = 0,
                url,
				y = 0,
				i,d;
			for(i = 0; i < l; i++){
				d = datas[i];
				if(i % num === 0){
					h.push('<div class="main_guess_slide_items fl">');
					h.push('<ul class="list01">');
					x++;
					y = 0;
				}
                y++;
                url = d['url'] || '';
                url = (url.indexOf('?') > -1 ? url+'&' : url+'?') + 'cre=tags&r=' + (d['r'] || 'user') + '&pos=' + x + '_' + y;
				h.push(tli.replace(/%pic%/,d['thumb'] ? tpic.replace(/%imgurl%/,d['thumb']) : '').replace(/%url%/g,url).replace(/%title%/g,d['title'] || '').replace(/%stitle%/g,d['stitle'] || d['title'].slice(0,20)).replace(/%ptime%/,(function(q){
					if(!q){return '';}
					q = new Date(q * 1000);
					return (q.getMonth() + 1) + '月' + q.getDate() + '日 ' + fixNum(q.getHours()) + ':' + fixNum(q.getMinutes());
				})(d['ctime'])));
				if(i % num === num - 1 || i === l - 1){
					h.push('</div>');
					h.push('</ul>');
				}
			}
			w.html(h.join(''));
			sp = new ScrollPic();
			sp.scrollContId = wid;
			sp.pageWidth = 300;
			sp.frameWidth = 300;
			sp.speed = 10;
			sp.autoPlay = false;
			sp.initialize();
		};
	wnext.on('click',function(){
		if(sp){sp.next();}
	});
	window.guessCallback = function(s){
		var datas;
		if(!s){
			handdleFailure();
			return;
		}
		datas = (function(d,t){
			var ti,tl,td;
			d = d || [];
			t = t || [];
			for(ti = 0, tl = t.length; ti < tl; ti++){
				td = t[ti];
				td['r'] = 'hot';
				d.push(td);
			}
			return d;
		})(s.data,s.top);
		if(!datas.length){
			handdleFailure();
			return;
		}
		gdata = datas;
	};
	window.GUESSSLIDE = {
		refresh : function(){
			getScript({
				url : api.replace(/%length%/,page * num).replace(/%fields%/,fields.join(',')) + '&_=' + new Date().getTime(),
				timeout : 10e3,
				ontimeout : function(){
					handdleFailure();
				},
				onerror : function(){
					handdleFailure();
				},
				oncomplete : function(){
					paint(gdata);
				}
			})
		}
	};
})();

(function(){
        var w = $('#main_guess'),
        w3 = $('.main_l'),
        w4 = $('.main_r'),
        interval = null,
        timer = null,
        resizePos = function(){
            var hpl = w3.height(),
                hpr = w4.height(),
                tpl = w3.offset().top,
                tpr = w4.offset().top,
                st = $('document,body').scrollTop(),
                hh = w.height();
            if(st > tpr + hpr - 10){
                w.addClass('main_guess_fixed');
                w.css({
                    top : Math.min(0,hpl + tpl - st - hh) + 'px'
                });
            }else{
                w.removeClass('main_guess_fixed');
                w.css({
                    top : 0
                });
            }
        };
    interval = setInterval(resizePos,20);
    $(window).on('load',function(){
        interval && clearInterval(interval);
        interval = null;
        $(window).on('scroll',function(){
            timer && clearTimeout(timer);
            timer = setTimeout(resizePos,20);
        });
    })
})();
</script>
