					$('.mcom_num[data-comment="'+cms_id+'"]').html(val.total);
					$('.mcom_num[data-comment="'+cms_id+'"]').html(val.total).parent().attr({
						target:'_blank',
						href:'http://comment5.news.sina.com.cn/comment/skin/default.html?channel='+cms_channel
								+'&newsid='+cms_id
						}).show();
					});
				
				});
		})
	
	})();
	
