var len = jiandata.length;
function insert(d,n){
  var str = '';
  str+='<div class="m_li">';
  str+='<div class="name f1"><img class="img11" src="http://n.sinaimg.cn/games/lyqq/video3.png" width="45" height="25" alt=""/><span><a href="'+d[1]+'" target="_blank" title="">'+d[0]+'</a></span><br />'+d[2]+'</div>';
  str+='<div class="video"><a href="'+d[1]+'" target="_blank" title="'+d[0]+'"><img src="'+d[3]+'" /></a><span class="png">第'+(len-n+1)+'期</span></div>';
  str+='</div>';
  $('#mylist').append(str);
}
var cur = 0;
function init(n){
  for (var i = 0; i < n; i++) {
    insert(jiandata[i],i+1);
  };
  cur = n;
}
init(7);//开始时候显示几个填几个
$('.m_left .m_more').click(function(e) {
    e.preventDefault();
    if(cur>=len){
      $(this).text('真没了')
      return;
    }
 for (var i = 0; i < 4; i++) {
    	insert(jiandata[cur],cur+1);
   		cur++;
  	}
    var scr = $(window).scrollTop();
    $('html,body').animate({scrollTop: scr+394},400);
});

</script>
  
