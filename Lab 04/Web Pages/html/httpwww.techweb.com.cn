  var time = $('.reviewTime-on').html();

  var now = new Date();
  var day = $('#datepicker_top').val().replace(/-/g,'');
  var d = $('#datepicker_top').datepicker('getDate');
  if (day == '' || day == '请选择日期') {
    $('#datepicker_top').focus();
    return;
  }

  var tm = d.getFullYear()+'';
  if (d.getMonth()<9) {
    tm +='0'+(d.getMonth()+1);
  } else {
    tm += (d.getMonth()+1);
  }

  if (d.getDate()<10)
  {
    tm += '0' + d.getDate();
  } else {
    tm += d.getDate();
  }

  if (time =='11:00') {
    time = 'am';
  } else if (time == '23:00') {
    time = 'pm';
  }
