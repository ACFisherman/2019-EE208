<script type="text/javascript">
	//室内解毒js 原谅我找不到index.js
	$(function (){
		//tab切换
		tabFn();
		function tabFn(){
			var $li = $('.hz_shinei .hz_item li');
			var $box = $('.hz_shinei .hz_eff .hz_cell');

			$li.each(function (index, ele){
				$(this).mouseover(function (){
					$li.removeClass('active');
					$box.removeClass('active');
					$(this).addClass('active');
					$box.eq(index).addClass('active');
				})
			})
		}

		//小的轮播图
		lunbo();
		function lunbo(){
			var $box = $('.hz_shinei .hz_extBox');

			//3个轮播图分开
			$box.each(function (index, ele){
				var $parent = $(this);
				var $ul = $parent.find('.hz_extBox_ul');

				$parent.hover(function (){
					$(this).addClass('hz_extBox_ac');
				}, function (){
					$(this).removeClass('hz_extBox_ac');
				});

				dj.get("ui.focusimg",["ui.gun"],function(){
				    $ul.focusimg({
				    	speed:500,
				    	lazyload: true,
				        item : ".hz_extBox_item",
				        index : 0,
				        auto : false,
				        interval : 5000,
				        vertical : false,
				        circle : true,
				        prev : ".hz_cell" + (index + 1) + " .hz_extBox_l",
				        next : ".hz_cell" + (index + 1) + " .hz_extBox_r"
				    });
				});
			})
		}

		//免费适用
		shiyongFn();
		function shiyongFn(){
			var $a = $('.hz_shinei .hz_cell2_int_r');
			var $name = $a.eq(0);
			var $tel = $a.eq(1);
			var $province = $('#s_province');
			var $city = $('#s_city');
			var $county = $('#s_county');
			var $btn = $('.hz_shinei .hz_cell2_btn');
			var reTel1 = /^1[34578][0-9]{9}$/,
			reTel2 = /^(0\d{2,3})?-?[1-9]\d{6,7}(-\d{1,4})?$/;
			//城市联动
			new province_city_county_select().init();

			$btn.click(function (){
				var name = $name.val();
				var tel = $tel.val();
				var province = $province.val();
				var city = $city.val();
				var county = $county.val();
				if (!name) {
					alert('请填写您的称呼');
					return false
				};
				if (!reTel1.test(tel) && !reTel2.test(tel)) {
					alert('请填写您的联系方式');
					return false
				};
				if (!province || !city || !county) {
					alert('请填写城市信息')
					return false
				};

				 $.ajax({
					url: 	'http://supports.jiaju.com/index.php?app=api&ctrl=Mt&act=signup&did=2516758161',
					data: 	{
						city: 	province + city + county,
						name: 	name,
						mobile: tel
					},
					dataType: 	'jsonp',
					success: 	function (data){
						var code = data.code;
					    if (parseInt(code) == 1) {
					        alert("报名成功！");
					        $name.val('');
					        $tel.val('');
					    } else {
					        alert(data.msg);
					    }
					}
				})
			});
		}
	});
</script>
