    <script type="text/javascript">
        ;(function(){
            // 滚动图
            var byId = function(id){
                return document.getElementById(id);
            };
            var getScroll = function(prefix,width,time,autoPlay){
                var SP = new ScrollPic();
                SP.scrollContId   = prefix + 'Cont'; //内容容器ID
                SP.dotListId      = prefix + 'Dots';//点列表ID
                SP.dotClassName   = "";//点className
                SP.dotOnClassName = "selected";//当前点className
                SP.listType       = "";//列表类型(number:数字，其它为空)
                SP.listEvent      = "onmouseover"; //切换事件

                SP.frameWidth     = width||1000;//显示框宽度 **显示框宽度必须是翻页宽度的倍数
                SP.pageWidth      = width||1000; //翻页宽度
                SP.upright        = false; //垂直滚动

                SP.speed          = 10; //移动速度(单位毫秒，越小越快)
                SP.space          = 100; //每次移动像素(单位px，越大越快)
                SP.autoPlay       = autoPlay||false; //自动播放
                SP.autoPlayTime   = time||5; //自动播放间隔时间(秒)
                SP.number = 'number';
                var prev = byId(prefix + 'Prev');
                if(prev){
                    prev.onmousedown = function() {
                        SP.pre();
                        return false;
                    }
                }
                var next = byId(prefix + 'Next');
                if(next){
                    next.onmousedown = function() {
                        SP.next();
                        return false;
                    }
                }
                SP.onpagechange = function(){
                    // console.log(this.pageIndex);
                    // SP.$('pageIndex').innerHTML = this.pageIndex;
                };
                SP.initialize(); //初始化
                return SP;
            };
            var SP1 = getScroll('SI_Scroll_1_',1000,5,true);
            var SP2 = getScroll('SI_Scroll_2_',936,10);
            var SP3 = getScroll('SI_Scroll_3_',936,10);
            var SP4 = getScroll('SI_Scroll_4_',936,10);
            var SP5 = getScroll('SI_Scroll_5_',936,10);
            var SP6 = getScroll('SI_Scroll_6_',936,10);
            var SP7 = getScroll('SI_Scroll_7_',936,10);
            //点击查看详情
        var util = {
            byId: function(id) {
                return document.getElementById(id);
            },
            hasClass: function(ele, cls) {
                if (!ele) {
                    return false;
                }
                return ele.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)'));
            },
            addClass: function(ele, cls) {
                if (!this.hasClass(ele, cls)) {
                    ele.className = ele.className.replace(/(^\s*)|(\s*$)/g, '') + ' ' + cls;
                }
            },
            removeClass: function(ele, cls) {
                if (this.hasClass(ele, cls)) {
                    var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)');
                    ele.className = ele.className.replace(reg, ' ');
                }
            },
            toggleClass: function(ele, cls) {
                if (this.hasClass(ele, cls)) {
                    this.removeClass(ele, cls);
                } else {
                    this.addClass(ele, cls);
                }
            },
            addEvent: function(obj, type, fn) {
                if (obj.attachEvent) {
                    obj['e' + type + fn] = fn;
                    obj[type + fn] = function() {
                        obj['e' + type + fn](window.event);
                    }
                    obj.attachEvent('on' + type, obj[type + fn]);
                } else obj.addEventListener(type, fn, false);
            },
            fixEvent: function(e) {
                e = window.event || e;
                e.target = e.target || e.srcElement;
                return e;
            },
            winSize:function(target){
                var w, h;
                if (target) {
                    target = target.document;
                }
                else {
                    target = document;
                }

                if(target.compatMode === "CSS1Compat") {
                    w = target.documentElement[ "clientWidth" ];
                    h = target.documentElement[ "clientHeight" ];
                }else if (self.innerHeight) { // all except Explorer
                    if (target) {
                        target = target.self;
                    }
                    else {
                        target = self;
                    }
                    w = target.innerWidth;
                    h = target.innerHeight;

                }else if (target.documentElement && target.documentElement.clientHeight) { // Explorer 6 Strict Mode
                    w = target.documentElement.clientWidth;
                    h = target.documentElement.clientHeight;

                }else if (target.body) { // other Explorers
                    w = target.body.clientWidth;
                    h = target.body.clientHeight;
                }
                return {
                    width: w,
                    height: h
                };
            }
        };
        var wrap = util.byId('SI_Scroll_1_Cont');
        var body = document.getElementsByTagName('body')[0];
        var resize = function(){
            var size = util.winSize();
            if(size.width>1024){
                util.removeClass(body,'body-1024');
            }else{
                util.addClass(body,'body-1024');
            }
        };
        util.addEvent(wrap,'click',function(e){
            e = util.fixEvent(e);
            if(e.target.className=='trigger'){
                util.toggleClass(wrap,'scroll-cont-show-txt');
            }
        });
        resize();
        util.addEvent(window,'resize',function(e){
            resize();
        });
    })();
    </script>
