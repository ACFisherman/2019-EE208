<script type="text/javascript">
    /*2017.12.29 start*/
    //二期优化黄金ETF和白银ETF获取数据
    function getData() {
        var etfGold = [];
        var etfSilver = [];

        $.ajax({
            url: 'http://app.cnfol.com/dataapi/index.php/golddata/getETF?limitnum=20',
            cache: false,
            type: 'get',
            dataType: 'jsonp',
            async: false,
            jsonp: 'jsoncallback',
            success: function(data) {
                for (var i = 0; i < data.length; i++) {

                    if (data[i].variety == "g") {
                        etfGold.push(data[i]);
                        var html = "";
                        if (etfGold.length < 3) {
                            html = '<tr><td>' + data[i].date + '</td><td>' + data[i].inv + '</td><td>' + data[i].change + '</td></tr>';
                            $("#ETFg").append(html);
                        }
                    } else {
                        etfSilver.push(data[i]);
                        var html = "";
                        if (etfSilver.length < 3) {
                            html = '<tr><td>' + data[i].date + '</td><td>' + data[i].inv + '</td><td>' + data[i].change + '</td></tr>';
                            $("#ETFs").append(html);
                        }

                    }
                }

                var gold_data = [];
                var silver_data = [];
                var positions_g = [];
                var positions_s = [];

                var min_g = 100000;

                var min_s = 100000;
                for (var i = 0; i <5; i++) {
                    var time = moment(etfGold[i]['date']).unix() * 1000;
                    time = time + 24 * 3600 * 1000;
                    min_g = parseFloat(etfGold[i].inv) < min_g ? parseFloat(etfGold[i].inv) : min_g;
                    gold_data.push([time, parseFloat(etfGold[i]['inv'])]);
                    if (i % 5 == 0 || i == (etfGold.length - 1)) positions_g.push(time);

                }
                gold_data.sort();

                for (var i = 0; i <5; i++) {
                    var time = moment(etfSilver[i]['date']).unix() * 1000;
                    time = time + 24 * 3600 * 1000;
                    min_s = parseFloat(etfSilver[i].inv) < min_s ? parseFloat(etfSilver[i].inv) : min_s;
                    silver_data.push([time, parseFloat(etfSilver[i]['inv'])]);
                    if (i % 5 == 0 || i == (etfSilver.length - 1)) positions_s.push(time);

                }
                silver_data.sort();
                min_g = min_g - 10;
                min_s = min_s - 10;
                var columnoptions = {
                    chart: {
                        width: 278,
                        alignTicks: false
                    },
                    legend: {
                        enabled: false
                    },
                    credits: {
                        enabled: false
                    },
                    yAxis: {
                        labels: {
                            enabled: false
                        },
                        min: min_g,
                        title: {
                            text: ''
                        }
                    },
                    xAxis: [{
                        tickPositioner: function() {
                            return positions_g;
                        },

                        labels: {

                            formatter: function() {

                                var tt = Highcharts.dateFormat("%m-%d", this.value);
                                return tt;
                            }
                        }
                    }],
                    tooltip: {
                        formatter: function(e) {

                            var html = '<span>' + Highcharts.dateFormat("%Y年%m月%d日", this.x) + '</span><br/> ETF:' + this.y + '<br/>';

                            return html;
                        }
                    },

                    title: {
                        text: 'etf'
                    },

                    series: [{

                        name: 'ETF',
                        data: gold_data
                    }]
                };

                var colgoldopt = columnoptions;
                var colsilveropt = columnoptions;
                // create the chart
                $('#eftgold').highcharts(columnoptions);

                colgoldopt['series'] = [{

                    name: 'ETF',
                    data: gold_data
                }];
                $('#eftgold').highcharts(colgoldopt);
                colsilveropt['yAxis']['min'] = min_s - 10;
                colsilveropt['xAxis']['tickPositioner'] = function() {
                    return positions_s;
                };
                colsilveropt['series'] = [{

                    name: 'ETF',
                    data: silver_data
                }];
                $('#eftsilver').highcharts(colsilveropt);
            }
        });
    }
    getData();


    // 国际黄金白银(生成随机数)
    setInterval(marketcenter,parseInt((Math.random()*500+60000),10),'http://apphq.cnfol.com/worldGV.php');
    // 国际指数
    setInterval(marketcenter,parseInt((Math.random()*500+80000),10),'http://apphq.cnfol.com/worldIndex.php');

    // 黄金T+D
    setInterval(marketcenter,parseInt((Math.random()*500+80000),10),'http://apphq.cnfol.com/chinaGV.php');

    // 纸黄金
    setInterval(marketcenter,parseInt((Math.random()*500+80000),10),'http://apphq.cnfol.com/chinaPGV.php');

    // 行情中心各个各个数据实时更新
    function marketcenter(url){
        $.ajax({
            url:url,
            cache: false,
