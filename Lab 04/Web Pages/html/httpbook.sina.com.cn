	function diffTime(stime,etime=0){
		if (etime==0) {
			etime=Date.parse(new Date());
		}
		var day_e    = 24*3600*1000;
		var hour_e   = 3600*1000;
		var minute_e = 60*1000;
		var second_e = 1000;
		var usedTime = etime - stime;  //两个时间戳相差的毫秒数  
		var days=Math.floor(usedTime/(day_e));
		//计算出小时数  
		var leave1=usedTime%(day_e);    //计算天数后剩余的毫秒数  
		var hours=Math.floor(leave1/(hour_e));
		//计算相差分钟数  
		var leave2=leave1%(hour_e);        //计算小时数后剩余的毫秒数  
		var minutes=Math.floor(leave2/(minute_e));
		var leave3=leave2%minute_e;
		var seconds=Math.floor(leave3/(second_e));
		var months=Math.floor(days/30);
		var time='';
		if(months>=1){
			time=months + "个月前";
		}else if(days>=1){
			time=days + "天前";
		}else if(hours>=1){
			time=hours + "小时前";
		}else if(minutes>=1){
			time=minutes + "分钟前";
		}else{
			time=seconds + "秒前";
		}
		time+='更新';
		return time;
	}  
