            <script language="javascript">
			function showlineup(x){
				var iframeId=document.getElementById("iframeId1");
				 iframeId.src=encodeURI(x);
			}
			function makernd(year){
				var cur_league = 2019;
				var cur_rnd = 7;
				var o_select = document.getElementById("lineup_rnd");
				o_select.options.length = 0;
				var max_rnd = 38;
				if(cur_league == year){
					max_rnd = cur_rnd;
				}
				for(i=1;i<=max_rnd;i++){
					o_select.options[i] = new Option(i,"./position_iframe.php?lid=8&id=20&year="+year+"&f=1&rnd="+i); 
				}
			}
			
            </script>
