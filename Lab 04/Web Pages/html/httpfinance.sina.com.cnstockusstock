        <script>			
		seajs.use(["usStockList_custom","usStockList_upDown","$"],function(usStockList_custom,usStockList_upDown,$){
			
			//中概股及涨跌榜
			usStockList_custom({
				wrapId:'usStockList_zhongGaiGu',
				lists:STOCKLISTCFG.ZhongGaiGuList
			})
			
			var usStockList_zhongGaiGu_upDown = new usStockList_upDown({
				initParam:{
					page:1,
					num:20,
					concept:17,
					sort:"chg",
					asc:0
				},
				urlPrefix:"//money.finance.sina.com.cn/q/api/jsonp_v2.php/",
				urlSuffix:"/US_ChinaStockService.getData",
				wrapid:"usStockList_zhongGaiGu_upDown_ulWrap",
				btns:"#usStockList_zhongGaiGu_upDown .list02_head a",
				isArray:true
			})
			
			var $Tab = $(".blkstocks-zhongGaiGu .blkstocks-tab-item");
			var $Cont = $(".blkstocks-zhongGaiGu .blkstocks-cont-item");
			var timer = null;
            var upDownInit = false;

			$Tab.hover(function(){
                var _that = this;
                timer = setTimeout(function(){
                    var index = $Tab.index($(_that));
                    $Tab.removeClass("active");
                    $(_that).addClass("active");
                    $Cont.hide();
                    $Cont.eq(index).show();
                    if(index === 1 && !upDownInit) {
                        usStockList_zhongGaiGu_upDown.init();
                        upDownInit = true;
                    }
                },400)
			},function(){
                clearTimeout(timer);
            })
		})
        </script>
