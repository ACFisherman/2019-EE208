                    if(SUDA && SUDA.uaTrack){
                        SUDA.uaTrack("2014_usstock_index", "suggest_show");
                    }
                    showed = true;
                }
            }
		});
		
		window.submitCheckSuggest = function(__form) {
			var __stringValue = $("suggestInputSearch").value;
                            __stringValue = trim(__stringValue);
			if (__stringValue === "拼音/代码/名称" || __stringValue === "") {
				$("suggestInputSearch").value = "";
                $("suggestInputSearch").focus();
                alert("请输入拼音/代码/名称");
                return false;
			} else {
				return true;
			}
		}
                function trim(val){
                      return val.replace(/(^\s+)|(\s+$)/g,"");
                }		
		function $(__stringId) {
			return document.getElementById(__stringId);
		}
		
		$("usStockSlt").onchange = function () {
			if($("usStockSlt_hq").selected == true) {
				this.form.action = "//biz.finance.sina.com.cn/suggest/lookup_n.php";
				suggestServer.changeLink("//biz.finance.sina.com.cn/suggest/lookup_n.php?strict=1&country=@type@&q=@code@");
			}
			if($("usStockSlt_news").selected == true) {
				this.form.action = "//search.sina.com.cn/";
				suggestServer.changeLink("//search.sina.com.cn/?t=keyword&c=news&col=1_6|1_7&q=@code@");
			}
		}
	})()
	seajs.use("$",function($){
		$("#suggestInputSearch").focus(function(){
			$(this).css("color","black");
		});
		
		$("#suggestInputSearch").blur(function(){
			$(this).css("color","#999");
		});
	})
	</script>
