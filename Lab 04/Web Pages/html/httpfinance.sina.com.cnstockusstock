            <script>
            seajs.use(["sina.ScrollPic","$"],function(ScrollPic,$){
                var newsScrollPic = new ScrollPic();
                newsScrollPic.scrollContId   = "rightScrollWrap";        //内容容器ID
                newsScrollPic.frameWidth     = 270;                     //显示框宽度
                newsScrollPic.pageWidth      = 270;                     //翻页宽度
                newsScrollPic.upright        = false;                   //垂直滚动
                newsScrollPic.speed          = 30;                      //移动速度(单位毫秒，越小越快)
                newsScrollPic.space          = 20;                      //每次移动像素(单位px，越大越快)
                newsScrollPic.autoPlay       = true;                    //自动播放
                newsScrollPic.autoPlayTime   = 6;                       //自动播放间隔时间(秒)
                newsScrollPic.dotListId        = "rightDotContainer";   //点列表ID
                newsScrollPic.dotClassName     = "dotItem";             //点className
                newsScrollPic.dotOnClassName   = "dotItemOn";           //当前点className
                newsScrollPic.arrLeftId      = "scrollPic1-prev";       //左箭头ID
                newsScrollPic.arrRightId     = "scrollPic1-next";       //右箭头ID
                newsScrollPic.initialize();

                $(".right-scroll-box").hover(function(){
                    $("#scrollPic1Nav").show();
                },function(){
                    $("#scrollPic1Nav").hide();
                })
            })
            </script>
