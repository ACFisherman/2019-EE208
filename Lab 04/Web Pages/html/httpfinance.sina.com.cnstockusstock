                <script>
                seajs.use(["$","underscore"],function($,_){
					
					var templateStr = '<li><em><%= rank %></em><a href="<%= link %>"><%= title %></a></li>';
					var templateFunc = _.template(templateStr);
					
					$.ajax({
						url:"//top.news.sina.com.cn/ws/GetTopDataList.php?app_key=1991664137&top_channel=finance&top_type=day&top_cat=finance_news_0_suda&top_time=today&top_show_num=10&top_order=DESC&__app_key=1970133308&format=json",
						dataType:"jsonp",
						success:function(data){
							var dataArr = data.result.data;
							var curRank = 1;
							$.each(dataArr,function(index,item){
								var html = templateFunc({
									rank:curRank++,
									link:item.url,
									title:item.title
								})
								$(html).appendTo($("#newsRankWrap"));
							})
						},
						error:function(){
							//alert("请求失败");	
						}
					})						
				});
                </script>
            </div>
