        <script>
            seajs.use(["$"],function($){
                var existArr = [];
                var $expertPicLink = $("#js-expert-pic-link");
                var $expertList = $("#js-expert-list");
                var $expertListLink = $("#js-expert-list a");

                if($expertListLink.length >= 5) {
                    return;
                }

                var showNum = $expertListLink.length;
                existArr.push($expertPicLink.attr("href"));
                $expertListLink.each(function(){
                    existArr.push($(this).attr("href"));
                })

                $.ajax({
                    url:"http://api.roll.news.sina.com.cn/zt_list?channel=finance&cat_1=column&cat_2=mgzl&show_all=0&tag=1&format=json",
                    dataType:"jsonp",
                    data:{
                        show_num:5
                    },
                    success:function(data){
                        var dataArr = data.result.data;
                        $.each(dataArr,function(index,item){
                            if(check(item.url)){
                                return;                 //相当于continue
                            };
                            var html = "<li><a href=" + item.url + ">" + item.title + "</a></li>";
                            $expertList.append(html);
                            showNum++;
                            if(showNum === 5) {
                                return false;           //相当于break
                            }
                        })
                    }
                })

                function check(url){
                    var result = false;
                    $.each(existArr,function(index,item){
                        //console.log(url,item);
                        if(item.indexOf(url) !== -1){
                            result = true;
                            return false;
                        }
                    })
                    return result;
                }
            })
        </script>
