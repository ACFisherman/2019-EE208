            <script>
            seajs.use(["BIAO","$"],function(BIAO,$){
				
				BIAO.boot();
				
				var now = 0;
				var timer = null;
				var $curP = $("#timeBlk-show p");
				var $selectP = $("#timeBlk-wrap p");
                var show = false;
				
				$("#timeBlk-show").click(function(){
                    if(!show) {
                        $("#timeBlk-wrap").show();
                        show = true;
                    }
					else {
                        $("#timeBlk-wrap").hide();
                        show = false;
                    }
					return false;
				})
				
				$selectP.click(function(){
					var index = $selectP.index($(this));
					now = index;
					showCur(now);
                                        try{ SUDA.uaTrack("key=2014_usstock_index", "worldtime_show");}catch(e){}
				})
				
				$(document).click(function(){
					$("#timeBlk-wrap").hide(); show = false;
				})
				
				function showCur(now) {
					clearInterval(timer);
					function func(){
						$curP.html($selectP.eq(now).html());
					}
					timer = setInterval(func,1000);
					func();	
				}
				showCur(0);
			});
            </script>
