                                        group: 0,
                                        encoding: "utf-8",
                                        hotPageNum: 3,
                                        latestPageNum: 3,
                                        replyShowNum: 3,
                                        replyPageNum: 15,
                                        replyClickMoreTimes: 3,
                                        clickMoreTimes: 0,
