        thumbUrl: '',
        tj: 1,
		as: 1    },
    h5attr: {
        autoPlay: false,  //是否自动播放
        controls: true, //是否显示控制条
        loop: false,        
        poster: 'http://news.sina.com.cn/c/sz/2016-03-08/doc-ifxqafha0479291.shtml', //视频加载前欲加载的图片地址，即播放器一开始显示的截图
        preload: 'auto'
    }
}; 
function reSizePlayer(width, height) {
    if(typeof width != 'undefined' || typeof height != 'undefined') {
        var swf = document.getElementById('flashPlayer_conf_120').children[0];
        swf.width = width;
        swf.height = height;
        var ele =  document.getElementById('player_cont_conf_120')
        ele.style.width = "740px";
        return;
    }
    var swf, h ,w;
    if(typeof swf == 'undefined') {
        swf = document.getElementById('flashPlayer_conf_120').children[0];
        w = parseInt(swf.width);
        h = parseInt(swf.height);
    } else {
        throw new Error("playef not found error!"); 
    }
    var windowWidth = document.body.scrollWidth ;
    if(windowWidth >= 1360 && windowWidth < 1600) {
        swf.width = 920;
        swf.height = 520;
    } else if(windowWidth >= 1600) {
        swf.width = 1120;
        swf.height = 520;
    } else if(windowWidth < 1360) {
        swf.width = 720;    
        swf.height = 520;
    }
}
$widgetadd({
    id:'part_video_conf_120',
    loadTime: 'DOMContentLoaded',
    require: part_video_required_conf_120,
    isDepend: true,
    exports: function() {
        for(var i = 0; i < bShareEntry_conf_120.length; i++) {
            bShare.addEntry(bShareEntry_conf_120[i]);
        }
        window.videoList1 = new videoList('videoList_conf_120', playerConfig, videos);
        window.videoList1.init();
        if(window._SINANEWSDATA.subjectName && window._SINANEWSDATA.subjectName == 'shadow_simple_subject') { // 新版两栏式才这样 
            reSizePlayer();
            window.onresize = function(){
                reSizePlayer();
            }
        } else {
            reSizePlayer(740, 520);
        }
    }
});
