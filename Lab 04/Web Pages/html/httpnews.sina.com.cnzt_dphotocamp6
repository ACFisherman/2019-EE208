var playerConfig =      {
    url: "",  // flash播放器地址,
    container : "flashPlayer_conf_120",
    id: "myMovie",
    width: 720,
    height: 520,
    params : {
     allowNetworking : "all",
     allowScriptAccess : "always",
     wmode : "opaque",
     allowFullScreen : "true",
     quality : "high"
    },
