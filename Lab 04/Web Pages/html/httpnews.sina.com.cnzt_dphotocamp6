<script type="text/javascript">
	// 没有登录组件的话，页面一般不会定义$SCOPE，这里定义否则played.js会报错
	if(!$SCOPE) {
		var $SCOPE = {};	
	}
  $widgetadd({
     id: 'pl_slide_conf_94',
     loadTime: 'DOMContentLoaded',
     initialize: true,
     isDepend: true,
     // 依赖视频组件
     require: ['http://sjs0.sinajs.cn/video/sinaplayer/js/page/player.js','widget.focusSlide.index'],
     exports:function(node, widget){
       var Slide = $.widget.focusSlide.index;
       new Slide(node[0],{
        // 滚动速度，必须大于0，速度越小滚动越快，默认为6
        speed:6,
        // 是否循环滚动，默认为true
        isLoop:false,
        // 是否自动滚动，默认为true
        autoPlay:false,
        // 图片比例
        imgAspectRatio:"16/9",
        // 图片最大高度
        imgMaxHeight:750,
        // 视频关闭按钮css class
        videoCloseClz: 'icon_del',
        // 视频播放器窗口css class
        videoPlayerClz: 'slide-video-player'
       });
     }
   });
    </script>
