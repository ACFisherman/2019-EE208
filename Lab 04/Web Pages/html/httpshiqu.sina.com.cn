						<% mark = false;}});%>

						<a href="<%= o.url %>" target="_blank" class="words home_words"><%= o.title %></a>
					</div>
					<a href="<%= o.url %>" target="_blank" class="desc">
						<%= o.intro %>
					</a>
					<div class="handler">
						<a href="<%= commentUrlPrefix + '?channel=kj&newsid=' +o.commentid.split(':')[1] %>" target="_blank" class="comments"><i class="icon"></i><span class="num"></span></a>
						<span class="bdsharebuttonbox" data-tag="share_<%=  o.commentid %>">
							<a class="bds_more" data-cmd="more"></a>
						</span>
						<a target="_blank" title="点击收藏" class="collect" data-docid = "<%= o.docid %>"></a>
					</div>
				</div>
			</div>
			<% }); %>
		</div>
		<% }); %>
	</script>


