<script type="text/javascript">
 function DFfetchMMDD(str) {
      var typ = typeof str;
      if (typ == "string") {
          if (/[^0-9]/.test(str)) {
              return [str.slice(5, 7), str.slice(8, 10)];
          }
          str = Number(str);
          if (str < 10000000000) {
              str = str * 1000;
          }
          var n=str;
          str = new Date();
          str.setTime(n);
      } else if (typ == "number") {
          if (str <= 10000000000) {
              str = str * 1000;
          }
          var n=str;
          str = new Date();
          str.setTime(n);
      }
      var mm = str.getMonth() + 1;
      mm = mm < 10 ? "0" + mm : mm + "";
      var dd = str.getDate();
      dd = dd < 10 ? "0" + dd : dd + "";
      return [mm, dd];
  }

</script>

