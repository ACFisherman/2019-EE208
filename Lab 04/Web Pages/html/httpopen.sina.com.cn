.pictxt .pic span{display:block; text-align:center;}
.pictxt img{display:block;}
.pictxt .pic a img{text-decoration:none}
.pictxt .txt h5{ font-size:12px;}
.blk_wb{ padding-top:14px;}
.slide02{ height:260px; overflow:hidden;zoom:1; margin-top:10px; position:relative;}
.slide02 .wbCont{ height:130px;  width:297px;overflow:hidden;zoom:1;}
.slide02 .pictxt{ padding:5px 0; overflow:hidden;zoom:1;}
.slide02 .pic{ float:left; padding-right:0px; padding-top:5px;}
.slide02 .txt{ background-position:0 -280px; padding:10px 10px 10px 15px; width:222px; float:left; height:60px; position:relative;}
.slide02 .txt p{ right:5px; position:absolute; bottom:5px;}

/* 微博精选接口连接 */
.weibo_selected{float:left; width:300px;}
.pictxt .pic{ float:left; padding-right:8px;_padding-right:6px;}
.pictxt .pic span{display:block; text-align:center;}
.pictxt img{display:block;}
.pictxt .pic a img{text-decoration:none}
.pictxt .txt h5{ font-size:12px;}
.blk_wb{ padding-top:14px;}
.slide02{ height:180px; overflow:hidden;zoom:1; margin-top:10px; position:relative;}
.slide02 .wbCont{ height:90px;  width:297px;overflow:hidden;zoom:1;}
.slide02 .pictxt{ padding:5px 0; overflow:hidden;zoom:1;}
.slide02 .pic{ float:left; padding-right:0px; padding-top:5px;}
.slide02 .txt{ background-position:0 -280px; padding:10px 10px 10px 15px; width:222px; float:left; height:60px; position:relative;}
.slide02 .txt p{ right:5px; position:absolute; bottom:5px;}
.wbCont .txt{ background: url(http://i3.sinaimg.cn/edu/deco/2012/0508/gk_yan_main_.png) 0 -280px no-repeat;}

/* 公开课策划*/
 .blk02 {border-top:0;padding:25px 0;  }
.blk02 .zcbox2 {width:129px;margin-right:25px; }
.blk02 .zcwords {line-height:30px;height:30px;  }
/* 上图-下文 */
.zcbox2 {float:left;text-align:center; }
.zcbox2 .zcimg {display:block;margin:0 auto;}
.zcbox2 .zcwords {line-height:25px;height:25px;color:#646464; }
.zcbox2 a span{font-size:13px;display:block;padding-top:8px;color:#0673A5;text-align:left;}
.titImg18{margin-top:11px;width:200px;height:24px;background:url(http://i2.sinaimg.cn/edu/deco/2010/1108/images/cg_wzc20.png) no-repeat 0 -380px;}
.zctit4 h2 {float: left;padding-bottom: 0;padding-left: 33px;padding-right: 0;padding-top: 0;}
.zchide {display: none;}
.zctit4 .more {color: rgb(53, 53, 53);float: right;padding-right: 15px;text-align: right;}
.zctit4{height:43px;line-height:43px;background:url(http://i2.sinaimg.cn/edu/deco/2010/1108/images/cg_wzc20.png) no-repeat 0 -420px; overflow:hidden; }
.zctit4 h2 { float:left;padding:0 0 0 33px;}
.zctit4 .more {float:right;text-align:right;padding-right:15px;color:#353535;}

