    $(".tablist div").each(function(){
        $(this).on("click",function(){
            $(".tablist").find("div").removeClass("xuan");
            $(this).addClass("xuan");
            var num = $(this).index();
            var val = $(this)[0].innerText;

            
            if(num<7){
                $(".morebtn").removeClass("xuan2");
            }
            if(num>=7){
                $(".tablist").removeClass('tablisted');
                $(".morebtn").find("span").html(val);
            }
            // mySwiper.slideTo(num, 1000, false);
            console.log(val)
            var ispic = '';  
            if(val == '开眼'){
                ispic = "&ispic=1";
            } 
            var is_english = '';  
            if(val == '中/英文'){
            	val = '';
                is_english = "&is_english=1";
            }   
            $('.articlelist').find('ul').empty();
            $.ajax({
                type: "GET",
                url: "/news/listAjsx/?page=1"+ispic+"&newsColumn="+val+is_english,
                dataType : "json",
                beforeSend:function(e){
                    $('.loading').show();
                },
                success: function(res){ 
                    $('.loading').hide();  
                    var text = '';             
                    if(res.status == 1)
                    {   
                        if(val == '开眼'){
                                                                                }
                        $.each(res.data,function(key,value){
                        	var v_title = value.title;
                        	if(value.title_en){ v_title = value.title_en; }
                            text += "<li><div class='kacear'></div><div class='listcont'>";
                            if(value.pic){
                                text += "   <div class='leftimg'> <a href='https://www.xhaiwai.com/news/" + value.id + ".htm' target='_blank'><img src='" + value.pic + "' alt='" + v_title + "' class='leftimg'>";
                                if(value.is_english == 1){
	                                text += "<span class='english'>中/英文</span>";
	                            }
                                text += "</a></div>";
                            }                        
                            text += "    <div class='rightconts'>";
                            text += "        <div class='rightcont'>";
                            text += "           <h2>";
                            text += "               <a class='lei' target='_blank' href='/news-n" + flip_news_type[value.newcolumn] + "/'>" + value.newcolumn + "</a>";
                            text += "               <a href='https://www.xhaiwai.com/news/" + value.id + ".htm' target='_blank'>" + v_title + "</a>";
                            text += "            </h2>";
                            text += "            <a href='https://www.xhaiwai.com/news/" + value.id + ".htm' target='_blank'><p>" + value.abstract + "</p>";
                            text += "            </a>";
                            text += "            <div class='contdown'> ";
                            text += "				<div class='labe'>";
                            if(value.country_code){
		                        text += "               <a href='/tag-news-c" + value.country_code + "/' class='first' target='_blank'><span>" + value.country_name + "</span></a>";
		                    }
		                    if(value.keysList){
		                    	$.each(value.keysList,function(kk,kv){
									text += "			<a href='/tag-news-" + kv.k_id + "/' target='_blank'><span>" + kv.hotKey + "</span></a>";
								})
							}
							text += "				</div>";
                            text += "                <div class='time'>" + value.format_time + "</div>";
                            text += "            </div>";
                            text += "        </div>";
                            text += "    </div>";
                            text += "  </div>";
                            text += "</li>";
