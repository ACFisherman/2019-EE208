        <script type="text/javascript" src="/js/useract.js"></script>

        <script type="text/javascript">

            // 新增报名js
            // 手机号效果
            function _checkPhone(value) {
                $(".error").html("");

                var phone = $.trim(value);
                // console.log(phone)
                if (phone == "") {
                  $(".error").html("请输入您的手机号码");
                } else {
                  if (!checkPhone(phone)) {
                    $(".error").html("请输入正确的手机号码");
                  } else {
                    $.ajax({
                      type: "POST",
                      url: "/useract/havePhone/",
                      dataType: "json",
                      data: {"phone": $.trim(phone),},
                      success: function(data) {
                        if (data.status != "1") {
                          $(".error").html("手机号码已存在");
                        }
                      }
                    });
                  }
                }
              }
            $(".shuru .iphs").focus(function(){
                $(".shuru").addClass("shuru2"); 
            });
            $(".shuru .clear").on("click",function(){
                $(".iphs").val("");
                $(".iphs").focus(); 
                $(".clear").css("display","none"); 
            });
            $(".shuru .iphs").blur(function(){
                $(".shuru").removeClass("shuru2"); 
                _checkPhone($(this).val());
                console.log($(this).val());
            });

            $(".shuru .iphs").on("keydown",function(){
                $(".clear").css("display","block");
            });
           // 校验码
             function _checkWebVerifiCode(value) {
                $(".error").html("");
                console.log(value)
                var _data = {
                  "code": $.trim(value),
                }
                if (_data["code"] == "") {
                  $(".error").html("请输入图形验证码");

                } else {
                  $.ajax({
                    type: "POST",
                    url: "/news/verificode/",
                    dataType: "json",
                    data: _data,
                    success: function(data) {
                        console.log(data)
                      if (data.status != "1") {
                        $(".error").html("图形验证码不正确");
                        $("#yw0").trigger("click");
                      } else {
                        $(".error").html("");
                      }
                    }
                  });
                }
              }
            $(".jiaoyan .yanma").focus(function(){
                $(".jiaoyan").addClass("jiaoyan2"); 
            });
            $(".jiaoyan .clear2").on("click",function(){
                $(".yanma").val("");
                $(".yanma").focus(); 
                $(".clear2").css("display","none"); 
            });
            $(".jiaoyan .yanma").blur(function(){
                $(".jiaoyan").removeClass("jiaoyan2"); 
                _checkWebVerifiCode($(this).val());
                console.log($(this).val());
            });

            // $(".jiaoyan .yanma").on("keydown",function(){
            //     $(".clear2").css("display","block");
            // });
            // 手机验证码



             //发送手机验证码
              var countdown = 60; 
              var timeTask;
              function settime(timeLeft, sendCode) { 
                    if (countdown == 0) {
                      sendCode.show();
                      timeLeft.hide();
                      $(".yanzheng").removeClass("yanzhengs");

                    } else {
                      //sendCode.hide();
                      timeLeft.show();
                      timeLeft.html(countdown + "s");
                      countdown--; 
                    }
                    timeTask = setTimeout(function() {settime(timeLeft, sendCode)}, 1000);
                  }
                $(".send_out2").on("click",  function() {
                  // $("span[name=\"error_phone\"]").html("");
                 
                  // $("span[name=\"error_webverificode\"]").html("");
                  // $("span[name=\"error_webverificode\"]").hide();
                  // $("span[name=\"error_verificode\"]").html("");
                  // $("span[name=\"error_verificode\"]").hide();
                  // console.log( $(".iph").val());

                  var _data = {
                    "phone": $(".iphs").val(),
                    "code": $(".yanma").val(),
                  }

                  $.ajax({
                    type: "POST",
                    // url: "/useract/sendcode/",
                    url: "/news/verificode/",
                    dataType : "json",
                    data: _data,
                    success: function(data) {
                      if (data.status == "1") {
                        //$(".send_out").hide();
                        $(".send_out02").show();
                        $(".send_out2").hide();
                        $(".yanzheng").addClass("yanzhengs");
                        countdown = 60;
                        settime($(".send_out02"), $(".send_out2"));
                        // $("span[name=\"error_verificode\"]").html("<i class=\"icon icon01\"></i>发送验证码成功");
                        // $("span[name=\"error_verificode\"]").show();
                        // $(".error").html("发送验证码成功");

                        return;
                      } else {
                        if (data.status == "-1") {
                          // $("span[name=\"error_phone\"]").html("<i class=\"icon icon01\"></i>" + data.msg);
                          // $("span[name=\"error_phone\"]").show();
                            $(".error").html(data.msg);

                        }
                        if (data.status == "-2") {
                          // $("span[name=\"error_phone\"]").html("<i class=\"icon icon01\"></i>" + data.msg);
                          // $("span[name=\"error_phone\"]").show();
                            $(".error").html(data.msg);

                        }
                        if (data.status == "-3") {
                          // $("span[name=\"error_webverificode\"]").html("<i class=\"icon icon01\"></i>" + data.msg);
                          // $("span[name=\"error_webverificode\"]").show();
                            $(".error").html(data.msg);

                        }
                        if (data.status == "-4") {
                          // $("span[name=\"error_webverificode\"]").html("<i class=\"icon icon01\"></i>" + data.msg);
                          // $("span[name=\"error_webverificode\"]").show();
                            $(".error").html(data.msg);

                        }
                        if (data.status == "-5") {
                          // $("span[name=\"error_verificode\"]").html("<i class=\"icon icon01\"></i>" + data.msg);
                          // $("span[name=\"error_verificode\"]").show();
                            $(".error").html(data.msg);

                        }
                      }
                    }
                  });
                });


              function _checkVerifiCode(value) {
                $(".error").html("");
                var verificode = $.trim(value);
                if (verificode == "") {
                  $(".error").html("请输入手机验证码");
                } else {
                  if (!checkNum(verificode)) {
                    $(".error").html("请输入正确手机验证码");

                  }else{
                    $(".denglu").removeAttr("disabled");
                    $(".denglu").css("opacity",1);
                  }
                }
              }
            $(".yanzheng .yanzh").focus(function(){
                $(".yanzheng").addClass("yanzheng2"); 
            });
            $(".yanzheng .clear3").on("click",function(){
                $(".yanzh").val("");
                $(".yanzh").focus(); 
                $(".clear3").css("display","none"); 
            });
            $(".yanzheng .yanzh").blur(function(){
                $(".yanzheng").removeClass("yanzheng2"); 
                   _checkVerifiCode($(this).val());
            });

            $(".yanzheng .yanzh").on("keydown",function(){
                $(".clear3").css("display","block");
            });



            //注册开始
            $(".denglu").on("click", function() {
              var _data = {
                "phone": $(".iphs").val(),
                "checkcode": $(".yanzh").val(),
                // "pwd": $("[name=\"pwd\"]").val(),
                // "pwd_confirm": $("[name=\"pwd_confirm\"]").val(),
              }
              _checkPhone(_data["phone"]);
              _checkWebVerifiCode($(".yanma").val());
              _checkVerifiCode(_data["checkcode"]);
              // _checkPwd(_data["pwd"]);
              // _checkPwdConfirm(_data["pwd_confirm"]);
              // if ($("span[name=\"error_phone\"]").css("display") == "block" || $("span[name=\"error_webverificode\"]").css("display") == "block" || $("span[name=\"error_verificode\"]").css("display") == "block") {
              //   return;
              // }
              $.ajax({
                type: "POST",
                url: "/useract/reg/",
                dataType: "json",
                data: _data,
                success: function(data) {
                  if (data.status == "1") {
                    window.location.href = data.msg;
                  } else {
                    alert(data.msg);
                    // popup("alert", data.msg, function() {
                    //   // $("[name=\"pwd\"]").val("");
                    //   // $("[name=\"pwd_confirm\"]").val("");
                    // });  
                  }
                }
              });
            });

































            // 关闭
            $(".close").on("click",function(){
                $(".baoru").hide();
                $(".baowei").hide();  
            });


            // 二维码
            $(".iph").hover(function() {
                $(".erma").show();
            }, function() {
                $(".erma").hide();
            });

            //开眼
            $(".kaya").on("mouseover",function(){
                $(".kaiyanlist").show();
            // $(".kaya").on("click",function(){
            //     $(".kaiyanlist").show();
            });
            $(".kaya").on("mouseout",function(){
                $(".kaiyanlist").hide();
            });
            $(".kaiyanlist a").each(function(){
                $(this).on("click",function(){
                    $(".kaiyanlist a").removeClass("acrt");
                    $(this).addClass("acrt");
                    $(".kaiyanlist").slideUp();
                })
            })

            
            //搜索
            $("#searchBtn1").on("click",function(){
                var search = $(this).prev().val();
                if(search){
                    $.ajax({
                        type: "POST",
                        url: "/index/searchView/",
                        data:{ 'type':1,'key':search },
                        success: function(data) {
                            window.open("https://www.xhaiwai.com/search-news/"+search+"/");
                        }
                    });
                    // window.open("https://house.xhaiwai.com/building/kw-"+search+"/");
                    // window.location.href="https://house.xhaiwai.com/building/kw-"+search+"/";
                }                
            });
                
            // 侧边栏
            $("#kkff").on("mouseover",function(){
                $(this).addClass('lis');
                $(this).find(".tupi").addClass("tupis");
                $(".kefu2").show();
                $(".diy").hide();
            });
            $("#kkff").on("mouseout",function(){
                $(this).removeClass('lis');
                $(this).find(".tupi").removeClass("tupis");
                $(".kefu2").hide();
                $(".diy").show();
            });
            $("#ddhh").on("mouseover",function(){
                $(this).addClass('lis');
                $(this).find(".dian").addClass("dians");
                $(".dianhua").show();
                $(".diy").hide();
                $(".diys").hide();

            });
            $("#ddhh").on("mouseout",function(){
                $(this).removeClass('lis');
                $(this).find(".dian").removeClass("dians");
                $(".dianhua").hide();
                $(".diy").show();
                $(".diys").show();

            });
            $("#ssjj").on("mouseover",function(){
                $(this).addClass('lis');
                $(this).find(".shouji").addClass("shoujis");
                $(".shouji2").show();
                $(".diyt").hide();
                $(".diys").hide();

            });
            $("#ssjj").on("mouseout",function(){
                $(this).removeClass('lis');
                $(this).find(".shouji").removeClass("shoujis");
                $(".shouji2").hide();
                $(".diyt").show();
                $(".diys").show();

            });
            $("#bbcc").on("mouseover",function(){
                $(this).addClass('lis');
                $(this).find(".back").addClass("backs");
                $(".diyt").hide();

            });
            $("#bbcc").on("mouseout",function(){
                $(this).removeClass('lis');
                $(this).find(".back").removeClass("backs");
                $(".diyt").show();
            });

            // 退出登录
            $(".registrat").on("mouseover",function(){
                $(".xhw-userEixt").show();
            });
            $(".registrat").on("mouseout",function(){
                $(".xhw-userEixt").hide();
            });
            $("#quitBtn").on("click",function(){
                //退出动作
                $.ajax({
                    type: "POST",
                    url: "/login/exit",
                    success: function(data) {
                        var jsondata = $.parseJSON(data);
                        if (jsondata.status == 1) {
                            location.reload();
                        } else {
                            alert(jsondata.msg);
                            return false;
                        }
                    }
                });
            });

            //
            function backToTopFun() {
                var st = $(document).scrollTop();
                // var begin = 800;
                // (st > begin) ? $('.sidebar').show(): $('.sidebar').hide();
                var menu = "index";
                var begin = 88;
                // if(menu != 'houseDetail'){
                    (st > begin) ? $('.xhw_header').addClass("fixed"):$('.xhw_header').removeClass("fixed");
                // }
                
                //侧边栏的展示
                var sidebar = 750;
                (st>sidebar)?$('.sidebar').show():$('.sidebar').hide();
            };

            $(window).on("scroll", backToTopFun);

            backToTopFun();
 
            $("#bbcc").on("click",function(){
                 $('body,html').animate({scrollTop:0},900);
            })


        </script>
            <script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {

jQuery(document).on('click', '.imgCode, #yw0', function(){
	jQuery.ajax({
		url: "\/news\/captcha\/refresh\/1\/",
		dataType: 'json',
		cache: false,
		success: function(data) {
			jQuery('#yw0').attr('src', data['url']);
			jQuery('body').data('captcha.hash', [data['hash1'], data['hash2']]);
		}
	});
	return false;
});

});
/*]]>*/
</script>
