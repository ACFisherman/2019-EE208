						} else if (zdf < 0) {
							zxj = "<span class='fgreen'>"+zxj+"</span>";
							zdf = "<span class='fgreen'>"+zdf+"%"+"</span>";
						}
									else {
							zxj = "<span>"+zxj+"</span>";
							zdf = "<span>0.00%</span>";
						}
						if (hq_arr[6] * 1 == 0) {
							zxj = "--";
							zdf = "--";
						}
						document.getElementById(symbol_arr[i]+"_id1").innerHTML = zxj;
						document.getElementById(symbol_arr[i]+"_id2").innerHTML = zdf;
					}
				};
				setTimeout("l_updata()",5000);

			};
			function subFloat(val, len){
					if(len<0)return val;
					/* 先四舍五入,再处理 */
					var kkk10 = 1,temval;
					for(var kk=0;kk<len;kk++)kkk10 *= 10;
					temval = Math.round(val*kkk10);
					temval /= kkk10;

					var varstr = String(temval);
					var loc = varstr.indexOf(".");
					var kkk = "";

					if (loc >= 0){
						var thedif = (loc+len+1)-varstr.length;
						if (len == 0){
							varstr = varstr.substring(0, loc);
						}
						else if (len>0 && thedif<=0){
							varstr = varstr.substring(0, loc+len+1);
						}
						else if (len>0 && thedif>0){
							for(var kk=0;kk<thedif;kk++)kkk += "0";
							varstr = varstr+kkk;
						}
					}
					else {
						for(var kk=0;kk<len;kk++)kkk += "0";
						if (len>0) varstr += "."+kkk;
					}
					return varstr;
			};
			function l_updata(){
				document.getElementById("data_script").src = "http://hq.sinajs.cn/format=js&func=setData('stock_list');&list="+stock_list;
			}
			l_updata();
			//-->
		</script>
