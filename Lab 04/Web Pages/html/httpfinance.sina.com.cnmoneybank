<script type="text/javascript">
    var curBid = '';//当前银行id
	var curPid = '';//当前省份id
	var curCid = '';//当前城市id
var bankTel = '';
    jQuery(document).ready(function(){
		fetchData('bank');
		jQuery('#bankSubmit').click(function(){
			jQuery('#curBid').val(curBid);
			jQuery('#curPid').val(curPid);
			jQuery('#curCid').val(curCid);
		});
	});
	(function($){
		$.loadScript = function(url, opt, callback){
			var fnName = "SINAFINANCE" + (+new Date()) + parseInt( Math.random() * 100000 );
			var script = document.createElement("script");
			window[ fnName ] = function(){
				callback.apply( this, arguments );
				try {
					delete window[ fnName ];
					$(script).remove();
				} catch (e) {
				
				}
			};
			var param = [];                                                                                                         
			$.each(opt,function(i,v){
			   param.push(i + "=" + v);
			});
			param.push("callback=" + fnName);
			//获取数据;
			var url = url + param.join("&");		
			script.type = "text/javascript";
			script.src = url;
			script.charset = opt.charset || 'gb2312';
			document.getElementsByTagName("head")[0].appendChild(script);
		}
	})(jQuery);
	//ajax查询数据
	function fetchData(ftype)
	{
		if(ftype == 'bank') //银行列表
		{
			myData = 'phonetype=full';
			myUrl = 'http://stock.finance.sina.com.cn/licai/api/openapi.php/BasicService.getBankList';
		}
		else if(ftype == 'province')//省份列表
		{
			myData = 'bid=' + curBid;
			myUrl = 'http://stock.finance.sina.com.cn/licai/api/openapi.php/BasicService.getProvinceList';
		}
		else if(ftype == 'city')//城市列表
		{
			myData = 'bid=' + curBid + '&pid=' + curPid;
			myUrl = 'http://stock.finance.sina.com.cn/licai/api/openapi.php/BasicService.getCityList';
		}
		else
		{
			return;
		}

		jQuery.loadScript(myUrl+"?"+myData+"&",{charset:'gb2312'},function(rs){
			if(ftype == 'bank')
			{
				showBanks(rs.result.data);
			}
			else
			{
				showOptions(rs.result.data,ftype);
			}
		});
	}
	function showBanks(data)
	{
		var rsShow = '';
		var size = jQuery(data).size();
		if(size > 0)
		{
			for(var index in data)
			{
				tmp = data[index];
				rsShow += '<option value="' + tmp.id + '" phone="' + tmp.phone + '">' + trim(tmp.name) + '</option>';
			}
			jQuery('#bankList').html(rsShow);
			new DivSelect('bankList','div_bankList');
			
			if(size > 10)
			{
				jQuery('#div_bankList .ds_list').css('overflow-y','auto').height(200);
			}
			bankTel = jQuery('#bankList').find('option[selected]').attr('phone');
			bankTel = bankTel || '暂无';
			jQuery('#bankTel').text(bankTel);
			jQuery('#div_bankList .ds_title').css('overflow','hidden').css('padding-right','15px').css('height','20px');

			curBid = jQuery("#bankList").val();
			fetchData('province');//省份列表
		}
		
		
		jQuery("#div_bankList .ds_list").click(function(){
			curBid = jQuery("#div_bankList .dsl_cont p[class=selected]").attr('value');
			var bankTel = jQuery("#div_bankList .dsl_cont p[class=selected]").attr('phone');
			if(bankTel == 'null' || bankTel == null || trim(bankTel) == '')
			{
				bankTel = '暂无';
			}
			jQuery('#bankTel').text(bankTel);
			initOptions('city');
			fetchData('province');
		});
	}
	//省份和城市下拉框
	function showOptions(data,type)
	{	
		var rsShow = '<option value="">选择省</option>';
		
		if(type == 'city')
		{
			rsShow = '<option value="">选择市</option>';
		}

		var size = jQuery(data).size();
		if(size > 0)
		{
			for(var index in data)
			{
				tmp = data[index];
				//省列表和市列表
				rsShow += '<option value="' + tmp.id + '">' + trim(tmp.name) + '</option>';
			}
			jQuery('#' + type + 'List').html(rsShow);
			jQuery('#div_' + type + 'List').remove();
			new DivSelect(type + 'List' , 'div_' + type + 'List');
				
			myObj = jQuery('#div_' + type + 'List .ds_list');
			if(size > 10)
			{
				jQuery(myObj).css('overflow-y','auto').height(200);
			}
			
			jQuery(myObj).click(function(){
				myId = jQuery('#div_' + type + 'List .dsl_cont p[class=selected]').attr('value');
				if(type == 'province')
				{
					curPid = myId;
					curCid = '';
					fetchData('city');
				}
				else if(type == 'city')
				{
					curCid = myId;
				}
			});
			
			//初始状态从select下拉框取数据
			if(type == 'province')
			{
				curPid = jQuery('#provinceList').val();
				fetchData('city');
			}
		}
		else
		{
			if(type == 'province')
			{
				initOptions('city');
			}
			initOptions(type);
		}
	}
	//初始数据
	function initOptions(type)
	{
		if(type == 'province')
		{
			curPid = '';
			text = '选择省';
		}
		else if(type == 'city')
		{
			curCid = '';
			text = '选择市';
		}
		jQuery('#div_' + type + 'List').remove();
		jQuery('#' + type + 'List').html('<option value="">' + text + '</option>');
