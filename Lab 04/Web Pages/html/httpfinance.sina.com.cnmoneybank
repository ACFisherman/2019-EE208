			var stock_list = "sh600000,sh600015,sh600016,hk03328,sz000001,hk00939,sh600036,hk03988,sh601398,hk01398,hk03968,sh601166,sz002142,sh601009,sh601998,hk00998,sh601939,sh601988,sh601328,sh601169,sh601288,hk01288,sh601818,hk01988";

			function setData(stock_lists){
