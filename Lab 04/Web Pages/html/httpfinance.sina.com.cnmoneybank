					if(hq_str!=""){
						var hq_arr = hq_str.split(",");
						if (exchange == "sh" || exchange == "sz") {
							zxj = hq_arr[3];
							zdf = subFloat((hq_arr[3]*1000-hq_arr[2]*1000)/(hq_arr[2]*10),2);
						} else if (exchange == "hk") {
							zxj = subFloat(hq_arr[6],2);
							zdf = subFloat(hq_arr[8],2);
						}

						if (zdf > 0) {
							zxj = "<span class='fred'>"+zxj+"</span>";
