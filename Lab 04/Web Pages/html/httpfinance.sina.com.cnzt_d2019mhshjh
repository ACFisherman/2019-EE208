<script type="text/javascript">
    function fixPos(obj, maxObj, offset) {
        this.obj = this.$(obj);
        this.fixCss = obj + '_fixed';
        this.ie6 = 'undefined' == typeof(document.body.style.maxHeight);
        this.objPosition = this.getPosition(this.obj);
        this.maxObj = maxObj || false;
        this.offset = offset || 0
    }
    fixPos.prototype = {
        constructor: fixPos, $: function (ele) {
            return document.getElementById(ele)
        }, getPosition: function (el) {
            var x = 0, y = 0;
            while (el) {
                x += el.offsetLeft || 0;
                y += el.offsetTop || 0;
                el = el.offsetParent
            }
            return {x: x, y: y}
        }, getScrollTop: function () {
            var scrollPos;
            if (typeof window.pageYOffset != 'undefined') {
                scrollPos = window.pageYOffset
            } else if (typeof document.compatMode != 'undefined' && document.compatMode != 'BackCompat') {
                scrollPos = document.documentElement.scrollTop
            } else if (typeof document.body != 'undefined') {
                scrollPos = document.body.scrollTop
            }
            return scrollPos
        }, hasClass: function (ele, cls) {
            return ele.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)'))
        }, addClass: function (ele, cls) {
            if (!this.hasClass(ele, cls)) ele.className += " " + cls
        }, removeClass: function (ele, cls) {
            if (this.hasClass(ele, cls)) {
                var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)');
                ele.className = ele.className.replace(reg, ' ')
            }
        }, addEvent: function (o, type, fn) {
            o.attachEvent ? o.attachEvent('on' + type, fn) : o.addEventListener(type, fn, false)
        }, init: function () {
            if (typeof(window.ontouchstart) !== 'undefined') {
                this.obj.style.display = "none";
                return
            }
            var that = this;
            that.addEvent(window, 'scroll', function () {
                var st = that.getScrollTop();
                if (st > that.objPosition.y) {
                    that.addClass(that.obj, that.fixCss);
                    if (that.maxObj) {
                        if (st + that.obj.offsetHeight > that.maxObj.offsetHeight + that.getPosition(that.maxObj).y - that.offset) {
                            that.obj.style.top = that.maxObj.offsetHeight + that.getPosition(that.maxObj).y - st - that.obj.offsetHeight - that.offset + 'px';
                            if (that.ie6) that.obj.style.top = that.maxObj.offsetHeight + that.getPosition(that.maxObj).y - that.obj.offsetHeight - that.offset + 'px'
                        } else {
                            that.obj.style.top = 0;
                            if (that.ie6) that.obj.style.top = st + 'px'
                        }
                    }
                } else {
                    that.removeClass(that.obj, that.fixCss)
                }
            })
        }
    };
	
	 if(document.getElementById("sidebar")){
    var doc = document, toTop = function () {
            doc.documentElement.scrollTop = 0;
            doc.body.scrollTop = 0;
        },
        addEvent = function (o, s, fn) {
            if (o.attachEvent) {
                o.attachEvent('on' + s, fn);
            } else {
                o.addEventListener(s, fn, false);
            }
            return o;
        }, btn = document.getElementById('toTop');
    addEvent(btn, 'click', toTop);
		 
    var p1 = new fixPos('sidebar');
    p1.init();
	}
	
	if(document.getElementById("navTop")){
    var p2 = new fixPos('navTop');
    p2.init();
	}
</script>

<script>
    $(function () {
        $('.spsmall .box').each(function (index, element) {
            var img = jQuery(this).find('img:first'), info = jQuery(this).find('.info');
            info.height(img.height());
            $(this).hover(function () {
                info.stop().animate({top: 0}, 200);
            }, function () {
                info.stop().animate({top: 310}, 200);
            });
        });
        $('.itemscroll').delegate('.item', 'mouseenter', function () {
            var info = jQuery(this).find('.info:first');
            info.stop().animate({top: 0}, 200);
        }).delegate('.item', 'mouseleave', function () {
            var info = jQuery(this).find('.info:first');
            info.stop().animate({top: 310}, 200);
        });
    })
</script>
