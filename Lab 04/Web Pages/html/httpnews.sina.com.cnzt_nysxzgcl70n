<link rel="stylesheet" type="text/css" href="http://n0.sinaimg.cn/finance/stpv2/swiper.min.css">
<script type="text/javascript" src="http://n.sinaimg.cn/finance/stpv2/swiper.min.js"></script> <!-- v2通用js -->
<script>
function initSwiper(className, obj) {
    var swiper = new Swiper(className + ' .swiper-container', {
        slidesPerView: obj.slidesViewNum || 4,
        autoplay: obj.autoPlay ? obj.autoPlay : 0,
        autoplayDisableOnInteraction: false,
        initialSlide: (obj.activeIndex - 1) || 0,
        loop: obj.loop ? ($(className + ' .swiper-container .swiper-slide').length > obj.slidesViewNum ? true : false) : false,
        pagination: className + ' .pagination',
        paginationClickable: true,
        prevButton: className + ' .arrow-left',
        nextButton: className + ' .arrow-right',
        // onSlideClick: function(swiper){
        //     alert('事件触发了;');
        // }
    });
    $(className + ' .swiper-slide').mouseenter(function() {
        swiper.stopAutoplay();
        if (className == '.guardian') {
            if (obj.loop) {
                var index = $(className + ' .swiper-slide').index($(this));
                // console.log(index, swiper.activeIndex, swiper.activeLoopIndex)
                if (index == swiper.activeIndex) {
                    $(this).addClass('active-first');
                    $('.lef-column').hide();
                } else if (index == swiper.activeIndex + obj.slidesViewNum - 1) {
                    $(this).addClass('active-last');
                    $('.right-column').hide();
                }
            }
        }
    })
    $(className + ' .swiper-slide').mouseleave(function() {
        swiper.startAutoplay();
        if (className == '.guardian') {
            if (obj.loop) {
                var index = $(className + ' .swiper-slide').index($(this));
                if (index == swiper.activeIndex) {
                    $(this).removeClass('active-first');
                    $('.lef-column').show();
                } else if (index == swiper.activeIndex + obj.slidesViewNum - 1) {
                    $(this).removeClass('active-last');
                    $('.right-column').show();
                }
            }
        }
    })
    $(className + ' .arrow-left').on('click', function(e) {
        e.preventDefault();
        swiper.swipePrev();
    })
    $(className + ' .arrow-right').on('click', function(e) {
        e.preventDefault();
        swiper.swipeNext();
    })
    return swiper;
}
function controlDisplay(className, bool) {
    if (!!bool) {
        $(className).show();
        return true;
    } else {
        $(className).remove();
        return false;
    }
}
function Initguardian() {
    if (controlDisplay('.guardian', window.guardian.isShow)) {
        var slideTpl = '<div class="swiper-slide">\
        <a href="@href@" target="_blank">\
        <div class="pic"><img src="@coverImage@"></div>\
        <div class="intro">@title@</div>\
        </a>\
        </div>';
        var slideArr = $.map(window.guardian.context, function(val, i) {
            return slideTpl.replace('@href@', val.href).replace('@coverImage@', val.coverImage).replace('@title@', val.title);
        })
        $('.guardian .swiper-wrapper').html(slideArr.join(''));
        var swiper = initSwiper('.guardian', window.guardian);
    }
}
Initguardian();
</script>
