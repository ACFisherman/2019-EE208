				itemClass: 'list_cont',
				autoPlay: false,
				dotId: 'fin_scroll_dot__conf_65',
				leftId: 'fin_scroll_arrleft__conf_65',
				rightId: 'fin_scroll_arrright__conf_65'
			})
		}
		if (window.video_play) {
			var $slide_list = $('#fin_scroll_cont__conf_65 .slide_list');
			$slide_list.on('click', function() {
				var _vid = $(this).attr('data-videoid');
				var _pic = $(this).attr('data-pic') || '';
				var _title = $(this).attr('data-title') || '';
				var _width = 1000;
				var _height = 565;
				var _id = 'ybsk_video_box_conf_65'; //唯一id
				if (_vid) {
					window.video_play(_vid, _id, _title, _width, _height, _pic);
					$slide_list.removeClass('active');
					$(this).addClass('active');
				}
			})
		}
		$('#ybsk_video_box_conf_65').on('mouseover', function() {
			var idname = $(this).attr('id');
			if (window.videoAllArr) {
				$.each(window.videoAllArr, function(i, item) {
					if (item.playerid.indexOf(idname) == -1 && item.playerid.indexOf('ybsk_first_video') == -1) {
						item.pause();
						// console.log(item)
					}
				})
			}
		});
	})();
</script>
