    (function(){
        var jsonp = function(url, cb, fnName,dpc) {
            var remove = function(id){
                var dom = document.getElementById(url);
                dom && head.removeChild(dom);
            };
            var head = document.getElementsByTagName('head')[0];
            var js;
            url += (/\?/.test(url) ? "&" : "?");
            //添加回调
            fnName = fnName||'jsonp_' + (new Date()).getTime();
            window.fnName = fnName;
            if (typeof cb == 'function') {
                eval(fnName + '=function(m){cb(m)}');
            }
            url = url + 'callback=' + fnName;
            if(dpc){
                url +='&dpc=1';
            }
            remove(url);
            js = document.createElement('script');
            js.src = url;
            js.id = url;
            js.type = 'text/javascript';
            js.language = 'javascript';
            head.appendChild(js);
        };
        jsonp('//comment5.news.sina.com.cn/count/info?key=2019xzg70-1&pid=0',function(msg){
