	(function() {
		if ($('#zx_mhsh_conf_14').length > 0) {
			var videoArr = [];
			$('#zx_mhsh_conf_14 [data-vids]').each(function() {
				var _this = $(this);
				var _vid = _this.attr('data-vids');
				var _id = 'video_' + (_this.attr('id') || new Date());
				_this.attr('id', _id);
				var _type = _this.attr('data-type');
				var _width = _this.attr('data-width');
				var _height = _this.attr('data-height');
				var _title = _this.attr('data-title') || '';
				var _pic = _this.attr('data-pic') || '';
				if (_vid && _id) {
					if (_type == 'live') {
						window.$LivePlayer.playerInit({
							liveid: _vid, //视频id
							container: _id, //播放器容器id
							srckey: '0' //引用来源key值，默认是0
						});
					} else {
						var videoObj = video_play(_vid, _id, _title, _width, _height, _pic);
						videoArr.push(videoObj);
					}
				}
			})
			$('#zx_mhsh_conf_14 .mhsh_box').on('mouseover', function() {
				$('#zx_mhsh_conf_14 .mhsh_box').removeClass('active');
				$(this).addClass('active');
				var index = $('#zx_mhsh_conf_14 .mhsh_box').index($(this));
				var idname = $(this).find('.video_container').attr('id');
				if (window.videoAllArr) {
					$.each(window.videoAllArr, function(i, item) {
						if (item.playerid.indexOf(idname) == -1) {
							item.pause();
						}
					})
				}
