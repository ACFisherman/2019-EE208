<script type="text/javascript">
	_base_url = "http://stock2.finance.sina.com.cn/xintuo/api/jsonp.php/xintuo=/XinTuoService.getXinTuoData?";
	_ttype = ['', '1', '2', '3'];	
	_type = ['' ,'001004', '001003', '001002', '001001', ''];
	_type_p = ['' ,'权益类', '股权类', '货款类', '证券类', '另类投资类'];
	_max = 10;
	_pnum = 8;

	active(_base_url, {'ttype':_ttype[1], 'type':_type[1], 'page':'1', 'num':_pnum}, 'cont01_1');
	a_active(1, 1, 'cont01_1');
	next_pre_page('1', '1');
	
	$(function(){
		for(var j=1;j<=3;j++){
			for(var k=1;k<=5;k++){
				(function(index1, index2){
					$("#btn0" + index1 + "_" + index2).mouseover(function(){
						var cid = "cont0" + index1 + "_" + index2;						
						a_active(index1, index2, cid);
						active(_base_url, {'ttype':_ttype[index1], 'type':_type[index2], 'page':'1', 'num':_pnum}, cid);
						next_pre_page(index1, index2);
					});
					$("#btn0"+index1).mouseover(function(){
						var cid = "cont0" + index1 + "_1";
						a_active(index1, 1, cid);
						active(_base_url, {'ttype':_ttype[index1], 'type':_type[1], 'page':'1', 'num':_pnum}, cid);
						next_pre_page(index1, '1');
					});
					
				})(j, k);
			}
		}
	});
	
	function next_pre_page(index1, index2){
		$("#pre_page_0" + index1 + '_' + index2).click(function(){
			var pre_page = 0;
			
			if ($("#pre_page_0" + index1 + '_' + index2).hasClass("grap")) return void(0);
			for(var n=1;n<=_max;n++){
				(function(index){								
					if ($("#link_cont0"+index1+"_"+index2+"_"+index).hasClass("cur")){
						pre_page = index - 1;
					}								
				})(n);
			}
			if (pre_page != 0){
				page(index1, index2, "cont0" + index1 + "_" + index2, pre_page);
			}else{
				return void(0);
			}
			change_cla("#link_cont0"+index1+"_"+index2+"_", pre_page);
			check("#link_cont0"+index1+"_"+index2+"_");
		});
		$("#next_page_0" + index1 + '_' + index2).click(function(){
			if ($("#next_page_0" + index1 + '_' + index2).hasClass("gray")) return void(0);
			var pre_page = 0;
			for(var n=1;n<=_max;n++){
				(function(index){
					
					if ($("#link_cont0"+index1+"_"+index2+"_"+index).hasClass("cur")){
						pre_page = index + 1;
					}								
				})(n);
			}
			if (pre_page != 0){
				page(index1, index2, "cont0" + index1 + "_" + index2, pre_page);
			}else{
				pre_page = 2;
				page(index1, index2, "cont0" + index1 + "_" + index2, pre_page);
			}
			change_cla("#link_cont0"+index1+"_"+index2+"_", pre_page);
			check("#link_cont0"+index1+"_"+index2+"_");
		});
	}
	
	function a_active(i1, i2, cid){
		var pid = "page" + i1;
		var page_cont = '<a class="gray" id="pre_page_0'+i1+'_'+i2+'" href="javascript:void(0);">上一页</a>';
		for(var i=1;i<=_max;i++){
			(function(index){
				var aid = 'link_cont0'+i1+'_'+i2+'_'+index;
				page_cont += '<a href="javascript:void(0);" id="'+aid+'">'+index+'</a>';
				
			})(i);
		}
		page_cont += '<a href="javascript:void(0);" class="next_page" id="next_page_0'+i1+'_'+i2+'">下一页</a>';
		$("#" + pid).html(page_cont);
		for(var i=1;i<=_max;i++){
			(function(index){
				$("#link_cont0"+i1+"_"+i2+"_"+index).click(function(){
					change_cla("#link_cont0"+i1+"_"+i2+"_", index, pid);
					active(_base_url, {'ttype':_ttype[i1], 'type':_type[i2], 'page':index, 'num':_pnum}, cid);
				});
			})(i);
		}
	}
	
	function change_cla(aid, obj){
		for(var i=1;i<=_max;i++){
			(function(index){
				if(index != obj){
					$(aid + index).removeClass("cur");
				}else{
					$(aid + index).addClass("cur");
				}
			})(i);
		}
		check(aid);
	}
	
	function check(aid){
		if ($(aid + "1").hasClass("cur")) {
			$("#pre_page_0" + aid.substring(11, 14)).removeClass().addClass("gray");
		}else{
			$("#pre_page_0" + aid.substring(11, 14)).removeClass();
		}
		if ($(aid + _max).hasClass("cur")) {
			$("#next_page_0" + aid.substring(11, 14)).removeClass().addClass("gray");
		}else{
			$("#next_page_0" + aid.substring(11, 14)).removeClass();
		}
	}
	
	function page(i1, i2, cid, p){
		active(_base_url, {'ttype':_ttype[i1], 'type':_type[i2], 'page':p, 'num':_pnum}, cid);
	}
	
	function active(url, opt, cid){
		var param = [];
		$.each(opt,function(i,v){
           param.push(i + "=" + v);
        });
		url += param.join("&");
		loadData(url, cid);
	}
	
	function loadData(url_xt, cid){
		cont = $("#" + cid);
		getScript(url_xt, view, "gbk");
	}
	
		function view(){
		data = xintuo.data;
		content = '<table cellspacing="0" cellpadding="0"><tr><th style="width:285px;">产品名称</th><th class="blank"></th><th style="width:70px;">发行机构</th><th class="blank"></th><th style="width:72px;">信托类型</th><th class="blank"></th><th style="width:60px;">信托期限</th><th class="blank"></th><th style="width:98px;">资金门槛（万元）</th><th class="blank"></th><th style="width:72px;">发售时间</th><th class="blank"></th><th style="width:72px;">截止日期</th><th class="blank"></th><th style="width:64px;">预计收益率</th></tr>';
		for(var i = 0;i < data.length;i++ ){
			name = data[i]["name"] == null ? '--' : data[i]["name"].substring(0,20);
			saleBeginTime = data[i]["saleBeginTime"]==null?'--':data[i]["saleBeginTime"].substring(0,10);
			yearRate = data[i]["yearRate"]==null?'--':data[i]["yearRate"]+"%";
			leastMoney = data[i]["leastMoney"]==null?'--':data[i]["leastMoney"];
			company = data[i]["company"]==null?'--':data[i]["company"].substring(0,4);
			if (data[i]["runEndTime"] != null){
				var tmp1 = data[i]["runEndTime"].substring(0,10).replace(/-/g,"/");
				var dateStr1 = tmp1.substring(5,10) + "/" +tmp1.substring(0,4);
				var tmp2 = data[i]["runBeginTime"].substring(0,10).replace(/-/g,"/");
				var dateStr2 = tmp2.substring(5,10) + "/" +tmp2.substring(0,4);
				var runEndtime = new Date(dateStr1);
				var runBeginTime = new Date(dateStr2);
				var mnum = (runEndtime-runBeginTime)/(1000*3600*24);
				mnum = parseInt(mnum/30);
			}else{
				mnum = "--";
			}
			var id = $(".selected").attr("id");
			var id_num = id.substring(4,5);
			var type_seleted = $("#cont0" + id_num + " .selected").html();
			saleEndTime = data[i]["saleEndTime"]==null?'--':data[i]["saleEndTime"].substring(0,10);
			content += '<tr><td><a target="_blank" class="linkgrey01" href="http://stock.finance.sina.com.cn/xintuo/view/vProdInfo.php?prodid='+data[i]["code"]+'">'+name+'</a></td><td class="blank"></td><td><a target="_blank" class="linkgrey01" href="http://stock.finance.sina.com.cn/xintuo/view/vTrustInfo.php?comid='+data[i]["companycode"]+'">'+company+'</td><td class="blank"></td><td>'+type_seleted+'</td><td class="blank"></td><td>至'+mnum+'月</td><td class="blank"></td><td>'+leastMoney+'</td><td class="blank"></td><td>'+saleBeginTime+'</td><td class="blank"></td><td>'+saleEndTime+'</td><td class="blank"></td><td>'+yearRate+'</td></tr>';
		}
		cont.html(content + '</table>');
	}
</script>
