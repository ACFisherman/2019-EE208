        function getURL0(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = decodeURIComponent(window.location.search).substr(1).match(reg);
            if (r != null) return unescape(r[2]);
            return null;
        }
        // 判断是dp环境
        if(global_host.host != ''&&global_host.path != ''&&getURL0('dp_debug') == 1){
            window.host = 'https://' + global_host.host;
            window.host1 = '//' + global_host.path;
        }else{
            window.host = '//' + window.location.host;
            window.host1 = '//' + hosts[window.location.host] + window.location.pathname;
