<script type="text/javascript">
	//用户登录
	//截取汉字
	String.prototype.substr2=function(a,b){
		var s = this.replace(/([^\x00-\xff])/g,"\x00$1");return(s.length<b)?this:s.substring(a,b).replace(/\x00/g,'');};

	var lHtml, user, un;
	function uListener(){ //监听登录改变
		if(typeof sinaSSOManager === 'object'){
			var u = sinaSSOManager.getSinaCookie();
			if(!user && u){
				user = u;
				un = u.nick.substr2(0, 12);
				if(u.nick != un){un += '..'}
				if(!lHtml){lHtml = document.getElementById('cheadUserInfo').innerHTML}
				document.getElementById('cheadUserInfo').innerHTML = '欢迎你，<a href="https://login.sina.com.cn/" target="_blank" class="nick">' + un + '</a> <a href="https://login.sina.com.cn/sso/logout.php" id="user_quit_btn" class="logout">退出</a>';
			}else if(user && !u){
				document.getElementById('cheadUserInfo').innerHTML = lHtml;
				user = u;
			}
		}
	}
	uListener();
	setInterval(uListener,1000);
	//导航吸顶
	window.onscroll=function  () {
	    
	    var loaded = true;
	    
	    var start_top = $("#menu").offset().top;
	    var stop_top = $("#scrollarea").offset().top ;

	    var scrolltop = $(window).scrollTop();
	    var start_cha = start_top - scrolltop;
	    var stop_cha = stop_top - scrolltop;
	    if(loaded && start_cha <= 0 ){

	    	$("#menu").addClass('tabfixed');
	    	loaded=false;
	    }
	    if (!loaded && stop_cha >= 0) {
	          $("#menu").removeClass("tabfixed");
	               
	          loaded = true;
	    }
	    if (!loaded && start_cha > 0) {
	        $("#menu").removeClass('tabfixed');
	        loaded = true;
	    }
	}
	
</script>
