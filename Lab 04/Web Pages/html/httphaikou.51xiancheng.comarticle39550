	<script type="text/javascript">
	var p = 1;
	var pcount = 10;
	var cate = '4';
	var cur_art_id = '39550';
	function addEventSimple(obj,evt,fn) {
	if (obj.addEventListener)
		obj.addEventListener(evt,fn,false);
	else if (obj.attachEvent)
		obj.attachEvent('on'+evt,fn);
	}
	addEventSimple(window, 'load', function(){
		//加载猜你喜欢
		$("#list-more-btn").click(function(event) {
			$.get('/interface/article/ajaxList', {'page':p, 'pcount':pcount,'cate':cate}, function(json) {
				if(json.error_code!=0){
					//加载失败
				}else{
					var html = '';

					$.each(json.data, function(k, v) {
						if(v.id == cur_art_id){
							return true;
						}
						html += '<li>\
							<h2><a href="/article/'+v.id+'" target="_blank">'+v.title+'</a></h2>\
							<div class="clear">\
								<div class="news-img"><a href="/article/'+v.id+'" target="_blank"><img width="150" height="120" src="'+v.tiny_image+'" alt="" /></a></div>\
								<div class="news-text">\
									<div class="tag-box">\
										<span class="fr">作者：<a href="http://www.51xiancheng.com/pgc/user/'+v.author_uid+'" class="grey" target="_blank">'+v.screen_name+'</a></span>\
									</div>\
									<p class="describe"><a href="/article/'+v.id+'" class="grey" target="_blank">'+v.intro+'</a></p>\
									<p><span class="fl">'+v.publish_time+'</span><span class="fr"><em class="icon-collect">'+v.view_count+'</em></span></p>\
								</div>\
							</div>\
						</li>';
					});
					$("#list-container").append(html);

					//如果结果为空或长度太短，则隐藏加载按钮
					if(json.data.length==0 || json.data.length<pcount){
						$('#list-more-btn').hide();
						return;
					}
					p++;
				}
			});
		}).trigger('click');

		//分享到微博
		$("#share-btn").click(function(event) {
			var content = '很喜欢这篇精选文章《享受美妆乐趣不花冤枉钱》http://haikou.51xiancheng.com/article/39550?f=share（来自城市生活精选指南@鲜城 ）';
			window.open("/index/mblog/index?f=article&art_id=39550&content="+encodeURIComponent(content),'_blank','scrollbars=no,width=600,height=500,left=75,top=20,status=no,resizable=yes');
		});


		$(".pay-btn-1").mouseover(function(e){
			$(".qrcode-1").removeClass("undis");
		});
		$(".pay-btn-2").mouseover(function(e){
			$(".qrcode-2").removeClass("undis");
		});

		$(".payment-pos").mouseout(function(){
			$(this).addClass("undis");
		});

		});
	</script>
