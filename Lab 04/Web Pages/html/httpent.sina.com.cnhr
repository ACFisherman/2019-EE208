							if (data.code && data.code == 1) {
								if (data.data.following == true) {
									$this.replaceWith($('<span class="focused"><i class="icon">√</i>已关注</span>'));
								} else {
									//$this.replaceWith($('<a href="javascript:;" class="focus" data-type="focusBtn"><i>+</i>关注</a>'));
								}
							}
                        },
                        error: function () {
                        }
                    });
                })
            }).register('logout_success', function() {
			  $(".btn-wrap .focused", self.container).html('<a href="javascript:void(0);" class="btn" data-type="focusBtn"><i class="icon">+</i>加关注</a>');
            }).init();
        });
        }
        
        var focusBtn = function(){
            $("[data-type='focusBtn']").on('click',function(){
				console.log($("[data-type='focusBtn']").length);
                var $this = $(this);
                var uid = $(this).parent().prev('[data-type="focus"]').data("uid");
                var loginLayer = window.SINA_OUTLOGIN_LAYER;
                if (!loginLayer.isLogin()) {
                    loginLayer.show();
                    var scrollHD = setInterval(function () {
                        var scrollTop = $(window).scrollTop();
                        if (scrollTop > 0) {
                            scrollTop -= 50;
                            if (scrollTop <= 0) {
                                scrollTop = 0;
                            }
                            $(window).scrollTop(0);
                        } else {
                            clearInterval(scrollHD);
                        }
                    }, 50);
                    return;
                }
                new weiboPost({
                    url:'//api.sina.com.cn/weibo/2/friendships/create.json',
                    uid: uid,
                    source: '2835469272',
                    success: function (data) {
                        $this.replaceWith($('<span class="focused"><i class="icon">√</i>已关注</span>'));
                    },
                    error: function () {
                    }
                });
            })
        };
        
        
        //msgEvent();
        focusBtn();
    });
</script>


