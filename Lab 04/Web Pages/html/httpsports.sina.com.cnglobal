        requirejs(['step!weibo-all','$'],function(){
            var _apis = Weibo.apis, Login = Weibo.Login, Widgets = Weibo.Widgets;
            function weiboFollow(uidArr) {
                var params = {
                    data: {uids: uidArr.join(',')},
