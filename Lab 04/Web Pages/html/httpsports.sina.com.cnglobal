                // 判断登录
                if (!Login.check()) {
                    var loginDlg = Widgets.getLoginDialog();
                    loginDlg.loginCallbackOnce = function() {
                        _apis.batchFollow(params);
                    };
                    loginDlg.show();
                    return;
                }
                // 关注
                _apis.batchFollow(params);
            }

            //一键关注
            $("body").delegate('[action="focusAll"]','click',function(e){
                var target=$(e.currentTarget);
                var container=target.parent().prev();
                var checkedList=container.find(':checked'),
                        ids=[],
                        listItem;
                for(var i= 0,len=checkedList.length;i<len;i++){
                    listItem=checkedList[i];
                    ids.push($(listItem).attr('data-uid'));
                }
                weiboFollow(ids);
            });
        });
