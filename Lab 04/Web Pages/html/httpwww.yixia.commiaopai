    //图片验证，手机号验证，语音发送
    function infoRegAjax(phone,cap){
        var data={
            "phone":phone,
            "reqid":reqid,
            "cap":cap,
            "version":"6.6.51",
            "verifytype":1
        };
        $.ajax({
            url:"/m/sms_captcha.json",
            data:data,
            type:"POST"
        }).success(function(obj){
            if(obj.status!=200){
                alert(obj.msg);
            }else if(obj.status==200){
                //验证通过发送语音
                $(".register_box").show();
                $(".yzm_pic").hide();
                showtime(60);
            }
        })
    }
    //图片验证码点击更新
    $(".yzmPic_img img").click(function(){
        var yzmPicVal=$("#phoneReg").val();
        var _this=$(this);
