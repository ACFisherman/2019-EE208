<script type="text/javascript">
$(function(){
    hotSlide();
    $(".lazyload-mod img[data-original]").lazyload({effect: "fadeIn"});
    //最新动态
    $(".hot-l .list_wrap dl dt").each(function(){
        $(this).find("a").hover(function(){
             $(this).find("img").stop(false,true).animate({width:"120%",height:"120%",marginLeft:"-10%",marginTop:"-10%"},300);
        },function(){
            $(this).find("img").stop(false,true).animate({width:"100%",height:"100%",marginLeft:"0%",marginTop:"0%"},300);
        });
    }); 
    // 高清美图
    $(".gallery-section .con").each(function(){
        $(this).find("a").hover(function(){
             $(this).find("img").stop(false,true).animate({width:"120%",height:"120%",marginLeft:"-10%",marginTop:"-10%"},300);
             $(this).find(".r-covImg").stop(false,true).animate({bottom:"0px"},300);
        },function(){
            $(this).find("img").stop(false,true).animate({width:"100%",height:"100%",marginLeft:"0%",marginTop:"0%"},300);
            $(this).find(".r-covImg").stop(false,true).animate({bottom:"-387px"},300);
        });
    }); 
    // 近期赛事
    var mSlide = new Swiper('.match-wrap .match-slide',{
        autoplay : 3000,
        loop: true,
        paginationClickable :true,
        autoplayDisableOnInteraction : false,
        slidesPerView : 4,
        slidesPerGroup : 1
    }); 
    $('.match-wrap .swiper-button-prev').click(function(){
        mSlide.swipePrev(); 
    });
    $('.match-wrap .swiper-button-next').click(function(){
        mSlide.swipeNext(); 
    });
    // 幻灯

    // 战队动态
    $("#stepScroll").stepScroll({autoPlay:false});
