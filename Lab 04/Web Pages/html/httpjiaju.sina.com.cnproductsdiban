<script>
    //搜索框切换
    //新闻，装修图片，装饰公司，产品品牌搜索
    function jiaju_index_search(type, f) {
        if (type == "news") {
            if(f.keyword.value==""||f.keyword.value=="输入关键词"){
                window.open("http://news.jiaju.sina.com.cn/search-"+f.city.value+"--");
            }else{
                window.open("http://news.jiaju.sina.com.cn/search-"+f.city.value+"--"+ f.keyword.value);
            }
        } else if (type == "picture") {
            if(f.keyword.value==""||f.keyword.value=="输入关键词，看美图"){
                window.open("http://zx.jiaju.sina.com.cn/search/t/");
            }else{
                window.open("http://zx.jiaju.sina.com.cn/search/t-" + f.keyword.value+"/");
            }
        } else if (type == "company") {
            if(f.keyword.value==""||f.keyword.value=="输入关键词，找靠谱公司"){
                window.open("http://zx.jiaju.sina.com.cn/search/r/");
            }else{
                window.open("http://zx.jiaju.sina.com.cn/search/r-"+ f.keyword.value+"/");
            }
        } else if (type == "brand") {  // 品牌搜索
            if(f.keyword.value==""||f.keyword.value=="输入关键词，看口碑、找优惠"){
                window.open("http://jiancai.jiaju.sina.com.cn/pinpai/all.html");
            }else{
                window.open("http://jiancai.jiaju.sina.com.cn/pinpai/brand/r-0-1/?search=" + f.keyword.value);
            }
        }
        dcsMultiTrack("DCS.dcsuri","/tracelog.htm","DCS.act-jd","search","DCS.content-jd",type,"WT.ti","search");
        return false;
    }
    $(function() {
        var bindState = false;
        var showItem = $("#selectedText");
        var selectItem = $("#selectUl");
        var bindEvent = function() {
            $(document.body).click(function() {
                selectItem.hide();
            });
            selectItem.find("li").click(function() {
                showItem.text($(this).text());
                $(".hideform").hide();
                $("#hideform_" + $(this).attr("name")).show();
            });
        };
        showItem.click(function(e) {
            e.stopPropagation();
            selectItem.show();
            if (!bindState) {
                bindEvent();
                bindState = true;
            }
        });
    });
    /*jQuery(function() {
     //默认显示搜索词条，不添加则默认装修图片
     var objTxt="新闻";
     $("#selectedText").html(objTxt);
     $(".hideform").hide();
     for(var j=0;j<$("#selectUl li a").length;j++){
     if($("#selectUl li a").eq(j).text()==objTxt){
     var nName = $("#selectUl li").eq(j).attr("name");
     $("#hideform_"+nName).show();
     }
     }
     });*/
</script>
