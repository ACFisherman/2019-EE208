<script type="text/javascript">
//焦点图
dj.get("ui.focusimg", ["ui.gun"], function() {
    jQuery("#focal04 .scroll-wrap-inner .clear").focusimg({
        item: "li",
        index: 0,
        auto: true,
        interval: 5000,
