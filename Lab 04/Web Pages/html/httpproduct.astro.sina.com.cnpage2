				var index = src.getAttribute('data-index');

				searchTag = +index;
				searchHandler.innerHTML = str + '<i>K</i>';
				searchHandler.setAttribute('data-post',index);

				if (src.getAttribute('data-open') != null) {
					searchOpen();
				}

				hideSearchList();
			}
