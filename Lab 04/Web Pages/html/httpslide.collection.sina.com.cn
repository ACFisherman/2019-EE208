							.selEntry{ width:92px; background-position:-97px -3px;}
							.selAblum{ width:72px; background-position:-96px -32px; }
					.sel-n-form{ width:246px; position:relative; margin:6px 0 0 0;}
						.sel-n-borderLeft{ float:left; width:1px; height:84px; background-position:-708px -5px; border-right:1px solid #000;}
						.sel-nf-name{ position:absolute; left:6px; top:1px;  }
