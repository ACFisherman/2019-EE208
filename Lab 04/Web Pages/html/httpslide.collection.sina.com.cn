		.sel{ width:950px; margin:0 auto; } 
			.sel-header{ height:89px; margin:0 0 45px 0;} 
				.sel-h-nav{ }
					.sel-hn-link{ height:84px; cursor:pointer;}
					.selAllNav{  width:73px; background-position:-204px -97px;}
					.selAllNav:hover, .selAllNavOn{ background-position:-204px -5px;}
					.selAuctionNav{  width:117px; background-position:-278px -97px;}
					.selAuctionNav:hover, .selAuctionNavOn{ background-position:-278px -5px;}
					.selExhibitionNav{  width:131px; background-position:-397px -97px;}
					.selExhibitionNav:hover, .selExhibitionNavOn{  background-position:-397px -5px;}
					.selArtistNav{  width:84px; background-position:-530px -97px;}
					.selArtistNav:hover, .selArtistNavOn{  background-position:-530px -5px;}
					.selOthersNav{  width:274px; width:95px; background-position:-616px -97px;}
					.selOthersNav:hover, .selOthersNavOn{ background-position:-616px -5px;}
				.sel-nav{ width:264px; height:84px; }
					.sel-n-box{ height:30px;  margin:13px 0 0 58px;}
