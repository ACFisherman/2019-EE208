							.sel-nf-submit{ width:74px;  background-position:-2px -51px;}
							.sel-nf-submit:hover{ background-position:-2px -80px;}
						
			.sel-content{ list-style:none;  margin:0; }
				.sel-c-row{ margin:0 -10px 0 0; }
					.csel-cr-clum{ margin-right:10px;   }
						.csel-crc-item{  padding:0;  margin:0 0 11px 0;   }
						.csel-crc-item-x{ width:194px}
						.csel-crc-item-xx{ width:337px;}
							.csel-crci-link{ color:#D5D5D5; text-decoration:none; cursor:pointer;  display:block; padding:9px 6px 0px 6px; border:1px solid #636363;  border-radius:1px;  background:#636363;  }						
							.csel-crci-link:hover{   border:1px solid #FF8500;   background:#D5D5D5;   }	

							
							
							.csel-crci-img-x, .csel-crci-img-xx{ vertical-align:top;  border:0; }					
							.csel-crci-img-x{ width:180px; height:150px;  }
							.csel-crci-img-xx{ width:323px; height:356px;  }
							.csel-crci-depict, .csel-crci-depict-xx{ font:12px/3 "MS Song"; color:#D5D5D5; margin:0; text-align:center;  height:34px; overflow:hidden; margin:0 auto;   }
							
							.csel-crci-link:hover .csel-crci-depict, .csel-crci-link:hover .csel-crci-depict-xx{ color:#626262; text-decoration:underline; }
												
							
		
			.sel-pagination{ text-align:center; height:28px; margin:34px auto 45px; }
				.sel-p-indicator{ display:inline-block; width:18px; height:18px; background:#636363; border:1px solid #797B7B; color:#D5D5D5; text-align:center; font:10px/1.8 'MS Song'; text-decoration:none; margin:4px 4px; cursor:pointer; }
				.sel-p-indicator-last{ width:48px; }
				.sel-p-indicator-first{ width:48px; }
				.sel-p-indicator:hover, .sel-p-indicator-current{ background:#FF8400; border:1px solid #FF8400;  color:#000; }
				.sel-p-indicator-current:hover{color:#FFF;}
				
				.sel-p-control{ display:inline-block; *display:inline; margin:0 3px;   }
					.sel-pc-goto{   width:18px !important; height:18px; background:#636363; border:1px solid #797B7B; margin:3px 5px; font:bold 14px/1.28 'MS Song'; line-height:normal; color:#FF8400; text-align:center; } 
					.sel-pc-submit{  width:40px; color:#000; background:#FF8400; border:1px solid #FF8400; position:relative; }
					.sel-pc-submit:hover{ color:#FFF;  }
					
				
			.sel-footer{ border-top:1px solid #FF8400;  padding:24px 0 80px; text-align:center; }
				.sel-f-rows{ margin:0; padding:0;  txt-align:center; color:#FFF; font:12px/1.5 "MS Song" }
				.sel-f-copyright{ font:12px/2 'Arial','MS Song';}
					.sel-fr-link{ color:#FFF; text-decoration:none; }
					.sel-fr-link:hover{ color:#FF8400; text-decoration:underline; }		

			.iFt-Song12pt{ font:12px/1 "MS Song";}
			.sel-nf-key{ color:#000; line-height:26px\9;}
			.sel-nf-keyFocus{ background:#FF8700; color:#fff}				
	</style>

</head>
<body>
	<div class="sel" id="selFixedWidth" >
		<!--头部导航 开始-->
		<div class="sel-header udv-clearfix">
			<div class="sel-h-nav fl"  id="selNav">
				<a id="selAll" class="sel-hn-link fl selSprite selAllNav selAllNavOn depict" href="/">全部</a>
				<a id="selAuction" class="sel-hn-link fl selSprite selAuctionNav depict" href="/?category=177,164">拍卖</a>
				<a id="selExhibition" class="sel-hn-link fl selSprite selExhibitionNav depict" href="/?category=163">展览</a>
				<a id="selArtist" class="sel-hn-link fl selSprite selArtistNav depict" href="/?category=175">名家</a>
				<a id="selOthers" class="sel-hn-link fl selSprite selOthersNav depict" href="/?category=193,189,237">其他</a>
			</div>
			<div class="sel-nav fr">
				<b class="selSprite sel-n-borderLeft"></b>
				<div class="sel-n-box fr">
					<div class="sel-nb-logo fl selSprite pngfix"></div>
					<a class="sel-nb-link fl selSprite selEntry depict pngfix" href="http://collection.sina.com.cn">新浪收藏</a>			
					<a class="sel-nb-link fl selSprite selAblum depict pngfix" href="http://slide.collection.sina.com.cn">高清图集</a>
				</div>
				<div class="sel-n-form fr">
					<form action="http://slide.collection.sina.com.cn" method="get" id="selForm">
						<input class="sel-nf-key iFt-Song12pt fl" name="srrKey" placeholder="请输入搜索关键字" x-webkit-speech type="text" id="selSrr" autocomplete="off"/>  <!-- sel-nf-keyFocus-->
						<input class="sel-nf-submit fl selSprite depict" type="submit" value="搜索"/>
					</form>  
				</div>
			</div>
		</div>
		<!--头部导航 结束-->
		
		<!--页面内容 开始-->
		<ul class="sel-content udv-clearfix" id="listSelId">
		</ul>
		<!--页面内容 结束-->
	
		<!--分页 栏 开始-->
		<div class="sel-pagination">
			<div class="sel-p-control" id="paginationId"></div><div class="sel-p-control"><input title="直达N页" class="sel-pc-goto" id="pgNum" name="gotoPage"/><span id="gotoPageBtn" class="sel-p-indicator sel-pc-submit" >确定</span></div>
		</div>
		<!--分页 栏 结束-->
		
		
		<script>
			
		seajs.use(['jquery', 'arale.placeholder'], function($ , placeholder){
				
				window.onresize = function(){document.body.style.width = ($(window).width() > 1154 ? $(window).width() : 1154) + "px";}

				
				var navConfig = [
					{'id':'selAll', 'clazz':'selAllNavOn', 'target':"all"}
					, {'id':'selAuction', 'clazz':'selAuctionNavOn',  'target':"177,164"}
					,{'id':'selExhibition', 'clazz':'selExhibitionNavOn',  'target':"163"}
					,{'id':'selArtist', 'clazz':'selArtistNavOn', 'target':"175"}
					,{'id':'selOthers', 'clazz':'selOthersNavOn', 'target':"193,189,237"}
				];	
				
				/*
				* 生成URL参数 对象
				*/
				function doURLParaObj(){
					var parasObject = {};
					var urlSearch = (window.location.search);
					var urlParas = urlSearch.substr(1, urlSearch.length);
					if(!!urlParas)        //20130307
					{
						var arr = urlParas.split("&");				
						for(var cC = 0; cC < arr.length; ++cC)
						{							
							var paraC = arr[cC].split("=");
							parasObject[paraC[0]] = paraC[1];
							if(paraC[0] == 'k') {
								parasObject['name'] = paraC[1];
							}
						}					
					}
					return parasObject;
				}
				
				var urlSearch = (window.location.search);
				var urlSearchEnd = doURLParaObj();
				var entry = (!!urlSearchEnd.category) ? urlSearchEnd.category : 'all';

				var categoryID =  updateNavBar(entry);  //更新 导航样式
				
				var queryString = navConfig[categoryID].target;
				
				
				//修改 对象的制定属性 2/3
				function changeObjAttr(urlObj , obj)  
				{
					var tmp;
					tmp = urlObj;
					for(var key in obj) {
						tmp[key] = obj[key];
					}
					
					return tmp;				
				}
				
				//返回URL参数字符串 3/3
				function produceURLQueryString(urlObj){
					var result = [];
					for(var attr in urlObj)
					{
						result.push(attr + "=" + urlObj[attr]);
					}
					
					return result.join("&");
					
				
				}
						
			
				$("#selForm").submit(function(event){				
					if($("#selSrr").val())
					{					
						//window.location.href = "/?" + queryString + "&ie=utf-8&k=" + encodeURIComponent($("#selSrr").val());
						window.location.href = "/?ie=utf-8&k=" + encodeURIComponent($("#selSrr").val());
					}
					else{
						$(this).blur();
					}
				//	window.location.href = "list.html?" + queryString + "<<<<";
					
					
					event.preventDefault();
				});
				
				function updateNavBar(print){
					var index = 0;
					for(var ccc = 0; ccc < navConfig.length; ++ccc )
					{
						if(navConfig[ccc].target === print)
						{					
							$("#" + navConfig[ccc].id).addClass(navConfig[ccc].clazz);		
							index = ccc;
						}
						else
						{
							
							$("#" + navConfig[ccc].id).removeClass(navConfig[ccc].clazz);
						}
					}
					return index;
				}			
		
				
				var numOfPages = 0;

				var LIST_ROW = 3;  //配置行数
				var PAGE_BONUD = 10; //配置页 左右距离
				var PROMOT_LOCAION = 3; //配置首页推介位置3
			
				
											var clientWidth = $(window).width();
									
				var clientWidth = $(window).width();
				if (clientWidth <= 1154) {
					clientWidth = 1154;
					var bodyClientWidth = ($(window).width() > 1154) ? $(window).width() : 1154;
					document.body.style.width = bodyClientWidth + "px";
				}
				var avalableWidth = clientWidth - 40; 	//-40 一保留左右留白共40px
				
				
				
				
									var numOfColums = (((avalableWidth + 10) - 347)/204 >>> 0 ) + 1;
					var clientWidth = ((numOfColums - 1) * 204 + 337); 
								
				var clientWidth = ((numOfColums - 1) * 204 + 337); 
				if(clientWidth > 950)
				{
					document.getElementById("selFixedWidth").style.width = clientWidth + "px";						
				} else {
					clientWidth = 950;	
				}
				var numOfPicsPerPage = ((numOfColums - 1) * 2 + 1) * LIST_ROW;     //计算每页图片总数。 3 每页显示三排  

				var listConfigSeed = [3, 4 , 0];			
	

				
				
				var currentPage = 1;  //页码从1开始记

				
				/*
				* 根据参数获取响应页码数据，并进行渲染、生成分页
				* 
				*/
				function updatePage(pageNum){ 
				
					document.getElementById("pgNum").value = pageNum;
					var category = (entry == "all") ? '' : "category="+entry;
					keyword = window.location.search.indexOf('k=')+1? window.location.search.substring(window.location.search.indexOf('k=')+2).split('&')[0] : ''
					$.ajax({
						 type : "get",
						 async:true,
						 cache:true,
						 url : "http://slide.collection.sina.com.cn/interface/api_album_list.php?" + category,
						 data:{
							"app_key":2049634077
							,"format":"json"
							,"ch_id":26
							,"rl":1
							,"size_others":"323_356,180_150"
							,"page":pageNum
							,"num":numOfPicsPerPage
							,"name":keyword ? decodeURIComponent(keyword) : '' 
							,"dpc":1				
						 },
						 dataType : "jsonp",
						 jsonp: "jsoncallback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
						 jsonpCallback:"noop",//自定义的jsonp回调函数名称，默认为jQuery自动生成的随机函数名
						 success : function(data){	
							
							
							if(data.status.code === "0")
							{
								if(data.count >>> 0)
								{
									renderData(data, pageNum);
									currentPage = pageNum;
									generatePagination(data.total, pageNum);  //更新分页
								}								
							}
							else
							{
								updatePage(pageNum);   //重新获取数据
							}  
						 },
						 error:function(){
							updatePage(pageNum);
						 }
					 }); 				
				}				
			
				updatePage(1);   //初始化页面
				
				function noop(){};
				
				
				/*更新分页
				 *@param total: 数据数量
				 *@param current：当前页码
				 */
				function generatePagination(total, current){
					var fixedPagePara = (total >>> 0) % numOfPicsPerPage === 0 ? 0 : 1;
					numOfPages = (((total >>> 0)/numOfPicsPerPage) >>> 0) + fixedPagePara;				
			//		numOfPages = ((((total >>> 0) + numOfPicsPerPage)/numOfPicsPerPage) >>> 0);
				
					var htmlStr = "";			
					if(current > 1)
					{					
						htmlStr += "<a class=\"sel-p-indicator sel-p-indicator-first\" href=\"javascript:void(0);\">第一页</a>";
					}
					
					var begin = current - PAGE_BONUD > 0 ? current - PAGE_BONUD : 0;
					var end = current + PAGE_BONUD < numOfPages ? current + PAGE_BONUD : numOfPages
					for(var ccPg = begin; ccPg < end; ++ccPg)
					{
						htmlStr += "<a class=\"sel-p-indicator\ ";
						if(ccPg + 1 == current)  // 添加当前页标志
						{
							htmlStr += "sel-p-indicator-current";  
						}
						htmlStr += " href=\"javascript:void(0);\">" + (ccPg + 1) + "</a>";						
					}	
					
					
					if(current != numOfPages)
					{
						htmlStr += "<a class=\"sel-p-indicator sel-p-indicator-last\" href=\"javascript:void(0);\">最末页</a>";
					}
					document.getElementById("paginationId").innerHTML = htmlStr;
					
					$(document).scrollTop(0);
					
				}
				
				function renderData(data, pageNum){				
					var begin = 0;
					var end = 0;
					var numPerRow = numOfPicsPerPage/LIST_ROW;
					var htmlInStr = "";
					var rowData = [];			
					for(var ccRow = 0 ; ccRow < LIST_ROW; ++ccRow)		
					{
						rowData = [];						
						htmlInStr += "<li class=\"sel-c-row udv-clearfix\">";
						begin = ccRow * numPerRow;
						end = (ccRow + 1) * numPerRow;	
						
						rowData = rowData.concat(data.data.slice(begin, end));	
						var newListConfigTap = generateListConfig(pageNum);			
						if(pageNum === 1 &&  ccRow === 0)   //如果是第一页将推荐数据 加入合适位置
						{	
							
							if(queryString == 'all')
							{
								rowData[newListConfigTap[0] * 2] = data.rl;		
							}
			
						}
						for(var ccItem = 0; ccItem < rowData.length; ++ccItem)
						{
            if(!rowData[ccItem]) continue;
            try{
							if(newListConfigTap[ccRow] === ccItem/2)  //大图
							{
								htmlInStr += "<div class=\"csel-cr-clum fl\"><div class=\"csel-crc-item csel-crc-item-xx\"><a class=\"csel-crci-link\" href=\"" +rowData[ccItem].url+ "\" target=\"_blank\"><img class=\"csel-crci-img-xx\" src=\"" + rowData[ccItem].img_323_356 + "\" alt=\"" + rowData[ccItem].name + "\" title=\"" + rowData[ccItem].short_name + "\"><p class=\"csel-crci-depict-xx\">" + rowData[ccItem].short_name + "</p></a></div></div>";
							}
							else		//小图
							{
								htmlInStr += "<div class=\"csel-cr-clum fl\"><div class=\"csel-crc-item csel-crc-item-x\"><a class=\"csel-crci-link\" href=\"" +rowData[ccItem].url+ "\" target=\"_blank\"><img class=\"csel-crci-img-x\" src=\"" +rowData[ccItem].img_180_150+ "\" alt=\"" + rowData[ccItem].name + "\" title=\"" + rowData[ccItem].short_name + "\"><p class=\"csel-crci-depict\">" + rowData[ccItem].short_name + "</p></a></div>";
								
								if(rowData[ccItem+1])
								{
									htmlInStr += "<div class=\"csel-crc-item csel-crc-item-x\"><a class=\"csel-crci-link\" href=\"" +rowData[ccItem+1].url+ "\" target=\"_blank\"><img class=\"csel-crci-img-x\" src=\"" +rowData[ccItem + 1].img_180_150+ "\" alt=\"" + rowData[ccItem +1].name + "\" title=\"" + rowData[ccItem + 1].short_name + "\"><p class=\"csel-crci-depict\">" + rowData[ccItem + 1].short_name + "</p></a></div>";
									
									++ccItem;
								}								
								htmlInStr += "</div>"
							}
            }catch(err){
                //console.log(err)
            }
						}					
				
						htmlInStr += "</li>";
						

					}
					
					document.getElementById("listSelId").innerHTML = htmlInStr;					
					
				}
				
				//页码 结合 listConfigSeed 生成 参数指定页的 大图配置;  每次渲染前进行重新计算
				function generateListConfig(pageNum){
					var newListConfig = [];			
					for(var counter = 0; counter < listConfigSeed.length; ++counter){				
						newListConfig.push((listConfigSeed[counter] +　pageNum) % numOfColums);	// !!保证多次浏览到相同内容 大图位置固定				
					}
					if(pageNum === 1)
					{
						newListConfig[0] = PROMOT_LOCAION;
					}
					return newListConfig;
				}
 
				$("#selSrr").focus(function(event){   //搜索框
					$(this).addClass("sel-nf-keyFocus").prev().hide();
				}).blur(function(){					
					if(!$(this).val())
					{
						$(this).prev().show();
					}
					$(this).removeClass("sel-nf-keyFocus");					
				});
			
				var curParam = doURLParaObj();
				var addParam = {"client_width":clientWidth};
				
				$("#gotoPageBtn").click(function(){
					var page = parseInt($("#pgNum").val());
					if(page <= 0) {
						page = 1;
					}
					page = (page >>> 0);
					if(page <= 0) {
						page = 1;
					} 
					else if(page > numOfPages)
					{		
						page = numOfPages;
					}
						
					addParam.page = page;
					var lastParam = changeObjAttr(curParam , addParam);
					window.location.href = "http://" + window.location.host + "?" + produceURLQueryString(lastParam);
				});

				$("#paginationId").click(function(event){   //切换 页面内容
					$target = $(event.target);
					var newPageNum = ($target.text() >>> 0);
					
					if(newPageNum){
						addParam.page = newPageNum;
					}
					else
					{
						var index = $target.index();
						
						if(!index)
						{
							addParam.page = 1; 
						}
						else
						{	
							addParam.page = numOfPages;
						}
						
					}

					var lastParam = changeObjAttr(curParam , addParam);
					if($target.hasClass("sel-p-indicator")) {
						window.location.href = "http://" + window.location.host + "?" + produceURLQueryString(lastParam);
					}
				
				});				
				
				
			});
		</script>		
		
		

