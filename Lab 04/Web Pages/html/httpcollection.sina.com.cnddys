	<script type="text/javascript">
    var sidebar = $(".top_list"),
        sideList = $('.blk_10'),
        bot_ele = $('.coo_list'),
        offset = 150,
        lh = sideList.height(),
        ua = navigator.userAgent.toLowerCase(),
        isIE6 = /msie 6/.test(ua);

    function updatePosition() {
        var r = { 'a': false, 'code': 1 };
        var scrolltop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop;
        if (scrolltop > $(".feed_content").offset().top) {
            r.a = true;
            var p = bot_ele.offset().top - scrolltop - offset;
            var q = bot_ele.offset().top - scrolltop - offset - sidebar.height();
            if (p > (sidebar.height() - lh)) {
                r.code = 2;
            }
            if (p > sidebar.height()) {
                r.code = 3;
            }
        }
        return r;
    }

    $(window).bind('scroll resize', function() {
        var r = updatePosition();
        if (r.a) {
            sidebar.addClass('top_list_fixed');
            if (r.code == 1) {
                sidebar.fadeOut(200);
            }
            if (r.code == 2) {
                sidebar.fadeIn(200);
                sideList.css('visibility', 'hidden');
            }
            if (r.code == 3) {
                sidebar.fadeIn(200);
                sideList.css('visibility', 'visible');
            }
        } else {
            sidebar.removeClass('top_list_fixed').fadeIn(200);
            sideList.css('visibility', 'visible')
        }
    });
    </script>
