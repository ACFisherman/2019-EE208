	<script type="text/javascript">
	$(function(){
		var doughnutData = [
			{
				value: 75,
				color:"#ff6600",
				highlight: "#ff7f29",
				label: "Percentage"
			},
			{
				value: 25,
				color: "#ffc9a5",
				highlight: "#fedcc5",
				label: "whole"
			}
		];

		var doughnutDataNo = [
			{
				value: 75,
				color:"#bdbdbd",
				highlight: "#d0cfcf",
				label: "Percentage"
			},
			{
				value: 25,
				color: "#ededed",
				highlight: "#f4f3f3",
				label: "whole"
			}
		];
		
		

			/*BGWR评分start*/
			var score = 0;       //判断是否有CGWR评分,若有 score = 1，若无 score = 0
			var product=decodeURI('炽焰帝国2');
			 $.ajax({
				      type: "GET",
				      url: "http://interface.sina.cn/games/gpapi/cgwr/game_pc_cpk_cgwr_score_rank.d.html?callback=?",
				      data:{
						  product:product,
						  productID:"cczmvun4439115"
					    },
				      dataType: "jsonp",
				      success: function(data){  
						  //console.log(data); 
						  if(data.status==1){
						  	var item=data.data;
				          	score=1;
				          	$('#allpank').html(item.year+'第 '+item.yearrank+' 位<br>总榜第 <span class="org">'+item.allrank+'</span> 位');
				          	//$("#yearpank").text(data.yearrank);
							  doughnutData[0]['value']=item.sumscore*10;
							  doughnutData[1]['value']=100-item.sumscore*10;
				          	$(".chart_score").text(item.sumscore);
				          }
		
				        var ctx = document.getElementById("chart-area").getContext("2d");
						var ctx_no = document.getElementById("chart-area-no").getContext("2d");
						if(score == 1){
							$('.rscore-has').show();
							$('.rscore-no').hide();
							window.myDoughnut = new Chart(ctx).Doughnut(doughnutData, {responsive : true,percentageInnerCutout : 80,segmentStrokeWidth : 0});
						}else{
							$('.rscore-has').hide();
							$('.rscore-no').show();
							window.myDoughnut = new Chart(ctx_no).Doughnut(doughnutDataNo, {responsive : true,percentageInnerCutout : 80,segmentStrokeWidth : 0});
						}
					}
			});
			
			
			/*BGWR评分end*/
		});
		
	</script>
