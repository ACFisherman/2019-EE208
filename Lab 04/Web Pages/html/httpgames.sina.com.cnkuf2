				<script>
					$(function(){
						var totalpage;
						var product = '炽焰帝国2';
						var productID = 'cczmvun4439115';
						var index = 1;
						var title =1;
						var snub = 10;//一页有几条内容
						var sbtn =2; //默认显示5页
						$(".jump").live('click', function(event) {
							var title2 = parseInt($('.isNow').text());
							title = parseInt($(this).text());
							if(title2==title){
								return false;
							}else{
								loding();
								getAjax(title,snub);
							}
						});
						// 下一页
						$('.next').live('click', function(event) {
							if($(this).hasClass('Disabled')){
								return false;
							}else{
								loding();
								title++;
								getAjax(title,snub);	
							}
			
						});
						// 上一页
						$('.prev').live('click', function(event) {
							if ($(this).hasClass('Disabled')) {
								return false;
							}else{
								loding();
								title--;
								getAjax(title,snub);
							}
			
						});
						$('.next5').live('click', function(event) {
							if ($(this).hasClass('Disabled')){
								return false;
							}else{
								loding();
								title=Math.ceil(title/sbtn)*sbtn+1;
								getAjax(title,snub);
							}
						});
						function loding(){
							$("#vidolistdiv").html();
							$("#vidolistdiv").html('<div class="addloading loading_data"><img src="http://n.sinaimg.cn/game/homepage/loading.gif"/> 加载中....</div>')
						}
						function getAjax(spage,spagesize){
							$.ajax({
								dataType: "jsonp",
								async:true,
								url: "http://interface.sina.cn/games/gpapi/product/web_videolist.d.html",
								data: {page: spage,product:product,productID:productID,pagesize: spagesize},
								success: function(data) {
									var str = data.data.result;
									totalpage = data.data.totalPage;
									// tab签上的小图标
									var totalsize = data.data.total;
									var totalcount
									if(totalsize!=0){
										if(totalsize>=100){
											totalcount = 'n+';
										}else if(0<totalsize<100){
											totalcount = totalsize;
										}
										var spanstr= '<span>'+totalcount+'</span>';
										$('#gamevideo').append(spanstr);
									}
									// tab签上的小图标 end
									$("#vidolistdiv").html(str);
										if (totalpage==0) {
											$("#vidolistdiv").html('暂无视频');
										}else if(totalpage==1){
											$(".pageZone").html('<span title="1" class="jump isNow">1</span>');
			
										}else{
											if(totalpage<=sbtn){
												sbtn = totalpage;
											}
											var str = '<span title="上一页" class="prev '+(spage>1?'':'Disabled')+'">上一页</span>';;
											var start = Math.ceil(spage/sbtn)*sbtn-(sbtn-1);
											var len = totalpage>start?start+sbtn:totalpage+1;
											for (var i = start; i < len; i++) {
											klass = (spage==i)?'isNow':'';
											str+='<span class="jump '+klass+'">'+i+'</span>';
											};
											var disable = spage<totalpage?'':'Disabled';
											str +='<span title="下一页" class="next '+disable+'">下一页</span>';
											if(Math.ceil(spage/sbtn)<Math.ceil(totalpage/sbtn)&&sbtn<totalpage){
												str +='<span title="下'+sbtn+'页" class="next5">下'+sbtn+'页</span>';
											}else{
												str +='<span title="下'+sbtn+'页" class="next5 Disabled">下'+sbtn+'页</span>';
											}
											$(".pageZone").html(str);
										}
									}
									// success end
							});	
						}
						getAjax(1,snub);
					});	
			</script>
