				function AutoResizeImage(maxWidth,maxHeight,objImg){
					var img = $(objImg);
					//console.log(img);
					var hRatio;
					var wRatio;
					var Ratio = 1;
					var w = img.width();
					var h = img.height();
					//console.log(w,h);
					
					//console.log(naturalHeight,naturalWidth);
					wRatio =w/maxWidth;
					hRatio = h/maxHeight;
					if(wRatio>hRatio&&wRatio>1){
						Ratio = wRatio
					}else if(hRatio>wRatio&&hRatio>1){
						Ratio = hRatio
					}
					
					/*
					if (maxWidth ==0 && maxHeight==0){
						 Ratio = 1;
					}else if (maxWidth==0){
						 if (hRatio<1) Ratio = hRatio;
					}else if (maxHeight==0){
						 if (wRatio<1) Ratio = wRatio;
					}else if (wRatio<1 || hRatio<1){
						Ratio = (wRatio<=hRatio?wRatio:hRatio);
					}
						if (Ratio<1){
							w = w * Ratio;
							h = h * Ratio;
					}
					*/
					h = h/Ratio;
					w = w/Ratio;
					//console.log(img,wRatio,hRatio,Ratio,w,h);
					img.css('width',w);
					img.css('height',h);

					}
