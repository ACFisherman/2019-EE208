<script type="text/javascript">
    //焦点图特别处理
    (function () {
        var focusControl = {
            isFlag: false,
            focusData: {},
            //是否有焦点图广告
            setFlag: function () {
                if (ads.Data && ads.Data.t00) {
                    for (var i = 0; i < ads.Data.t00.length; i++) {
                        var data = ads.Data.t00[i];
                        if (data.type == "focus") {
                            this.isFlag = true;
                            this.focusData = data;
                            break;
                        } else {
                            this.isFlag = false;
                        }
                    }
                } else {
                    this.isFlag = false;
                }
            },
            //创建广告dom
            ceateAdDom(callback) {
                var data = this.focusData,
                    id = data.params.wrapId || "special_focus",
                    box = document.getElementById(id);
                if (box && data.params.src && data.params.src.lastIndexOf(".jpg") != -1) {
                    var width = data.params.width + "px",
                        height = data.params.height + "px",
                        alt = data.params.alt,
                        src = data.params.src,
                        link = data.params.link,
                        wrap = box.getElementsByTagName("ul")[0],
                        li = document.createElement("li");

                    li.innerHTML = '<a target="_blank" href="' + data.params.link + '"><img width="' +
                        width +
                        '" height="' + height + '" src="' + data.params.src + '" alt="' + data.params.alt +
                        '"></a>';
                    wrap.appendChild(li);
                    callback && callback();
                }
            },
            //初始化焦点图
            init: function () {
                this.setFlag();
                if (this.isFlag) {
                    this.ceateAdDom(PicListStart);
                } else {
                    PicListStart();
                }
            }

        }

        focusControl.init();
    })();
