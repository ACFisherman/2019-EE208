<script type="text/javascript" >
var nowItemId = 0;
jQuery(function(){
    $('.bottombgbox li').click(function(){
            var iVal = $(this).index();
            if (nowItemId == iVal) return;
            nowItemId = iVal;
            $('.bigimg>li').eq(iVal).css("z-index",3).siblings('li').css("z-index",0);
            $('.bigimg>li').eq(iVal).css("opacity",0).animate({"opacity":1},1000).siblings('li').css("opacity",1).animate({"opacity":0},10);
            $('.bottombgbox li').eq(iVal).addClass('haschosen').siblings('li').removeClass('haschosen');
            $('.sidebar li').eq(iVal).css('display','block').siblings('li').css('display','none');
        });
})
</script> 

