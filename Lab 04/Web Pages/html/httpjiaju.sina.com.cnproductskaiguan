    dj.get("ui.focusimg", ["ui.gun"], function () {
        $("#tab-box1 >ul").focusimg({
            item: "li",
            index: 0,
            auto: true,
            interval: 5000,
            vertical: false,
            circle: true,
            prev: "#tab-box1 .l-btn",
            next: "#tab-box1 .r-btn"
        })
        $("#tab-box2 >ul").focusimg({
            item: "li",
            index: 0,
            auto: true,
            interval: 5000,
            vertical: false,
            circle: true,
            prev: "#tab-box2 .l-btn",
            next: "#tab-box2 .r-btn"
        })
        $("#tab-box3 >ul").focusimg({
            item: "li",
            index: 0,
            auto: true,
            interval: 5000,
            vertical: false,
            circle: true,
            prev: "#tab-box3 .l-btn",
            next: "#tab-box3 .r-btn"
        })
    })

    $('.big-right li').mouseover(function () {
        $('.big-right li').removeClass('active');
        $(this).addClass('active');
    })
    $('.big-left-l li').mouseover(function () {

        $('.big-left-l li').removeClass('active');
        $(this).addClass('active');
        $('.tab-box').removeClass('active');
        $('.tab-box').eq($(this).index()).addClass('active');
    })

    $('#big-box-small').insertBefore($('#hLine4'));
</script>
