    //统计代码
    function tjBtn(key, option, url, urlObj,product) {
        if (typeof lejuTj !== 'undefined') {
            //自定义参数,多个参数以此类推
            var event_params = new Object();
            event_params.bid = '10';//必填
            event_params[key] = option;
            lejuTj.event(product, event_params);
        }
        if (typeof url !== 'undefined') {
            if (urlObj === '_blank') {
                window.open(url);//新页面打开
            } else {
                window.location.href = url;//本页面打开
            }
        }
    }
</script>
