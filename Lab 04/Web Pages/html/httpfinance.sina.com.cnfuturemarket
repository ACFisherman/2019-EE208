			<script>
				/**
				 * 设置伦敦和纽约时间
				 */
				function loadScript(__objectLoader, __stringUrl, __functionCallback) {
					var __elementScript = document.createElement("script");
					__elementScript.type = "text/javascript";
					__elementScript._functionCallback = typeof __functionCallback != "undefined" ? __functionCallback : new Function();
					__elementScript[document.all ? "onreadystatechange" : "onload"] = function() {
						if (document.all && this.readyState != "loaded" && this.readyState != "complete") {
							return;
						}
						this._functionCallback(this);
						this._functionCallback = null;
						this[document.all ? "onreadystatechange" : "onload"] = null;
						this.parentNode.removeChild(this);
					};
					__elementScript.src = __stringUrl;
					( typeof __objectLoader == "string" ? document.getElementById(__objectLoader) : __objectLoader).appendChild(__elementScript);
				}

				var intBJ = -1;
				 function fillGTime() {
                            intBJ = intBJ == -1 ? window["StandardBJTime"] * 1000 : intBJ + 1000;
                            var __dateBJ = new Date(intBJ);
                            var __dateLD = new Date(intBJ + getGlobalTimezone("UK") * 60 * 60 * 1000);
                            var __dateNY = new Date(intBJ + getGlobalTimezone("US") * 60 * 60 * 1000);
                            document.getElementById("g_time").innerHTML = '伦敦时间 ' + __dateLD.toString().match(/\d\d:\d\d:\d\d/)[0] + ' | 纽约时间 ' + __dateNY.toString().match(/\d\d:\d\d:\d\d/)[0];
                            document.getElementById("global_time").innerHTML = '北京 ' + __dateBJ.toString().match(/\d\d:\d\d:\d\d/)[0] + ' | 伦敦 ' + __dateLD.toString().match(/\d\d:\d\d:\d\d/)[0] + ' | 纽约 ' + __dateNY.toString().match(/\d\d:\d\d:\d\d/)[0];
                        }

				loadScript("g_time", "//hq.sinajs.cn/?format=json&rn="+ (new Date()).getTime() +"&func=window.StandardBJTime=hq_json_sys_time;if%28typeof%28StandardBJTime_Callback%29==%27function%27%29StandardBJTime_Callback%28%29;&list=sys_time", function() {
					setInterval(fillGTime, 1000);
				});

				/**
				 * 设置DownButton
				 */
				function getElement(__stringId) {
					return document.getElementById(__stringId);
				}

				function getRandom() {
					return (new Date()).getTime() + Math.random().toString().replace("0.", "");
				}

				var intIndexJYS = 0, intIndexPZ = 0, intIndexYF = 0, intImgKey = "";
				// alert(intIndexJYS)
				var JYSSelect = getElement("JYS");
				var PZSelect = getElement("PZ");
				var YFSelect = getElement("YF");
				//交易所
				function fillJYS() {
					var __stringResult = '';  JYSSelect.options.length = 0;
					for (var i in JYS) {
						JYSSelect.options[i] = new Option(JYS[i],i);
					}
					intIndexJYS = 0;
					fillPZ();
				}

				//品种
				function fillPZ() {
					var __stringResult = ''; PZSelect.options.length = 0;
					for (var i in PZ[intIndexJYS]) {
						PZSelect.options[i] = new Option(PZ[intIndexJYS][i],i);
					}
					intIndexPZ = 0;
					fillYF();
				}

				//合约
				function fillYF() {
					var __stringResult = '';  YFSelect.options.length = 0;
					for (var i in YF[intIndexJYS][intIndexPZ]) {
						YFSelect.options[i] = new Option(YF[intIndexJYS][intIndexPZ][i][0],i);
					}
					intIndexYF = 0;
				}

				function gotoYF() {
					intImgKey = YF[intIndexJYS][intIndexPZ][intIndexYF][1];
					intImgKey = intImgKey.replace(/CFF_IF/, "IF");
					var _url = intImgKey.substr(0, 2) == "IFFalse" ? ("http://finance.sina.com.cn/futures/quote/cff/bc.shtml?code=" + intImgKey) : ("http://finance.sina.com.cn/futures/quotes/" +intImgKey + ".shtml");
					window.open(_url);
				}

				fillJYS();
			</script>
