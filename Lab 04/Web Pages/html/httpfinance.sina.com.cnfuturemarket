		loginLayer.addOpener('login_userIn');
		var submit_form = document.forms['zjzt'], submit_check = document.getElementById('anonymous'), is_hide = 1, sureLogin = false;
		function check_submit() {
			submit_check.value = submit_check.checked == true ? 1 : 2;
			is_hide = submit_check.value;
		}
		function sumbit_info() {
			if (submit_form['content'].value.trim().length == 0) {
				alert("请填入提问内容!");
				return false;
			}
			submit_form['content'].value = '';

			if (!sureLogin && is_hide == 2) {
				alert("请输入用户名、密码!");
				return false;
			}
			submit_form.submit();
		}
