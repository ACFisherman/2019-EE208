			function fillContract() {
				var __arrayName = {"czce": "郑商所", "dce": "大商所", "shfe": "上期所"};
				var __stringHtml = "";
				for (var i in fu_index_contract) {
					__stringHtml += '<option value="' + i + '">' + __arrayName[i] + '</option>';
