									formatter: function (data) { //悬浮框内容
										var data1 = soy_goods_chart[data.index];
										return '日期：' + data1.opendate
											+ '<br>价格: ' + data1.price
											+ '<br>涨跌额: ' + data1.zde
											+ '<br>涨跌幅：' + (data1.zdf * 100).toFixed(0) + '%';
									}
								},
								domId: 'spzs_chart_02',
								xAxis: {
									axisLine: {
										show: false
									},
									text: {
										//rotate: Math.PI * 0.08,
										style: {
											fillStyle: '#666'
										}
									},
									style: {
										strokeStyle: '#C43C38'
									},
									eachLen: 120
								},
								yAxis: {
									left: {
										show: true,
										text: {
											style: {
												fillStyle: '#666'
											},
											formatter: function(text) {
												return numFormatter(text, 1);
											}
										},
										name: {
											text: ''
										}
									}
								},
								legend: {
									show: false
								}
							});
							chart.addData({
								data: map(soy_goods_chart, function(d) {
									return Number(d.price);
								}),
								axisArr: map(soy_goods_chart, function(d) {
									return d.opendate;
								}),
								name: '',
								param: {
									style: {
										strokeStyle: '#336699'
									}
								},
								type: 'Line'
							});
						}
					});
				}
