				var __select = __elementSelect.form["t_breed"];
				var __options = __select.options;
				for (var i = 0; i < __select.options.length; i++) {
					__select.removeChild(__select.options[0]);
				}
				var __stringKey = __elementSelect.value;
				for (var i = 0; i < fu_index_contract[__stringKey].length; i++) {
					__select.options[i] = new Option(fu_index_contract[__stringKey][i][2], fu_index_contract[__stringKey][i][1]);
				}
			}
