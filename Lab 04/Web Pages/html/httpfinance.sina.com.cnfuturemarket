		var slide_02=new ScrollPic();
		slide_02.id='slide';
		slide_02.prevId='left_btn';//按钮ID (上一个)
		slide_02.nextId='right_btn';//按钮ID (下一个)
		slide_02.moveNum=1;//每次滚动几个单元格 [默认1个]
		slide_02.isLoop=false;//无缝循环滚动 [默认是]
		slide_02.speed = 50;//速度 越小越快 [默认20]
		slide_02.autoPlay=true;
		slide_02.playTime = 8000;
		slide_02.init();
	}();
	</script>
</div>
