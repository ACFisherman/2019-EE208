				var __date = new Date((new Date()).getTime() - 1000 * 60 * 60 * 24);
				document.getElementById(__stringId).value = __date.getFullYear() + "-" + ((__date.getMonth() + 1) > 9 ? __date.getMonth() + 1 : "0" + (__date.getMonth() + 1)) + "-" + (__date.getDate() > 9 ? __date.getDate() : "0" + __date.getDate());
			}
