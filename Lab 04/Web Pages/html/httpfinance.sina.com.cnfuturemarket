		<script type="text/javascript">
			(function(){
				jQuery.ajax({
					url:"//stock.finance.sina.com.cn/futures/api/openapi.php/GoodsIndexService.getLatestdata",
					dataType:"jsonp",
					success:function(d){
						var data=d.result.data;
						var arr=["BDI","GP","PS"];
						for(var i=0; i<arr.length; i++){
							var ele=jQuery(".spzs_item").eq(i).find("ul")
							var html=ele.html();
							
							html+='<li class="left">'+data[arr[i]].price+'</li>';
							if(data[arr[i]].zde>=0){
								html+='<li class="lired">'+parseFloat(data[arr[i]].zdf).toFixed(2)+'%</li>';
								html+='<li class="lired">'+parseFloat(data[arr[i]].zde).toFixed(2)+'</li>';
							}
							else{
								html+='<li class="ligreen">'+parseFloat(data[arr[i]].zdf).toFixed(2)+'%</li>';
								html+='<li class="ligreen">'+parseFloat(data[arr[i]].zde).toFixed(2)+'</li>';
							}
							
							ele.html(html);
						}
					}
				});
			
				/*jQuery(".spzs_item").mouseover(function(){
					//正在动画时，运动中的两个容器，display的值都是block，鼠标滑到了另一项上
					if(jQuery(".spzs_chart").is(":animated")==false){
						jQuery(".spzs_chart").not(jQuery(this).next()).slideUp(500);
						jQuery(this).next().slideDown(500);
					}
				});*/
				
				jQuery(".spzs_item").click(function(){
					//正在动画时，运动中的两个容器，display的值都是block，鼠标滑到了另一项上
					if(jQuery(".spzs_chart").is(":animated")==false){
						jQuery(".spzs_chart").not(jQuery(this).next()).slideUp(500);
						jQuery(this).next().slideDown(500);
					}
				});
			})();
		</script>
		<!--<script src="//finance.sina.com.cn/jiawei/js/nyanPass0.1.0.js"></script>-->
		<script src="//n.sinaimg.cn/finance/jdemo/nyanCharts.min.js"></script>
		<script type="text/javascript">
			~function(){
				function isClass(obj_) {
				if (obj_ === null) return "Null";
				if (obj_ === undefined) return "Undefined";
				return obj_.toString().slice(8, -1);
			}
				function merge(to_, from_, overwrite_) {
					if (!from_) return to_;
					if (!to_) to_ = {};
					for (var _prop in from_) {
						if (from_.hasOwnProperty(_prop)) {
							if (isClass(from_[_prop]) === 'Object') { // \u5982\u679C\u662F\u5BF9\u8C61\uFF0C\u9012\u5F52
								!to_[_prop] && (to_[_prop] = {});
								merge(to_[_prop], from_[_prop], overwrite_);
							} else if (overwrite_ || !(_prop in to_)) { // \u4E0D\u662F\u5BF9\u8C61
								to_[_prop] = from_[_prop];
							}
						}
					}
					return to_;
				}
				function numFormatter(pr_, p_) {
					pr_ = Number(pr_);
					if (isNaN(pr_)) return '-';
					var abs = Math.abs(pr_);
					if (abs < 1e5) return pr_.toFixed(p_);
					if (abs < 1e7) return (pr_ / 1e4).toFixed(p_) + '\u4E07';
					if (abs < 1e8) return (pr_ / 1e7).toFixed(p_) + '\u5343\u4E07';
					return (pr_ / 1e8).toFixed(p_) + '\u4EBF';
				}
				function map(obj, cb, context) {
					if (!(obj && cb)) {
						return;
					}
					if (obj.map && obj.map === map) {
						return obj.map(cb, context);
					} else {
						var result = [];
						for (var i = 0, len = obj.length; i < len; i++) {
							result.push(cb.call(context, obj[i], i, obj));
						}
						return result;
					}
				}
				function loader(options) {
					var varStr = options.varStr;
					var head = document.getElementsByTagName('head')[0],
						script = document.createElement('script'),
						success = options.success,
						callback = options.callback ? '&callback=' + varStr : '',
						url = options.url;

					callback = options.oneParams ? '?callback=' + varStr : callback;

					callback = options.jsoncallback ? '&jsoncallback=' + varStr : callback;

					script.src = url + callback;
					script.type = "text/javascript";
					script.onload = script.onreadystatechange = function() {
						if (!(this.readyState && "loaded" !== this.readyState && "complete" !== this.readyState)) {

							if (!callback) {
								success();
							}
							script.onload = script.onreadystatechange = null;
							head.removeChild(script);
						}
					};

					head.appendChild(script);
					if (callback) {
						window[varStr] = function(res) {
							if (success) {
								res.result ? success(res.result) : success(res);
							}
							delete window[varStr];
						};
					}
				}
				//初始化图
				function initChart(param) {
					return new RectAxisChart(merge({
						position: {
							width: 0.26,
							height: 0.78,
							left: 0.16,
							top: 0.04
						},
						xAxis: {
							axisLine: {
								show: false
							},
							tick: {
								show: false
							},
							text: {
//								rotate: -Math.PI * 0.08,
								style: {
									fillStyle: '#666'
								}
							},
							style: {
								strokeStyle: '#C43C38'
							},
							eachLen: 120
						},
						yAxis: {
							left: {
								show: false,
								axisLine: {
									show: false
								},
								tick: {
									show: false
								},
								name: {
									posY: -8
								}
							},
							right: {
								show: false,
								axisLine: {
									show: false
								},
								tick: {
									show: false
								},
								name: {
									posY: -8
								}
							}
						},
						line: {
							smooth: 'spline',
							dot: {
								show: true,
								r: 3,
								style: {
									lineWidth: 1,
									fillStyle: 'white'
									//strokeStyle: 'white',
								},
								animate: 1500
							},
							style: {
								//lineDash: [4, 3],
								//strokeStyle: '#C43C38'
							}
						},
						grid: {
							show: true,
							line: {
								type: 'y',
								style: {
									lineDash: [4, 3],
									strokeStyle: '#CEDDFA'
								}
							}
						},
						zoom: {
							show: false
						},
						legend: {
							show: false
						}
					}, param, true));
				}

				// 获取某date的x月前日期
				function getXMonthAgo(date, x) {
					var d = new Date(date);
					d.setMonth(d.getMonth() - x);

					return d.getFullYear() + '-' + (d.getMonth() + 1) + '-' + d.getDate();
				}

				var RectAxisChart = nyanCharts.RectAxisChart;
				function loadedDataDBI(){
					loader({
						url: '//stock.finance.sina.com.cn/futures/api/openapi.php/GoodsIndexService.get_goods_index?symbol=BDI&table=0&startdate=' + getXMonthAgo(new Date, 3),
						varStr: 'BDI_' + Math.floor(Math.random() * 10000),
						callback: true,
						success: function(oc) {
							var bdi_goods_chart = oc.data.data;
							var chart1 = initChart({
								tooltip: {
									show: true, //显示
									width: '120px', //悬浮框宽度
									font: '12px 微软雅黑', //悬浮框字体
									formatter: function (data) { //悬浮框内容
										var data1 = bdi_goods_chart[data.index];
										return '日期：' + data1.opendate
											+ '<br>价格: ' + data1.price
											+ '<br>涨跌额: ' + data1.zde
											+ '<br>涨跌幅：' + (data1.zdf * 100).toFixed(0) + '%';
									}
								},
								position: {
									width: 0.76,
									height: 0.80,
									left: 0.20,
									top: 0.08
								},
								domId: 'spzs_chart_01',
								xAxis: {
									axisLine: {
										show: false
									},
									text: {
										//rotate: Math.PI * 0.08,
										style: {
											fillStyle: '#666'
										}
									},
									style: {
										strokeStyle: '#C43C38'
									},
									eachLen: 120
								},
								yAxis: {
									left: {
										show: true,
										text: {
											style: {
												fillStyle: '#666'
											},
											formatter: function(text) {
												return numFormatter(text, 1);
											}
										},
										name: {
											text: ''
										}
									}
								},
								legend: {
									show: false
								}
							});
							chart1.addData({
								data: map(bdi_goods_chart, function(d) {
									return Number(d.price);
								}),
								axisArr: map(bdi_goods_chart, function(d) {
									return d.opendate;
								}),
								name: '',
								param: {
									style: {
										strokeStyle: '#336699'
									}
								},
								type: 'Line'
							});
						}
					});
				}
				loadedDataDBI();

				function loadedDataSOY(){
					loader({
						url: '//stock.finance.sina.com.cn/futures/api/openapi.php/GoodsIndexService.get_goods_index?symbol=SOY&table=0&startdate=' + getXMonthAgo(new Date, 3),
						varStr: 'SOY_' + Math.floor(Math.random() * 10000),
						callback: true,
						success: function(oc) {
							var soy_goods_chart = oc.data.data;

							var chart = initChart({
								position: {
									width: 0.76,
									height: 0.80,
									left: 0.20,
									top: 0.08
								},
								tooltip: {
									show: true, //显示
									width: '120px', //悬浮框宽度
