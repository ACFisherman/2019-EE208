                <script>
                     function gungun(positions){
                            
                            this.box=$(positions.self);
                            this.hoverHouse=positions.hoverHouse ? $(positions.hoverHouse) : this.box;
                            this.speed=positions.speed || 300;
                            this.interval=positions.interval || 5000;
                            this.runing=true;
                            this.childWidth=this.box.children(0).outerWidth(true);
                            this.leftBtn= $('.z-btn_left') ;
                            this.rightBtn=$('.z-btn_right') ;
                        }
                        gungun.timer=null;
                        gungun.prototype.clone=function(){
                            this.inner=this.box.html();
                            this.box.html(this.inner+this.inner);
                            this.box.width(this.childWidth*this.box.children().length);   
                        }

                        gungun.prototype.event=function(pro){
                            this.ow=this.box.width()/2;
                            var self=this;
                            if(this.runing){
                                this.runing=false;

                                if(pro=='right'){
                                   
                                    if(this.box.position().left<=-this.ow){
                                        this.box.css({left:0});
                                    }
                                    this.leftWidth=this.box.position().left-this.childWidth;
                                }else if(pro=='left'){
                                    
                                    if(this.box.position().left>=0){
                                        this.box.css({left:-this.ow});
                                    }
                                    this.leftWidth=this.box.position().left+this.childWidth;
                                }

                                this.box.stop(true,true).animate({left:this.leftWidth},this.speed,function(){

                                    self.runing=true;
                                })
                            }
                        }
                        gungun.prototype.cleartimer=function(){

                            clearTimeout(gungun.timer);
                        }
                        gungun.prototype.createtimer=function(){
                            var self=this;
                            this.cleartimer();
                            gungun.timer=setInterval(function(){
                                
                                 self.rightBtn.click();
                                
                            },this.interval)
                        }
                        
                        gungun.prototype.init=function(){
                            var self=this;
                            this.clone();
                            this.createtimer();
                            
                            this.leftBtn.bind('click',function(){
                                
                                self.event('left');
                            })
                            
                            this.rightBtn.bind('click',function(){
                                
                                self.event('right');
                            })
                            this.hoverHouse.hover(function() {
                                self.cleartimer()
                            }, function() {
                                self.createtimer()
                            });
                               
                        };
                        var ul1=new gungun({self:'.z-item_boss_ul',hoverHouse:'.z-sdpp-box'});
                        ul1.init();
                        var ul2=new gungun({self:'.works-list ul'});
                        ul2.init();
                        /* 十大品牌hover */
                    $(document).ready(function(){
                        $(".z-top10").delegate("li", "hover", function(){
                            $(this).siblings().removeClass("active");
                            $(this).addClass("active");
                        });
                        $('.z-top10').each(function(){

                             $(this).find('li').eq(0).addClass('active');
                        })
                       
                    });
                </script>
