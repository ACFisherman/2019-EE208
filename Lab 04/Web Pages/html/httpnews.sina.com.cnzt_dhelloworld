    $widgetadd({
      id: 'pl_weibotopic_conf_18',
      isDepend: true,
      loadTime: 'onVisibleReady',
      require: [],
      exports: function(node){
        var GLOBLE =  window._SINANEWSDATA.weibo;
        var config = '';
        for(var key in GLOBLE){
          config += '&'+key+'='+GLOBLE[key];
        }
        var iframe_width = !!GLOBLE.width ? GLOBLE.width : '100%';
        $('[node-type=content]',node).html('<iframe width="'+iframe_width+'" height="'+GLOBLE.height+'"  frameborder="0" scrolling="no" src="http://widget.weibo.com/livestream/listlive.php?language=zh_cn'+config+'"></iframe>')
      }
    });
	
	</script>

