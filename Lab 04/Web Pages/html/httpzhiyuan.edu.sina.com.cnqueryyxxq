    <script type="text" id="fsx_tmpl">
        <div class="query_result fenshuxian_result">
            <h2 class="result_title"><%=it.title%></h2>
            <div class="result_data">
                <div class="detailed_data">
                <%if(it.level==='1'){%>                    <div class="control_bar">
                        <span class="desc">显示项</span>
                        <span class="sub_ctrl sub_ctrl_first">
                                <input type="checkbox" checked="checked" disabled class="score_check"/>
                                分数
                            </span>
                        <span class="sub_ctrl">
                                <input type="checkbox" control=".fc" checked="checked" class="score_gap_check"/>
                                分差
                            </span>
                        <span class="sub_ctrl">
                                <input type="checkbox" control=".wc" checked="checked" class="rank_check"/>
                                位次
                            </span>
                        <span class="sub_ctrl">
                                <input type="checkbox" control=".xs" checked="checked" class="diff_check"/>
                                难度系数
                            </span>
                        <span class="sub_ctrl">
                                <input type="checkbox" control=".detailed_data_charts" checked="checked" class="chart_check"/>
                                曲线图
                            </span>
                        <span class="sub_ctrl">
                                <input type="radio" control=".type1" name="data_type_radio" class="type1Ctrl" />
                                安全线/门槛线
                                <input type="radio" control=".type2" name="data_type_radio" checked="checked" class="type2Ctrl" />
                                平均分/最低分
                            </span>
                    </div>
                    <table class="data_table detailed_data_table">
                        <tbody>
                            <tr>
                                <th class="empty"></th>
                                <%for(var i=0;i<it.year.length;i++){%>                                    <th><%=it.year[i]%></th>
                                <%}%>                            </tr>
                            <tr>
                                <td><%=it.table_type.batch_score%></td>
                                <%for(var year in it.data.batch_score){%>                                <td><%=it.data.batch_score[year]%></td>
                                <%}%>                            </tr>
                            <tr>
                                <td><%=it.table_type.enroll_max_score%></td>
                                <%for(var year in it.data.enroll_max_score){%>                                <td><%=it.data.enroll_max_score[year]%></td>
                                <%}%>                            </tr>
                            <tr class="type2">
                                <td><%=it.table_type.enroll_avg_score%></td>
                                <%for(var year in it.data.enroll_avg_score){%>                                <td><%=it.data.enroll_avg_score[year]%></td>
                                <%}%>                            </tr>
                            <tr class="type2">
                                <td><%=it.table_type.enroll_min_score%></td>
                                <%for(var year in it.data.enroll_min_score){%>                                <td><%=it.data.enroll_min_score[year]%></td>
                                <%}%>                            </tr>
                            <tr class="type1">
                                <td><%=it.table_type.enroll_safe_score%></td>
                                <%for(var year in it.data.enroll_safe_score){%>                                <td><%=it.data.enroll_safe_score[year]%></td>
                                <%}%>                            </tr>
                            <tr class="type1">
                                <td><%=it.table_type.enroll_threshold_score%></td>
                                <%for(var year in it.data.enroll_threshold_score){%>                                <td><%=it.data.enroll_threshold_score[year]%></td>
                                <%}%>                            </tr>
                            <tr class="type1 type2 fc">
                                <td><%=it.table_type.fc_max_score%></td>
                                <%for(var year in it.data.fc_max_score){%>                                <td><%=it.data.fc_max_score[year]%></td>
                                <%}%>                            </tr>
                            <tr class="type2 fc">
                                <td><%=it.table_type.fc_avg_score%></td>
                                <%for(var year in it.data.fc_avg_score){%>                                <td><%=it.data.fc_avg_score[year]%></td>
                                <%}%>                            </tr>
                            <tr class="type2 fc">
                                <td><%=it.table_type.fc_min_score%></td>
                                <%for(var year in it.data.fc_min_score){%>                                <td><%=it.data.fc_min_score[year]%></td>
                                <%}%>                            </tr>
