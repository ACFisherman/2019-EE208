        });
            
        var a = dataids.join(",");
        function cmtNum(data){
            var numberlist= data.result.count;
            for(var o in numberlist){
                var str = numberlist[o].total == 0 ? "评论" : numberlist[o].total;
                $("[newslist='"+o+"']").find('[comment-num="comment"]').text(str);      
            }
        }
        $.ajax({
            url: "http://comment5.news.sina.com.cn/cmnt/count?format=json&newslist="+a,
            dataType: "jsonp",
            jsonp: 'callback', 
            jsonpCallback: "cmtNum",
            success: function(data){
                cmtNum(data);
            }
        });
