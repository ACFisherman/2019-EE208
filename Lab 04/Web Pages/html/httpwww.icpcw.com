<script type="text/javascript">
    var context = {
        swicthPart1: null,
        swicthPart2: null
    };
    $("body").ready(function () {
        $(".tabNewsContent>div:gt(0)").hide();
        $(".tabnewstitle a[data]").css("cursor", "pointer").click(function () {
            $(".tabNewsContent>div").hide();
            $(".tabNewsContent>div[data='" + $(this).attr("data") + "']").show();
        });
        $('[data="searchBox"] li').click(function () {
            if ($(this).hasClass("unSelectedText")) {
                $('[data="searchBox"] .selectedText').removeClass("selectedText").addClass("unSelectedText");
                $(this).removeClass("unSelectedText").addClass("selectedText");
            }
        })
        $('[data="searchBox"] #sercharTxt').keypress(function () {
            if (event.keyCode == 13) {
                $('[data="searchBox"] #sercharBtn').click();
            }
        });
        $('[data="searchBox"] #sercharBtn').click(function () {
            var txt = $("#sercharTxt").val();
            var type = $('[data="searchBox"] .selectedText').html();
            if (type == "图片") {
                location.href = "/SearchPic/?" + encodeURI(txt)
            }
            else {
                location.href = "/Search/?" + encodeURI(txt)
            }
        });
        context.swicthPart1 = new picSwitchBox({
            container: $(".part1"),
            name: "part1"
        });
        context.swicthPart2 = new picSwitchBox({
            container: $(".part2"),
            name: "part2",
            switchCallBack: function (i) {
                $(".picture-switch-hander .current").removeClass("current");
                $(".picture-switch-hander li:eq(" + i + ")").addClass("current");
            }
        });
        for (var i = 0; i < context.swicthPart2.pictures; i++) {
            $(".picture-switch-hander ul").append("<li></li>");
        }
        $(".picture-switch-hander li").click(function () {
            context.swicthPart2.showPicture($(this).index());
        }).first().addClass("current");
        $(".handlerbox").click(function () {
            var direction = $(this).hasClass("left") ? "left" : "right";
            if (direction == "left") {
                if (context.swicthPart1.index > 0) {
                    context.swicthPart1.showPicture(context.swicthPart1.index - 1);
                }
            } else if (direction == "right") {
                if (context.swicthPart1.index + 1 < context.swicthPart1.pictures) {
                    context.swicthPart1.showPicture(context.swicthPart1.index + 1);
                }
            }
        });

    });
</script>
