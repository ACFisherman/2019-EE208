    <script>
        window.nav_list = {
            nav_0 : "", // 热门及其他
            nav_1 : "travel-nav-guide", // 攻略
            nav_2 : "travel-nav-des", // 目的地
            nav_3 : "travel-nav-theme", // 主题游
            nav_4 : "travel-nav-tourist", // 旅行家
            nav_5 : "travel-nav-hot", // 热门
            nav_6 : "travel-nav-discover", // 发现
            nav_7 : "travel-nav-flight", // 航空
            nav_8 : "travel-nav-hotel", // 酒店
            nav_9 : "travel-nav-photo" // 图片
        }

        function checkUrl(str) {
            if(str == "http://travel.sina.com.cn") return 0;
            else if(str.indexOf("gonglue/") != -1) return 1;
            else if(str.indexOf("lvyou/") != -1) return 2;
            else if(str.indexOf("zhuti/") != -1) return 3;
            else if(str.indexOf("sheying/") != -1) return 3;
            else if(str.indexOf("qinzi/") != -1) return 3;
            else if(str.indexOf("miyue/") != -1) return 3;
            else if(str.indexOf("youlun/") != -1) return 3;
            else if(str.indexOf("gaoerfu/") != -1) return 3;
            else if(str.indexOf("meishi/") != -1) return 3;
            else if(str.indexOf("gouwu/") != -1) return 3;
            else if(str.indexOf("zijia/") != -1) return 3;
            else if(str.indexOf("lvxingjia/") != -1) return 4;
            else if(str.indexOf("-remen/") != -1) return 5;
            else if(str.indexOf("-faxian/") != -1) return 6;
            else if(str == "http://travel.sina.com.cn/air/") return 7;
            else if(str == "http://travel.sina.com.cn/hotel/") return 8;
            else if(str == "http://vi.travel.sina.com.cn/") return 9;
            else return 0;
        }
        var initial_url = window.location.href;
        var nav_index = checkUrl(initial_url);
        var nav_value = nav_list["nav_" + nav_index];
        if(nav_value != "") {
            var ele = document.getElementById(nav_value);
            //ele.setAttribute("class", nav_value" on");
            if(typeof(ele) == 'object' && ele != null){
                ele.className += ' on ';
            }
        }
    </script>


