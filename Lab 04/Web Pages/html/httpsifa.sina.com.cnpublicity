			var day		= all_data_news.data[i_news]['create_date'].split("-")[2];
			var hour	= all_data_news.data[i_news]['create_time'].split(":")[0];
			var min		= all_data_news.data[i_news]['create_time'].split(":")[1];
			var url		= all_data_news.data[i_news]['url'];
			var title	= all_data_news.data[i_news]['title'];
			var media		= all_data_news.data[i_news]['media'];
			var comment_url	= all_data_news.data[i_news]['comment_url'];
		
			html_news = html_news + "<li><a href='"+url+"' target='_blank'><span class='num'>"+j_news+"</span><div class='ranking_list_title OverFlow'>"+title.substr2(0,60)+"</div></li>";
		
		
		}
		$(document).ready(function(){
			if (html_news!='') {
				$(".ranking_list").html(html_news);
			}
