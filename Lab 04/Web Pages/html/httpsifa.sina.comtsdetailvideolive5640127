<script type="text/javascript">

	var zhan_flag = 0;
	$(document).ready(function() {
		
		var blogId = $('#blogId').val();
		
		if (zhan_flag == 0) {
			$('.live-operate').find('._prasie_on').removeClass('icon-vote-on');
		}
		else {
			$('.live-operate').find('._prasie_on').addClass('icon-vote-on');
		}
		
		// 生成手机观看二维码
		var url = "http://sifa.sina.com/ts/detail/video/live/5640127";
		
		//二维码手机观看地址
		var ishowurl = toUtf8(url);
		
		/* 如果是ie8 则用table，如果 */
		var version =  browse.getVersion();
		if (version == 'IE8.0') {
			$('._mobile_code').qrcode({
				render: 'table',
				width: 136, //宽度 
			    height: 136, //高度 
			    correctLevel : 0,
			    text: ishowurl //任意内容
			});
		}
		else {
			$('._mobile_code').qrcode({
				render: 'canvas',
				width: 136, //宽度 
			    height: 136, //高度 
			    correctLevel : 0,
			    text: ishowurl //任意内容
			});
		}
		
		function toUtf8(str) {    
		    var out, i, len, c;    
		    out = '';    
		    len = str.length;    
		    for(i = 0; i < len; i++) {    
		        c = str.charCodeAt(i);    
		        if ((c >= 0x0001) && (c <= 0x007F)) {    
		            out += str.charAt(i);    
		        } else if (c > 0x07FF) {    
		            out += String.fromCharCode(0xE0 | ((c >> 12) & 0x0F));    
		            out += String.fromCharCode(0x80 | ((c >>  6) & 0x3F));    
		            out += String.fromCharCode(0x80 | ((c >>  0) & 0x3F));    
		        } else {    
		            out += String.fromCharCode(0xC0 | ((c >>  6) & 0x1F));    
		            out += String.fromCharCode(0x80 | ((c >>  0) & 0x3F));    
		        }    
		    }    
		    return out;    
		}
		
		// 视频播放列表第一个选中。
		var idx = $('._case_list').find("li[id='5640127']").index();
		if (idx != -1) {
			$('._case_list').find("li").eq(idx).addClass('on choosed').siblings('li').removeClass('on choosed');
		}
		
		// 点击视频列表页面刷新
		$('._case_list').find('li').click(function() {
			
			var url = $(this).attr("url");
			window.location.href = url;
		});
		
		//点赞效果
		
		//点赞效果
		$('.live-operate').off('click', '.icon-vote').on("click",".icon-vote",function(){
			
			//var praval = $.cookie('WEIBOCASEPRASIE');
			
// 			var vals = null;
// 			if (praval) {
// 				vals = praval.split('_');
// 			}
			
// 			var self=$(this);
			
			// 0 点赞， 1 取消赞
// 			var prasie_type = (vals == null || vals[0] != blogId) ? 0 : 1;
			
			if ($(this).hasClass("off")) {
				return;
			}
			else {
				$(this).addClass('off');
			}
			
			var self=$(this);
			$.ajax({
				url: "http://sifa.sina.com/ts/detail/prasie/live/5640127",
				type: 'get',
				dataType: 'jsonp',
				data: {'type': zhan_flag, 'blogId': $('#blogId').val()}
			}).success(function(data) {
				
				if (data == 'error') {
					alert('案件可能已经不存在了');
					return;
				}
				//变更 完后，标志变为1
				if (zhan_flag == 0) {
					zhan_flag = 1;
					self.addClass("icon-vote-on");
					self.parent().find("span").html(data);
				}
				else {
					zhan_flag = 0;
					self.removeClass("icon-vote-on");
					self.parent().find("span").html(data);
				}
				self.removeClass('off');
				
			}).error(function(xhr, textstatus, error) {
				console.log(xhr, textstatus, error);
			});
		});
		
		// 视频观看数量
		getcourtnum.courtNum('0', '3555691');
		
		var liveId = '0';
		var videoId = '3555691';
		
		// 获取点赞数
		$.ajax({
			url: "http://sifa.sina.com/ts/detail/getPrasie",
			type: 'get',
			dataType: 'jsonp',
			data : {'liveId':liveId, 'videoId' : videoId}
		}).success(function(data) {
			
			if (data != null) {
				$('._prasie').text(data);
			}
			
		}).error(function(xhr, textstatus, error) {
			console.log(xhr, textstatus, error);
		});
		
		// 分享插件使用本系统的appkey
		var jiathis_config = {appkey:{"tsina":"3052661848"}};
		
		$(".scroll").niceScroll({
	       touchbehavior:false,
	       cursorwidth:20,
	       cursorborder:"0px",
	       cursorborderradius:"3px",
	       background:"#444",
	       autohidemode:"true",
	       cursorcolor: "#868686",
	    });
		
		if($("._case_list li").size() > 0){
			//div个数
			var liCount = $("._case_list li").size();
			//div index
			var liIndex = $("._case_list").find("li[id='5640127']").index();
			//获得	
			var scrollDivHight = $("._case_list").height();
			
			var y = (scrollDivHight * liIndex)/liCount ; 
			$(".scroll").getNiceScroll(0).doScrollLeft(0, 1); 
			$(".scroll").getNiceScroll(0).doScrollTop(y, 1);
		}

		
		
// 		$(".scroll").getNiceScroll(0).doScrollTop(srcollTop, 10);
	
		// 二维码关闭
		$('._close_code').click(function(event) {
			$(this).parents('._code_div').fadeOut();
			event.stopPropagation();
		});
		
		// 显示二维码
		$('._code_phone').click(function(event) {
			$(this).find('._code_div').fadeIn();
			event.stopPropagation();
			event.preventDefault();
		});
	});
	
</script>

<script type="text/javascript">
<!-- 获取浏览器版本 -->
var browse = (function() {
	return {
		getVersion: function() {
			var userAgent = navigator.userAgent,   
		     rMsie = /(msie\s|trident.*rv:)([\w.]+)/,   
		     rFirefox = /(firefox)\/([\w.]+)/,   
		     rOpera = /(opera).+version\/([\w.]+)/,   
		     rChrome = /(chrome)\/([\w.]+)/,   
		     rSafari = /version\/([\w.]+).*(safari)/;
		     var browser;  
		     var version;  
		     var ua = userAgent.toLowerCase();  
		     function uaMatch(ua) {  
		         var match = rMsie.exec(ua);  
		         if (match != null) {  
		             return { browser : "IE", version : match[2] || "0" };  
		         }  
		         match = rFirefox.exec(ua);  
		         if (match != null) {  
		             return { browser : match[1] || "", version : match[2] || "0" };  
		         }  
		         match = rOpera.exec(ua);  
		         if (match != null) {  
		             return { browser : match[1] || "", version : match[2] || "0" };  
		         }  
		         match = rChrome.exec(ua);  
		         if (match != null) {  
		             return { browser : match[1] || "", version : match[2] || "0" };  
		         }  
		         match = rSafari.exec(ua);  
		         if (match != null) {  
		             return { browser : match[2] || "", version : match[1] || "0" };  
		         }
		         if (match != null) {  
		             return { browser : "", version : "0" };  
		         }  
		     }  
		     var browserMatch = uaMatch(userAgent.toLowerCase());  
		     if (browserMatch.browser) {  
		         browser = browserMatch.browser;  
		         version = browserMatch.version;  
		     }  
		     return browser+version; 
		}
	}
})();

/**
 * 获取当前案件的播放数量
 */
var getcourtnum = (function() {
	return {
		courtNum : function(liveId, videoId) {
			if (videoId > 0) {
				$.ajax({
					url: 'http://click.videoincloud.com/getcountnumber?videoid=' + videoId,
					type: 'get',
					dataType : 'jsonp'
				}).success(function(result) {
					$("._view_count").text(result);
				}).error(function(xhr, textstatus, error) { 
					console.log(xhr, textstatus,error);
				});
			}
			else {
				var liveCount = 0;
				$.ajax({
					url: 'http://click.videoincloud.com/getcountnumber?liveid=' + liveId,
					type: 'get',
					dataType : 'jsonp'
				}).success(function(result) {
					$("._view_count").text(result);
				}).error(function(xhr, textstatus, error) {
					console.log(xhr, textstatus,error);
				});
			}
		}
	}	
})();
</script>

