    // JavaScript Document
    //  加入收藏 <a onclick="AddFavorite(window.location,document.title)">加入收藏</a>

    function AddFavorite(sURL, sTitle)
    {
        try
        {
            window.external.addFavorite(sURL, sTitle);
        }
        catch (e)
        {
            try
            {
                window.sidebar.addPanel(sTitle, sURL, "");
            }
            catch (e)
            {
                alert("请使用Ctrl+D进行添加");
            }
        }
    }            
</script>
