<script type="text/javascript">
  seajs.use('$', function ($) {
    var $win = $(window);
    var followBar = $('#followBar');
    var followBarClose = followBar.find('#followBarClose');

    followBarClose.click(function () {
      followBar.css({'display': 'none'});
    });

    var toTopBtn = $('#toTop');
    toTopBtn.click(function () {
      $win.scrollTop(0);
    });

    $win.bind('scroll resize', function () {
      if ($win.scrollTop() === 0) {
        toTopBtn.css('display', 'none');
      } else {
        toTopBtn.css('display', 'block');
      }
    });
  });
</script>
