<script type="text/javascript">
seajs.use('$',
function($) {
    var searchType1 = new DivSelect('slt_01'),
    selectType = $('#slt_01'),
    form = $('#J_SearchForm'),
    key = $('#J_Search_Cnt'),
    items = $('#items');
    function placeholderIE(input) {
        var toggleTip = function() {
            defaultValue = input.defaultValue;
            $(input).addClass('gray').val(text);
            input.onfocus = function() {
                if (input.value === defaultValue || input.value === text) {
                    this.value = '';
                    $(input).removeClass('gray')
                }
            };
            input.onblur = function() {
                if (input.value === '') {
                    this.value = text;
                    $(input).addClass('gray')
                }
            }
        };
        var text = input.getAttribute('placeholder');
        if (!text) {
            if (input.attributes && input.attributes.placeholder) {
                text = input.attributes.placeholder.value
            }
        };
        toggleTip()
    };
    var supportPlaceholder = 'placeholder' in document.createElement('input'),
    changeKey = function(obj) {
        var action = $(obj).find("option:selected").data('action');
        var index = $(obj).get(0).selectedIndex;
        var keyname = $(obj).find("option:selected").data('searchstring');
        var itemvalue = $(obj).find("option:selected").data('list');

        var p = $(obj).find("option:selected").data('p');
        var s = $(obj).find("option:selected").data('s');
        if(!!p){
         $('.keyinput input[name=p]').val(p)
        }else{
          $('.keyinput input[name=p]').val('data')
        }
        if(!!s){
         $('.keyinput input[name=s]').val(s)
        }else{
          $('.keyinput input[name=s]').val('default')
        }

        var placeholder = '\u8BF7\u8F93\u5165' + $(obj).find("option:selected").text() + '\u5173\u952E\u8BCD';
        form.attr('action', action);
        items.val(itemvalue);
        key.attr('name', keyname).attr('placeholder', placeholder);
        if (index > 0) {
            form[0].acceptCharset = 'utf-8';
            form[0].onsubmit = function() {
                document.charset = 'utf-8'
            }
        } else {
            form[0].acceptCharset = 'gbk';
            form[0].onsubmit = function() {
                document.charset = 'gb2312'
            }
        } ! supportPlaceholder && placeholderIE(key[0])
    };
    window.changeKey = changeKey;
    changeKey($('#slt_01'))
    window.placeholderIE = placeholderIE; ! supportPlaceholder && placeholderIE(key[0]);
    form.submit(function() {
        var defaultValue = key.attr('placeholder');
        if (!key.val() || key.val() == defaultValue) {
            alert(defaultValue);
            key.focus();
            return false
        }
    })
})
