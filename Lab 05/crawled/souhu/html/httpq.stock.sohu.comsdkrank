<script type="text/template" id="profitRankTpl">
{{ data.forEach(function(val,i){ }}
	{{ var personUrl = "/sdk/performance?email=" + val.email; }}
	{{ if(i%3 == 0){ }}
	<div class="split-line"></div>
	{{ } }}
	<div class="right-itemBlock">
		<a href="{{= personUrl }}">
			<div class="rankIndex-box">
				<span class="rankIndex">{{= val.index }}</span>
			</div>
			<div class="left-one">
				<img src="{{= val.imageUrl }}" alt="头像">
				<p class="username">{{= val.name }}</p>
				{{ if(val.concern == 1){ }}
				<span class="btn-focus btn-disable" data-email='{{= val.email }}'>已关注</span>
				{{ }else{ }}
				<span class="btn-focus" data-email='{{= val.email }}'>关注</span>
				{{	} }}
			</div>
			<div class="right-one">
				<div class="right-item">
					<p class="grey">收益</p>
					<p class="{{= window.commonFunc.getClassNameByValue(val.profit) }} profitVal">{{= val.profit }}</p>
					<img src="//d1.biz.itc.cn/jlp/{{= val.email }}/jlp_{{= period }}.png" alt="收益" class="pic" onerror="javascript:this.src='//k.sohu.com/static/node-stock/v1-0-4/_common/images/nopic.gif'">
				</div>
				<div class="right-item right-item-bottom">
					<span>
						<p class="{{= window.commonFunc.getClassNameByValue(val.winningProbability) }}">{{= val.winningProbability }}</p>
						<p class="grey">胜率</p>
					</span>
					<span>
						<p>{{= val.changeHands }}</p>
						<p class="grey">换手</p>
					</span>
					<span>
						<p class="{{= window.commonFunc.getClassNameByValue(val.greatestReward) }}">{{= val.greatestReward }}</p>
						<p class="grey">近60日回撤</p>
					</span>
				</div>
			</div>
		</a>	
	</div>
	
{{ }); }}
</script>
