<script type="text/javascript">
	var addBookmark = function(url){
    url = url || window.location.href;
    var title = document.title;
    if (window.sidebar) {
        window.sidebar.addPanel(title, url, "");
    } else if (window.ActiveXObject) {
        window.external.AddFavorite(url, title);
    }
}
function fortune_hq(o){
	for(it in o){
		if(typeof swichIT[it] == 'function'){
			swichIT[it](o[it])
		}
	}
};
function Commet(url,it){
	this.itv = null;
	this.url = url || null;
	this.it = it || 60*1000;
	this.running = false;
};
Commet.prototype = {
	//有待优化
	request:function(url){
		var url = url || this.url
		$.getScript(url)
	},
	heatBeat:function(){
		var instance = this;
		instance.request(instance.url);
		this.itv = setInterval(function(){
			instance.request(instance.url)
		},instance.it)
		this.running = true;
	},
	stop:function(){
		clearInterval(this.itv);
		this.running = false;
	}
}

function rankList(o){
	var str = ['<tr><th>股票代码</th><th>股票名称</th><th>买入金额/占比</th><th>卖出金额/占比</th></tr>'];
	$.each(o,function(i){
		var d = o[i];
		str.push('<tr><td>' + d[1] + '</td><td><p class="rank-name"><a href="' + d[6] + '">' + d[0] + '</a></p></td><td><p class="rank-progress"><span class="red">' + d[2] + '</span><span style="width:' + parseInt(d[3]*100/2) + 'px;" class="pro-type"></span><span class="gray">' + (d[3]*100).toFixed(2) + '%</span></p></td><td><p class="rank-progress"><span class="green">' + d[4] + '</span><span style="width:' + parseInt(d[5]*100/2) + 'px;" class="pro-type"></span><span class="gray">' + (d[5]*100).toFixed(2) + '%</span></p></td></tr>');
	})
	$('.stock-ranking tbody').html(str.join(''))
}
function rankTop(o){
	var arr = [];
	$.each(o,function(i){
		var d = o[i],r = d.RankRange;
		$.each(r,function(j){
			arr.push(r[j].RankList);
		})
	})
	$.each($('.lj-moeny-tb tbody'),function(i){
		var str = [],a = arr[i];
		str.push('<tr class="head">' + $(this).children('.head').html() + '</tr>');
		str.push('<tr class="head">' + $($(this).children('.head')[1]).html() + '</tr>')
		$.each(a,function(j){
			var d = a[j],c = parseFloat(d[3]) > 0?'red':parseFloat(d[3]) < 0?'green':'';
			str.push('<tr><td class="t1"><p class="stock-name"><a title="' + d[0] + '" target="_blank" href="' + d[5] + '">' + d[0] + '</a></p></td><td class="t2"><span>' + d[1] + '</span></td><td class="t3"><span>' + d[2] + '</span></td><td class="t5"><span class="' + c + '">' + d[4].toFixed(2) + '</span></td><td class="t4"><span class="' + c + '">' + (d[3]*100).toFixed(2) + (d[3] == 0?'':'%') + '</span></td></tr>')
		})
		$(this).html(str.join(''))
	})
}
function industryName(o){
	$('.right-tb h3').html(o + ' - 资金流向TOP5 <span>万元</span>');
}
function marketTop(id){
	$.getScript('//ushq.stock.sohu.com/AFundFlow/' + id + '.html')
}
function time(o){
	var o = o + '';
	$('.hy-money div.time').html(o.substring(0,4) + '-' + o.substring(4,6) + '-' + o.substring(6,8) + '&nbsp;' + o.substring(8,10) + ':' + o.substring(10,12))
}
function ranktime(o){
	var o = o + '';
	$('.moeny-row-name span.time').html(o.substring(0,4) + '-' + o.substring(4,6) + '-' + o.substring(6,8) + '&nbsp;' + o.substring(8,10) + ':' + o.substring(10,12))
}

