<script>
    require(['jquery', 'global.fun', 'app/tplEngine', 'app/unitTool', 'app/lazyload', 'superSlide'], function ($, global, tplEngine, unitTool, lazyload) {
        //点击活动type按钮
        $('.hot-event-warp [data-type]').click(function () {
            var type = $(this).data('type');
            var liLen = $('.' + type + '-event-box').find('li').length;
            $(this).addClass('on').siblings().removeClass('on');
            $('.event-scorll').addClass('none');
            $('.' + type + '-event-scorll').removeClass('none');
        });

        //展开更多
        $('.hot-event-warp .load-hot-event-more').click(function () {
            var type = $(this).closest('.event-box').data('type');
            $('.' + type + '-event-scorll').css({maxHeight: 'none'});
            $(this).closest('.load-' + type + '-event-wrap').hide();
        });

        //试用公告
        $.get('/api/event/getBulletins', {}, function (repalyData) {
            var len = unitTool.getLength(repalyData.result);
            if (len) {
                var tplFunCache = tplEngine.init($('#event-notice-tpl').html()),
                    html = tplFunCache({data: repalyData.result});
                $('.notice-bd ul').html(html);
                $(".event-notice-wrap").show();
                $(".event-notice-wrap").slide({
                    mainCell: ".notice-bd ul",
                    effect: "topLoop",
                    autoPlay: true,
                    autoPage: true,
                    interTime: 4000
                });
            } else {
                $(".event-notice-wrap").hide();
            }
        }, 'json');

    });
</script>

