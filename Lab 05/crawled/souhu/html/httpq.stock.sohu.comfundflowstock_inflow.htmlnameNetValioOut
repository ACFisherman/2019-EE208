<script type="text/javascript">
var addBookmark = function(url){
    url = url || window.location.href;
    var title = document.title;
    if (window.sidebar) {
        window.sidebar.addPanel(title, url, "");
    } else if (window.ActiveXObject) {
        window.external.AddFavorite(url, title);
    }
}
function jsReady(){
	return true;
}
function fortune_hq(o){
	for(it in o){
		if(typeof swichIT[it] == 'function'){
			swichIT[it](o[it])
		}
	}
};
function Commet(url,it){
	this.itv = null;
	this.url = url || null;
	this.it = it || 60*1000;
	this.running = false;
};
Commet.prototype = {
	//有待优化
	request:function(url){
		var url = url || this.url
		$.getScript(url)
	},
	heatBeat:function(){
		var instance = this;
		instance.request(instance.url);
		this.itv = setInterval(function(){
			instance.request(instance.url)
		},instance.it)
		this.running = true;
	},
	stop:function(){
		clearInterval(this.itv);
		this.running = false;
	}
}

var ph = (function(){
	var paras = document.URL.split('?')[1];
	if(!paras) return;
	function getValue(name){
		var start = paras.indexOf(name + '='),end = (paras.indexOf('&',start)>-1?paras.indexOf('&',start):paras.length);
		if(start < 0) return null;
		start = start + name.length + 1;
		return paras.substring(start,end)
	}
	function getURL(n,i,t){
		return '//ushq.stock.sohu.com/AFundFlow/' + n + i + t + '.html';
	}
	return {
		cmt:new Commet(),
		name:getValue('name'),
		io:getValue('io'),
		time:getValue('time') || '1Day',
		currentTag:null,
		currentStock:null,
		formatData:function(o){
			var d = o,flcjtj,zzjlx,myChart;
			var DataXML = '<chart animation="0" bgColor="0xffffff" BorderColor="0xffffff" showPercentageInLabel="0" showValues="0" showLabels="0" showLegend="0" baseFontSize="14"><set color="0xFF9999" label="\u8D85\u5927\u4E70\u5355" value="' + d[16] + '" /><set color="0xF44D4D" label="\u5927\u4E70\u5355" value="' + d[17] + '" /><set color="0xC5050D" label="\u4E2D\u4E70\u5355"  value="' + d[18] + '" /><set color="0x6F0202" label="\u5C0F\u4E70\u5355"  value="' + d[19] + '" /><set color="0x7FEA79" label="\u8D85\u5927\u5356\u5355" value="' + d[20] + '" /><set color="0x1DB915" label="\u5927\u5356\u5355" value="' + d[21] + '" /><set color="0x107E0A" label="\u4E2D\u5356\u5355"  value="' + d[22] + '" /><set color="0x014202" label="\u5C0F\u5356\u5355"  value="' + d[23] + '" /></chart>';
			$('.right .title').html(d[0] + '(' + d[1] + ')');
			if(window.ActiveXObject){
				flcjtj = window['flcjtj'];
				zzjlx = window['zzjlx'];
			}else{
				flcjtj = document['flcjtj'];
				zzjlx = document['zzjlx'];
			}
			flcjtj.setData(d[8] +  ',' + d[9] + ',' + d[10] + ',' + d[11] +  ',' + d[12] + ',' + d[13] + ',' + d[14] + ',' + d[15]);
			zzjlx.setData(d[6] +  ',' + d[7]);
			myChart = new FusionCharts( "Pie2D.swf", "myChartId", "235", "140");
			myChart.setXMLData(DataXML);
			myChart.render("chartContainer");
		},
		init:function(){
			var instance = this,io = instance.io,name = instance.name,str;
			instance.currentTag = $('.inflow-menu li[c=' + instance.time + ']');
			instance.currentTag.addClass('current');
			instance.cmt.url = getURL(instance.name,instance.io,instance.time)
			instance.cmt.heatBeat();
			
			if(io == 'In'){
				switch(name){
					case 'NetVal':str = '个股净流入排行';break;
					case 'CorpVal': str = '机构增仓排行';break;
					case 'Rate': str = '个股加速流入排行';break;
				}
			}else{
				switch(name){
					case 'NetVal': str = '个股净流出排行';break;
					case 'CorpVal': str = '机构减仓排行';break;
					case 'Rate': str = '个股加速流出排行';break;
				}
			}
			$('.left .headerInfo').html('[' + str + ']')
			
			$('.inflow-menu li').click(function(){
				instance.time = $(this).attr('c');
				instance.currentTag.removeClass('current')
				$(this).addClass('current');
				instance.currentTag = $(this);
				instance.currentStock = null;
				
				instance.cmt.url = getURL(instance.name,instance.io,instance.time);
				instance.cmt.request();
			})
		},
		rankList:function(o){
			var str = [],c;
			$.each(o,function(i){
				var d = o[i],c = parseFloat(d[4]) > 0?'red':parseFloat(d[4]) < 0?'green':'';
				str.push('<tr field="' + i + '"><td class="tc">' + (i+1) + '</td><td class="tc"><a title="' + d[0] + '" href="' + d[24] + '">' + d[1] + '</a></td><td class="tc">' + d[0] + '</td><td>' + d[2] + '</td><td><span class="' + c + '">' + d[3].toFixed(2) + '</span></td><td><span class="' + c + '">' + (d[4] * 100).toFixed(2) + (d[4] == 0?'':'%') + '</span></td><td>' + d[5] + '</td></tr>')
			})
			$('.inflow-tb tbody').html(str.join(''));
			if(!ph.currentStock){
				c = 0;
				$('.inflow-tb tr')[1].className = 'tb-bluebg';
				ph.currentStock = $($('.inflow-tb tr')[1]);
			}else{
				c = parseInt(ph.currentStock.attr('field'));
				$('.inflow-tb tr')[c+1].className = 'tb-bluebg';
				ph.currentStock = $($('.inflow-tb tr')[c+1]);
			}
			setTimeout(function(){
				ph.formatData(o[c]);
			},500)
			
			$(".inflow-tb tr").mouseover(function(){
				if(ph.currentStock && this != ph.currentStock[0]){
					$(this).addClass("tb-bluebg");
				}
			}).mouseout(function(){
				if(ph.currentStock && this != ph.currentStock[0]){
					$(this).removeClass("tb-bluebg");
				}
			}).click(function(){
				var c = $(this).attr('field'),d = o[c];
				ph.currentStock.removeClass('tb-bluebg');
				ph.currentStock = $(this);
				ph.currentStock.addClass('tb-bluebg')
				ph.formatData(d);
			})
		}
	}
})();
ph.init();
function time(o){
	var o = o + '';
	$('.left p.headerTime').html(o.substring(0,4) + '-' + o.substring(4,6) + '-' + o.substring(6,8) + '&nbsp;' + o.substring(8,10) + ':' + o.substring(10,12))
};
var swichIT = {
	RankList: ph.rankList,
	Time:time
}

</script>
