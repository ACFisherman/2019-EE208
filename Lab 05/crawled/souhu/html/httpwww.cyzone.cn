<script>
    template.defaults.rules[1].test = /{#([@#]?)[ \t]*(\/?)([\w\W]*?)[ \t]*#}/;
    function goToAction(action, action_url) {
        return action == 'shop_detail' ? window.open('/product/show_shop?' + action_url ):
            action == 'shop_search_list' ?  window.open('/search/search/index?' +
                action_url) : action == 'web_url' || action == 'activity_url' || action == 'ad_detail' ||
            action ==
            'news_detail' ?  window.open(action_url) : '';
    }
    function getQueryString(name) {
        //截取url的数据
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return decodeURIComponent(r[2]);
        return null;
    }

    $(function () {

        $('.js-settled').click(function () {
            if ($.cookie('user_id')) {
                window.location.href = '/content/settled/index';
            } else {
                goToLogin();
            }
        });

        function msgInit() {
            // https://api1.cyzone.cn/message/getUnReadCount
            var api_url = window.TOP_API + 'message/getUnReadCount';
            $.cors(api_url, {}, function (json) {
                if (json.code == 0) {
                    if (json.data.num > 0) {
                        $('.js-msg i').show();
                    } else {
                        $('.js-msg i').hide();
                    }

                }
            }, 'json', 'GET');
        };

        msgInit();

        $('.js-msg').click(function () {
            if ($.cookie('user_id')) {
                window.location.href = '/member/index/msg';
            } else {
                goToLogin();
            }
        });
        // 全局搜索  https://api1.cyzone.cn/v1/shop/product/getDynamicView
        //https://devapi2.cyzone.cn/v1/shop/product/getDynamicView
        var api_1 = 'https://api1.cyzone.cn/v1/shop/product/getDynamicView';
        if (getQueryString('action') == 'test') {
            api_1 = 'https://devapi2.cyzone.cn/v1/shop/product/getDynamicView'
        }
        function getSearchDyData() {
            $.post(api_1, {
                menu_id: 31
            }, function (res) {
                var dynamicList = [];
                for (var k = 0; k < res.data.dynamic_data.length; k++) {
                    dynamicList = dynamicList.concat(res.data.dynamic_data[k].data);
                }
                var html = template('list_box', {
                    data: dynamicList
                });
                $('.dynamic-view').html(html);
            });
        }
        $('.js-search-btn-box').click(function () {
            $('.global-search').fadeIn();
            $('.search-input').focus();
        });
        $('.js-search-close').click(function () {
            $('.global-search').fadeOut();
        });
        $('.search-input').keydown(function (event) {
            if (event.keyCode == 13) {
                if ($('.search-input').val()) {
                    window.location.href= "/search/search/index?wd="+$('.search-input').val();
                }
            }
        });

        $('.js-search-btn').click(function () {
            if ($('.search-input').val()) {
                window.location.href= "/search/search/index?wd="+$('.search-input').val();
            }
        });

        getSearchDyData();
    });
</script>





