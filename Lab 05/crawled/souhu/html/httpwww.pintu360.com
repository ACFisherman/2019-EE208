    function getAdvertCookie(name)
    {
        var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
        if(arr=document.cookie.match(reg))
            return unescape(arr[2]);
        else
            return null;
    }
    function sendAdvertRequest(dataId,flag,type,articleId){
        var datas = {fnName:'addAdvertClick',id:dataId,flag:flag};
        $.ajax({
            type: "post",
            url: "../service/article_comment_agree_service.php",
            dataType: "json",
            data:datas,
            success: function (data) {
            }, error: function (error) {
            }
        });
        if(articleId){
            setCookie(type,articleId+","+dataId,60*60*24);
        }else{
            setCookie(type,dataId,60*60*24);
        }
    }
