        function logout(){
            $.ajax({
                type: "post",
                url: "../service/ajax_login_service.php",
                cache: false,
                async:false,
                dataType: "json",
                data: {
                    fnName: "logout"
                },
                success : function(data){
                    if(data){
                        delCookie("client_name");
                        delCookie("head_image");
                        delCookie("login_stated");
                        delCookie("token_stated");
                        delCookie("uid");
                        var sourceURL = parseURL(window.location.href);
                        var path = sourceURL.path.split("/");
                        if(path[path.length - 1] == 'personalCenter.php'
                            || path[path.length - 1] == 'myComment.php'
                            || path[path.length - 1] == 'myMessage.php'
                            || path[path.length - 1] == 'myCollect.php'
                            || path[path.length - 1] == 'myContribute.php'){
                            window.location.href = "/";
                        }else{
                            window.location.href = window.location.href;
                        }
                    }
                }
            });
        }
        document.onkeydown = function(eventTag){
            var event =  eventTag||window.event;
            var currentKey = event.charCode||event.keyCode;
            if(currentKey === 13 || currentKey === 108){
                document.getElementById("go_search").click();
            }
        };
        function search() {
            var keywords = $("#search_keywords").val();
            if(keywords == ''){
                return;
            }
            $("#search_keywords").val('');
            window.location.href = "search.php?q=" + keywords;
        }
        $(function () {
            if(getCookie("login_stated") === 'true') {
                getClientMessageCount();
            }
            if(getCookie("login_stated") === 'true') {
                window.setInterval(function () {
                    getClientMessageCount();
                }, 100000);
            }
        })
        function getClientMessageCount(){
            $.ajax({
                async:false,
                type: "post",
                url: "../controller/client_info_controller.php",
                dataType: "json",
                data:{fnName:"getUser_notifyCount"},
                success: function (data) {
                    if (data && data.total > 0) {
                        $(".red_circle").addClass("current")
                        $(".message").find("i").text('('+data.total+')');
                        clientTotalMessage = data.total;
                        if(data.total >=100){
                            $(".message").find("i").text('(n+)');
                        }
                    }
                }, error: function (error) {
                    //alert("出错啦");
                }
            })
        }
