<script type="text/javascript">
function strLen(str){
	var i = l = 0;    
	if(str == null) return l;
	        for( ; i<str.length; i++){
				if( str.charCodeAt( i ) > 255 ){
				   l += 2;
				}else{
				   l++;
				}		   
		    }
    return l; 		
}
</script>

<script type="text/javascript">
jQuery(document).ready(function() {
	 var url = "http://dj.shxc.gov.cn/CMS/client/faq_getAllFAQ.action";
     var type = 8;
     var params = {"type":type};
     $.ajax({				
				async: false,
				type : "GET",
				dataType : "jsonp", 
				jsonp : "callback",
				data : params,
				timeout : 5000,
				url : url,
				success : function(msgs) {
				   if(msgs.faqList.length>0){
				      for(var i=0;i<msgs.faqList.length;i++){
				         var obj= document.getElementById('myfaq');
				         var t_c = msgs.faqList[i].titleContent;
				         var r_c = msgs.faqList[i].replyContent;	         
				         if(t_c != "" && jmz.GetLength(msgs.faqList[i].titleContent) > 30){
				            obj.innerHTML = '<h4><a href="http://dj.shxc.gov.cn//dj///layout/FaqBoardList.shtml">'+t_c.substring(1,30)+'...<a></h4>';				           				         				            				            
				         }else{
				            obj.innerHTML = '<h4><a href="http://dj.shxc.gov.cn//dj///layout/FaqBoardList.shtml">'+t_c+'<a></h4>';	
				         }
				         if(r_c != "" && jmz.GetLength(msgs.faqList[i].replyContent) > 35){
				            obj.innerHTML  = obj.innerHTML + '<p>答：'+r_c.substring(1,35)+'...</p>';
				         }else{
				            obj.innerHTML  = obj.innerHTML + '<p>答：'+r_c+'</p>';
				         }                       		           				         				            
                         break;
				      }
				   }
				}
			});
});
var jmz = {};

jmz.GetLength = function(str) {   
    var realLength = 0, len = str.length, charCode = -1;
    for (var i = 0; i < len; i++) {
        charCode = str.charCodeAt(i);
        if (charCode >= 0 && charCode <= 128) realLength += 1;
        else realLength += 2;
    }
    return realLength;
};
</script>
