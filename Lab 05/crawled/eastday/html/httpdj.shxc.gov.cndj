function updateInfo(value){
    var url = "http://dj.shxc.gov.cn/XCBORG/front/user_updateUserAvator.action";
    var params = {"name":value[0],"orgId":value[2],"personId":value[3]};
    var img = '';
	$.ajax({
			async : false,
			type : "GET",
			dataType : "jsonp",
			jsonp : "callback",
			cache : false,
			timeout : 5000,
			url : url,
			data : params,
			success : function(data) {
				if(data.message=="SUCCESS"  ){	
					 img = data.img;
				}
			}
		});
	return img;
}
