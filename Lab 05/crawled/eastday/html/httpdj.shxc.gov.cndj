<script type="text/javascript">
$(function() {
	var $tab = $('#tabs1');
	var $tabLinks = $('#tabs1').find('a');
	var index = 0;
	var timer;
	function tabStart() {
		timer = setInterval(function() {
			$tabLinks.eq(index).click();
			index++;
			if(index > $tabLinks.length) index = 0;
		}, 3000);
	}
	function tabStop() {
		clearInterval(timer);
	}
	$('#tabs1').hover(tabStop, tabStart);
	$('#tabContents1').hover(tabStop, tabStart);
	tabStart();
});
</script>
