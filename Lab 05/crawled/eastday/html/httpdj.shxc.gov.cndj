function changeValidateCode(obj) {
	//获取当前的时间作为参数，无具体意义   
	var timenow = new Date().getTime();
	//每次请求需要一个不同的参数，否则可能会返回同样的验证码   
	//这和浏览器的缓存机制有关系
	obj.src = "http://dj.shxc.gov.cn/SSOServer/SSO/SSO_validationCode.action?d=" + timenow;
}

function logout(){
	var url = "http://dj.shxc.gov.cn/SSOServer/SSO/SSOServer_doLogout.action";
	var params = null;
	$.ajax({
		async : false,
		type : "GET",
		dataType : "jsonp",
		jsonp : "callback",
		cache : false,
		timeout : 5000,
		url : url,
		data : params,
		success : function(msg) {
			if(msg.message=="SUCCESS"  ){
				alert("登出成功");
				location.reload(); 
			}
		}
	});
	
}
