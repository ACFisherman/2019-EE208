function validate(){
	var username = $.trim(jQuery("#username").val());
	var password = $.trim(jQuery("#password").val());
	var vcode = $.trim(jQuery("#validateCode").val());
	var name_len = strLen(username);
	var pwd_len = strLen(password);
	var code_len = strLen(vcode);
	
	if(name_len == 0){
		alert("用户名不能为空!");
		return 0;
	}
	
	if(pwd_len == 0){
		alert("密码不能为空!");
		return 0;
	}
	
	if(code_len == 0){
		alert("验证码不能为空!");
		return 0;
	}
	return 1;
}

function strLen(str){
	var i = l = 0;    
	if(str == null) return l;
	        for( ; i<str.length; i++){
				if( str.charCodeAt( i ) > 255 ){
				   l += 2;
				}else{
				   l++;
				}		   
		    }
    return l; 		
}
