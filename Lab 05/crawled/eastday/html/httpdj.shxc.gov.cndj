<script type="text/javascript">
	var jq=jQuery.noConflict();
	jq(document).ready(function (){
		//jCarousel Plugin
	    jq('#carousel').jcarousel({
			vertical: true,
			scroll: 1,
			auto: 2,
			wrap: 'circular',
			initCallback: mycarousel_initCallback
	   	});

	//Front page Carousel - Initial Setup
   	jq('div#slideshow-carousel a img').css({'opacity': '0.5'});
   	jq('div#slideshow-carousel a img:first').css({'opacity': '1.0'});
   	jq('div#slideshow-carousel li a:first').append('<span class="arrow"></span>')

  
  	//Combine jCarousel with Image Display
    jq('div#slideshow-carousel li a').hover(
       	function () {
       		if (!jq(this).has('span').length) {
        		jq('div#slideshow-carousel li a img').stop(true, true).css({'opacity': '0.5'});
   	    		jq(this).stop(true, true).children('img').css({'opacity': '1.0'});
       		}		
       	},
       	function () {
        		
       		jq('div#slideshow-carousel li a img').stop(true, true).css({'opacity': '0.5'});
       		jq('div#slideshow-carousel li a').each(function () {

       			if (jq(this).has('span').length) jq(this).children('img').css({'opacity': '1.0'});

       		});
        		
       	}
	).click(function () {
	    jq('span.arrow').remove();        
		jq(this).append('<span class="arrow"></span>');
		window.open(jq(this).attr('href'));
       	//jq('div#slideshow-main li').removeClass('active');        
       	//jq('div#slideshow-main li.' + jq(this).attr('rel')).addClass('active');	
        	
       	return false;
	});
});


//Carousel Tweaking
function mycarousel_initCallback(carousel) {
	
	// Pause autoscrolling if the user moves with the cursor over the clip.
	carousel.clip.hover(function() {
		carousel.stopAuto();
	}, function() {
		carousel.startAuto();
	});
}
</script>
