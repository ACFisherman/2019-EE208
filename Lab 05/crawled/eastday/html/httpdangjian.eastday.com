var apage=Math.ceil($("#slide_win li").length-3);
	var page=1;
	var show_width=241;
	$("#next").click(function(){
		if(!$("#slide_win ul").is(":animated")){
			if(page==apage){
				$("#slide_win ul").animate({left:'0px'},"slow");
				page=1;
			}else{
				$("#slide_win ul").animate({left:'-='+show_width},"slow");
				page++;
			}
		}
	});
	$("#prev").click(function(){
		if(!$("#slide_win ul").is(":animated")){
			if(page==1){
				$("#slide_win ul").animate({left:'-='+show_width*(apage-1)},"slow");
				page=apage;
			}else{
				$("#slide_win ul").animate({left:'+='+show_width},"slow");
				page--;
			}
		}
	});
