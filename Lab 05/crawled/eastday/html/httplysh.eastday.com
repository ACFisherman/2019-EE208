        function GetSpotWeatherResult()
        {
            $.ajax({
                //url: "http://localhost:4909/api/GetSpotWeatherResult",
                url:"http://lysh.eastday.com/lyj/WebApiService/api/GetSpotWeatherResult",
                type: "get",
                dataType: "json",
                success: function (data) {
                    if (data.state == 0) {
                        alert(data.message);
                    }
                    else {

                        $("#spotWeatherList").html("");

                        for (var i = 0; i < data.swList.length; i++) {
                            var elm = "<li>";

                            var name = data.swList[i].Name;
                            if (name.indexOf("东方绿舟")<0)
                            {
                                elm += "<h2>" + data.swList[i].Name + "</h2>";                                
                            }
                            else
                            {
                                elm += "<h2>东方绿舟</h2>";
                            }                            

                            //判断天气图标显示
                            var items = data.swList[i].Des.split('转');
                            if (items.length > 1)
                            {
                                elm += "<p><img src='/images/lyj/JDWeather/Image/Icon/" + GetWeatherIcon(items[0]) + "' /><br><img src='/images/lyj/JDWeather/Image/Icon/" + GetWeatherIcon(items[1]) + "' /></p>";
                            }
                            else
                            {
                                elm += "<p><img src='/images/lyj/JDWeather/Image/Icon/"+GetWeatherIcon(items[0])+"' class='one' /></p>"
                            }

                            elm += "<p class='grey12a'>" + data.swList[i].High + "~" + data.swList[i].Low + "℃<br />" + data.swList[i].Des + "<br />" + data.swList[i].Direction + data.swList[i].Power + "</p>"
                            elm += "</li>";
                            $("#spotWeatherList").append(elm);
                        }                       

                    }
                }
            });
        }
    </script>


