<script language="javascript">
        $(document).ready(function () {
            var len = $(".w_cnt li").length;
            var s_len = len % 4;
            var num_len = parseInt(len / 4);
            var inum_len = 1;
            var move_len = 4 * 116 + "px";
            var flag = 0;
            $("#next").click(function () {
                if (!$(".w_cnt").is(":animated")) {
                    inum_len++;
                    if (inum_len == num_len) {
                        move_len = s_len * 116 + "px";
                        inum_len = num_len;
                        $("#next").hide();
                        $("#nextoff").show();
                        flag = 1;
                    }
                    else {
                        move_len = 4 * 116 + "px";
                    }
                    $(".w_cnt").animate({ left: '-=' + move_len }, "slow");
                    $("#prevoff").hide();
                    $("#prev").show();
                }
            });
            $("#prev").click(function () {
                if (!$(".w_cnt").is(":animated")) {
                    move_len = 4 * 116 + "px";
                    inum_len--;
                    if (inum_len == 1 && flag == 1) {
                        inum_len == 1;
                        move_len = s_len * 116 + "px";
                        $("#prev").hide();
                        $("#prevoff").show();
                        flag = 0;
                    }
                    else {
                        move_len = 4 * 116 + "px";
                    }
                    $(".w_cnt").animate({ left: '+=' + move_len }, "slow");
                    $("#nextoff").hide();
                    $("#next").show();

                }
            });
        });

