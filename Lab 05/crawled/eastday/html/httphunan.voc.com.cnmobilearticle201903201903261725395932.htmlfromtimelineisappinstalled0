				uid = data.uid;
				username = data.username;
				if((data.data != "" && uid != 0)){
					$.each(data.data.posts, function(i,item){	
						if(item.tid !== ""){
							logincontent += '<li><a class="cmt-title"><strong class="cmt-name"><span class="t_ucc_link">'+item.author+'</span></strong><time class="">'+item.dateline+'</time></a><div class="cmt-content">'+item.content+'</div></li>';
