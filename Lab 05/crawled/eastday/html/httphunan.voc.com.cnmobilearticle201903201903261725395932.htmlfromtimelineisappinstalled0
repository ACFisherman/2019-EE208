				$(".cmt-list").html(logincontent);
				$.getJSON("https://cgi.voc.com.cn/app/mobile/push.php?action=get_bbs_reply&aid="+_BbsTid+"&num=10&status=1&jsoncallback=?", function(data){
					$(".cmt-list").append(data.content);
