$(function(){
    var m = $("meta[name='liveappInfo']");
    var wxpic1 = "http://img1.voc.com.cn/UpLoadFile/2019/03/26/201903261725332003.jpg";
    if(wxpic1 == "http://img1.voc.com.cn/UpLoadFile/"){
        wxpic1 = "";
    }
    var wxpic2 = "http://m.voc.com.cn/images/wx_lg.jpg";
    var shareTitle = document.title,
    shareUrl = window.location.href,
    shareDesc = "",
    shareImg = wxpic1 || wxpic2;

    $.ajax({
        url: "https://cgi.voc.com.cn/api/weixin/sample.php",
        dataType: 'jsonp',
        jsonp: 'jsonpcallback',
        success: function (data) {
            $.each(data, function (index, result) {
                if (result['status'] == 1) {
                    var appId = result['appId'],
          timestamp = result['timestamp'],
          nonceStr = result['nonceStr'],
          signature = result['signature'];

                    wx.config({
                        debug: false,
                        appId: appId,
                        timestamp: timestamp,
                        nonceStr: nonceStr,
                        signature: signature,
                        jsApiList: [
              'onMenuShareTimeline',
              'onMenuShareAppMessage',
              'onMenuShareQQ',
              'onMenuShareWeibo'
                        ]
                    });
                }
            });
        }
    });

    wx.ready(function () {
        wx.onMenuShareAppMessage({
            title: shareTitle,
            desc: shareDesc,
            imgUrl: shareImg,
            link: shareUrl,
            success: function (res) {

            }
        });

        wx.onMenuShareTimeline({
            title: shareTitle,
            imgUrl: shareImg,
            link: shareUrl,
            success: function (res) {

            }
        });

        wx.onMenuShareQQ({
            title: shareTitle,
            desc: shareDesc,
            imgUrl: shareImg,
            link: shareUrl,
            success: function (res) {

            }
        });

        wx.onMenuShareWeibo({
            title: shareTitle,
            desc: shareDesc,
            imgUrl: shareImg,
            link: shareUrl,
            success: function (res) {

            }
        });
    });
});

