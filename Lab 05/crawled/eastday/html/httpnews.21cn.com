<script type="text/javascript">
    var picId = 0;
    function setTab(m,n){
        var tli=document.getElementById("menu"+m).getElementsByTagName("li");
        var mli=document.getElementById("main"+m).getElementsByTagName("li");
        for(i=0;i<tli.length;i++){
            tli[i].className=i==n?"hover":"off";
            mli[i].style.display=i==n?"block":"none";
        }
    }
    //排行榜接口
    $.ajax({
        url:'http://review.21cn.com/review/topn.do?operationId=9406&topn=16',
        type:'post',
        async:false,
        dataType:'jsonp',
        timeout:3000,
        jsonp:'jsoncallback',
        success:function(data){
            var html1 ='';
            var html2='';
            $("#ranklogo").css("display","none");
            if(data.length == null || data.length=="" || data.length <= 15){
                $("#rank").css("display","none");
                $("#tuj").css("display","block");
            }else{
                for(var i=0;i<8;i++){
                    html1 +='<p><a class="_block_news_rank" href="'+data[i].contentUrl+'">'+data[i].contentTitle+'</a></p>';
                }
                for(var i=8;i<16;i++){
                    html2 +='<p><a class="_block_news_rank" href="'+data[i].contentUrl+'">'+data[i].contentTitle+'</a></p>';
                }
                $("#hotRank1").html(html1);
                $("#hotRank2").html(html2);
                $("#rank").css("display","block");
            }
        },
        complete : function(XMLHttpRequest,status){ //请求完成后最终执行参数
            if(status=='timeout'){
                $("#ranklogo").css("display","none");
                $("#rank").css("display","none");
                $("#tuj").css("display","block");
            }
        }
    });

    //焦点图
    $(".handle").each(function(index, element) {
        $(this).click(function(e) {
            var thePdiv=$(this).parent(".slider-item");
            if(!thePdiv.hasClass("current")){
                $("#slider .current").animate({"width":"50px"},300,function(){$(this).removeClass("current")});
                thePdiv.animate({"width":"680px"},300,function(){$(this).addClass("current")});
            }
        });
    });

    $(".hover-text").find("li").bind("mouseenter",function(e){
        $(this).find("p").animate({bottom:"0"},"fast");
    })
    $(".hover-text").find("li").bind("mouseleave",function(e){
        $(this).find("p").animate({bottom:"-60px"},"fast");
    })

    jQuery.fn.FocusIMG = function (opt) {
        var jQueryThis = $(this);
        var set = {
            showPreNext:true, //是否展示上一页下一页
            showTitle:true, //是否展示标题
            timespan:5000,  //时间间隔
            scrollBox:null,
            onChanngeEnd:function(i){}
        }
        $.extend(set, opt);
        var sWidth = jQueryThis.width(); //获取焦点图的宽度（显示面积）
        if(set.scrollBox!=null){
            sWidth=set.scrollBox.width();
        }
        var len = $("ul li", jQueryThis).length; //获取焦点图个数
        var index = 0;
        var picTimer;
        set.onChanngeEnd(0);
        //以下代码添加数字按钮和按钮后的半透明条，还有上一页、下一页两个按钮
        var btn = "<div class='btn'>";
        for (var i = 0; i < len; i++) {
            var curClass = '';
            if (i == 0) {
                curClass = 'cur';
            }
            btn += "<span class='" + curClass + "'></span>";
        }
        btn += "</div><div class='preNext pre'></div><div class='preNext next'></div>";
        jQueryThis.append(btn);
        //本例为左右滚动，即所有li元素都是在同一排向左浮动，所以这里需要计算出外围ul元素的宽度
        $("ul", jQueryThis).css("width", sWidth * (len));
        //标题
        if (!set.showTitle) {
            $("h3", jQueryThis).css("visibility", "hidden");
            jQueryThis.hover(function () {
                $("h3", jQueryThis).css("visibility", "visible");
            }, function () {
                $("h3", jQueryThis).css("visibility", "hidden");
            });
        }
        //上一页、下一页按钮透明度处理
        if (!set.showPreNext) {
            $(".preNext", jQueryThis).css("opacity", "0");
            jQueryThis.hover(function () {
                $(".preNext", jQueryThis).css("opacity", "0.7");
            }, function () {
                $(".preNext", jQueryThis).css("opacity", 0);
            });
        } else {
            $(".preNext", jQueryThis).css("opacity", "0.4").mouseenter(function () {
                $(this).stop(true, false).animate({"opacity":"0.7"}, 300);
            }).mouseout(function () {
                $(this).stop(true, false).animate({"opacity":"0.4"}, 300);
            });
        }
        if (/*Modernizr.touch*/false) {
            // if (true) {
            //iScroll for touch version
            var myScroll;
            var domId = $(this).attr('id'),scroll_domId=domId;
            if(set.scrollBox!=null){
                scroll_domId = set.scrollBox.attr('id');
            }
            var myScroll = new iScroll(scroll_domId, {
                vScroll:true,
                snap:true,
                momentum:false,
                hScrollbar:false,
                onScrollEnd:function () {
                    var currentLI = document.getElementById(domId).querySelector('div.btn > span:nth-child(' + (this.currPageX + 1) + ')');
                    if (currentLI) {
                        document.getElementById(domId).querySelector('div.btn > span.cur').className = '';
                        currentLI.className = 'cur';
                        set.onChanngeEnd(this.currPageX);
                    }
                }
            });
            //上一页按钮
            $(".pre", jQueryThis).click(function () {
                index -= 1;
                if (index == -1) {
                    index = len - 1;
                }
                myScroll.scrollToPage('prev', 0);
            });

            //下一页按钮
            $(".next", jQueryThis).click(function () {
                index += 1;
                if (index == len) {
                    index = 0;
                }
                myScroll.scrollToPage('next', 0);
            });
        } else {
            //pc version
            //为小按钮添加鼠标滑入事件，以显示相应的内容
            $(".btn span", jQueryThis).mouseenter(function () {
                index = $(".btn span", jQueryThis).index(this);
                showPics(index);
            });
            //上一页按钮
            $(".pre", jQueryThis).click(function () {
                index -= 1;
                if (index == -1) {
                    index = len - 1;
                }
                showPics(index);
            });

            //下一页按钮
            $(".next", jQueryThis).click(function () {
                index += 1;
                if (index == len) {
                    index = 0;
                }
                showPics(index);
            });
            //鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
            jQueryThis.hover(function () {
                clearInterval(picTimer);
            },function () {
                picTimer = setInterval(function () {
                    index++;
                    if (index == len) {
                        index = 0;
                    }
                    showPics(index);
                }, set.timespan);
            }).trigger("mouseleave");

            //显示图片函数，根据接收的index值显示相应的内容
            function showPics(index) { //普通切换
                var nowLeft = -index * sWidth; //根据index值计算ul元素的left值
                $("ul", jQueryThis).stop(true, false).animate({"left":nowLeft}, 300); //通过animate()调整ul元素滚动到计算出的position
                $(".btn span", jQueryThis).removeClass('cur').eq(index).addClass('cur'); //为当前的按钮切换到选中的效果
                set.onChanngeEnd(index);
            }
        }
        return this;
    }
    $('#focus-image').FocusIMG({});

    //图片
    $("#new-pic-list").css("width",170*$("#new-pic-list li").length);
</script>

