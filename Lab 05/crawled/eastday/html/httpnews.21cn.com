	window.open('http://passport.21cn.com/api/bind/request.do?accountType=wechat&display=json2&saveLogin=5184000&serviceId=0&nickNameGenType=autoGen&returnUrl='+sina_a+'')
});

if(Cookie.get("SSON") && Cookie.get("SSONKEY") != 'null'){
	$.ajax({
		url:'http://passport.21cn.com/api/getUser.do?jsoncallback=?',
		dataType:'json',
		timeout:10000,
		error:function () {
			return false;
		},
		success:function (data) {
			$('.login_new').hide();
			$('.exit_login').show();
			$('#exit_login').attr("href",'http://passport.21cn.com/api/logout.do?returnUrl='+sina_a+'')
			$('.already_login').html('欢迎您，'+'<a style="float:none; display:inline;" href="http://e.189.cn">'+data.account.screenName+'</a>');
			if(( /^(13|15|18|17)+[0-9]{9}$/.exec(data.account.userName))
				|| (/^(13|15|18|17)+[0-9]{9}@189\.cn$/.exec(data.account.userName))){
				$('#free_mail').attr('href','http://mail.189.cn/webmail/jsp/189misc/fromCloudL.jsp');
			}else{
				$('#free_mail').attr('href','http://mail.21cn.com');
			}
		}
	});
}
});
