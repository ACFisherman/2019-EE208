		<script type="text/javascript">
		//判断用户是否注册／登录
		//判断用户是否已实名认证
		function FastRegistration(that){
			$.ajax({
                type: 'POST',
                url: 'CheckMemberRegister',
                data: {id:that},
                dataType:'json',
                success: function (data) {
                	if(data.data == "1"){
                		layer.open({
            				  type: 2,
              				  title: '实名认证',
              				  shadeClose: false,
              				  shade: 0.8,
              				  area: ['360px', '480px'],
              				  content: "au_realname_personal_hswb" //iframe的url
          				});
                	}
                	if(data.data == "2"){
                		top.layer.alert('实名认证正在审核中，请等待实名通过!', {
				    		skin: 'layui-layer-molv', //样式类名
							closeBtn: 0
						});
	                	return false;
                	}
                	if(data.data == "3"){
            			layer.open({
            				  type: 2,
            				  title: '登录注册',
            				  shadeClose: true,
            				  shade: 0.8,
            				  area: ['360px', '480px'],
            				  content: "au_register_hswb" //iframe的url
            			});
                	}
                	if(data.data == "4"){
                		layer.alert('你已完成报名，可以去下载元素，上传作品!', {
    			    		skin: 'layui-layer-molv', //样式类名
    						closeBtn: 0
    					});
                    	return false;
                	}
                },
                error: function(data){
                	layer.alert('请刷新页面重新报名!', {
			    		skin: 'layui-layer-molv', //样式类名
						closeBtn: 0
					});
                	return false;
                },
			});
		}
		//判断用户是否注册
		//判断用户是否已实名认证
		function FastRegistrationRegister(that){
			$.ajax({
                type: 'POST',
                url: 'CheckMemberRegister',
                data: {id:that},
                dataType:'json',
                success: function (data) {
                	if(data.data == "1"){
                		layer.open({
            				  type: 2,
              				  title: '实名认证',
              				  shadeClose: false,
              				  shade: 0.8,
              				  area: ['360px', '480px'],
              				  content: "au_realname_personal_hswb" //iframe的url
          				});
                	}
                	if(data.data == "2"){
                		top.layer.alert('实名认证正在审核中，请等待实名通过!', {
				    		skin: 'layui-layer-molv', //样式类名
							closeBtn: 0
						});
	                	return false;
                	}
                	if(data.data == "3"){
            			layer.open({
            				  type: 2,
            				  title: '登录注册',
            				  shadeClose: true,
            				  shade: 0.8,
            				  area: ['360px', '480px'],
            				  content: "au_register_hswb" //iframe的url
            			});
                	}
                	if(data.data == "4"){
                		layer.alert('你已完成报名，可以去下载元素，上传作品!', {
    			    		skin: 'layui-layer-molv', //样式类名
    						closeBtn: 0
    					});
                    	return false;
                	}
                },
                error: function(data){
                	layer.alert('请刷新页面重新报名!', {
			    		skin: 'layui-layer-molv', //样式类名
						closeBtn: 0
					});
                	return false;
                },
			});
		}
		//判断用户是否登录
		//判断用户是否已实名认证
		function FastRegistrationSignIn(that){
			$.ajax({
                type: 'POST',
                url: 'CheckMemberRegister',
                data: {id:that},
                dataType:'json',
                success: function (data) {
                	if(data.data == "1"){
                		layer.open({
            				  type: 2,
              				  title: '实名认证',
              				  shadeClose: false,
              				  shade: 0.8,
              				  area: ['360px', '480px'],
              				  content: "au_realname_personal_hswb" //iframe的url
          				});
                	}
                	if(data.data == "2"){
                		top.layer.alert('实名认证正在审核中，请等待实名通过!', {
				    		skin: 'layui-layer-molv', //样式类名
							closeBtn: 0
						});
	                	return false;
                	}
                	if(data.data == "3"){
            			layer.open({
            				  type: 2,
            				  title: '登录注册',
            				  shadeClose: true,
            				  shade: 0.8,
            				  area: ['360px', '480px'],
            				  content: "au_sign_in_hswb" //iframe的url
            			});
                	}
                	if(data.data == "4"){
                		layer.alert('你已完成报名，可以去下载元素，上传作品!', {
    			    		skin: 'layui-layer-molv', //样式类名
    						closeBtn: 0
    					});
                    	return false;
                	}
                },
                error: function(data){
                	layer.alert('请刷新页面重新报名!', {
			    		skin: 'layui-layer-molv', //样式类名
						closeBtn: 0
					});
                	return false;
                },
			});
		}
		</script>
		<!-- 文件管理使用提示 -->
		<script>
		function showInform(){
	      	document.getElementById("doubtLayer_div").style.display='block';
	         // document.getElementById("inform").css("display","block");
	      }
	      //隐藏悬浮层
	      function hiddenInform(event){
	    	  document.getElementById('doubtLayer_div').style.display='none';
	      }
		</script>
