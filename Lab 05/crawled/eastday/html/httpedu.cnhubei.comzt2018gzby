	<script type="text/javascript">

	$(function() {

		var speed=500;



		$("#box li").hover(function() {

			$(this).stop().animate({width:"748px"},speed).siblings().stop().animate({width:55},speed)// 当鼠标移动到li上，当前变600 其余的兄弟变90

		}, function() {

			$("#box li").stop().animate({width:"120px"},speed)

			// 鼠标离开的时候，快速复原

		});





	});

	</script>
