                    alert('您已经提交过，请勿重复提交!')
                }else if(data.statusCode == 302){
                    alert('您的姓名包含敏感词,请修改后再提交')
                }else{
                    alert('提交失败！');
                }

            },
            error: function (e, a, d) {
                alert('目前网络不可用!');
                return false;
            }
        });

    }
    function Load_ina_ad_position(){var d=$(".ina_ad_pos");for(var a=0;a<d.length;a++){var c=d[a].id;var b=document.getElementById(c).value;if(b!=""){$("#pos_"+c).html(b)}}};//end ad
    setTimeout("Load_ina_ad_position()", 2000);

</script>


