<script type="text/javascript">

	/**********************基层党建内部资料下载登入***************************/
function downloadLoginValidate(url,username, password,vcode, callback) {
	
	var data = {
			"username" : username,
			"password" : password,
			"vcode" : vcode
		};
	$.ajax({
		async : false,
		type : "GET",
		dataType : "jsonp",
		jsonp : "callback",
		cache : false,
		timeout : 5000,
		url : url,
		data : "username=" + username + "&password=" + password+"&vcode="+vcode,
		success : function(msg) {
			if(msg.message=="2"){
				//alert("登录成功 ,返回首页！");
				//$.cookie( "DownloadUser" ,  msg.message, { expires: 1, path: '/' });
				var date = new Date();  
	            date.setTime(date.getTime() + (1 * 1000 * 60 * 15)); //15分钟
	            //date.setTime(date.getTime() + (10 * 1000)); //10秒
	            $.cookie( "DownloadUser" ,  msg.message, { expires: date, path: '/' });
				
				 //$.cookie("DownloadUser", null);
				 callback(msg);
				
			}else if(msg.message=="0"){
				//alert("用户名或者密码错误！");
				callback(msg); //前台更改样式
			}else{
				//alert("验证码错误！");
				callback(msg); //前台更改样式
			}
		}
	});
}

	var ifLogin=null;
		$(document).ready(function(){	
			//alert("documentready");
