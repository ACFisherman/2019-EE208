		jQuery('#userPhoto').attr('src',  getUserInfo('idCard') );
		//jQuery("#userinfo").show();
	}
		
		
	function checkFalse( ){
		//alert("checkFalse");
		//jQuery("#userinfo").hide();
		ifLogin=false;
