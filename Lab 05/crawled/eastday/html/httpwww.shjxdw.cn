<script type="text/javascript">
	 $(document).ready(
	 	function(){
	 	    var url = "http://cms.shjxdw.cn//client/Daily_readDaily.action";
	 	    var params = null;
	 		$.ajax({				
				async: false,
				type : "GET",
				dataType : "jsonp",
				jsonp : "callback",
				data : params,
				timeout : 5000,
				url : url,
				success : function(msgs) {
				   if(msgs.Message == "SUCEESS"){				   	
				   		var result = msgs.Result;
				   		var array = result.split("-");
				   		$("#motto").empty();
				   		$("#unit").empty();
				   		var motto = document.getElementById("motto");
				   		var unit = document.getElementById("unit");
				   		var daily_motto = array[0];
				   		motto.innerHTML = "<p style="+"font-family:'宋体';font-weight:bold;font-size:16px>"+daily_motto+"</p>";
				   		unit.innerHTML = array[1]+"&nbsp;"+"("+array[2]+")";
				   }		
				}
			});
	 	});
</script>


