		function changeValidateCode(obj) {
			//获取当前的时间作为参数，无具体意义   
			var timenow = new Date().getTime();
			//每次请求需要一个不同的参数，否则可能会返回同样的验证码   
			//这和浏览器的缓存机制有关系
			obj.src = "http://sso.shjxdw.cn/SSO/SSO_validationCode.action?d=" + timenow;
		}
		function changeValidateCodeClient(obj) {
			//获取当前的时间作为参数，无具体意义   
			var timenow = new Date().getTime();
			//每次请求需要一个不同的参数，否则可能会返回同样的验证码   
			//这和浏览器的缓存机制有关系
			obj.src = "http://cms.shjxdw.cn//client/SSO_validationCode.action?d=" + timenow;
		}
		function login(){
					var username = jQuery("#username").val();
					var password = jQuery("#password").val();
					var vcode = jQuery("#vcode").val();
					var saveLogin= jQuery("#saveLogin").attr("checked");
		 			
			 		ssologin(encodeURIComponent(username),password,vcode,saveLogin,loginBack);
		}
		function downloadLogin(){
				var username = jQuery("#usernameD").val();
				var password = jQuery("#passwordD").val();
				var vcode = jQuery("#vcodeD").val();
				
				var url = "http://cms.shjxdw.cn//client/Connect_loginValidate.action";
		 		downloadLoginValidate(url,encodeURIComponent(username),password,vcode,downloadLoginBack);
		}
		
		function  loginBack(meg){
  			var obj=$("#validate");
  			changeValidateCode(obj[0]);
			if(meg.message=='SUCCESS'  ){
				checkSuccess(  );
			}else{
				alert(meg.message);
			} 
		}
		
		function  downloadLoginBack(meg){
			if(meg.message=="2"){
				window.location.href = "http://www.shjxdw.cn//jcdjnbzl/";
				$("#hiddenDownLoadContent").hide();
				//window.open('http://www.shjxdw.cn//jcdjnbzl/');
			}else if(meg.message=="0"){
				alert("用户名或密码错误");
			}else{
				alert("验证码错误");
			}
		}
		
	function checkSuccess( ){
