<script>
$(function() {
$(".ui-tabs > li > a,.ui-tabs-2 > li > a").mouseover(function(e) {
if (e.target == this) {
var tabs = $(this).parent().parent().children("li");
var panels = $(this).parent().parent().parent().children(".tab-content");
var index = $.inArray(this, $(this).parent().parent().find("a"));
if (panels.eq(index)[0]) {
if(tabs.eq(index).attr("class") == "li-l"){
	tabs.removeClass("li-l").eq(index).addClass("tab-on-l");
}
else{
tabs.removeClass("tab-on")
.eq(index).addClass("tab-on");
}
panels.addClass("hide")
.eq(index).removeClass("hide");
}
}
}); }); 
</script>
