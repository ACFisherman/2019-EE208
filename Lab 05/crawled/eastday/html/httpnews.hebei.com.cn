<script>
(function(win, doc, $) {
    var $tabs = $('#tabSlide'),
        $tabLi = $tabs.find('ul li'),
        $bigpic = $tabs.find('.bigpic03'),
        G = 1,
        t = 0;

    $tabLi.each(function(index) {
        $tabLi.eq(index).mouseover(function() {
            $tabLi.removeClass('current');
            $tabLi.eq(index).addClass('current');
            $bigpic.eq(index).stop().fadeIn(2000).siblings().hide();
            G = index + 1;
            if (G > $tabLi.length - 1) {
                G = 0;
            }
        });
    });

    $tabs.on("mouseenter", function() {
        $("#prevSlide").show();
        $("#nextSlide").show();
    });
    $tabs.on("mouseleave", function() {
        $("#prevSlide").hide();
        $("#nextSlide").hide();
    });

    $("#prevSlide").on("click", function() {
        G--;
        if (G < 0) {
            G = $tabLi.length - 1;
        }
        if (G < 0) {
            G = $tabLi.length - 1;
        }
        $tabLi.removeClass('current');
        $tabLi.eq(G).addClass("current");
        $bigpic.hide().eq(G).fadeIn(200);
    });
    $("#nextSlide").on("click", function() {
        
        $tabLi.removeClass('current');
        $tabLi.eq(G).addClass("current");
        $bigpic.hide().eq(G).fadeIn(200);
        G++;
        if (G > $tabLi.length - 1) {
            G = 0;
        }
    });

    t = setInterval(function() {
        $tabLi.removeClass('current');
        $tabLi.eq(G).addClass('current');
        $bigpic.hide().eq(G).fadeIn(1000);
        G++;
        if (G > $tabLi.length - 1) {
            G = 0;
        }
    }, 3000);
    $tabs.mouseenter(function() {
        clearInterval(t);
    }).mouseleave(function() {
        t = setInterval(function() {
            $tabLi.removeClass('current');
            $tabLi.eq(G).addClass('current');
            $bigpic.hide().eq(G).fadeIn(1000);
            G++;
            if (G > $tabLi.length - 1) {
                G = 0;
            }
        }, 3000);
    });

})(window, document, jQuery);
</script>
