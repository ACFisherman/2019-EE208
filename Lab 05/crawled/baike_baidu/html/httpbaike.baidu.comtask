      Counter = require('wiki-common:widget/ui/counter/counter.js');

  require('wiki-task:widget/taskIndex/banner/banner.js');

  var joinNumCounter = new Counter({
      container: '#joinNum',
      numHeight: 26,
      value: 0,
      duration: 2000
  });
  var releaseNumCounter = new Counter({
      container: '#releaseNum',
      numHeight: 26,
      value: 0,
      duration: 2000
  });

  setTimeout(function() {
    joinNumCounter.countUpTo(259377);
    releaseNumCounter.countUpTo(3717);

    var times = 0;
    setInterval(function() {
      if (times < 10) {
        $.getJSON('/api/usertask/gettaskcounter?r=' + Math.random(), function(res) {
             if (!res.errno) {
                times ++;
                res.data.joinUserNum && joinNumCounter.countUpTo(res.data.joinUserNum);
                res.data.taskNum && releaseNumCounter.countUpTo(res.data.taskNum);
             } else {
                times = 10;
             }
        });
      }
    }, 10000);
  }, 500);
}();
