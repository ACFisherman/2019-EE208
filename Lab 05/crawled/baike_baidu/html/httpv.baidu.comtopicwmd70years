        url: "http://v.baidu.com/kan/api/ipLocation?" + $ip,
        dataType: 'jsonp',
        success: function (data) {
            var $c = data.data.province;
            var $city = false;
            if ($c != '北京' && $c != '广东' && $c != '上海') {
                var $li = $('#navMainMenu li');
                for (var i = 0; i < $li.length; i++ ) {
                var navStr = $($li[i]).children().children('span').text().match(/[\u4E00-\u9FA5]{2}/g);
