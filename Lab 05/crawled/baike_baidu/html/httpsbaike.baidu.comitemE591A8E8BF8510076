!function(){	require.async(["wiki-common:widget/lib/jquery/jquery"],function($){
		var items=$(".time-line .tl-item");
		var container=$(".time-line");
		container.delegate(".tl-item","mouseover",function(event) {
			var target=$(event.target);
			if(!target.hasClass("tl-item")){
				target=target.parent();
			}
			if(!target.hasClass("focus")){
				items.removeClass("focus");
				target.addClass("focus");
			}
		});
		$('li .tli-content-null').parent().css('height', '20px');
	});
}();
