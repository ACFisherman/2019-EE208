            itemsTag: ".wrapCarouselItem",
	        itemSize:document.documentElement.clientWidth<1235?185:195,
            originalIndex:0,
            speed:300,
            easing:null,
            beforeStart:function(){},
            afterEnd:function(current,__this){
                var displied = __this.__data.displied;
                if( $.inArray(0,displied) !== -1 ){
                            maskRight.show();
                            tabRight.show();
                            tabLeft.hide();
                            maskLeft.hide();
                        }else if( $.inArray( __this.__data.allItems.length-1 ,displied)!==-1 ){
                            tabRight.hide();
                            tabLeft.show();
                            maskLeft.css("display","block");
                            maskRight.hide();

                            //$musicList[0].style.left = -112 + "px";

                        }else{
                            tabRight.show();
                            tabLeft.css("visibility","visible");
                            tabLeft.css("display","block");

                            maskLeft.css("display","none");
                            maskRight.css("visibility","visible");
                        }
            },
            visible:7,
            step:6,
            buffer:0.569,
            firstBig:true   
	    });
	    carouselMusic.render("#musicNewsArea");

        $("#leftCarouselControl .leftCtrl").click(function(){
            carouselMusic.prev();
        });

        $("#rightCarouselControl .rightCtrl").click(function(){
