!function(){require.async(["common:static/ui/carousel/jCarouselLite.js", "common:static/vendor/jquery/jquery.js"],function(carousel,$){
	var itemSize = document.documentElement.clientWidth<1235?1000:1235;
	var c = new carousel({
				itemsTag:".tang-carousel-item",
				itemSize:itemSize,
				originalIndex:0,
				duration:5000,
				speed:300,
				easing:null,
				beforeStart:function(){},
				afterEnd:function(a,b){
					$("#focus .tang-carousel-control ul li").removeClass("item-selected");
					$("#focus .tang-carousel-control ul li").eq(Math.floor(a%5)).addClass("item-selected");
				},
				auto:true,
				visible:1,
				step:1
			});
	
	c.render("#focus .tang-carousel-container");	
	$("#focus  .tang-carousel-control ul li").each(function(index,ele){
		$(ele).on("mousemove",function(){
			if(!$(this).hasClass("item-selected")){
				c.go(index);
			}
		})
	});

	$(window).on("window:resize",function(arg,key){
		if(key.type == "big" || key.type == "w1280" || key.type == "w1920"){
			c.refresh(1235);
		}
		if(key.type=="small"){
			c.refresh(984);
		}
	});
});
