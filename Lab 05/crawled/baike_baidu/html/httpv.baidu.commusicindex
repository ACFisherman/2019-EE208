    require.async(["common:static/ui/carousel/jCarouselLite.js", "common:static/vendor/jquery/jquery.js"],function(carousel,$){
        
        var tabLeft = $("#leftCarouselControl .leftCtrl");
        var tabRight = $("#rightCarouselControl .rightCtrl");

        var maskLeft = $("#leftCarouselControl .leftMusicMusk");
        var maskRight = $("#rightCarouselControl .rightMusicMusk");

