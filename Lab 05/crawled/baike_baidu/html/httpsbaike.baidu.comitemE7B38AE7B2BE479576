        var rsdataList = {"foot":[{"fentryTableId":35311,"fentryTableName":"\u7cd6\u7c7b","fentryTableType":1,"entryId":2683253,"templateId":0,"entryTitle":"\u7cd6\u7c7b","isManual":0,"isExpand":1}],"top":[]};

        if (rsdataList.top || rsdataList.foot) {
            rsInsertData.push([25287, rsdataList]);
        }
    </script>
<dl class="lemma-reference collapse nslog-area log-set-param" data-nslog-type="2" log-set-param="ext_reference">
