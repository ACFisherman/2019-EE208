    // 底部投诉带入当前页面 url
    var map = [1, 2, 6, 5];
    $('.wgt-footer-main .suggestion').find('a').each(function(i) {
        $(this).attr('href', 'http://help.baidu.com/newadd?word=%E4%B8%AD%E5%8D%8E%E4%BA%BA%E6%B0%91%E5%85%B1%E5%92%8C%E5%9B%BD%E5%85%AC%E8%81%8C%E4%BA%BA%E5%91%98%E6%94%BF%E5%8A%A1%E5%A4%84%E5%88%86%E6%B3%95%EF%BC%88%E8%8D%89%E6%A1%88%EF%BC%89' + '&&submit_link=' + encodeURIComponent(window.location.href) + '&prod_id=10&category=' + map[i]);
