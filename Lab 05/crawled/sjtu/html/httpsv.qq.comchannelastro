if (currentChannelNav.length && currentChannelNavIcon) {
    currentChannelNav[0].innerHTML = '<img src="' + currentChannelNavIcon + '" alt="" class="channel_nav_icon">' + currentChannelNav[0].innerText
}

var unusualPic = document.getElementsByClassName('unusual_pic')
