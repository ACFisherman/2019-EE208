    //日历插件
    var date = new Date();
    var year = date.getFullYear();
    var month = date.getMonth() + 1;
    var day = date.getDate();
    month = (month < 10 ? "0" + month : month);
    day = (day < 10 ? "0" + day : day);
    var d=year+"-"+month+"-"+day;
    calUtil.setMonthAndDay();
    calUtil.init(CreatZczphArea(year + "-" + month));
    function CreatZczphArea(Month) {
        // var date2= new Date(Date.parse(Month.replace(/-/g,  "/")));      //转换成Data(); 
        // var year2 = date2.getFullYear();
        // var month2 = date2.getMonth()+1;
        // var zczph = new Array();
        // var signList = new Array();
        var date2= Month.split("-");      //转换成Data(); 
        var year2 = date2[0];
        var month2 = date2[1];
        var zczph = new Array();
        var signList = new Array();
        // console.log(Month,date2,year2,month2);
        http3('POST', '/preach/dategetlist', {"school_id":school_id,"year":year2,"month":month2},function (e) {
            if(e.code == 0){
            //    console.log(e)
                var arr=[];
                var ddd="0";
                for (var i = 0; i < e.data.length; i++) {
                    for(var j=0;j<e.data[i].list.length;j++){
                        var obj={};
                        obj["date"]=e.data[i].key;
                        obj["value"]=e.data[i].list[j].title;
                        obj["id"]=e.data[i].list[j].id;
                        arr.push(obj);
                    }
                    if(e.data[i].key==d){
                        ddd=e.data[i].list.length;
                    }
                }
                zczph = arr;
                $(".chang").html(ddd);
            }
        });
        if (zczph.length > 0) {
            for (var i = 0; i < zczph.length; i++) {
                var data = {
                    signmon: zczph[i].date.substring(0, 7),
                    signDay: new Date(zczph[i].date.replace(/-/g, "/")).getDate(),
                    signBt: zczph[i].value,
                    zczphid: zczph[i].id
                }
                if (data.signmon == Month) {
                    signList.push(data);
                }
            }
        }
        return signList;
    }
