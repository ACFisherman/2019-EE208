<script>
var fn_one, fn_show, fn_ascroll;
$(function() {
	var last_time = "";
	fn_show = function(time) {
		//time
		if(!time) time = last_time;
		else last_time = time;
		var dt = new Object();
		//type
		dt.type = $('#ul-type input:checked').attr('value');
		//place
		dt.place = $('#tab-place input:checked').attr('value');
		//do ajax
		var aj = {
			cache:false,
			beforeSend:function() {
				$('.loading').show();
			},
			url:'calendar/?a=list&' + time,
			data:dt,
			type:'GET',
			dataType: "html",
			success: function(res){
				$('.loading').hide('default');
				$('#mainview').html(res);
				$('.maintab').tabs('enable',3);
				$('.maintab').tabs('select',3);
				location.hash = time;
			}
		}
		$.ajax(aj);
	};
	$('#ul-type label').click(function() {
		fn_show();
	});
	$('#tab-place label').click(function() {
		fn_show();
	});
	$('.picker').datepicker({
		dateFormat:'yymmdd',
		dayNamesMin:['日','一','二','三','四','五','六'],
		monthNames:['一月', '二月', '三月', '四月', '五月', '六月', '七月', '八月', '九月', '十月', '十一月', '十二月'],
		nextText:'下月',
		prevText:'上月',
		onSelect:function(d) {
			fn_show('m=theday&day='+d);
		}
	});
	$('.maintab').tabs({
		select:function(event,ui) {
			if(ui.index==3) return;
			fn_show($(ui.tab).attr('time'));
		},
		disabled:[3],
		spinner:"Loading...",
		//fx: { opacity: 'toggle', duration: '50' },
		cache:false
	});
	$('.tab').tabs({
		fx: { opacity: 'toggle', duration: 200 }
	});
	fn_one = function(evid) {
		//do ajax
		var aj = {
			cache:false,
			beforeSend:function() {
				$('.loading').show();
			},
			url:'calendar/?a=one&evid=' + evid,
			type:'GET',
			dataType: "html",
			success: function(res){
				$('.loading').hide('default');
				$('#mainview').html(res);
				$('.maintab').tabs('enable',3);
				$('.maintab').tabs('select',3);
				location.hash = "a=one&evid="+evid;
			}
		}
		$.ajax(aj);
	};
	if(location.hash.length > 0) fn_show(location.hash.substr(1));
	else fn_show("&m=recent&range=7");
});
</script>

