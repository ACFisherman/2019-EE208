if ( '/library/content/server_info_zh_CN.html'.indexOf('http://') == 0 || '/library/content/server_info_zh_CN.html'.indexOf('https://') == 0 ) {

    function timeout_gateway_120() { 
        var myframe = $("#id_gateway_120");
        var loaded = false;
        window.console && console.log("/library/content/server_info_zh_CN.html timeout check...");
        try {
            var iframeLoc = myframe.get(0).contentWindow.location;
            iframeLoc = String(iframeLoc);
            var windowLoc = String(window.location);
            window.console && console.log("iframe.location="+windowLoc);
            var ipos = -1;
            var slashcount = 0;
            for (var i=0; i<windowLoc.length; i++ ) {
                if ( windowLoc[i] == '/' ) slashcount++;
                if ( slashcount == 3 ) {
                    ipos = i;
                    break;
                }
            }
            if ( ipos == -1 ) {
                loaded = false;
            } else { 
                var prefix = windowLoc.substring(0, ipos);
                window.console && console.log("iframe.prefix="+prefix);
                loaded = iframeLoc.indexOf(prefix) == 0;
            }
        } catch(ex) {  // This is an expected/normal/hoped for occurance
            loaded = true;
        }
        if ( ! loaded ) {
            $('#alert_gateway_120').show();
        } else {
            window.console && console.log('/library/content/server_info_zh_CN.html load success');
        }

    }

    var timer_gateway_120 = false;
    $(document).ready(function() {
        timer_gateway_120 = window.setTimeout("timeout_gateway_120()",8000);
    });
} else {
    window.console && console.log('/library/content/server_info_zh_CN.html does not start with http:// or https://');
}
</script>
