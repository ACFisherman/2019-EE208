                                <script type="text/javascript">
                                    $(function(){
                                    	$("body").click(function(event){
										//console.log(!$(event.target).is("#span"));
											if(!$(event.target).is("#span")){
												$(".new").hide();	
											}
										});
                                    
                                        $(".set").click(function(){
                                            var ul=$(".new");
                                            if(ul.css("display")=="none"){
                                                ul.slideDown();
                                            }else{
                                                ul.slideUp();
                                            }
                                        });
        
                                        $(".new li").click(function(){
                                            var li=$(this).text();
                                            $(".set").html(li);
                                            $(".new").hide();
                                        });
                                    })
                                     function search(form){
							            var title = form.title.value;
							            if(title == ""){
							                alert("请输入要搜索的内容！");
							            }else{
							            	var span = $("#span").html();
							            	if('标题搜索'==span){
								            	form.action="/index!search.action";
								            	form.target="_blank";
								                form.submit();
							            	}else{ 
							            		window.open('http://cn.bing.com/search?q='+title+'+site%3Awww.ao.fudan.edu.cn&qs=n&form=QBRE&pq='+title+'site%3Awww.ao.fudan.edu.cn');
							            	}
							            }
							            return false;
							        }
                                </script>
