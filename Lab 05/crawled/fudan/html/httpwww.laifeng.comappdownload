                    seajs.use(['JQ','UTIL'], function(J,U){
                        var categoryList = J('.category-list');
                        var hasData = parseInt(categoryList.data('has'));
                        if (!hasData) {
                            U.get('//v.laifeng.com/category/list', {}, function(resp){
                                if (resp.code == 0) {
                                  var data = (resp.data && resp.data.data) || [];
                                  var html = '';
                                  for (var i=0, len=data.length; i<len; ++i){
                                      var categoryId = data[i].categoryId;
                                      var categoryName = data[i].categoryName;
                                      html += '<span><a href="//www.laifeng.com/category/center?categoryId=' + categoryId + '">' + categoryName + '</a></span>';
                                  }
                                  categoryList.empty();
                                  categoryList.append(html);
                                  categoryList.data('has', 1);
                               }
                            });
                        }
                    });
                </script>
