<script type="text/javascript">

getData = function(options) {
    $.ajax({
        url: options.url,
        type: "POST",
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        data: "{\"page\":" + options.page + "}",
        success: function(data, textStatus, xhr) {
        	
            if (data!='') {
              
            	$("#result").html('');
            	
				var html=[];
				
                 $.each(data, function() {
                	html.push('<div class="list2">'+
                    		  '<h3>'+this.title+'</h3>'+
                    		  '<p class="ptitle">'+
                    		  '<span class="b1">时间：'+this.addTime.substring(0,10)+'</span>'+
                    		  '<span class="b2">来源：'+this.source+'</span>'+
                    		  '<span class="b3">点击：'+this.hits+'</span>'+
                    		  '</p>'+
                    		  '<p class="info2"><img src=\"http://shmc.fudan.edu.cn:80/'+this.defaultpic+'\" width="220" height="128" align="left" />'+
                    		   ''+this.intro.substring(0,200)+'...</p>');
                    
                    if(this.url != '' && this.url != null){
                    	html.push('<p class="more2"><a href="content/'+this.id+'">更多详细</a></p>'+
                      		  '<p class="clear"></p></div> ');
                    }else{
                    	html.push('<p class="more2"><a href="/html/news_xiangqing_'+this.id+'.html">更多详细</a></p>'+
                      		  '<p class="clear"></p></div> ');
                    }
                    
                });
                   
                $("#result").append(html.join(""));
                
                $('#loadMore').html('==点击加载更多==');
                $('html,body').animate({scrollTop:300},1000);
                 
            }else{
            	
            	alert('没有更多的信息了，这是最后一页！');
            	
            	$('#loadMore').html('==最后一页==');
            	
            	//$.getData({url:options.url,page:--options.page});
            	
            }
        },
        error: function(xmlHttpRequest, textStatus, errorThrown) {
            alert('加载数据失败！');
        }
    });
};

