 function show_num(n){ 
 	var numstr = String(n);
 	var len = String(numstr).length; 
 	var diff = 9-len;
 	for(var i=0;i<diff;i++)
 	{
 		numstr='0'+numstr;
 	}
 	
 	len = String(numstr).length; 
 	
    var it = $(".t_num i"); 
    
   
    for(var i=0;i<len;i++){ 
   
        if(it.length<=i){ 
            $(".t_num").append("<i></i>"); 
        } 
        var num=String(numstr).charAt(i); 
        //alert(num);
        var y = -parseInt(num)*36; //y轴位置 
        var obj = $(".t_num i").eq(i); 
        
        obj.animate({ //滚动动画 
            backgroundPosition :'(0 '+String(y)+'px)'  
            }, 'slow','swing',function(){} 
        ); 
        // alert(num);
    } 
} 


