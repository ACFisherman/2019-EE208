                var domList = document.getElementsByClassName('v-num');
                var domlist1 = document.getElementsByClassName('num-play');
                for (var i = 0, len = domList.length; i < len; i++){
                    domList[i].innerHTML = '';
                }
                for (var i = 0, len=domList.length; i < len; i++){
                    domlist1[i].innerHTML = '';
                }
            </script>
