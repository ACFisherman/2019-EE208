<script type="text/javascript">
(function($){
//	$('#qheader_nav').hide();
	var randObj = $('.yk-error .yk-error-bg'), randNum = Math.floor(Math.random()*100)%4 +1;
	var errText = [
		'呃......您找的页面刚探索冥王星去了......',
		'哦~您找的页面飘得有点远......',
		'休息一下，您找的页面快挖到了......',
		'稍等，我们这就去叫醒您找的页面......'
	];
//	randObj.removeClass('yk-error-bg1 yk-error-bg2 yk-error-bg3 yk-error-bg4 yk-error-bg5');
	randObj.addClass('yk-error-bg'+randNum);
	if ( errText[randNum-1] ) {
		$("#errorcont").html(errText[randNum-1]);
	}
	$(document).ready(function(){
		var num = 15; //设置倒计时秒数
		var timer = setInterval(function(){
			num--;
			if(num==-1){
				clearInterval(timer);
				window.location.href="//www.youku.com";
			}else{
				$('#countdown').html(num);
			}
		},1000);
         
               //下载app
               $("#ikuDownLoad").on('click', function (evt) {
                    if (window.ikuDownloadIng) return false;
                    var target =this;
                    if (!target) {
                        return false;
                    }
                    var src;
                    src = $(target).attr('data-down-href');
                    if (!src || window.ikuDownloadIng) return false;
                    window.ikuDownloadIng = true;
                    var iframe = document.createElement("iframe");
                    iframe.width = 0;
                    iframe.height = 0;
                    iframe.src = src;
                    document.body.appendChild(iframe);
                    setTimeout(function () {
                        document.body.removeChild(iframe);
                        window.ikuDownloadIng = false;
                    }, 2000);
                    return false;
                })
	});
})(jQuery);
</script>
