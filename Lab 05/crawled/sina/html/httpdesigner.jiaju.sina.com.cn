<script>

/*
需加效果的文本框、文本域加上autocomplete="off"属性
需页面上加上提示文本的样式
传入匹配的文本框、文本域即可
*/
var gj = {
	changeColor:function(a){//改变文本颜色
		a.each(function(i,d){
			$(d).after($('<txt/>').addClass("ext_txt").css("color","#999"));//添加文本
			var si = $(d).nextAll(".ext_txt");//存储文本框所对应的<txt>标签
			var val = $(d).val();
			$(d).data("v",val);//存储文本框最开始的value
			$(d).val("");
			si.html(val);
			$(d).css("color","#666");
			var interval='';
	 		$(d).focus(function(){
				si.css("color","#ccc");
				interval = setInterval(function(){
					val = $(d).val();
					if (val != '')
						{
							si.empty();
							clearInterval(interval);
						}
				},300);
	 		}).blur(function(){
				clearInterval(interval);
	 			var val = $(d).val();
	 			if (val == ''){
	 				si.html($(d).data("v"));
					si.css("color","#999");
	 			}else{
					si.empty();
				}
	 		});
			si.click(function(){
				$(d).focus();
				si.css("color","#ccc");
	 		});
 		});
	},
	changeBack:function(a){//改变文本框背景
		a.each(function(i,d){
			$(d).focus(function(){
				$(d).parent(".mod_ipt").removeClass("mod_ipt").addClass("mod_ipt_on");
			}).blur(function(){
				$(d).parent(".mod_ipt_on").removeClass("mod_ipt_on").addClass("mod_ipt");
			});
		});
	},
	msgHandle:function(m){
		return "<span class=\"err_txt\">"+m+"</span>";
	}
};


</script>


