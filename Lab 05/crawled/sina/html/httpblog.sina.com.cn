                                    Login.prototype.init = function () {
                                        jsLoader('http://i.sso.sina.com.cn/js/ssologin.js', function () {

                                            var _self = this;
                                            //手动登陆成功
                                            window.sinaSSOController.customLoginCallBack = function (info) {
                                                //提示
                                                if (info.result) {
                                                    if(_self.pinPop){
                                                        _self.pinPop.destory();
                                                    }
                                                    //登陆成功
                                                    _self.state.isLogin = true;
                                                    _self.checkLoginStatus(function (userInfo) {
                                                        if (userInfo) {
                                                            _self.showLogin(userInfo);
                                                            window.__SinaTopBar__.user.fireLoginSuccess();
                                                        } else {
                                                            _self.showLogout();
                                                        }
                                                    });
                                                    _self.state.isReq = false;
                                                } else {
                                                    var errorMsg = info.reason;
                                                    switch(info.errno){
                                                        case '2070':
                                                            $(_self.pinPop.getDOM()).find('.pin-error-text').show();
                                                            break;
                                                        case '6102':
                                                            errorMsg = '请输入用户名或密码！';
                                                        default:
                                                            var ide = setInterval(function(){
                                                                if(window.BlogPop){
                                                                    clearInterval(ide);
                                                                    var errPop = new BlogPop({
                                                                        btnText:'我知道了',
                                                                        content: '<p style="text-align:center;">'+errorMsg+'</p>',
                                                                        icon:'warning',
                                                                        closeFn:function(el){
                                                                            errPop.destory();
                                                                        },
                                                                        okFn: function(el){
                                                                            errPop.destory();
                                                                        }
                                                                    });
                                                                    errPop.show();
                                                                    _self.state.isReq = false;
                                                                }
                                                            },100);
                                                            break;
                                                    }
                                                }
                                            }

                                            //手动退出成功
                                            window.sinaSSOController.customLogoutCallBack = function (result) {
                                                window.__SinaTopBar__.user.fireLogoutSuccess();
                                                _self.showLogout(true);
                                            }
                                            //页面自动登陆成功
                                            window.sinaSSOController.autoLogin(function (info) {
                                                _self.checkLoginStatus(function (userInfo) {
                                                    if (userInfo) {
                                                        _self.showLogin(userInfo);
                                                    } else {
                                                        _self.showLogout();
                                                    }
                                                });
                                            });
                                            this.initEvent();
                                        }.bind(this));
                                    }
                                    Login.prototype.initEvent = function () {
                                        var $loginName = $('#loginName'),
                                            $loginPassword = $('#loginPassword'),
                                            $mailList = $loginName.next(),
                                            $submitBtn = $('#submit-btn'),
                                            _self = this;
                                        $loginName.on('focus', function () {

                                            $loginName.on('keyup', function (e) {
                                                if (e.which === 13) {
                                                    if($mailList.find('.active').length){
                                                        $(this).val($mailList.find('.active span').text());
                                                        $mailList.hide();
                                                    }
                                                    $loginPassword.focus();
                                                }
                                                if(e.which === 40){
                                                    //下
                                                    _self.selectMailList(true);
                                                }
                                                if(e.which === 38){
                                                    //上
                                                    _self.selectMailList(false);
                                                }
                                            });
                                            $loginName.on('blur', function (e) {
                                                $loginName.off('blur');
                                                $loginName.off('keyup');
                                                var ide = setTimeout(function(){
                                                    $mailList.hide();
                                                    clearTimeout(ide);
                                                },150);
                                            });
                                        });
                                        $loginName.on('input', function () {
                                            _self.renderMailList($(this).val());
                                        });
                                        $loginName.on("propertychange",function(){
                                            if(window.event.propertyName!='value') return;
                                            _self.renderMailList($(this).val());
                                        });

                                        $loginPassword.on('keyup', function (e) {
                                            if (e.which === 13) {
                                                // console.log('keyup click.........');
                                                $submitBtn.click();
                                            }
                                        });

                                        $mailList.delegate('li','click',function(){
                                            $loginName.val($(this).find('span').text());
                                            $loginPassword.focus();
                                            $mailList.hide();
                                        }).delegate('li','mouseenter',function(){
                                            $mailList.find('.active').removeClass('active');
                                            $(this).addClass('active');
                                        });

                                        $submitBtn.on('click', function () {
                                            if(_self.state.isReq){
                                                return;
                                            }
                                            _self.state.isReq = true;
                                            var loginName = $loginName.val();
                                            var password = $loginPassword.val();
                                            var autoLoad = $('#auto').is(':checked');
                                            // _self.showPin();
                                            //prelogin的验证码问题。 优先进行验证操作

                                            window.sinaSSOController.prelogin({
                                                username: loginName,
                                                checkpin:1
                                            },function(result){
                                                //展示内容
                                                if(result.showpin){
                                                    _self.showPin(function(){
                                                        window.sinaSSOController.login(loginName, password, autoLoad ? 30 : null);
                                                    });
                                                }else{
                                                    window.sinaSSOController.login(loginName, password, autoLoad ? 30 : null);
                                                }
                                            });
                                        });

                                        $('.logined').delegate('.logout-btn', 'click', function () {
                                            window.sinaSSOController.logout();
                                        });
                                    }
                                    //验证码流程
                                    Login.prototype.showPin = function(cb){
                                        var _self = this;
                                        function pin(){
                                            var _self = this;
                                            this.pinPop = new BlogPop({
                                                btnText:'登陆',
                                                content: this.pinTpl,
                                                closeFn:function(el){
                                                    this.pinPop.destory();
                                                },
                                                okFn: function(el){
                                                    sinaSSOController.loginExtraQuery['door'] = $(el).find('input').val();
                                                    cb();
                                                },
                                                destoryFn: function(el){
                                                    $('#pin-img').off('click');
                                                    $('#change-pin').off('click');
                                                }
                                            });
                                            this.changePin();
                                            this.changePinEvent();
                                            this.pinPop.show();
                                        }
                                        //定时轮训加载BlogPop对象
                                        var ide = setTimeout(function(){
                                            clearTimeout(ide);
                                            if(window.BlogPop){
                                                pin.call(_self);
                                            }else{
                                                _self.showPin();
                                            }
                                        },100);
                                    }


                                    Login.prototype.changePin = function(){
                                        var url = sinaSSOController.getPinCodeUrl();
                                        $('#pin-img').attr('src',url);
                                    }
                                    Login.prototype.changePinEvent = function(){
                                        var _self = this;
                                        $('#pin-img').click(function(){
                                            _self.changePin();
                                        });
                                        $('#change-pin').click(function(){
                                            _self.changePin();
                                        })
                                    }

                                    Login.prototype.limitNum = function(num){
                                        num = num - 0;
                                        if(num >= 1000){
                                            var str = Math.floor(num/1000) + '';
                                            var list = str.split('').reverse();
                                            var result = [];
                                            for (var i=0; i < list.length; i++){
                                                if(i % 3 == 0 && i !== 0){
                                                    result.push(',');
                                                }
                                                result.push(list[i]);
                                            }
                                            return result.reverse().join('') + 'k+';
                                        }else{
                                            return num;
                                        }
                                    }

                                    Login.prototype.selectMailList = function(isDown){
                                        var $list = $( '.mail-list li');
                                        if(!$list.length){
                                            return;
                                        }
                                        if(!$(".mail-list").find('.active').length){
                                            if(isDown){
                                                $($list[0]).addClass('active');
                                            }else{
                                                $($list[$list.length - 1]).addClass('active');
                                            }
                                            return;
                                        }

                                        var selectIndex = 0;
                                        for(var i = 0; i< $list.length; i ++){
                                            var $item = $($list[i]);
                                            if($item.hasClass('active') && isDown){
                                                selectIndex = i + 1;
                                                if(selectIndex >= $list.length){
                                                    selectIndex = 0;
                                                }
                                                $item.removeClass('active');
                                            }else if($item.hasClass('active') && !isDown){
                                                selectIndex = i - 1;
                                                if(selectIndex < 0){
                                                    selectIndex = $list.length - 1;
                                                }
                                                $item.removeClass('active');
                                            }
                                        }
                                        $($list[selectIndex]).addClass('active');
                                    }

                                    Login.prototype.renderMailList = function(val){
                                        if(!val.length){
                                            $('.mail-list').hide();
                                            return;
                                        }
                                        var list = ['@sina.com', '@sina.cn', '@vip.sina.com', '@qq.com', '@163.com', '@126.com', '@hotmail.com', '@gmail.com'];
                                        var html = '';
                                        var isNeedFilter = false;
                                        if(val.indexOf('@')>=0){
                                            try{
                                                var prefix = val.split('@')[0];
                                                var suffix = val.split('@')[1];
                                                isNeedFilter = true;
                                            }catch(e){
                                                prefix = val;
                                                suffix = '';
                                                isNeedFilter = false;
                                            }
                                        }
                                        for(var i = 0; i<list.length; i++){
                                            var item = list[i];
                                            if(item.indexOf('@'+suffix) === 0 && isNeedFilter){
                                                html += '<li class=""><span>'+prefix + list[i] +'</span></li>';
                                            }else if(!isNeedFilter){
                                                html += '<li class=""><span>'+val + list[i] +'</span></li>';
                                            }
                                        }
                                        $('.mail-list ul').html(html);
                                        if(html){
                                            $('.mail-list').show();
                                        }else{
                                            $('.mail-list').hide();
                                        }
                                    }
									Login.prototype.changeALFCookie = function(state,uid){
										if(state == 'on'){
											document.cookie = "ALF=" + uid +';expires=' + new Date((new Date()).getTime() + 60 * 60 * 1000).toGMTString() + ';domain=.sina.com.cn';
										}else{
											document.cookie = "ALF=;expires=Thu, 01 Jan 1970 00:00:00 GMT;domain=.sina.com.cn"
										}
									}
                                    Login.prototype.showLogout = function () {
                                        //触发顶部的退出登陆状态
                                        // window.__SinaTopBar__.user.fireLogoutSuccess();
                                        window.msgBus.publish('logout');
										this.changeALFCookie('off');
										$('#loginName').val('');
										$('#loginPassword').val('');
                                        $('.not-login').show();
                                        $('.logined').hide();
