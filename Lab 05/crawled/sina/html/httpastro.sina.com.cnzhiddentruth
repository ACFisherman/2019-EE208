	<script type="text/javascript">
		var timesLi = jQuery('.timeline-time li');
		for(var i =0;i<timesLi.length;i++){
			var tempLi = timesLi.eq(i);
			var tempLiP = tempLi.parent()
			tempLi.width(tempLiP.width()/(tempLiP.find('li').length));
		}
		var timeLineTime = jQuery('.timeline-time');
		var timeLineDay = jQuery('.timeline-day');
		var timeItem = jQuery('.timeline-time li');
		function getMGL(index){
			var tempMGL = 0;
			for(var i =0;i<index;i++){
				tempMGL+=timeItem.eq(i).width();
			}
			tempMGL+=timeItem.eq(index).find('a').width()/2;
			return tempMGL;
		}
		function rePosDate(index){
			timeLineTime.animate({'marginLeft':-getMGL(index)},{queue:false, duration:'fast'});
			timeLineDay.animate({'marginLeft':-getMGL(index)},{queue:false, duration:'fast'});
			jQuery('.timeline-time li a').removeClass('selected');
			jQuery('.timeline-time li a').eq(index).addClass('selected');
		}
		rePosDate(0)
		var slide_01 = new Slider({
			wrapId : 'slider',
			itemClass : 'slider_item',
			sameSize : true,
			startOn : 0,
			// startOnPos : 300,
			scrollBy : 1,
			// scrollDis : 400,
			speed : 3,
			isVertical : false,
			isLoop : true,
			autoPlay : false,
			autoInterval : 5,
			absoluteAni : true,
			onReady : function () {
				var that = this;
				var btn_prev = document.getElementById('hd_slider_prev');
				var btn_next = document.getElementById('hd_slider_next');	
				var tipObj = jQuery('.timeline-tip');		       
				btn_prev.onclick = function () {
					that.prev();
				};
				btn_next.onclick = function () {
					that.next();
				};
				$('.timeline-time li a').on('click',function(){
					var tempIndex = jQuery(this).index('.timeline-time li a');
					that.slideTo(tempIndex);
				});
				$('.timeline-time li a').on('mouseenter',function(){
					if(!jQuery(this).hasClass('selected')){
						var tempTip = jQuery(this).attr('data-tip');
						var tempIndex = jQuery(this).index('.timeline-time li a');
						//console.log($(this).offset().left-$('.timeline-data ').offset().left);
						tipObj.find('span').html(tempTip);
						var curLeft = jQuery(this).offset().left;
						var parentLeft = jQuery('.timeline-data').offset().left;
						var curRight = jQuery('.timeline-data').width()-(curLeft-parentLeft);
						if(curRight<tipObj.width()+20){
							tipObj.addClass('timeline-tip-reverse');
							tipObj.css({'left':(curLeft-parentLeft)+jQuery(this).width()/2-tipObj.width()});
						}else{
							tipObj.removeClass('timeline-tip-reverse');
							tipObj.css({'left':(curLeft-parentLeft)+jQuery(this).width()/2});
						}
						tipObj.show();
					}
				})
				jQuery('.timeline-time li a').on('mouseout',function(){
					tipObj.hide();
				})
jQuery('#slider').find('div').eq(0).width(2000000)
			},
			onAniStart : function () {
				// body...
			},
			onAniEnd : function () {
				// body...
			},
			onIndexChanged : function (index) {
				var that = this;
				var tipObj = jQuery('.timeline-tip');
				rePosDate(index%(that.states.total/2));
				tipObj.hide();
			},
			onEdge : function (isLeft) {
				// body...
			}
		});
	</script>
