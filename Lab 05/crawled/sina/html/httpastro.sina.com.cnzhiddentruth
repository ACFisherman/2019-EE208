	<script type="text/javascript">
	(function(){
		/* navTop fixed */
		function initFixElement(classname){
			var defaultHeight = [];
			var fixElements = jQuery('.'+classname);
			for(var i =0;i<fixElements.length;i++){
				defaultHeight.push([fixElements.eq(i),getPosition(fixElements[i]).y]);
			}
			jQuery(window).bind('scroll load resize',function(){
				var e = Math.max(document.documentElement.scrollTop , document.body.scrollTop);
	            var d = document.documentElement.clientHeight;
	            for(var j=0;j<defaultHeight.length;j++){
	            	tempItem = defaultHeight[j][0];
	            	tempDefaultHeight=defaultHeight[j][1]
	            	if(e>tempDefaultHeight){
	            		tempItem.addClass('navTop_fix');
	            	}else{
	            		tempItem.removeClass('navTop_fix');
	            	}
	            }
			})
		}
		function initScrollFollow(){
			function scrollTo(obj){
				if(typeof(obj.attr("id"))!="undefined"){
					var thisId = obj.attr('id');
			        var anchorId = thisId.replace('btn_rn_','anchor_');
			        var level = thisId.replace('btn_rn_','')
			        anchorTop = getPosition(document.getElementById(anchorId)).y-100;
			        jQuery('html,body').animate({scrollTop:anchorTop},'fast');
			        if(level=='14'){
	    				playFullVideo();
	    			}
				}
			}
			jQuery('.right-nav .rni-second').on('click',function(){
		        jQuery('.right-nav .rni-second').removeClass('selected');
		        jQuery(this).addClass('selected');
		        jQuery('.right-nav .rni-third').hide();
		        jQuery(this).parent().find('.rni-third').show();
		        scrollTo(jQuery(this))
		    });
		    jQuery('.rni-third a').on('click',function(){
		    	jQuery('.rni-third a').removeClass('selected');
		        jQuery(this).addClass('selected');
		        scrollTo(jQuery(this));
		    });
		}
		initFixElement('j_fixElm');
		initScrollFollow()
       jQuery(window).bind('load resize',function() {
            initFixElement('j_fixElm');
            initScrollFollow()
        });

		function getPosition(o) {
	        var p = {
	            x: 0,
	            y: 0
	        };
	        while (o.offsetParent) {
	            p.x += o.offsetLeft;
	            p.y += o.offsetTop;
	            o = o.offsetParent;
	        }
	        return p;
	    }
	    jQuery(window).bind('scroll load resize',function(event){
	    	var e = Math.max(document.documentElement.scrollTop , document.body.scrollTop);
	    	var anchors = jQuery('.j_anchor');
	    	for(var i=0;i<anchors.length;i++){
	    		//if(getPosition(anchors[i]).y-100 > e-50&&getPosition(anchors[i]).y-100 < e+50){
if(getPosition(anchors[i]).y-100<e){
	    			var level = anchors.eq(i).attr('id').replace('anchor_','');
	    			selectNode(level);
	    			if(level=='14'){
	    				playFullVideo();
	    			}
	    		}
	    	}
	    	/*if(e>getPosition(jQuery('.j_anchor:last')[0]).y){
	    		var level = jQuery('.j_anchor:last').attr('id').replace('anchor_','');
	    		selectNode(level);
	    	}*/
	    });
	    function playFullVideo(){
	    	if(jQuery('#fullVideo').html()==''){
	    		window.palyerConf.playerWidth=1000;
	            window.palyerConf.playerHeight=800;
	            var parent =jQuery('.movie_close').closest('.imgWrap').removeClass('imgWrap-open');
	            parent.find('.video_zoom').attr('status', '0').css('display', 'inline-block');
	            parent.find('.movie_play').empty().remove();
	            var Bigvideo = jQuery('<div class="movie_play"><div id="movie_play_cont"></div></div>').appendTo(jQuery('#fullVideo'));
		    	player = null;
	            player = sinaVideoPlayer(palyerConf);
	            player.on("flashInitCompleted", 
	            function() {
					player.playVideo('79156616', "0", 0);
					vid = null;
				});
	            player.init();
	    	}
	    }
	    function selectNode(level){
	    	var levelArray = level.split('-');
	    	jQuery('.rni-second').removeClass('selected');
	    	jQuery('.rni-third a').removeClass('selected');
	    	jQuery('.right-nav .rni-third').hide();
	    	var tempId = '';
	    	var tempLevel = [];
	    	for(var i = 0;i<levelArray.length;i++){
	    		if(levelArray[i]!=''){
	    			tempLevel.push(levelArray[i]);
	    			tempId='btn_rn_'+tempLevel.join('-');
	    			jQuery('#'+tempId).addClass('selected');
	    			jQuery('#'+tempId).parent().find('.rni-third').show();
	    		}
	    	}
	    }
       jQuery('.j_toTop').on('click',function(){
            jQuery('html,body').animate({scrollTop:'0'},'fast')
		});
/*for(var i=0;i<$('.j_txt').length;i++){
 $('.j_txt').eq(i).parent().height( $('.j_txt').eq(i).height());
}
function parallaxScroll() {
    var e = Math.max(document.documentElement.scrollTop , document.body.scrollTop);
    var parentsWrap = $('.j_txt').parent();
    for(var i = 0;i<parentsWrap.length;i++){
    	if(getPosition(parentsWrap[i]).y > e-50&&getPosition(parentsWrap[i]).y < e+50){
			$('.j_txt').eq(i).addClass('blk_01_parrla');

		}
		$('.j_txt').css('top', (0 + ((getPosition(parentsWrap[i]).y-e) * .2)) + 'px');
    } 
}
$(window).bind('scroll', function() {
        parallaxScroll();
    });*/
	})()
	</script>
