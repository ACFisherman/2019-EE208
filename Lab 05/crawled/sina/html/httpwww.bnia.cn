<script type="text/javascript">        

        function query(){            
            $.ajax({
                type: "POST",
                url: "http://www.bnia.cn:6070/simpleboot/member/certificateSearch",
                data:{"searchType": $("#type").val(), "condition": $("#member-number").val()},
                dateType:'json',
                success: function(result){
                    if(result.code == 200 && result.data != null) {
                        //alert(result.data);                        
                        if($("#type").val() == 'number') {
                            $('#mNumber').html($("#member-number").val());
                            $('#mName').html(result.data);
                        }else {
                            $('#mNumber').html(result.data);
                            $('#mName').html($("#member-number").val());
                        }
                        $('.sr_ShowDiv>ul').show();
                        $('#sr_message').html('');
                        showSearchResult();

                    } else {
                        //alert(result.msg);
                        $('.sr_ShowDiv>ul').hide();
                        $('#sr_message').html(result.msg);
                        showSearchResult();
                    }
                }
            });
        };

        function showSearchResult(){            
            $('#searchResult-mask,#searchResult').fadeIn();
        };
        $('#searchResult').on('click','.close , button',function() {
            $('#searchResult-mask,#searchResult').fadeOut();
        });


    
</script>

