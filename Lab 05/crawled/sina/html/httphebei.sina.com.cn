        };
        $.ajax(ajax_sets)
    };
	var setNewsList = function(channel, newsid) {
        if (!channel || !newsid) return '';
        if (newsid.match(/comos-/) || newsid.match(/\d+-\d+-\d+/)) {
            newsid = newsid + ':0'
        } else {
            newsid = newsid + ':1'
        }
        return channel + ':' + newsid
    };
};
	
