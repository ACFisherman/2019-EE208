        jsonp:"callback",
        success:function(data){
            if(data.result.status.code == 0){
                for( cid in data.result.count){

                    commentCountId = cid.replace('kj:', '').replace(':0','');
                    $('#'+commentCountId).text('评论('+data.result.count[cid].show+')');
                }
            }
    }});



