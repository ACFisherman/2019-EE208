        type:'get',
        url:'http://comment5.news.sina.com.cn/cmnt/count?format=json&newslist=kj:icezuev0710128:0,kj:icezuev0706813:0,kj:icezuev0486837:0,kj:icezuev0485495:0,kj:icezuev0479392:0,kj:icezuev0308145:0,kj:icezzrr0172714:0,kj:icezuev0131381:0,kj:icezzrq9119976:0,kj:icezueu9074133:0',
