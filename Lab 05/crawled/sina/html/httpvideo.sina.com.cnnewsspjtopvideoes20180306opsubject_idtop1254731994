	(function(){
		if(!/\((iPhone|iPad|iPod)/i.test(navigator.userAgent)){return};
		document.addEventListener('mousedown',function(e){
			var ele = e.target;
			do{
				if(ele.tagName == 'A'){
					ele.target = '_self';
					return;
				};
				if(ele.tagName == 'DIV'){return};
			}while(ele = ele.parentNode);
		},false);
	})();
</script>
