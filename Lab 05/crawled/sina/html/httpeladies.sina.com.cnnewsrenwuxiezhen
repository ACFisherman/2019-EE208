<script type="text/javascript">
function scrollImg(id){
	var posX,posY;
	if (window.innerHeight){
		posX = window.pageXOffset;
		posY = window.pageYOffset;
	}else if (document.documentElement && document.documentElement.scrollTop){
		posX = document.documentElement.scrollLeft;
		posY = document.documentElement.scrollTop;
	}else if (document.body){
		posX = document.body.scrollLeft;
		posY = document.body.scrollTop;
	}

	var tree = document.getElementById(id);
	if(posY > 300){
		//向上滚动的值超过300,需要滚动id的内容,这时只需要考虑高度
		tree.style.overflow = "";
		tree.style.position = "absolute";

		if(window.innerHeight){			// for ff and opera
			tree.style.left=(parseInt(document.body.scrollWidth)/2+300)+"px";
		}
		if((document.body.scrollHeight - posY) <= 700){

			var tempTop = (document.body.scrollHeight -  tree.offsetHeight -  200);

			tree.style.top = (document.body.scrollHeight -  tree.offsetHeight -  200)+"px";

			if(tempTop > posY){
				tree.style.top = (posY + 10) + "px";
			}else{
				tree.style.top = tempTop + "px";
			}
		}else{
			tree.style.top=(posY+10)+"px";
		}
	}else{
		tree.style.overflow = "hidden";
		tree.style.position = "";	
	}
	setTimeout(function(){scrollImg(id);},50);
}
scrollImg("tree");
</script>
