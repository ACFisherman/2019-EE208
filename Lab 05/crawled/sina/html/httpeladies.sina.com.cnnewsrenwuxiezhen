<script language="javascript">
	//搜索
	function search_change(o){
		var bs_radiovalue=o.value;
		if(bs_radiovalue=="img"){document.forms["cheadSearchForm"].c.value="img"}
		else if(bs_radiovalue=="video"){document.forms["cheadSearchForm"].c.value="video"}
		else{document.forms["cheadSearchForm"].c.value="news"}
	};

	document.cheadSearchForm.onsubmit = function(){
		var kw=this.q.value;
		if (typeof(this.c.value)=='undefined') {
			this.c.value = "news";
		}
		if (!kw || kw=='输入搜索关键字') {
			alert('请输入搜索关键词!');
			this.q.focus();
			return false;
		}
		if(this.c.value == 'video'){
			window.open('http://video.sina.com.cn/search/index.php?k='+kw);
			return false;
		}
	}

</script>

