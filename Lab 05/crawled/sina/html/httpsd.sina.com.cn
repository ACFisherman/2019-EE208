		link = content.link,
		monitor = content.monitor,
		pos = config.sinaads_ad_param_pos;
	
	el.style.cssText = ';position:absolute;left:-9999px;top:-9999px;';
	if(src){
		var fubiao = new core.Box({
			width : SIDE_SIZE[0],
			height : SIDE_SIZE[1] + SIDE_CLOSE_BTN_SIZE[1],
