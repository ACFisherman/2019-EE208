                cardList.opt.read.data.smartFlow = $this.data('topnews');
                cardList.opt.read.data.type = 'std_news,std_slide,std_video';
                cardList.opt.read.data.editLevel = '0,1,2,3,4';
                cardList.opt.read.data.pageSize = 20;
                //if ($this.data('mod') === 'nt_home_edu_hy') {
                cardList.opt.read.data.cTime = Math.round(((new Date()).getTime() - 1e3 * 60 * 60 * 24 * 30 * 2) / 1000);
                //}
