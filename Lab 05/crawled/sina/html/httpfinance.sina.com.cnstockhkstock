        <script>
            //初始化登录
            SFF.jsLoader(SFF.urls.jq, function() {
                var $ = jQuery;
                var _loginBtn = $('.user_login'),
                    _tabs = _loginBtn.closest('.tabs'),
                    _freshBtn = $('.fresh_btn', _tabs);
                SFF.dom.click.init();

                SFF.widgets.login.add('login_success', function() {
                    _loginBtn.addClass('hide');
                    _freshBtn.removeClass('hide');
                    $('[data-tabs="hkStock|myStock"]').removeClass('cur').trigger('click');
                });
                SFF.widgets.login.add('logout_success', function() {
                    _loginBtn.removeClass('hide');
                    _freshBtn.addClass('hide');
                    $('[data-tabs="hkStock|myStock"]').removeClass('cur').trigger('click');
                });
                SFF.widgets.login.init('SI_User');
            });
        </script>
