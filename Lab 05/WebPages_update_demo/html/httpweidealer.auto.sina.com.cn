			var num2 = $(this).addClass("curr").attr('data-rel');
			if(num==num2) return;
			$($('.cars-list')[num2]).show();
			$($('.cars-list')[num]).hide();
		});
	});
	
	// 构造大品牌
	//big_brand_id
	$('#big_brand_id').change(function(){
		var val = $(this).val();
		var tmp;
		$('#sub_brand_id').html('<option value="0">请选择</option>');
		$('#car_id').html('<option value="0">请选择</option>');
		if (val) {
			for (var i in brandTree[val].items) {
				tmp = $('<optgroup label="'+brandTree[val].items[i].name+'"></optgroup>');
				for (var j in brandTree[val].items[i].items) {
					$('<option data-rel="'+brandTree[val].items[i].val+'" value="'+brandTree[val].items[i].items[j].val+'">'+brandTree[val].items[i].items[j].name+'</option>').appendTo(tmp);
				}
				tmp.appendTo($('#sub_brand_id'));
			}
		}
	});
	// 构造子品牌和品牌
	//sub_brand_id
	$('#sub_brand_id').change(function(){
		var val = $(this).val();
		$('#car_id').html('<option value="0">请选择</option>');
		$('input[name="brand_id"]').val($(this).find("option:selected").attr('data-rel'));
		// 异步取得车型
		$.get('/ajax/notlogin.php?action=cars&subid='+val, function(data){
        	if (data.msg) {
        		for (var i in data.msg ) {
        			$('<option value="'+i+'">'+data.msg[i]+'</option>').appendTo($('#car_id'));
        		}
        	} else {
        		$('#car_id').html('<option value="0">无车型数据</option>');
        	}
    	},'json');
	});
	for (var i in brandTree) {
		$('<option value="'+brandTree[i].val+'">'+brandTree[i].name+'</option>').appendTo($('#big_brand_id'));
	}
});
