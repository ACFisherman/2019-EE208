	(function(){
		$(".blk-6-content").each(function(){
			var item = $(this).find(".p-item"),
				size = item.size(),
				parent = $(this).closest(".blk-6-wrap"),
				more = parent.find(".blk-6-more"),
				btn_more = parent.find(".more"),
				contanier = $(this);
		$.each(item,function(index,v){
				if(!(++index % 6)){
				$(v).css('margin-right','0');
				};
			})
			if(size > 12){
				more.show();
				btn_more.on('click',function(){
					if($(this).hasClass('slide')){
					$(this).removeClass('slide').children('span:first').html('展开更多');
						contanier.css('max-height','370px');
					}else{
					$(this).addClass('slide').children('span:first').html('收起');
						contanier.css('max-height','3000px');
					}
				});
			};
		$('.p-item').each(function(index){
				$(this).on('mouseenter',function(){
					$(this).children('.p-src').css('visibility','visible');
				}).on('mouseleave',function(){
					$(this).children('.p-src').css('visibility','hidden');
				});
			})
		});

	})();
</script>
	
	
