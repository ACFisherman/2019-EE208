<script language='javascript'>
$(document).ready(function(){
	var curdate=$('#focus_date li[class="cur"]');
	curdate=curdate.find('a').attr('year')+'/'+curdate.text().replace('月','/').replace('日','');
	$('#focus_date').attr('date',curdate);
	var createDate=function(ele,obj,direct){
		var date=ele.find('a').attr('year')+'/'+ele.text().replace('月','/').replace('日','');
		date=new Date(Date.parse(date));
		date=new Date(date.valueOf()+direct*1000*60*60*24);
		if(date.valueOf()==new Date(curdate).valueOf()){
			obj.addClass('cur');
		}else{
			obj.removeClass('cur');
		}
		obj.find('a').attr('year',date.getFullYear());
		obj.find('a').text((date.getMonth()+1)+'月'+date.getDate()+'日');
		return obj;
	};
	
	$('#focus_date li').bind('click',function(){
		var date=$(this).find('a').attr('year')+'-'+$(this).text().replace('月','-').replace('日','');
		$('#focus_date li[class="cur"]').removeClass('cur');
		$(this).addClass('cur');
		curdate=$(this).find('a').attr('year')+'/'+$(this).text().replace('月','/').replace('日','');
		$('#focus_date').attr('date',curdate);
		WEINEWS.showFocus(date);
	});
	var interval;
	var next=function(){
		var last=$('#focus_date li:last');
		var lastdate=last.find('a').attr('year')+'/'+last.text().replace('月','/').replace('日','');
		var now=new Date();
		now=now.getFullYear()+'/'+(now.getMonth()+1)+'/'+now.getDate();
		if(Date.parse(now)==Date.parse(lastdate)){
			return false;
		}
		last.after(createDate(last,$('#focus_date li:first'),1));
	}
	var prev=function(){
		var first=$('#focus_date li:first');
		var firstdate=first.find('a').attr('year')+'/'+first.text().replace('月','/').replace('日','');
		if(Date.parse(firstdate)==Date.parse('2012/8/1')){
			return false;
		}
		first.before(createDate(first,$('#focus_date li:last'),-1));
	}
	
	$('#focus_date .next').mousedown(function(){
		next();
		interval=setInterval(next,200);
	}).mouseup(function(){
		clearInterval(interval);
	}).mouseleave(function(){
		clearInterval(interval);
	});

	$('#focus_date .prev').mousedown(function(){
		prev();
		interval=setInterval(prev,200);
	}).mouseup(function(){
		clearInterval(interval);
	}).mouseleave(function(){
		clearInterval(interval);
	});

});
</script>

