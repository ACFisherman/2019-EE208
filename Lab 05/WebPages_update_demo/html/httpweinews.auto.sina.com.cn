<script language='javascript'>
$(document).ready(function(){
	var pop_delay_timer;
	//当前日期
	createCalander(new Date().getTime());
	//要求date格式为可转换为日期的格式
	function createCalander(time){
		$('.board').attr('date',time);
		var date=new Date(time);
		//年份
		var year=date.getFullYear();
		//月
		var month=date.getMonth()+1;
		//月天数
		var days=new Date(year,month,0).getDate();
		var day=date.getDate();
		$('.board .ui-datepicker-title .ui-datepicker-month').html(month);
		$('.board .ui-datepicker-title .ui-datepicker-year').html(year);
		//第一天为星期几
		var weeks=new Date(Date.parse(year+'/'+month+'/1')).getDay();
		var html='';
		for(var i=0;i<(days+weeks+((days+weeks)%7==0?0:(7-(days+weeks)%7)));i++){
			if(i%7==0){
				html+='<tr>';
			}
			if(i<weeks || i>=days+weeks){
				html+='<td class=" ui-datepicker-week-end ui-datepicker-other-month ui-datepicker-unselectable ui-state-disabled">&nbsp;</td>';
			}else{
				/*
				var todayStyle='';
				if(year==(new Date()).getFullYear() && month==(new Date()).getMonth()+1 && (i-weeks+1)==(new Date()).getDate()){
					todayStyle="style='font-weight:700;font-size:25px;'";//当前日期显示
				}
				html+="<td data-month='"+month+"' data-year='"+year+"'><a class='ui-state-default' day="+(i-weeks+1)+" href='javascript:void(0);' "+todayStyle+">"+(i-weeks+1)+"</a></td>";
				*/
				html+="<td data-month='"+month+"' data-year='"+year+"'><a class='ui-state-default' day="+(i-weeks+1)+" href='javascript:void(0);'>"+(i-weeks+1)+"</a></td>";
			}
			if(i%7==6){
				html+="</tr>";
			}
		}
		$('.board table tbody').html(html);

		$.get('/ajax/weinews.php?action=activities_month&month='+year+'-'+month,function(data){
		    if(data.code==0){
			    for(var i in data.content){
			    	var obj = $('.ui-datepicker-calendar td a[day='+i+']');
			    	obj.css('background-color',data.content[i]?'#0EA3DA':'#cccccc');
			    	obj.attr('color',obj.css('background-color'));
				    dateHover(obj);
				}
			}
		},'json');
	}
	$('.board .ui-datepicker-prev,.board .ui-datepicker-next').click(function(){
		var date = new Date(parseInt($('.board').attr('date')));
		var time = date.getTime();
		
		var year=date.getFullYear();
		var month=date.getMonth()+1;
		var days=new Date(year,month,0).getDate();
		
		if($(this).hasClass('ui-datepicker-prev')){
			time=time-days*24*60*60*1000;
		}else{
			time=time+days*24*60*60*1000;
		}
		createCalander(time);
	});
	function dateHover(obj){
		obj.hover(
			function(){
				$('.ui-datepicker-calendar td a').each(function(){
					$(this).css('background-color',$(this).attr('color'));
				});
			    obj.css('background-color','#FF8A00');
				clearTimeout(pop_delay_timer);
				$(".add_pop").remove();
				var year = $.trim(obj.parent().attr('data-year'));
				var mouth = $.trim(obj.parent().attr('data-month'));
				var day = $.trim(obj.attr('day'));
				if(year != '' && mouth  != '' && day != ''){
					pop_delay_timer = setTimeout(function(){
						$.get("/ajax/weinews.php?action=activity&date="+(year+'-'+mouth+'-'+day),function(data){
							if(data.code == 0){
								$('body').append(data.content);
								var popObj = $(".add_pop");
								popObj.hover(
									function(){
										clearTimeout(pop_delay_timer);
										popObj.addClass('mouse');
									},
									function(){
										pop_delay_timer = setTimeout(function(){
										    popObj.remove();
					        				obj.css('background-color',obj.attr('color'));
										},500);
									}
								);
								var top = obj.offset().top + 35;
								var pObj = obj.parent().parent();
								var tObj = pObj.parent().parent();
								var left = pObj.offset().left - popObj.width() + tObj.width();
								popObj.css('position','absolute').css("top", top).css("left", left);
							}
						},'json');
					},200);
				}
			},
			function(){
				clearTimeout(pop_delay_timer);
				var popObj = $(".add_pop");
				pop_delay_timer = setTimeout(function(){
				    if($('.mouse').length==0){
				    	popObj.remove();
				    	obj.css('background-color',obj.attr('color'));
					}
				},500);
			}
		);
	}
});


</script>






















