<script>
$.getJSON('http://m.caissa.com.cn/jp/index/getPpp?callback=?&pageSize=8&pageNo=1&terminal=PC',function(data){
                if(data&&data.code==200&&data.data.total>0){
                    var html = '';
                    $.each(data.data.list,function(i,v){
		        if (i > 5) return;
                        var dataString = v.dataList.length>1?v.dataList[0]+','+v.dataList[1]:v.dataList[0],queryType = v.queryType,proName = '';
                        if(queryType == 'ML'){
                        	var name = xxx(v.productNameAll.split(' ')[0]);
                        	for(var i = 0;i<name.length;i++){
                        		proName+='<span class="fl">'+name[i].replace(/-/g,'</span><s class="dc"></s><span class="fl">')+'</span> ';
                        	}
                        }else if(queryType =='OW'){
                        	proName = '<span class="fl">'+v.departure+'</span><s class="dc"></s><span class="fl">'+v.dest+'</span>'
                        }else{
                        	proName = '<span class="fl">'+v.departure+'</span><s class="wf"></s><span class="fl">'+v.dest+'</span>'
                        }
                        html += '<li pid="'+v.minPriceProductCode+'"><a href="http://flights.caissa.com.cn/international/Detail/index?outCity='+v.departure_id+'&inCity='+v.dest_id+'&productId='+v.minPriceProductCode+'&segDays='+v.days+'&queryType='+queryType+'" target="_blank"></a><div>'+proName+'</div><div class="clearfix"></div><em class="fr"><span class="f18">￥</span><span class="f24">'+v.priceLowest+'</span><span class="f14">起</span></em>'+dataString +'</li>';
                    });
                    $('#hotFlightProduct').html(html);
                }else{
                    $('#hotFlightProduct').hide();
                }
            })     

function xxx(a){
				var a = a.split(',')
				return a.reduce(function(list, item){
				 var lastcity = '';
				 var lastline = [];
				 if(list.length > 0){
				  lastline = list[list.length-1];
				  lastcity = lastline.length === 0 ? '' : lastline[lastline.length-1];
				 }
				 var temp = item.split('-');
				 if(temp[0] === lastcity){
				  lastline.push(temp[1]);
				 } else {
				  lastline = [];
				  lastline.push(temp[0]);
				  lastline.push(temp[1]);
				  list.push(lastline);
				 }
				 return list
				}, []).map(function(item){
				 return item.join('-')
				})
			}
</script>        
