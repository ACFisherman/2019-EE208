<script type="text/javascript">
    var StandardBJTime = 1570204800;
    function selectWeek(obj)
    {
        location.href = 'index.phtml?tradedate='+jQuery(obj).attr('dt');
    }
    !function ($)
    {
        var weekday = new function ()
        {
            var _days,_up,_down,_offset = 0;
            var _dayToCN = ['日','一','二','三','四','五','六'];
            function _draw()
            {
                var _now = StandardBJTime * 1000 + 1000 * 60 * 60 * 24 * _offset * 7;
                var _today = new Date(_now);
                var _dayToday = _today.getDay();
                if(_dayToday == 0)
                {
                    _dayToday = 6;
                }
                else
                {
                    _dayToday -= 1;
                }
                var _start = _now - 1000 * 60 * 60 * 24 * _dayToday;
                var _day;
                for(var i = 0;i < 7;i++)
                {
                    _day = new Date(_start + 1000 * 60 * 60 * 24 * i);
                    _days.eq(i).attr('dt',_day.getFullYear() + '-' + formatDT(_day.getMonth() + 1) + '-' + formatDT(_day.getDate()));
                    if(_days.eq(i).attr('dt') == '2019-10-05')
                        _days.eq(i).css({"color":"#FF0000","font-weight":"bold"});
                    else
                        _days.eq(i).css('color','#000000');
                    _days.eq(i).html(_day.getMonth() + 1 + '月' + _day.getDate() + '日(周' + _dayToCN[_day.getDay()] + ')');
                }
            }
            function formatDT(dt)
            {
                return dt < 10 ? '0' + dt : dt;
            }
            function _goUp()
            {
                _offset -= 1;
                _draw();
            }
            function _goDown()
            {
                _offset += 1;
                _draw();
            }
            this.init = function ()
            {
                _days = $('a.js-day');
                _up = $('#weekUp');
                _up.click(_goUp);
                _down = $('#weekDown');
                _down.click(_goDown);
                _draw();
            };
        } ();
        $(function ()
        {
            setInterval(function () { StandardBJTime += 1; },1000);
            weekday.init();
        });
    } (jQuery);
</script>            <!--e-->
