            <span class="zdnum"><a href="javascript:void(0)" data-sort="zdnumSort" class="{{=it.sort == 'zdnumSort' ? 'on'+it.asc : ''}}">涨跌额</a></span>
            <span class="percent"><a href="javascript:void(0)" data-sort="percentSort" class="{{=it.sort == 'percentSort' ? 'on'+it.asc : ''}}">涨跌幅</a></span>
        </div>
        <div id="mystock_list_%type%" class="mystock-list">
            <div class="mystock_body">
                <ul class="stock-list">
                    {{~it.data:value:index}}
                    <li class="{{=value.updown}} clearfix" data-symbol="{{=value.symbol || value}}" data-stock="{{=value.symbol || value}}">
                        <span class="name"><a target="_blank" class="stock-link" href="{{=value.Link}}"  suda-uatrack="key=touzi_pc_v2_market_today&value=touzi_pc_v2_market_today_%suda%">{{=value.name || '--'}}</a></span>
                        <span class="price stock-price">{{=value.price || '--'}}</span>
                        <span class="zdnum stock-ud">{{=value.zdnum || '--'}}</span>
                        <span class="percent stock-percent">{{=value.percent || '--'}}</span>
                        <span class="del" title="删除">×</span>
                    </li>
                    {{~}}
                </ul>
            </div>
        </div>

    </div>
</div>
</script>
