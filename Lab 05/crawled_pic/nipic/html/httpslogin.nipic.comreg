    <script type="text/javascript">
        $(function () {
            PWD.PWDStrength($("#Password"), $(".pwd-strength"));
            $("#Password").on("keyup", function () { PWD.PWDStrength($(this), $(".pwd-strength")); });
            $("#verifycodeImg").on("click", changeVerifyCode);
            $('input').placeholder();
            $('#regForm').on('submit',function(){
                $(this).find('.field-validation-error').each(function(i,v){
                    if(i!=0){
                        $(v).hide().siblings('.input-validation-error').removeClass('input-validation-error');
                    } else {
                            $(v).show()
                    }
                })
            })
            $('#Agree').on('change', function () {
                if ($(this).is(':checked')) {
                    $(this).parent().find('.field-validation-valid').hide();
                } else {
                    $(this).parent().find('.field-validation-error').show();
                }
            })
        })
    </script>
