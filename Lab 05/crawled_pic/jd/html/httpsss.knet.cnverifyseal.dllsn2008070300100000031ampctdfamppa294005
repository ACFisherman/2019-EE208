	<script type="text/javascript">
	
		//更多域名
		function more_domain_info_credit() {
			var box = document.getElementById("site_detail_info");
			var child = box.getElementsByTagName("div");
			var count=0;
			for ( var i = 0; i < child.length; i++) {
				var c = child[i];
				if (c.className == "mod") {
					if(count>0){
						if (domain_detail_info_open_flag==true) {
							c.style.display = "none";
							c.style.height ="20px";
						} else {
							c.style.display = "";
							c.style.height ="20px";
						}
					}
					count++;
				}
			}
			var link=document.getElementById("more_domain_info_link");
			var more=link.getAttribute("more");
			var hide=link.getAttribute("hide");
			if(domain_detail_info_open_flag==true){
				link.innerHTML=more+'&raquo;';
				domain_detail_info_open_flag=false;
			}
			else{
				link.innerHTML=hide+'&raquo;';
				domain_detail_info_open_flag=true;
			}
		}
		
	
		function autoclick_credit(count){
			var id="site_detail_info";
			var dom = document.getElementById(id);
		    if(!dom)return ;
			var arrObj = getElementsByClassName(id,"up","s");
			for(var i = 0; i<1 && i< count; i++){
				if(arrObj[i]==null)continue;
				arrObj[i].click();
			}
		}
		
		autoclick_credit(13);
		
	</script>
