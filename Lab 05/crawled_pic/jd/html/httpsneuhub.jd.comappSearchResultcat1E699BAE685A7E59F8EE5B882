    sortObj = {
       time: 1, //0，降序，1，升序
       price: 1 //0，降序，1，升序
    }
        
    window.initArrows();

    var cat1 = "智慧城市";
    var cat2 = "";
    var payType = "";
    var price = "";
    var text = "";
    var searchTag = "";
    var sortColumn = "";
    var sortType = "0";
    var pageSize = "10";

    // 排序按钮点击
    $('.sort-button').click(function() {
      $(this).addClass('active').siblings().removeClass('active');

      // 综合排序
      if ($(this).hasClass('sort-all')) {
        var query = getSearchCondition();
        window.location.href = "/appSearchResult?" + query;
      }

      // 按时间排序
      if ($(this).hasClass('sort-time')) {
        sortObj.time = sortObj.time == 0 ? 1 : 0;
        sortType = sortObj.time;
        sortColumn = "time";
        var query = getSearchCondition();
        if (query != "") {
          query += "&";
        }
        if (sortObj.time == 1) {
          query += "sortByPublish=asc";
        } else {
          query += "sortByPublish=desc";
        }
        window.location.href = "/appSearchResult?" + query;
      }

      // 按价格排序
      if ($(this).hasClass('sort-price')) {
        sortObj.price = sortObj.price == 0 ? 1 : 0
        sortType = sortObj.price;
        sortColumn = "price";
        var query = getSearchCondition();
        if (query != "") {
          query += "&";
        }
        if (sortObj.price == 1) {
          query += "sortByPrice=asc";
        } else {
          query += "sortByPrice=desc";
        }
        window.location.href = "/appSearchResult?" + query;
      }

      // 按成交量排序
      if ($(this).hasClass('sort-num')) {
      }

      // 上下箭头颜色变化
      window.initArrows();

    })

    //删除标签
    window.tagRemoveClickCallback = function () {
      // alert("tagRemoveClickCallback");
      // alert(getQuery());
      //window.location.href = getQuery();
    }

    $(".js-searchTag-list-top .neu-close").click(function () {
      $(this).parent().remove();
      window.location.href = getQuery();
    })

    //点击标签
    window.tagClickCallback = function () {
      window.location.href = getQuery();
    }

    //筛选条件点击
    window.filterClickCallback = function () {
      window.location.href = getQuery();
    }


    function getQuery() {
      var query = getSearchCondition();
      var tag = getTag();
      var text = getSearchText();

      if (tag != "") {
        if (query != "") {
          query += "&";
        }
        query += "tag=" + encodeURI(tag);
        text = "";
      }

      if (text != "") {
        if (query != "") {
          query += "&";
        }
        query += "text=" + encodeURI(text);
      }

      if (query == "") {
        return "/appSearchResult";
      } else {
        return "/appSearchResult?" + query;
      }
    }

    function addUrlConnectCode(href) {
      if (!href.endsWith("?")) {
        href += "&";
      }
      return href;
    }

    function getUrlParam(name) {
      var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)') //构造一个含有目标参数的正则表达式对象
      var r = window.location.search.substr(1).match(reg) //匹配目标参数
      if (r != null) return unescape(r[2])
      return null //返回参数值
    }

    function getSearchCondition() {
      var catNew1 = "";

      var query = "";
      catNew1 = $("#catOne ol.active").html().trim();
      cat2 = $("#catTwo ol.active").html().trim();
      payType = $("#catThree ol.active").html().trim();
      price = $("#catFour ol.active").html().trim();

      if (cat1 != "" && cat1 != catNew1 && catNew1 == "全部") {
        catNew1 = "";
        cat2 = "";
      }

      if (catNew1 != "" && cat2 != "") {
        if (catNew1 == "全部") {
          if (cat2 != "全部") {
            query += "cat1=" + encodeURI(jsCategories[0].name) +"&cat2=" + encodeURI(cat2);;
          }
        } else {
          query += "cat1=" + encodeURI(catNew1);
          if (cat2 != "全部") {
            query += "&cat2=" + encodeURI(cat2);
          }
        }
      }

      if (payType != "全部" && payType != "") {
        if (query == "") {
          query += "payType=" + encodeURI(payType);
        } else {
          query += "&payType=" + encodeURI(payType);
        }
      }

      if (price != "全部" && price != "") {
        if (query == "") {
          query += "price=" + encodeURI(price);
        } else {
          query += "&price=" + encodeURI(price);
        }
      }

      return query;
    }

    function getTag() {
      var tag = $(".js-searchTag-list-top .tag .searchTag-list-txt").html();
      if (typeof(tag) == "undefined") {
        tag = "";
      }

      if (tag != "" || tag.trim() != "") {
        return tag;
      } else {
        return "";
      }
    }

    function getSearchText() {
      var text = $("#seachinput").val();
      if (text != "" || text.trim() != "") {
        return text;
      } else {
        return "";
      }
    }

    function search() {

      var query = getSearchCondition();
      var text = getSearchText();

      if (text != "") {
        if (query != "") {
          query += "&";
        }
        query += "text=" + encodeURI(text);
      }

      if (query == "") {
        query = "/appSearchResult";
      } else {
        query = "/appSearchResult?" + query;
      }

      window.location.href = query;
    }

    function wordorder() {
      window.location.href = "/workorder/init/8/0";
    }

    function userinfo() {
      window.location.href = "/user/baseInfo";
    }

    $('.sr-pagination-cnt').pagination({

      showData: 10,
      current: 1,
      totalData: 46,
      jump: true,
      coping: true,
      homePage: '首页',
      endPage: '末页',
      prevContent: '上页',
      nextContent: '下页',
      callback: function(api) {
        var cur = api.getCurrent()

        var href = "/appSearchResult?";
        if (cat1 != "") {
          href += "cat1=" + encodeURI(cat1);
        }
        if (cat2 != "") {
          href = addUrlConnectCode(href);
          href += "cat2=" + encodeURI(cat2);
        }
        if (text != "") {
          href = addUrlConnectCode(href);
          href += "text=" + encodeURI(text);
        }
        if (searchTag != "") {
          href = addUrlConnectCode(href);
          href += "tag=" + encodeURI(searchTag);
        }
        if (payType != "") {
          href = addUrlConnectCode(href);
          href += "payType=" + payType;
        }
        if (price != "") {
          href = addUrlConnectCode(href);
          href += "price=" + price;
        }

        if (sortColumn != "" && (sortColumn == "price" || sortColumn == "time")) {
          href = addUrlConnectCode(href);
          if (sortColumn == "price") {
            if (sortType == 1) {
              href += "sortByPrice=asc";
            } else {
              href += "sortByPrice=desc";
            }
          } else if (sortColumn == "time") {
            if (sortType == 1) {
              href += "sortByPublish=asc";
            } else {
              href += "sortByPublish=desc";
            }
          }
        }

        href = addUrlConnectCode(href) + "pageSize=" + pageSize;
        location.href = href + "&pageNum=" + cur;
      }
    })

  </script>
</html>
