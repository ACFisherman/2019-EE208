  <script type="text/javascript" src="/static/portal/js/productDetail.3e006f8a0b105032af71.js"></script>
  <script>
  function search() {
    var seachinput = $("#seachinput").val();
    if (seachinput == null || seachinput.trim() == "") {
      window.location.href = "/appSearchResult";
    } else {
      window.location.href = "/appSearchResult?text=" + seachinput;
    }

  }

  function workorder() {
    window.location.href = "/workorder/init/8/0";
  }

  function userinfo() {
    window.location.href = "/user/baseInfo";
  }

  /**
   * 购买
   */
  function goodsPurchase() {
    if (window.isNegotiateApp) {
      $(location).attr('href', '/workorder/init/2/服务市场解决方案');
      return;
    }

    var vId = $('#version .pd-spec-active').data('vid');
    var chargeId = $('#pd .pd-spec-active').data('cid');
    //免费试用天数
    if (chargeId == 0) {
      $(location).attr('href', '/order/submit/page?id=' + vId + '&skuType=' + skuType + '&count=1&chargeId=0');
      return;
    }

    //免费产品
    if (window.isFreeApp) {
      $(location).attr('href', '/order/submit/page?id=' + vId + '&skuType=' + skuType + '&count=1' + '&chargeId=' + chargeId);
      return;
    }

    //收费产品(包含免费版本)
    var count = parseInt($("#pd-b-input").val());
    $(location).attr('href', '/order/submit/page?id=' + vId + '&skuType=' + skuType + '&count=' + count + '&chargeId=' + chargeId);
    /*order.orderRequestNew(apiId, skuType, 1, 0, function (orderId) {
        console.log("订单ID:" + orderId);
    });*/
  }

  /**
   * 设置价格
   */
  function setPrice(totalCount, price, discount) {
    var totalPrice = price * totalCount;
    var discountPrice = totalPrice * discount * 0.01;
    discountPrice = discountPrice.toFixed(2);
    $(".pd-price-num").html("￥" + discountPrice)
    $(".pd-price-original").html("[￥" + totalPrice + "]");
  }

  /**
   * 手动修改数量
   */
  $("#pd-b-input").bind("input propertychange", function(event) {
    var countStr = $('#pd-b-input').val();
    var count = +countStr;
    if (typeof count !== 'number' || isNaN(count) || count <= 0 || countStr.indexOf(".") != -1) {
      count = 1;
      $('#pd-b-input').val(count);
    }
  })

  /**
   * 添加数量
   */
  $('#pd-b-op-bar1').click(function() {
    var chargeId = $('#pd .pd-spec-active').data('cid');
    if (chargeId == 0 || window.isFreeApp) {
      //免费
      $('#pd-b-input').val(1);
    } else {
      //非免费
      var price = $('#pd .pd-spec-active').data('price');
      var discount = $('#pd .pd-spec-active').data('discount');
      var totalCount = parseInt($("#pd-b-input").val());
      setPrice(1, price, discount);
    }
  })

  /**
   * 减少数量
   */
  $('#pd-b-op-bar2').click(function() {
    var chargeId = $('#pd .pd-spec-active').data('cid');
    if (chargeId == 0 || window.isFreeApp) {
      //免费
      $('#pd-b-input').val(1);
    } else {
      //非免费
      var price = $('#pd .pd-spec-active').data('price');
      var discount = $('#pd .pd-spec-active').data('discount');
      var totalCount = parseInt($("#pd-b-input").val());
      if (totalCount == 0) {
        totalCount = totalCount + 1;
      }
      $("#pd-b-input").attr("value", totalCount);
      setPrice(1, price, discount);
    }
  })

  /**
   * 点击套餐版本后触发事件
   */
  $('.pd-spec').click(function() {
    $('.pd-spec-v').removeClass('pd-spec-active');
    $(this).addClass('pd-spec-active');
    var vId = $('#version .pd-spec-active').data('vid');
    getCharge(vId);
  })

  /**
   * 获取周期类型
   */
  function getCharge(vId) {
    //面议
    if (window.isNegotiateApp) {
      $('#pdVersion1').empty();
      $('#pdVersion2').empty();
      $('.pd-b-input-comp').remove();
      $(".pd-price-num").html("面议")
      $(".pd-price-original").html("");
      $("#pd-buybtn").html("立即咨询");
      return;
    }

    //判断套餐版本
    for (var i = 0; i < appVerisonList.length; i++) {
      var content;
      if (appVerisonList[i].id == vId) {
        var appCharge = appVerisonList[i].charges;
        var isFree = appVerisonList[i].isFree;
        var freeDays = appVerisonList[i].freeDays;
        $('#pd').empty();
        var flag = 0;
        var totalCount = parseInt($("#pd-b-input").val());
        for (var j = 0; j < appCharge.length; j++) {
          var count = appCharge[j].count;
          var id = appCharge[j].id;
          var price = appCharge[j].price;
          var discount = appCharge[j].discount;
          var chargeType = appCharge[j].chargeType;
          if (window.isFreeApp) {
            //免费
            content = "<div class='pd-spec pd-spec-c' data-cid='" + id + "' data-discount='" + discount + "' data-price='" + price + "'></div>"
            $('#pd').append(content);
            $('#pd .pd-spec').addClass('pd-spec-active');
            $("#pdVersion2").css('display', 'none');
            setPrice(1, 0.0, 100);
            return;
          } else {
            //收费
            if (freeDays != 0 && isFree == 1 && flag == 0) {
              content = "<div class='pd-spec pd-spec-c' data-cid='0' data-discount='" + 100 + "' data-price='" + 0.0 + "'>" + freeDays + "天免费</div>"
              $('#pd').append(content);
              flag = 1;
              $('#pd .pd-spec').addClass('pd-spec-active');
              //价格
              setPrice(1, 0.0, 100);
            }
            if (chargeType == 0) {
              //次
              content = "<div class='pd-spec pd-spec-c' data-cid='" + id + "' data-discount='" + discount + "' data-price='" + price + "'>" + count + "次 </div>"
            } else if (chargeType == 1) {
              //天
              content = "<div class='pd-spec pd-spec-c' data-cid='" + id + "' data-discount='" + discount + "' data-price='" + price + "'>" + count + "天 </div>"
            } else if (chargeType == 2) {
              //月
              content = "<div class='pd-spec pd-spec-c' data-cid='" + id + "' data-discount='" + discount + "' data-price='" + price + "'>" + count + "月 </div>"
            } else if (chargeType == 3) {
              //年
              content = "<div class='pd-spec pd-spec-c' data-cid='" + id + "' data-discount='" + discount + "' data-price='" + price + "'>" + count + "年 </div>"
            } else if (chargeType == 4) {
              //件
              content = "<div class='pd-spec pd-spec-c' data-cid='" + id + "' data-discount='" + discount + "' data-price='" + price + "'>" + count + "件 </div>"
            } else if (chargeType == 5) {
              //套
              content = "<div class='pd-spec pd-spec-c' data-cid='" + id + "' data-discount='" + discount + "' data-price='" + price + "'>" + count + "套 </div>"
            }

            //添加周期
            $('#pd').append(content);

            if (j == 0 && flag == 0) {
              $('#pd .pd-spec').addClass('pd-spec-active');
              //价格
              setPrice(1, price, discount);
            }
          }

        }
      }
    }
  }

  /**
   * 点击周期后触发事件
   */
  $('#pd').click(function(e) {
    $('#pd-b-input').val(1);
    var target = e.target
    if (target.className.indexOf('pd-spec-c') !== -1) {
      $('.pd-spec-c').removeClass('pd-spec-active');
      $(target).addClass('pd-spec-active');
      var vId = $('#version .pd-spec-active').data('vid');
      var cId = $('#pd .pd-spec-active').data('cid');

      //判断套餐版本
      for (var i = 0; i < appVerisonList.length; i++) {
        if (appVerisonList[i].id == vId) {
          var appCharge = appVerisonList[i].charges;
          var chargeType = appVerisonList[i].chargeType;
          var isFree = appVerisonList[i].isFree;
          var freeDays = appVerisonList[i].freeDays;
          var totalCount = parseInt($("#pd-b-input").val());
          if (isFree == 1 && cId == 0 && freeDays != 0) {
            setPrice(1, 0.0, 100);
          } else {
            for (var j = 0; j < appCharge.length; j++) {
              var chargeId = appCharge[j].id;
              if (chargeId == cId) {
                var count = appCharge[j].count;
                var price = appCharge[j].price;
                var discount = appCharge[j].discount;
                if (chargeType == 1 && isFree == 0) {
                  price = 0;
                }
                //价格
                setPrice(1, price, discount);
              }
            }
          }
        }
      }
    }
  })
  var apiId = 109;
  var skuType = 3;
  var appVerisonList = [];
  var isNegotiateApp = 2 == 2;
  var isFreeApp = 2 == 0;

  var vId = $('#version .pd-spec-active').data('vid');
  getCharge(vId);

  var intro = '为大型企业科技部门建立一套可控的人工智能管理平台，使其能够将AI能力对外开放。让企业在AI时代轻松聚合资源，拓展销路';
  $('.pd-product-desc').text(intro)
  </script>
