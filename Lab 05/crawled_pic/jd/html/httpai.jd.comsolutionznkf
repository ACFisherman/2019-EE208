<script>
    window.addEventListener('load',function(){
        var curUrl=location.href;

        var js_login=document.getElementsByClassName('login')[0]
        if(js_login){
            var js_login_href=js_login.getElementsByTagName('a')[0]
            js_login_href.setAttribute("href",'https://passport.jd.com/new/login.aspx?ReturnUrl='+curUrl)
        }

        var curUrl=location.href;

        var js_reg=document.getElementsByClassName('register')[0]
        if(js_reg){
            var js_login_href=js_reg.getElementsByTagName('a')[0]
            js_login_href.setAttribute("href",'https://reg.jd.com/reg/person?ReturnUrl='+curUrl)
        }

        var curUrl=location.href;

        var js_logout=document.getElementsByClassName('logout')[0]

        // console.log("js_login",js_login)

        if(js_logout){
            var js_login_href=js_logout.getElementsByTagName('a')[0]
            js_login_href.setAttribute("href",'https://passport.jd.com/uc/login?ltype=logout&ReturnUrl='+curUrl)
        }

        $('#search-input').on('keyup',function (e) {
            var searchUrl =$("#search-input").val();
            if (e.keyCode === 13) {
                window.location.href = "/searchResult?text="+encodeURI(searchUrl)+"&source=0&pageNum=1";
                //alert($("#search-input").val())
            }
        })
    })

</script>
