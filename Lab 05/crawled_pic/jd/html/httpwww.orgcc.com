        <dd class="mb20">请使用微信扫描二维码登录会员</dd>
        <dd><a href="javascript:void(0);" id="backlogin" class="lancss">返回其他方式登录</a></dd>
    </dl>
</div>
<script src="http://libs.baidu.com/jquery/1.8.1/jquery.min.js"></script>
<script src="http://res.wx.qq.com/connect/zh_CN/htmledition/js/wxLogin.js"></script>
<script src="/static/js/login.js"></script>
<script type="text/javascript" src="/static/js/pause.js"></script>
<script type="text/javascript" src="/static/js/swiper.c.js"></script>
<script type="text/javascript">
$(document).ready(function(){
	$("#sublink").click(function(){
    	$("#subform").submit();
    });
        
	var swiper = new Swiper('.swiper-container', {
		spaceBetween: 30,
		centeredSlides: true,
		autoplay: {
			delay: 2500,
			disableOnInteraction: false,
		},pagination: {
			el: '',
			clickable: false,
		},navigation: {
			nextEl: '.swiper-button-next',
			prevEl: '.swiper-button-prev',
		},
	});
	
	//tab标签切换
	$(".tabs > li > a").mouseover(function(e) { 
		if (e.target == this) { 
			var tabs = $(this).parent().parent().children("li"); 
			var panels = $(this).parent().parent().parent().children(".litabs"); 
			var index = $.inArray(this, $(this).parent().parent().find("a")); 
			if (panels.eq(index)[0]) { 
				tabs.removeClass("liselect") 
				.eq(index).addClass("liselect"); 
				panels.addClass("lihide") 
				.eq(index).removeClass("lihide"); 
			} 
		}
	});
	//艺术家翻牌
	var aLi = $('.artsbox dd');
	var aImg =  $('.artsbox dd img');
	var aSpan = $('.artsbox dd span');
	aLi.each(function(index){
		$(this).mouseover(function(){
			aSpan.eq(index).stop();
			aImg.eq(index).stop();
			aImg.eq(index).css({zIndex:1}).animate({top:75,height:0},100,'',function(){
				$(this).hide();
				aSpan.eq(index).show().css({zIndex:2}).animate({top:0,height:150},200)
			});
		});
		$(this).mouseout(function(){
			aSpan.eq(index).stop();
			aImg.eq(index).stop();
			aSpan.eq(index).css({zIndex:1}).animate({top:75,height:0},100,'',function(){
				$(this).hide();
				aImg.eq(index).show().css({zIndex:2}).animate({top:0,height:150},200)
			});
		});
	});
	
	//加载艺术家会员动态
    var scrtime;
    var $ul = $("#artscon ul");
    var liFirstHeight = $ul.find("li:first").height();
    $ul.css({ top: "-" + liFirstHeight + "px" });
    $("#artscon").hover(function () {
        $ul.pause();
        clearInterval(scrtime);
    }, function () {
        $ul.resume();
        scrtime = setInterval(function scrolllist() {
            $ul.animate({ top: 0 + "px" }, 1500, function () {
                $ul.find("li:last").prependTo($ul);
                liFirstHeight = $ul.find("li:first").height();
                $ul.css({ top: "-" + liFirstHeight + "px" });			
            });
        }, 3300);
    }).trigger("mouseleave");
});

$('#wdzuopin li').mouseenter(function(){
	$(this).find('.divA').stop().animate({bottom:'-60px'});
	$(this).find('.buylink2').css({left:'0'})
});
$('#wdzuopin li').mouseleave(function(){
	$(this).find('.divA').stop().animate({bottom:'15px'});
	$(this).find('.buylink2').css({left:-$(this).width()})
});
</script>
</body>
</html>