<script>
    var pic1 = document.getElementById("pic1");
    var parentLi = document.getElementById("parentLi");
    pic1.addEventListener('mousemove', function (evt){
        var x = evt.offsetX;
        var y = evt.offsetY;
        var winWidth = parentLi.offsetWidth;
        var winHeight = parentLi.offsetHeight;
        var halfWidth = winWidth / 2;
        var halfHeight = winHeight / 2;

        var rx = x - halfWidth;
        var ry = halfHeight - y;

        var max = 80;
        var dx = (pic1.getBoundingClientRect().width/max)*(rx / -halfWidth);
        var dy = (pic1.getBoundingClientRect().height/max)*(ry / halfHeight);
        pic1.style['transform'] = pic1.style['-webkit-transform'] = 'translate('+dx+'px,'+dy+'px)';

    }, false);
</script>


