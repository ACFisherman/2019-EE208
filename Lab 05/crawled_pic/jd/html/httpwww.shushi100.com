    /*精彩专题图片切换 start*/
    function arrowState(pre,next,li_num,li_index,num){
        if(li_index+num<li_num&&li_index-num>-1){
            pre.removeClass("nothing_more");
            next.removeClass("nothing_more");
            return;
        }
       if(!(li_index+num<li_num)){
            pre.addClass("nothing_more");
            next.removeClass("nothing_more");
        } 
        if(!(li_index-num>-1)){
            pre.removeClass("nothing_more");
            next.addClass("nothing_more");
        } 
    }
    function scrollList(parent,ul,li,m_num,space){
        var li_num=li.length;
        var li_w=li.outerWidth();
        var num=m_num||1;
        var li_index=0;
        var space=space||10;
        var prev=parent.find(".prev"),
            next=parent.find(".next");
        ul.css({"width":li_num*li_w+space*li_num});
        prev.click(function () {
                if(li_index+num<li_num){
                    li_index+=num;
                    ul.animate({ marginLeft: "-"+li_index*(li_w+space)+"px" }, 500);
                }
                arrowState(prev,next,li_num,li_index,num);
        });
        next.click(function () {
                if(li_index-num>-1){
                    li_index-=num;
                    ul.animate({ marginLeft: "-"+li_index*(li_w+space)+"px" }, 500);
                }
                arrowState(prev,next,li_num,li_index,num);
        });
    
    }
    /*参数：父容器、ul、li，一次滑动几个元素，默认是1，间隔默认是10*/
