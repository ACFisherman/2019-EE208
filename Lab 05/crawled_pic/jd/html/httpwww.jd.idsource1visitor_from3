<script type="text/javascript">
  (function () {
    jQuery(function () {
      jQuery("#virtual_floor .rechargePay_title a").click(function () {
        var item = jQuery(this);
        if (item.attr("data-urlType") == "IFRAME") {
          item.addClass("cur").siblings().removeClass("cur");
          $('#virtual_floor .cont iframe').attr("src", item.attr("data-url"));
          $("#virtual_floor .cont").addClass("show")
          return;
        }
        window.open(item.attr("data-url"), "_blank");
      });
    });
  })();
</script>
