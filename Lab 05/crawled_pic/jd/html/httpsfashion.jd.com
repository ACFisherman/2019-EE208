  <script>
      // 顶通
      (function($) {
        //素材中心广告组接口URL
        var adsUrl =
          "//api.m.jd.com/client.action?functionId=queryMaterialAdverts&body={'ids':'03309309','sourceCode':'pc-mobile','pageId':'2019'}&client=wh5&clientVersion=1.0.0";
        var b_source = {};

        getAdsSource(); //获取广告组资源
        //获取广告组资源
        function getAdsSource() {
          requestFun(adsUrl).then(function(data) {
            try {
              var _data = data.advertInfos;
              b_source = data.advertInfos[0].list[0];
              if(b_source) {
                renderSmallBanner();
              }
            } catch (err) {}
          });
        }
        //渲染顶通
        function renderSmallBanner() {
          var data = b_source;
          var now = new Date().getTime();
          var start = new Date(data.beginTime.replace(/-/g, "/")).getTime();
          var picUrl = data.pictureUrl;
          // var picUrl1 = data.extension.pictureUrl;
          //extension
          // if (now > start && picUrl != "") {

          // }
          var domStr =
            '<div class="topAdsPart" style="background:#811036">' +
            '<div class="main w" style="position:relative;width:1190px;">' +
            '<a href="' +
            data.link +
            '" clstag="channel|keycount|7242|top_ads_20190618" target="_blank">' +
            '<img src="' +
            picUrl +
            '" alt="" />' +
            "</a>" +
            '<i style="position:absolute;top:8px;right:8px;width:20px;height:20px;background:url(//img20.360buyimg.com/da/jfs/t1843/324/2134861539/1329/b076942c/56c699e8N54391b78.png) no-repeat;cursor:pointer;"></i>' +
            "</div>" +
            "</div>";
          $("#o-header-2013").after(domStr);
          $(".topAdsPart .main").delegate("i", "click", function(event) {
            event.stopPropagation();
            $(".topAdsPart").slideUp("400");
          });
        }
        //ajax
        function requestFun(url) {
          var dtd = $.Deferred();
          $.ajax({
            url: url,
            data: {},
            type: "get",
            dataType: "jsonp",
            success: function(data) {
              dtd.resolve(data);
            },
            error: function(data) {},
            complete: function() {}
          });
          return dtd.promise(); // 返回promise对象
        }
      })(jQuery);
    </script>
