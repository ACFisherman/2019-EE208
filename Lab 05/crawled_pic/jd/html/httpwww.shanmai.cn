<script>
    var search_keywords = document.getElementById("search_keywords"),
        search_suggest = document.getElementById("gov_search_suggest"),
        cpLock = false;
    function y_espe(html){
        var elem = document.createElement('div');
        var txt = document.createTextNode(html);
        elem.appendChild(txt);
        return elem.innerHTML;
    }
    function y_unespe(str) {
        var elem = document.createElement('div');
        elem.innerHTML = str;
        return elem.innerText || elem.textContent;
    }
    function tohanzi(data){
        if(data.indexOf("\\u") == -1) return data;
        data = data.split("\\u");
        var str ='';
        for(var i=0;i<data.length;i++){
            if(!data[i]) continue;
            if(data[i].length>4){
                str+=String.fromCharCode(parseInt(data[i].slice(0,4),16).toString(10));
                str+=data[i].slice(4);
            }else if(data[i].length==4){
                str+=String.fromCharCode(parseInt(data[i],16).toString(10));
            }else{
                console.log("编码位数少于4，错误");
            }
        }
        return str;
    }
    /*
        js拆分cookie
    */

    function y_getCookie(name){
        var list = document.cookie.split(";"),
            obj = {};
        for(var i=0;i<list.length;i++){
            var arr = list[i].split("=");
            obj[arr[0].trim()] = arr[1].trim();
        }
        return obj[name];
    }
    /*
    中文锁
    */
    search_keywords.addEventListener("compositionstart",function(){
        cpLock = true;
    });
    search_keywords.addEventListener("compositionend",function(){
        cpLock = false;
    });
    /*
    搜索词为空时
    */
    search_keywords.addEventListener("focus",function(){
        if(this.value){
            sendKeyWordToBack(this.value);
        }else{
            var search_name = y_getCookie("search_name");
            if(!search_name) return;
            var str = decodeURIComponent(search_name),
                list = str.slice(1,-1).split(","),
                html = '';
            /**/
            // console.log(str);
            for(var i=0;i<list.length;i++){
                html += '<li>' + tohanzi(list[i].slice(1,-1)).replace("+", " ") + '</li>';
            }
            search_suggest.getElementsByTagName("ul")[0].innerHTML = html;
            search_suggest.style.display="block";
        }
    });
    /*
    搜索词变化时
    */
    search_keywords.addEventListener("input",function(){
        var t = this;
        setTimeout(function(){
            if(cpLock) return;
            sendKeyWordToBack(t.value);
        },0);
    });
    document.getElementsByTagName("body")[0].addEventListener("click",function(e){
        if(e.target.id != "search_keywords") suggestDisplay();
    });
    search_suggest.addEventListener("click",function(e){
        if(e.target.tagName == "LI"){
            document.getElementById("search_keywords").value=y_unespe(e.target.innerHTML);
            suggestDisplay();
        }else if(e.target.tagName == "SPAN"){
            document.getElementById("search_keywords").value=y_unespe(e.target.parentNode.innerHTML);
            suggestDisplay();
        }
    });
    function suggestDisplay(){
        search_suggest.style.display="none";
    }
    function sendKeyWordToBack(keyword){
        /*
            小于1字符
             if(keyword.length <= 1) return;
            小于2字符
             if(keyword.replace(/[^\u0000-\u00ff]/g,"aa").length <= 2) return;
            空格
        */

        var clist = [" ", "  "];
        if(clist.indexOf(keyword) != -1) return;
        new Request({
            url:'//www.shanmai.cn/index-Key_word.html',
            data:{search:keyword},
            onSuccess:function(re) {
                if(!re) return;
                try{
                    var rs = JSON.decode(re);
                }catch(error){
                    console.log(error);
                    return;
                }
                if(!rs.data || rs.data.length == 0) return;
                var ul = search_suggest.getElementsByTagName("ul")[0],
                    html = '';
                for(var i=0;i<rs.data.length;i++){
                    html += '<li>' + rs.data[i].cat_name + '</li>';
                }
                if(keyword){
                    html = html.replace(new RegExp(keyword,"gm"),'<span style="color:red;">'+keyword+'</span>');
                }
                ul.innerHTML = html;
                search_suggest.style.display="block";
            }
        }).get();
    }
</script>
