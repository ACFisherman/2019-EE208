	(function(w,d) {
function getCookie(name) {
  let arr;
  const reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
  if (arr = d.cookie.match(reg))
    return unescape(arr[2]);
  else
    return null;
}
var jmp = getCookie('jmp');
if(jmp) {
  w.dataLayer = w.dataLayer || [];
  w.dataLayer.push({
    userID: jmp
  });
}

})(window,document);
