<script type="text/javascript">



    (function(w) {
        function getCookie(name) {
            let arr;
            const reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
            if (arr = document.cookie.match(reg))
                return unescape(arr[2]);
            else
                return null;
        }
        w.dataLayer = w.dataLayer || [];
        w.dataLayer.push({
            event: 'dataToGTM',
            PageType: 'HomePage',
            email: getCookie('criteo') || ''
        });
    })(window);

</script>
