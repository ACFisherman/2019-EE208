<script>
	(function(){
  var navSectionDom = document.getElementById('nav-section-app');
  var categoryTitle = document.getElementById('nav-section-category-title');
  var defaultCategoryTitle = 'หมวดสินค้า';
  var extraClass = 'allow-hover';
  window.commonHeaderConfig = window.commonHeaderConfig || {};
  if(!window.commonHeaderConfig.banHoverCategory) {
    navSectionDom.className = navSectionDom.className + ' allow-hover';
  }
  if(window.commonHeaderConfig.customCategoryTitle) {
    categoryTitle.innerText = window.commonHeaderConfig.customCategoryTitle;
    categoryTitle.parentNode.setAttribute('title', window.commonHeaderConfig.customCategoryTitle);
  } else {
    categoryTitle.innerText = defaultCategoryTitle;
    categoryTitle.parentNode.setAttribute('title', defaultCategoryTitle);
  }
  var host = window.location.host;
  var pathname = window.location.pathname;
  var hostArray = ['3w.jd.co.th', 'www.jd.co.th'];
  if (hostArray.indexOf(host) >= 0 && pathname === '/') {
    navSectionDom.style.borderBottom = 'none';
  }
})();
</script>
