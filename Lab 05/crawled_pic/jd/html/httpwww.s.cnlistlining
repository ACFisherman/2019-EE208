function changeinfoNum(id){
	if(id){
		var ckname = 'myinfo_'+id;
		var name = "l_"+id;	
		Cookie.set('S['+ckname+']','');
		var hideCkName = Cookie.get('S[hideCkName]');
		//不显示
		if(hideCkName){
			Cookie.set('S[hideCkName]',hideCkName+'|||'+id);
		}else{
			Cookie.set('S[hideCkName]',id);
		}
		$(name).hide(); //隐藏对应的节点
		count--;
		if(count >0){
			$('info_num').addClass('myinfo_tip_num');
			$('info_num').set('text',count);
