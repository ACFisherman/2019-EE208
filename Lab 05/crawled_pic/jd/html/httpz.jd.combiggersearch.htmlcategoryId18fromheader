<script type="text/javascript">
	$(function(){
		//项目状态标签默认选中
		var searchProjectStatus = $.trim($("#searchProjectStatus").val());
		if(searchProjectStatus == ""){
			$("#searchProjectStatusTitle").html("全部<strong></strong>");
		}else if(searchProjectStatus == "1"){
			$("#searchProjectStatusTitle").html("预热中<strong></strong>");
		}else if(searchProjectStatus == "2"){
			$("#searchProjectStatusTitle").html("众筹中<strong></strong>");
		}else if(searchProjectStatus == "4"){
			$("#searchProjectStatusTitle").html("众筹成功<strong></strong>");
		}else if(searchProjectStatus == "8"){
			$("#searchProjectStatusTitle").html("项目结束<strong></strong>");
		}
		//综合推荐   支持最多默认选中
		var searchTypeStatus = $.trim($("#searchTypeStatus").val());
		if(searchTypeStatus == "zhtj"){
			$("#searchTypeTile").html("综合推荐<strong></strong>");
		}else if(searchTypeStatus == "zxsx"){
			$("#searchTypeTile").html("最新上线<strong></strong>");
		}else if(searchTypeStatus == "jezg"){
			$("#searchTypeTile").html("金额最多<strong></strong>");
		}else if(searchTypeStatus == "zczd"){
			$("#searchTypeTile").html("支持最多<strong></strong>");
		}else if(searchTypeStatus == "jjjs"){
			$("#searchTypeTile").html("即将结束<strong></strong>");
		}else{
			$("#searchTypeTile").html("综合推荐<strong></strong>");
		}
		
		//分类
		jQuery("[name='category']").on("click",function(){ // 分类
		    jQuery("#categoryId").val(jQuery(this).attr("code"));
		    jQuery("#parentCategoryId").val(jQuery(this).attr("parentId"));
		    jQuery("#keyWord").val("");
		    searchData();
		});

		//搜索
		jQuery("#search_btn").on("click",function(){ //关键字搜索
		        searchData();
		});

		jQuery("#status_p li").on("click",function(){ // 全部 预热中 众筹中 众筹成功 项目成功   爆料
	        jQuery("#status").val(jQuery(this).attr("code"));
	        if(jQuery(this).attr("code")==2 || jQuery(this).attr("code")==0 || jQuery(this).attr("code")=='' ||jQuery(this).attr("code")==null){
	            jQuery("#jjjs").css("display","inline");
	        }else{
	            jQuery("#jjjs").css("display","none");
	        }
	        if(jQuery(this).attr("code")==1 ){
	            jQuery("#sort").val("jjks");
	            jQuery("#sort_p").css("display","none");
	        }else{
	            jQuery("#sort").val("zhtj");
	            jQuery("#sort_p").css("display","block");
	        }
	        searchData();
	    });
	    
		jQuery("#sort_p li").on("click",function(){ // 排序
	        jQuery("#sort").val(jQuery(this).attr("code"));
	        if(jQuery(this).attr("code")=='jjjs'){
	            jQuery("#status").val("2");
	        }
	        searchData();
	    })
	    
	    //猜你喜欢
	    var isGuessYouLikeHidden = $("#isGuessYouLikeHidden").val();
	  /* if(isGuessYouLikeHidden=="false"){
	    	getGuessData();//猜你喜欢
	    }
	    jQuery("#guess_change").on("click",function(){ //换一组猜你喜欢
	        getGuessData();
	    });*/

	    //预约
	    $(".item-btn").on('click', function () {
	        var phone = $(this).prev().val();
	        var projectId = $(this).attr("projectId");
	        var ele = $(this).parent().parent().parent();
	        var _this = $(this);
	        var regPhone = /^1[0-9]{10}$/;
	        if (regPhone.test(phone)) {
	            jQuery.ajax({
	                url: "//"+window.location.host+'/insertAdvance.action',
	                type: 'post',
	                data: {
	                    phone: phone,
	                    projectId: projectId,
	                    temp: Math.random()
	                },
	                dataType: 'json',
	                success: function (result) {
	                    initDate(result.Msg, ele, _this);
	                },
	                error: function () {
	                    initDate(4, ele, _this);
	                }
	            });
	        } else {
	            initDate(4, ele, _this);
	        }
	    });
	})

	function searchData(){
	    jQuery("#keyword").val(jQuery("#keyWord").val());
	    jQuery("#page").val(1);
	    jQuery("#main_form").attr("action","/bigger/search.html");
	    jQuery("#main_form").submit();
	}
	function getGuessData(){
	    jQuery.ajax({
	        url : "//"+window.location.host+"/bigger/guess_new.html",
	        dataType : "html",//这里的dataType就是返回回来的数据格式了html,xml,json
	        cache: false,//设置是否缓存，默认设置成为true，当需要每次刷新都需要执行数据库操作的话，需要设置成为false
	        success : function(html) {
	         //   jQuery("#guess_div").html(ReplaceAll(html,"http:",""));
	            jQuery("#guess_div").html(html);
	            $("img.lazyout-guess").lazyload({effect:"fadeIn",threshold : 50});
	            $(".info").find(".support").css("display","none");
	            $(".info").hover(function(){
	                $(this).find(".support").css("display","block");
	            },function(){
	                $(this).find(".support").css("display","none");
	            });
	        }
	    } )
	};

	//校验预约结果
	function initDate(data, ele, _this) {
	    if (ele.find(".temp-empty").length > 0) {
	        if (data == 1) {
	            ele.find(".temp-empty").attr("class", "order-tag-success temp-empty");
	            ele.find(".temp-empty").html("您已成功订阅");
	            /* ele.('<div class="order-tag-success temp-empty">您已成功订阅</div>'); */
	        } else if (data == 2) {
	            ele.find(".temp-empty").attr("class", "order-tag-get temp-empty");
	            ele.find(".temp-empty").html("您已订阅过此商品");
	        } else if (data == 3) {
	            ele.find(".temp-empty").attr("class", "order-tag-error temp-empty");
	            ele.find(".temp-empty").html("您提交太频繁了");
	        } else {
	            ele.find(".temp-empty").attr("class", "order-tag-error temp-empty");
	            ele.find(".temp-empty").html("请输入正确的手机号");
	        }
	    } else {
	        //order-tag-success
	        if (data == 1) {
	            ele.find(".temp-empty").removeClass("order-tag-success");
	            ele.append('<div class="order-tag-success temp-empty">您已成功订阅</div>');
	        } else if (data == 2) {
	            ele.find(".temp-empty").removeClass("order-tag-get");
	            ele.append('<div class="order-tag-get temp-empty">您已订阅过此商品</div>');
	        } else if (data == 3) {
	            ele.find(".temp-empty").removeClass("order-tag-error");
	            ele.append('<div class="order-tag-error temp-empty">您提交太频繁了</div>');
	        } else {
	            ele.find(".temp-empty").removeClass("order-tag-error");
	            ele.append('<div class="order-tag-error temp-empty">请输入正确的手机号</div>');
	        }
	    }
	    ele.find(".temp-empty").show();
	    setTimeout(function () {
	        ele.find(".temp-empty").hide();
	        _this.prev().val("");
	    }, 1500)
	}
</script>

<script type="text/javascript">
    $(document).ready(function(){
        page();//分页
    })
    /* function go(no){
         jQuery("#keyword").val(jQuery("#keyWord").val());
         jQuery("#page").val(no);
         jQuery("#main_form").attr("action","/bigger/search.html");
         jQuery("#main_form").submit();
     }*/
    jQuery("#productEnd").val();
    jQuery("#sceneEnd").val();
    function page(){
        $("#page_div").pagination(76,{
            current_page:1-1,
            num_edge_entries: 3,
            num_display_entries: 3,
            items_per_page:1,
            link_to:"javascript:;",
            prev_text:"上一页",
            ellipse_text:".&nbsp;.&nbsp;.",
            next_text:"下一页",
            prev_show_always:true,
            next_show_always:true,
            callback:function(page){//在翻页时候用的
//                if((page+1) != $("#page").val()){
                jQuery("#productEnd").val();
                jQuery("#sceneEnd").val();
                if(page<$("#page").val()) {
                    /* alert(+":"+16);
                        alert(-12-16);*/
                    jQuery("#productEnd").val(-12-16);
                }
                jQuery("#keyword").val(jQuery("#keyWord").val());
                jQuery("#page").val(page+1);
                jQuery("#main_form").attr("action","/bigger/search.html");
                jQuery("#main_form").submit();
//                }
            }
        });
        var pageTmp = $('#page_div').find('.current');
        $.each(pageTmp,function(key,val){
            if(!($(val).hasClass('prev') || $(val).hasClass('next'))){
                var pageTmpSingle = val;
                var prevObject = $(pageTmpSingle).prev();
                var tmpNum = $(pageTmpSingle).text();
                $(pageTmpSingle).remove();
                $(prevObject).after('<a href="javascript:;" class="now">'+tmpNum+'</a>');
            }
        });
        $.each(pageTmp,function(key,val){
            if ($(val).hasClass('prev')){
                $(val).remove();
            } else if ($(val).hasClass('next')){
                $(val).remove();
            }
        });
    }
</script>



<script type="text/javascript">
	seajs.use("project/cube_select", function(){
		$("[cube-type=select]").select();
	});
	$(document).ready(function(){
		$(".info").find(".support").css("display","none");
		$(".info").hover(function(){
			$(this).find(".support").fadeIn();
		},function(){
			$(this).find(".support").fadeOut();
		});
		//alert($("body").height())
		//var maxScroll=1500;
		$(window).bind("scroll", function() {
			var scrollTop = $(window).scrollTop();
			if(scrollTop > 200  ){
				$(".l-menubox").slideDown();
			}else{//fadeIn fadeOut
				$(".l-menubox").slideUp();
			}
		});
		//即将上架
		$('.info').each(function(){

			$(this).hover(function(){
				$(this).find('.item-order-wrap').css({'display':'block'});
				$(this).find('.order-wrap').stop().animate({
					marginTop:'0px'
				},500)
			},function(){
				var $itemW=$(this).find('.item-order-wrap');
				$(this).find('.order-wrap').stop().animate({
					marginTop:'40px'
				},500,function(){
					$itemW.css({'display':'none'})
				})

				//鼠标移出把提示部分隐藏起来，class名字可能会有不同，这里用节点关系操作
				//$(this).find('.item-order-wrap').next().css({'display':'none'})
			})

		})
	});

</script>
