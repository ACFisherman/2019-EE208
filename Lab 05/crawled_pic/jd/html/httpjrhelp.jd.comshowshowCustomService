            function hello() {
                var helloUrl = "//passport.jd.com/call/getHelloJson?m=ls";
                jQuery.ajax({
                    url: helloUrl, dataType: "jsonp", scriptCharset: "gb2312", success: function (a) {
                        if (a && a.info) {
                            $("#loginbar").html(a.info);
                        }
                        if (a && a.sso) {
                            $.each(a.sso, function () {
                                jQuery.getJSON(this)
                            })
                        }
                    }
                });
            };
            hello();
        })(jQuery);
    </script>

