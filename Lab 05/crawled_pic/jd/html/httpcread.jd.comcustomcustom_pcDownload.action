<script type="text/javascript">
    $(function(){
        $('.old-app').click(function(){
            $('.old-app').addClass('pressed');
            $('.new-app').removeClass('pressed');
            $('.dushu').fadeOut();
            $('.yuedu').fadeIn();
        });
        $('.new-app').click(function(){
            $('.new-app').addClass('pressed');
            $('.old-app').removeClass('pressed');
            $('.yuedu').fadeOut();
            $('.dushu').fadeIn();
        });
        $('.old-download').mouseover(function(){
            $('.old-code').fadeIn();
        });
        $('.old-download').mouseout(function(){
            $('.old-code').fadeOut();
        });
        $('.downLoad-btns a').mouseover(function(){
            //$(this).find('img').css('opacity','0.1')
            var downLoadImg = $(this).find('img')
            if (downLoadImg.prevObject[0].className === 'new-pc') {
              downLoadImg[0].src='/pc-img/pc-highlight.png'
              downLoadImg.css('opacity','1')
            } else if (downLoadImg.prevObject[0].className === 'new-android') {
              downLoadImg[0].src='/pc-img/android-highlight.png'
              downLoadImg.css('opacity','1')
            } else if (downLoadImg.prevObject[0].className === 'new-ios') {
              downLoadImg[0].src='/pc-img/ios-highlight.png'
              downLoadImg.css('opacity','1')
            } else {
              downLoadImg.css('opacity','0.1')
            }
	    })
	    $('.downLoad-btns a').mouseout(function(){
	        // $(this).find('img').css('opacity','1')
            var downLoadImg = $(this).find('img')
            if (downLoadImg.prevObject[0].className === 'new-pc') {
              downLoadImg[0].src='/pc-img/pc.png'
              downLoadImg.css('opacity','1')
            } else if (downLoadImg.prevObject[0].className === 'new-android') {
              downLoadImg[0].src='/pc-img/android.png'
              downLoadImg.css('opacity','1')
            } else if (downLoadImg.prevObject[0].className === 'new-ios') {
              downLoadImg[0].src='/pc-img/ios.png'
              downLoadImg.css('opacity','1')
            } else {
              downLoadImg.css('opacity','1')
            }
	    })
    });
    function getData(url, callback, params, other) {
        jQuery.ajax({
            type: 'get',
            url: url,
            dataType:"json",
            data: params,
            success:function(data){
                if (callback !== '' && typeof callback === 'function') {
                    callback(data, params, other);
                }
            },
            error: function(err) {
                console.log(err);
            }
        });
    }
    function setHref (data) {
        window.location.href = data.data.url;
    }
    var downUrl = 'https://jdread-api.jd.com/jdread/api/version';
    $('.new-android').click(function(){
        getData(downUrl, setHref);
    });

</script>
