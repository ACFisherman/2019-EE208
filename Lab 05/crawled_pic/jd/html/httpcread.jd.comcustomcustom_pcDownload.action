        jQuery.fn.UIDialog = function(options){
            var defaults = { IE :jQuery.browser.msie,
                IE6        :jQuery.browser.version == 6,
                FIREFOX    :jQuery.browser.mozilla,
                window     :$(window),
                document   :$(document),
                body       :$(document.body),
