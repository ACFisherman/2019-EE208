                    p.append(that.content);
                    that.IE6 && dialog.css({position:'absolute'});
                    that.setSize();
                    return that;
                },
                createBuyTips: function(){
                    var that = this,
                            dialog = $('<div class="ui-dialog"></div>').appendTo(this.body).hide(),
                            dialogTitle = $('<h1 class="ui-dialog-title"></h1>').appendTo(dialog),
