                    var that = this,
                            cache = jQuery.fn.UIDialog.cache;
                    for(var p in opts){ // opts 绑定到 instance
                        this[p] = opts[p];
                    }
                    that.me = me; // 被调用者
                    if("overdueTips" == opts.id){
                        that.createOverdueTips();
                    }
                    if("buyTips" == opts.id){
                        that.createBuyTips();
