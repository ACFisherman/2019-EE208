    <script type="text/javascript">
        $(document).ready(function () {
            $("#gotop123,#gotop1").hide().click(function () {    //将按钮链接放置到body标签中并隐藏。
                //$(document).scrollTop(0);    //当点击按钮链接时，返回页面顶部；同时，按钮链接隐藏。
