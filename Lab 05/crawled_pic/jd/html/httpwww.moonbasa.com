            });
            var $gotopPage = $('#gotop123');
            var $gotopPage2 = $('#gotop1');
            function backTopLeft() {    //设置按钮链接位置的函数。
                //btLeft 等于窗口宽度的一半+500像素：
                var btLeft = ($(window).width() / 2) - 720;
                $gotopPage.css({ 'left': btLeft });
                var btLeft2 = ($(window).width() - 980) / 2 + 1105;
                $gotopPage2.css({ 'left': btLeft2 });
            }
            backTopLeft();     //执行以上函数。
            $(window).resize(backTopLeft);     //当窗口有调整时，执行 backTopLeft 函数。
            //滚动条滚动时触发：
            $(window).scroll(function () {




                //如果滚动条滚动到顶部，隐藏按钮，否则显示：
                if ($(document).scrollTop() < 600) { $gotopPage.hide(); $gotopPage2.hide() } else { $gotopPage.show(); $gotopPage2.show() }
                //如果是IE6浏览器，并且不在页面顶部，则按钮不透明（不断刷新），以兼容IE6：
                if ($.browser.msie && $.browser.version == 6.0 && $(document).scrollTop() !== 0) { $gotopPage.css({ 'opacity': 1 }); $gotopPage2.css({ 'opacity': 1 }) }
            })
        });
    </script>
