      // 事业单位倒计时
  $(".sylxftime").each(function(){
        var endtimes = new Date("2019/10/19/00:00:00").getTime();//取结束日期(毫秒值)
        var nowtimes = new Date().getTime();        //今天的日期(毫秒值)
        var youtimes = endtimes-nowtimes;//还有多久(毫秒值)
        var timers = setInterval(function(){
                if(youtimes<=0){
                    clearInterval(timers);
                    return;
                }


                youtimes--;


                //定义 天数 小时    分钟  秒数
                days = parseInt(Math.floor(youtimes /3600 /24 /1000));
                // hour = Math.floor(youtime / 3600);
                // minute = Math.floor(youtime % 3600/60);

                // sec = youtime % 60;

                $(".sylxftime").html(days);

            },1000);
  });
