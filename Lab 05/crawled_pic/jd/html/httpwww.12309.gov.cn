	function zz_search(){
				var qt1 = $('#searchWord').val();
				if(qt1!=''){
					var searchword = encodeURI(qt1);
					var searchurl = '/guestweb/s?siteCode=N000007720'+'&searchWord='+searchword;
					window.open(searchurl);
				}else{
					alert('请输入关键字');
				} 
			}
			$(function(){
				$('#button').click(function(){
					zz_search();
				});
        document.getElementById("searchWord").onkeydown=function mykeyDown(e){ 
            e=e||event;
            if(e.keyCode==13){zz_search();} 
            return; 
        }
			})
</script>
	<script type="text/javascript">
	
			(function ($) {

				// 时间弹出插件调用
				$('.date_picker').date_input();

				// 高级搜索点开关闭
				(function () {
					var gjsostj = $(".gjsostj"), flag = true;
					$(".gjsos_search").on('click', function (e) {
						e.stopPropagation();
						if (flag) {
							gjsostj.show(); flag = false;
							bodyOnClick();
						} else { hide() }
					})
					function hide() { gjsostj.hide(); flag = true; }
					function bodyOnClick() {
						$('body').on('click', function (e) {
							if (!$(e.target).parents('form').length && !$(e.target).hasClass('selectable_day')) {
								hide();
							} else { bodyOnClick(); }
						})
					}
				})();

				//高级搜索公开时间加具体时间
				(function () {
					$(".gjsos_form input[type=submit]").on('click', function () {
						var startTime = $('#startTime').val();
						var endTime = $('#endTime').val();
						if (startTime) {
							$('#startTimeH').val(startTime + ' 00:00:00');
						}
						if (endTime) {
							$('#endTimeH').val(endTime + ' 23:59:59')
						}
						return true
					});
				})();


				$(".sosdlzc .zk").click(function () {
					$(".sosdlzc .sos").css("display", "block");
					$(this).css("display", "none");;
					$(".sosdlzc .sq").css("display", "block");

				});
				$(".sosdlzc .sq").click(function () {
					$(".sosdlzc .sos").css("display", "none");
					$(this).css("display", "none");;
					$(".sosdlzc .zk").css("display", "block");
				});

				// 左上角时间
				(function ($) {
					var date = new Date();
					this.day = new Array("星期日", "星期一", "星期二", "星期三", "星期四", "星期五", "星期六")[date.getDay()];
					var currentTime = date.getFullYear() + "年" + (date.getMonth() + 1) + "月" + date.getDate() + "日 " + "" + this.day +
						"";
					document.all.show.innerHTML = currentTime;
				})($);

				$(".dxk span").click(function () {
					$(this).addClass("checked").siblings().removeClass("checked");
				});

			})(window.jQuery);

