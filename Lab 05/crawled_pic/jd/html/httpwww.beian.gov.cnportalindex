<script type="text/javascript">
        var imgPath='';
		window.onload = function(){//判断是否是idc登陆
			//dl1:网站用户；dl2:IDC用户；dl3:APP商店；dl4:CDN用户
			$("#ewm").hide();
			$("#huangYing").hide();
			if($("#dengLuType").val()=='idc'){
				idcLogin();
			}else if($("#dengLuType").val()=='app'){
				appLogin();
			}else if($("#dengLuType").val()=='cdn'){
				cdnLogin();
			}else if($("#dengLuType").val()=='ptyh'){
				ptwzLogin();
			}else if($("#dengLuType").val()=='xcyh'){
				xcLogin();
			}
		}
		//dl2:IDC用户
		$('#qyLoginA').on('click',function(){
			idcLogin();
		})
		//dl1:网站用户
		$('#PTUSER').on('click',function(){
			ptwzLogin();
		});
		//dl3:APP商店
		$('#APPMarket').on('click',function(){
			appLogin();
		});
		//dl4:cdn用户
		$('#CDN').on('click',function(){
			cdnLogin();
		});
		//dl8:域名服务商、搜索引擎厂商、浏览器厂商、音视频服务商
		$('#xc').on('click',function(){
			xcLogin();
		});
		function idcEwm(){
			$("#dlk").hide();
			$("#ewm").show();
		}
		function djfh(){
			$("#dlk").show();
			$("#ewm").hide();
		}
		function djxs(){
			$("#dlk").hide();
			$("#ewm").show();
		}
		//idc登陆
		function idcLogin(){
			$('#dl1').attr("class","");
			$('#dl2').attr("class","active");
			$('#dl3').attr("class","");
			$('#dl8').attr("class","");
			$("#jrs").show();
			$("#ptyh").hide();
			$("#appMarket").hide();
			$("#xcPanel").hide();
			$("#ewm").hide();
			//$("#sec").hide();
			$("#huangYing").show();
			//$("#cbg").css("background-image","url(../img/adbg_idc.png)");
			$("#imgliucheng").attr("src","/iscs_upgrade/img/liuchengIdc.png");
		}
		//APP商店用户登陆
		function appLogin(){
			$('#dl1').attr("class","");
			$('#dl2').attr("class","");
			$('#dl3').attr("class","active");
			$('#dl8').attr("class","");
			$("#appMarket").show();
			$("#ptyh").hide();
			$("#jrs").hide();
			//$("#sec").hide();
			$("#xcPanel").hide();
			$("#ewm").hide();
			$("#huangYing").hide();
			$("#cbg").css("background-image","url(../img/adbg.png)");
			$("#imgliucheng").attr("src",imgPath+"/img/appBg.png");
		}
		//CDN用户登陆
		function cdnLogin(){
			$('#dl1').attr("class","");
			$('#dl2').attr("class","");
			$('#dl3').attr("class","");
			$('#dl8').attr("class","active");
			$("#xcPanel").show();
			$("#appMarket").hide();
			$("#ptyh").hide();
			$("#jrs").hide();
			//$("#sec").hide();
			$("#ewm").hide();
			$("#huangYing").hide();
			$("#cbg").css("background-image","url(../img/adbg.png)");
			$("#imgliucheng").attr("src",imgPath+"/img/appBg.png");
		}
		//普通网站用户登陆
		function ptwzLogin(){
			$('#dl1').attr("class","active");
			$('#dl2').attr("class","");
			$('#dl3').attr("class","");
			$('#dl8').attr("class","");
			$("#ptyh").show();
			$("#jrs").hide();
			$("#appMarket").hide();
			$("#xcPanel").hide();
			$("#ewm").hide();
			//$("#sec").hide();
			$("#huangYing").hide();
			$("#cbg").css("background-image","url(../img/adbg.png)");
			$("#imgliucheng").attr("src",imgPath+"/img/liuchengRight.png");
		}
		
		//域名服务商、搜索引擎厂商、浏览器厂商、音视频服务商
		function xcLogin(){
			$('#dl1').attr("class","");
			$('#dl2').attr("class","");
			$('#dl3').attr("class","");
			$('#dl8').attr("class","active");
			$("#xcPanel").show();
			$("#appMarket").hide();
			$("#ptyh").hide();
			$("#jrs").hide();
			$("#ewm").hide();
			//$("#sec").hide();
			$("#huangYing").hide();
			$("#cbg").css("background-image","url(../img/adbg.png)");
			$("#imgliucheng").attr("src",imgPath+"/img/appBg.png");
		}
	</script> 
