		if($(obj).attr("href") != undefined){
			if(($(obj).attr("href")).indexOf('/')>=0){
			    if($(obj).attr("href").indexOf("?")>=0){
					 $(obj).attr("href",$(obj).attr("href")+"&token="+taken_for_user);
		 	    }else{
		 	    	$(obj).attr("href",$(obj).attr("href")+"?token="+taken_for_user);
		 	    }
			}
		}
	});
	
