<script>
    $('document').ready(function() {

        seajs.use(['jdf/1.0.0/unit/globalInit/5.0.0/globalInit.js'],function(globalInit){
            globalInit();
        });

        // 吸顶
        if ($(window).scrollTop() > 146) {
            $('.fixed-wrapper').show();
        }
        $(window).bind('scroll', function() {
            if ($(window).scrollTop() > 146) {
                $('.fixed-wrapper').show();
            } else {
                $('.fixed-wrapper').hide();
            }
        })

        $(function() {
            // 飞入购物车
            var _target;
            var _topCart = $(".header-wrapper .outer-cart");
            var _bottomCart = $('.fixed-wrapper').find(".outer-cart");

            //确定是哪个购物车
            function targetCart(){
                if ($('.fixed-wrapper').css("display") == "block") {
                    _target = _bottomCart;
                }else{
                    _target = _topCart;
                }
                return _target;
            };


            //得到页面的scrollTop
            function getScrollTop() {
                var scrollTop;
                var bodyTop = document.body.scrollTop;
                var eleTop = document.documentElement.scrollTop;
                if ( bodyTop == 0 && eleTop == 0) {
                    scrollTop=0;
                } else {
                    scrollTop=bodyTop || eleTop;
                }
                return scrollTop;
            }

            $(".add-list").click ( function(event) {
                var $this = $(this);
                if($this.hasClass("diy-add-list")){
                    try {
                        var price= Number($("#eletricValue").text());
                        if(price==""||!isInteger(price*1) || price*1>5000 || price*1<10 || price == "0"){
                            return false;
                        }
                    } catch (e) {
                        return false;
                    }

                }
                seajs.use('jdf/1.0.0/unit/login/1.0.0/login.js',function(login){
                    login.isLogin(function(isLogin, userInfo){
                        if (isLogin) {
                            flyInstace($this,event);
                        } else {
                            login({
                                //firstCheck:false,
                                modal: true,//false跳转,true显示登录注册弹层
                                complete: function() {
                                    flyInstace($this,event);
                                }
                            })
                        }
                    });
                })
            });

            function flyInstace($this,event) {
                var _this = $this;
                var img = _this.parents("li").find('img').attr('src');
                if(img==null || img=="undefined"){
                    return false;
                }
                var flyer = $('<img class="u-flyer" src="' + img + '">');
                var offset = targetCart().find(".icon").offset();
                flyer.fly({
                    start: {
                        left: event.pageX,
                        top: event.pageY - getScrollTop()
                    },
                    end: {
                        left: offset.left + 10,
                        top: offset.top - getScrollTop() + 10,
                        width: 0,
                        height: 0
                    },
                    onEnd: function() {
                        this.destory();
                    }
                });
            }

            $('#addList').click(function(event) {
                // $('.diy-add-list').click(function(event) {

                var count=$("#eletricCount").val();
                var price=$("#eletricValue").text();
                if(!isInteger(count*1) || count<=0)
                {
                    toast("卡数量必须是正整数");
                    return false;
                }
                if(!isInteger(price*1) || count<0 )
                {
                    toast("卡金额必须是正整数");
                    return false;
                }
                if(price==""){
                    toast("请输入卡面值");
                    return false;
                }
                if(price*1>5000||price*1<10){
                    toast("请输入10到5000的整数");
                    return false;
                }
                var totalValue=count*price;
                if(totalValue>300000){
                    toast("总额不能超过30万哦");
                    return false;
                }

                var $this = $(this);

                var price = 0;
                try {
                    price = Number($("#eletricValue").text());
                } catch (e) {

                }
                //如果没填卡金额 不执行弹框
                if(price=="" ||!isInteger(price*1) || price*1>5000 || price*1 < 10){
                    return false;
                }else{
                    seajs.use('jdf/1.0.0/unit/login/1.0.0/login.js',function(login){
                        login.isLogin(function(isLogin, userInfo){
                            if (isLogin) {

                                $.ajax({
                                    type:"POST",
                                    url:"/market/ajaxcart/addCartEletric",
                                    data: {
                                        'cardValue' : price,
                                        'count' : count
                                    },
                                    dataType: 'json',
                                    success:function(data){
                                        if(data!=null && data.error=="NotLogin"){
                                            //没登录的话什么都不做,前端会弹登录框
                                            return false;
                                        }
                                        if(data!=null && data.code == "success") {
                                            var detail =data.detail;
                                            initCartHtml(detail);
                                            diyFlyInstance($this,event);
                                        }else{
                                            if(data.detail.indexOf("E卡清单塞满了")!="-1"){
                                                toast("卡清单塞满了");
                                            }else{
                                                toast("加入购物车失败");
                                            }
                                            return false;
                                        }
                                    },
                                    error:function(data){
                                        toast("加入购物车失败");
                                        return false;
                                    }
                                });

                            } else {
                                login({
                                    //firstCheck:false,
                                    modal: true,//false跳转,true显示登录注册弹层
                                    complete: function() {
                                        diyFlyInstance($this,event);
                                    }
                                })
                            }
                        });
                    })
                }
            })

            function diyFlyInstance($this,event){
                var _this = $this;
                var $img = _this.parents('.diy-cont').find('.c-left');
                //var img = $img.attr('data-src');
                var img=$("#cardImgSrc").attr("src");
                var flyer = $('<img class="u-flyer" src="' + img + '">');
                var offset = targetCart().find(".icon").offset();
                flyer.fly({
                    start: {
                        left: $img.offset().left,
                        top: $img.offset().top - getScrollTop()
                    },
                    end: {
                        left: offset.left + 10,
                        top: offset.top - getScrollTop() + 10,
                        width: 0,
                        height: 0
                    },
                    onEnd: function() {
                        this.destory();
                    }
                });
            }
        });
        $('.J-c-inp').bind('keyup focus blur', function() {
            if ($(this).val()) {
                $('.diy-wrapper .c-left .num').text($(this).val());
            } else {
                $('.diy-wrapper .c-right li').each(function() {
                    if ($(this).hasClass('selected')) {
                        var amount = $(this).find('span').text();
                        var reg = /[\u4e00-\u9fa5]/g;
                        amount= amount.replace(reg, "");
                        $('.diy-wrapper .c-left .num').text(amount);
                    }
                })
            }
        })
        $('.diy-wrapper .c-right li').bind('click', function() {
            $(this).addClass('selected').siblings().removeClass('selected');
            $('.diy-wrapper .c-left .num').text($(this).find('span').text().substring(0,$(this).find('span').text().indexOf('元')));
            $('.J-c-inp').val('');
        })

