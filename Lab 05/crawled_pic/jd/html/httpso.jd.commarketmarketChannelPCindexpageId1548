        var skuCache="";
        var pageSizeCache=0;
        for(var i=0;i<cacheValue.length;i++){
            var module=cacheValue[i];
            //卡图
            if(module["nodeSortNo"]=="10"){
                var children=module["children"];
                var field=children[0]["children"][0];
                var src=field["cloudPath"];
                $("#cardImgSrc").attr("src",src);
                //$("#cardImg").css("data-src",src);
                //$("#cardImg").css("background",'url('+src+') no-repeat');
                continue;
            }
            //sku
            if(module["nodeSortNo"]=="20") {
                var children=module["children"];
                skuCache=children;
                var skuLength=children.length-1;//第一个展示位是每页显示的行数,要抛去
                var pageSizeCol=children[0]["children"][0]["data"];//每页显示的行数
                var pageSize=5*pageSizeCol;
                pageSizeCache=pageSize;
                if(skuLength>pageSize){
                    seajs.use('jdf/1.0.0/ui/pager/1.0.0/pager', function(pager){
                        var pagerInstance = $('.pager').pager({
                            hasCssLink: true,
                            total: skuLength,
                            pageSize: pageSize,
                            //currentPage:3,
                            currentPageClass: 'ui-pager-current',
                            prevClass: 'ui-pager-prev',
                            nextClass: 'ui-pager-next',
                            callback:function(pageNo){
                                $('#skuDiv ul').each(function(){
                                    $(this).css("display","none");
                                })
                                $("#skuUl"+(pageNo==1?"":(pageNo-1))).css("display","block");
                            }
                        });
                    });
                }
                var loopLenth=skuLength>pageSize?pageSize:skuLength;//只加载一页的
                //先加载skuDiv
                if(skuLength>loopLenth){
                    var remainNum=parseInt(skuLength/loopLenth)+1;
                    if(remainNum>1){
                        for(var k=1;k<remainNum;k++){
                            $("#skuDiv").append("<ul class='card-cont clearfix' style='display:none' id=skuUl"+k+"></ul>");
                        }
                    }
                }
                $("#skuUl").empty();
                for(var j=0;j<skuLength;j++){
                    var remainNumm=parseInt(j/loopLenth);
                    var skuHtml=$("#skuTemplate").html();
                    var fieldSku=children[j+1]["children"][1];
                    var fieldTag=children[j+1]["children"][0];
                    skuHtml = skuHtml.replace("{skuid}", fieldSku["data"]);
                    skuHtml = skuHtml.replace("{skuid1}", fieldSku["data"]);
                    skuHtml = skuHtml.replace("{skuid2}", fieldSku["data"]);
                    skuHtml = skuHtml.replace("{skuImg}","<img src='"+fieldSku["sku_img"]+"'/>");
                    skuHtml = skuHtml.replace("{skuName}", fieldSku["sku_name"]);
                    var sku_price = fieldSku["sku_price"];
                    var index = sku_price.toString().indexOf(".");
                    var sku_price_int="",sku_price_float="";
                    if (index != -1) {
                        sku_price_int = sku_price.toString().substring(0, index);
                        sku_price_float = sku_price.toString().substring(index + 1);
                    } else {
                        sku_price_int = sku_price;
                        sku_price_float = "00";
                    }
                    skuHtml = skuHtml.replace("{priceInf}", sku_price_int);
                    skuHtml = skuHtml.replace("{priceFloat}", sku_price_float);
                    skuHtml = skuHtml.replace("{tag}", fieldTag["data"]);
                    skuHtml = skuHtml.replace("{skuprice}", sku_price);
                    skuHtml = skuHtml.replace("{clstag}", "clstag='pageclick|keycount|201712198|"+(j+6)+"'");
                    $("#skuUl"+(remainNumm==0?"":remainNumm)).append($(skuHtml));
                }
                continue;
            }
            //title
            if(module["nodeSortNo"]=="30"){
                var children=module["children"];
                var field=children[0]["children"][0];
                var src=field["cloudPath"];
                $("#titleImg").attr("src",src);
                continue;
            }
        }

        //加载频道页配置
        initPage();
        //加载查询条件
        initFilter();
        //初始化购物车
        initCart();
    })
</script>
