    for (var key in tags) {
        tagsArray.push(tags[key].tagName);
    }
    $.ajax({
        type: 'GET',
        url: '/api/wikiad/getsquirrels',
        data: {
            lemmaId: 168752,
            tags: tagsArray.join()
        },
        cache: false,
        dataType: 'JSON',
