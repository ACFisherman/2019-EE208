    $('.wgt-footer-main .suggestion').find('a').each(function(i) {
        $(this).attr('href', 'http://help.baidu.com/newadd?word=%E4%B8%96%E7%95%8C%E7%94%B5%E5%AD%90%E7%AB%9E%E6%8A%80%E5%A4%A7%E8%B5%9B' + '&&submit_link=' + encodeURIComponent(window.location.href) + '&prod_id=10&category=' + map[i]);
