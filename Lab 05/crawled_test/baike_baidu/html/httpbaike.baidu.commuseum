!function(){	var navigation=$(".navigation-wrap");
	var top=navigation.offset().top;
	$(window).scroll(function(){
		var st=$(window).scrollTop();
		var h=70;
		if(st>=top){
			navigation.addClass("fixed");
			if(st<top+30){
				h=70+top-st;
			}else{
				h=40;
			}
		}else{
			h=70;
			navigation.removeClass("fixed");
		}
		navigation.height(h);
		navigation.find(".text").css("margin-top",Math.floor((h-16)/2));
		navigation.find("a").height(h);
	});
}();
