        ADXml.setConfig({
            caid : "25422",
            arid : "4589508",
            adAddress : "//adpai.thepaper.cn/s?"
        })
        function is_weixin(){
            var ua = navigator.userAgent.toLowerCase();
            if(ua.match(/MicroMessenger/i)=="micromessenger") {
                return true;
            } else {
                return false;
            }
        }
        if(!is_weixin()){
            document.write('<div style="position:fixed;bottom:80px;right:10px" id="ad_demo"><script src="//adpai.thepaper.cn/s?c=59&WD-CLIENT-TYPE=05&arid=4589508&caid=25422" defer="defer"><\/script></div>');
        }
        function initPageAjax(url,data,callback){
            $.ajax({
                type:"get",
                url:url,
                cache: false,
                data:data,
                timeout:30000,
                dataType:"html",
                success: function(html){
                    if(callback) callback(html);
                },
                error:function(){}
            });
        }
        var bt = $('#toolBackTop');
        $(window).scroll(function() {
            bt.show();
            var st = $(window).scrollTop() || document.documentElement.scrollTop || document.body.scrollTop;
            if(st > 100){
                bt.show();
            }else{
                bt.hide();
            }
        });

        $(document).ready(function(){
           //初始热评论 传参文章ID
           initPageAjax("newDetail_hotComments_async.jsp",{contid:4589508},function(html){
               $("#hotCommentsDiv").html(html);
               initHtmlMob(html);
               setTimeout(function(){
                   $("#hot_talk_panel").slick({
                       dots: true,
                       slidesToShow: 1,
                       slidesToScroll: 1,
                       autoplay: true,
                       autoplaySpeed: 3000,
                       infinite:true,
                       touchMove:true,
                       pauseOnHover:false
                   });
               },100)
               
           });
            //相关推荐
            initPageAjax("recommend.jsp",{contid:4589508},function(html){
                $("#recommend").html(html);
            })
            //热门推荐
            initPageAjax("newDetail_hotRecommend.jsp",{},function(html){
                $("#newDetailHotRecommend").html(html);
                initHtmlMob(html);
                ADXml.initAdforRecommend();
            })
            
            audiosInit();
            var localStorage = window.localStorage;

            $('.user_header_praise').each(function(index,item){
                if(localStorage.getItem('hotPraise'+$(item).attr("id"))!=undefined){
                    $(item).children().attr("class","icon_praised")
                }
            })
            if($("#adWord").children().length>0){
                var adWordTXT = $("#adWord a");
                for(var i=0 ;i<adWordTXT.length;i++){
                    if(adWordTXT.eq(i).text().length>0){
                        var adWordTXTStyle = adWordTXT.eq(i).parent().attr("style");
                        adWordTXT.eq(i).attr("style",adWordTXTStyle);
                    }
                }
            }
            try
            {
                var contentHeight = getContentHeight();
                if(contentHeight > 700){
                    $('.news_part_all').show();
                }
            }catch(e){

            };
            $("#tiptitleOK").click(function(){
                $("#overlay").css('display','none').hide();
                $("#tiptitle").css('display','none').hide();
                $("#tiptitleNO").css('display','none').hide();
                $("#tiptitleStr").text("");
            });

            $('#cur_ans').click(function(){
                $("#cur_ans").addClass("cur");
                $("#comm_span").removeClass("cur");
                $("#commt_list").css("display","none");
                $("#aq_commt").css("display","");
            });

            $("#comm_span").click(function(){
                $("#cur_ans").removeClass("cur");
                $("#comm_span").addClass("cur");
                $("#aq_commt").css("display","none");
                $("#commt_list").css("display","");
            });
        });

        function getContentHeight(){ //责任编辑
            var contHeight = 0;
            for(var i=0;i<$('.news_part_limit >div').length;i++){
                contHeight += $('.news_part_limit >div').eq(i).height();
            }
            return contHeight;
        }

        function showMoreCon(){
            sendAjaxForStatClick('readMore');
            $(".news_part_limit").css("max-height","inherit");
            $(".news_part_all").hide();
        }
        function showShareBox(){
            if(is_weixin()){
                showShareHelpForWx();
            }else{
                $('.bg-opacity30').show();
                $(".header-v4").slideDown("150");
                $('.v3-shareBox').slideDown("150");
            }
            VideoDealShare.start($("video"));
        }
        $('.bg-opacity30,.v3-shareBox .closeS').on("click",function(){
            VideoDealShare.end($("video"));
        })
        //多音频处理
        function audiosInit(){
            var audioList = $(".cont_audio");
            audioList.each(function(){
                var src = $(this).find("source").attr("src");
                $(this).attr("src",src);
                $(this).audioPlayer();
            })
        }
        
    
        
