<!-----------------------以下是原来的部分------------------------------->

<script type="text/javascript">

function initChannelTab(){

    var currentTab = $(".channel_tab>a.on");
    var screenWidth = window.screen.width;//屏幕宽
    var tabWidth = screenWidth/6.5;//栏目快宽 一屏幕显示7个半
    var tab = $(".channel_tab>a");

    //默认滚动
    var currentIndex = 0;
    for(var i = 0;i<tab.length;i++){
        var tabItem = tab.eq(i);
        if(tabItem.hasClass("on")){
            currentIndex = i;
            break;
        }
    }
    var currentLeft = currentIndex*tabWidth;
    if(currentLeft > screenWidth || currentLeft - screenWidth < tabWidth){
        var needScroll = currentLeft - screenWidth + tabWidth + 10;
        $(".channeltab-panel").scrollLeft(needScroll);
    }
    //无滚动不显示过渡层
    if($(".channeltab-panel").scrollLeft() == 0){
        $(".gl").css("opacity","0");
    }
    //设置栏目快宽度使得末尾显示一半
    tab.width(tabWidth);
    var channelWidth = tabWidth * tab.length
    $(".channel_tab").width(channelWidth);
    //滑动事件控制过渡层显示隐藏
    $(".channeltab-panel").on("scroll",function(){
        var scroll = $(this).scrollLeft();
        if(scroll == 0){
            $(".gl").css("opacity","0");
        }else if(scroll == Math.floor(channelWidth - screenWidth)){
            $(".gr").css("opacity","0");
        }else{
            $(".gl").css("opacity","1");
            $(".gr").css("opacity","1");
        }
    })
    $(".channeltab-panel").css("opacity","1");
}
$(window).scroll(function() {
	var st = $(window).scrollTop() || document.documentElement.scrollTop || document.body.scrollTop;
	if (st > 54){
			$(".channeltab-panelbg").addClass("chnfloat");
	}else{
			$(".channeltab-panelbg").removeClass("chnfloat");
	}
});
$(document).ready(function(){

    initChannelTab();

    $(".list_lanmu").click(function(){
        if($(".lanmu_q").css("display")=="none"){
            $(".list_lanmu").addClass("bt_cur");
            $(".lanmu_q").css('display','block');
        }else{
            $(".list_lanmu").removeClass("bt_cur");
            $(".lanmu_q").css('display','none');
        }
    });

    $(".lanmu_shouqi").click(function(){
        $(".list_lanmu").removeClass("bt_cur");
        $(".lanmu_q").css('display','none');
    });

    var tbd=$("#lanmu_tb").filter(function(index) {return $("#lanmu_tb",this).css("display")!="none";});
    var tbdf=$("#lanmu_tb:eq(0)");
    var tbdl=$(".lanmu_tblast");    //var tbdl=$("#lanmu_tb").last();
    $(".lanmu_p").click(function(){
        if(tbd.prev("#lanmu_tb").length>0){
                tbd.css('display','none');
                tbd.prev("#lanmu_tb").css('display','');
                tbd=tbd.prev("#lanmu_tb");
        }else{
                tbd.css('display','none');
                tbdl.css('display','');
                tbd=tbdl;
        }
    });

    $(".lanmu_n").click(function(){
        if(tbd.next("#lanmu_tb").length>0){
                tbd.css('display','none');
                tbd.next("#lanmu_tb").css('display','');
                tbd=tbd.next("#lanmu_tb");
        }else{
                tbd.css('display','none');
                tbdf.css('display','');
                tbd=tbdf;
        }
    });

    $(".list_lm a").click(function(){
        $(this).addClass("on");
        $(this).siblings().removeClass("on");
        var sliderId=$(this).index();
        $("#slider"+sliderId).css("display","block");
        $("#slider"+sliderId).siblings(".sliderchl").css("display","none");
    });

});
</script>
