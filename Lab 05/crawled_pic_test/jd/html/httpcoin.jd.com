<script>
    seajs.use("tools/index", function(index){
         new index.banner({
             parent: $("#homeBanner"),
             activeClass: "current"
         });

         new index.exchange({
             parent: $("#exchange")
         });

        /*new index({
            parent:$("#bankList"),
			rechargeUrl:"/card/beforeCardCipher.html"
        });*/
    });
    $(function() {
        // 协议弹窗
        $('#J_cardFirst').click(function() {
            var signFlag = $("#signFlag").val();
            if(signFlag=='false'){
                jrBase.dialog({
                    element: $("#J_xieyiPop")
                });
            }
        });
        // 默认不选中同意协议
        $('.j_xieyi-check').attr('checked', false);
        // 同意协议按钮可点击
        $('.j_xieyi-check').click(function() {
            $('.j_xieyi-btn').toggleClass('disabled');
        });
        $('.j_xieyi-btn').click(function() {
            if (!$(this).hasClass('disabled')) {
                $.ajax({
                    type: 'post',
                    url: '/card/signProtocol.html',
                    dataType: 'json',
                    success: function (msg) {
                        if (msg.success == true) {
                            $('#J_xieyiPop').find('.closed-btn').click(); //关闭弹窗
                            $("#signFlag").val("true");//是否签署标识
                        } else {
                            $('#J_xieyiPop').find('.closed-btn').click(); //关闭弹窗
                            jrBase.alert({
                                title: "系统提示",
                                msg: "系统错误，请稍后重试！",
                                style: "error",
                                yesStyle: false,
                                yesText: "关闭"
                            });
                        }
                    },
                    error: function (msg) {
                        jrBase.alert({
                            title: "系统提示",
                            msg: "系统错误，请稍后重试！",
                            style: "error",
                            yesStyle: false,
                            yesText: "关闭"
                        });
                    }
                });

            }
        });



        //< xtj
        $(".dialog-expire-sure-btn").click(function(){
            if (!$(this).hasClass('disabled')) {
                signExpireProtocol();
            }
        })

        // 同意协议按钮可点击
        $('.dialog-expire-check').click(function() {
            $('.dialog-expire-sure-btn').toggleClass('disabled');
        });

        //马上去实名
        $(".dialog-expire-confirm-btn-realname").click(function(){
            $(".dialog-expire-confirm").hide();
        })

        // 挥泪清空
        $(".dialog-expire-confirm-btn-clear").click(function(){
            $(".dialog-expire-confirm").hide();
        })

        window.onload = function (ev) {
            var isLogin = "no";
            if(isLogin=="yes"){
                loadExpireInfo();
            }
//            var data = {};
//            window.expireData = {};
//            data.expireAmt="1",
//            data.showExpireFlag=true,
//            window.expireData.data = data || {};
//            showExpireAnnounce(window.expireData);
//            showExpireConfirm(window.expireData);
        }

        function showExpireAnnounce(res) {
            if(!res || !res.data || !res.data.showExpireFlag){
                return;
            }
            var data = res.data;
            var expireAmt = data.expireAmt || '0';
            var expireDate = data.expireDate || '目前';
            $(".dialog-expire-date").text(expireDate);
            $(".dialog-expire-amt").text(expireAmt);
            $(".dialog-expire").show();
        }

        function showExpireConfirm(res) {
            if(!res || !res.data || !res.data.showExpireFlag){
                return;
            }
            var data = res.data;
            var expireAmt = data.expireAmt || '0';
            var expireDate = data.expireDate || '目前';
            $(".dialog-expire-confirm-date").text(expireDate);
            $(".dialog-expire-confirm-amt").text(expireAmt);
            $(".dialog-expire-confirm").show();
        }

        // 加载过期信息
        function loadExpireInfo() {
            var apiUrl = "/expire/expireUserInfo.html"
            $.ajax({
                url: apiUrl,
                type: "GET",
                useDefaultXhrHeader: false,
                contentType: "application/json",
                success: function (data) {
                    console.dir(data);
                    window.expireData = data || {};
                    showExpireAnnounce(window.expireData)
                },
                error: function (e) {
                    console.log(e)
                }
            })
        }

        // 升级签约
        function signExpireProtocol() {
            var apiUrl = "/expire/signExpireProtocol.html"
            $.ajax({
                url: apiUrl,
                type: "GET",
                useDefaultXhrHeader: false,
                contentType: "application/json",
                data:{
                    bindSource: "1"//PC
                },
                success: function (data) {
                    $(".dialog-expire").hide();
                    if (!data || !data.success) {
                        if(data.resultCode=='202'){
                            window.jrBase.alert({
                                title: "系统提示",
                                msg: "您已实名,无需签署!",
                                style: "error",
                                yesStyle: false,
                                yesText: "关闭"
                            });
                        }else if(data.resultCode=='0018'){
                            window.jrBase.alert({
                                title: "系统提示",
                                msg: "您已签署协议!",
                                style: "error",
                                yesStyle: false,
                                yesText: "关闭"
                            });
                        }else{
                            window.jrBase.alert({
                                title: "系统提示",
                                msg: "签约失败,请联系管理员!",
                                style: "error",
                                yesStyle: false,
                                yesText: "关闭"
                            });
                        }
                    }else{
                        window.jrBase.alert({
                            title: "系统提示",
                            msg: "您已签署协议成功!",
                            style: "error",
                            yesStyle: false,
                            yesText: "关闭"
                        });
                    }
                },
                error: function (e) {
                    console.log(e);
                    jrBase.alert({
                        title: "系统提示",
                        msg: "系统错误，请稍后重试！",
                        style: "error",
                        yesStyle: false,
                        yesText: "关闭"
                    });
                }
            })
        }

        //> xtj
    });
</script>

